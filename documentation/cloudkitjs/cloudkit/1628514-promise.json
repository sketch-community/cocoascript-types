          "inlineContent": [
            {
              "type": "text",
              "text": "The asynchronous methods in CloudKit JS return a "
            },
            {
              "type": "codeVoice",
              "code": "Promise"
            },
            {
              "type": "text",
              "text": " object that resolves when the operation completes or is rejected due to an error. For a description of the "
            },
            {
              "type": "codeVoice",
              "code": "Promise"
            },
            {
              "type": "text",
              "text": " class returned by these methods, go to Mozilla Developer Network: Promise."
            }
          ]
        }
