{
  "abstract": [
    {
      "type": "text",
      "text": "Information about when the record was last modified. The properties of this object are: "
    },
    {
      "type": "codeVoice",
      "code": "timestamp"
    },
    {
      "type": "text",
      "text": " ("
    },
    {
      "type": "codeVoice",
      "code": "Number"
    },
    {
      "type": "text",
      "text": "), the time at which the record was created, and "
    },
    {
      "type": "codeVoice",
      "code": "user"
    },
    {
      "type": "text",
      "text": " ("
    },
    {
      "type": "codeVoice",
      "code": "String"
    },
    {
      "type": "text",
      "text": "), the ID of the user who modified the record. This field is used by the "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/recordsresponse"
    },
    {
      "type": "text",
      "text": " class. This value of this field is set by the server. Omit this key when saving a record. "
    }
  ],
