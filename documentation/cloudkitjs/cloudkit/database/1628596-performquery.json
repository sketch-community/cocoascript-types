    {
      "kind": "parameters",
      "languages": [
        "data"
      ],
      "parameters": [
        {
          "name": "query",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Either a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/query"
                },
                {
                  "type": "text",
                  "text": " dictionary or a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/queryresponse"
                },
                {
                  "type": "text",
                  "text": " object describing the matching criteria."
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary containing options to use when fetching records. Possible dictionary keys are:"
                }
              ]
            },
            {
              "type": "table",
              "header": "row",
              "rows": [
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Key"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Description"
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "zoneID"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "A "
                        },
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/zoneid"
                        },
                        {
                          "type": "text",
                          "text": " or zone name ("
                        },
                        {
                          "type": "codeVoice",
                          "code": "String"
                        },
                        {
                          "type": "text",
                          "text": ") that identifies the record zone in the database where you want to perform the operation. The default is the database default zone."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "resultsLimit"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "The maximum number of records to fetch. The default is the maximum number of records in a response that is allowed, described in "
                        },
                        {
                          "type": "link",
                          "title": "Data Size Limits",
                          "destination": "https://developer.apple.com/library/archive/documentation/DataManagement/Conceptual/CloudKitWebServicesReference/PropertyMetrics.html#//apple_ref/doc/uid/TP40015240-CH23"
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "continuationMarker"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Marks the location of the last batch of results. Use this key when the results of a previous fetch exceed the maximum. The default value is "
                        },
                        {
                          "type": "codeVoice",
                          "code": "null"
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "desiredKeys"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "An array of strings containing record field names that limits the amount of data returned in this operation. Only the fields specified in the array are returned. The default is "
                        },
                        {
                          "type": "codeVoice",
                          "code": "null"
                        },
                        {
                          "type": "text",
                          "text": ", which fetches all record fields."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "zoneWide"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Boolean value determining wether all zones should be searched. This key is ignored if "
                        },
                        {
                          "type": "codeVoice",
                          "code": "zoneID"
                        },
                        {
                          "type": "text",
                          "text": " is non-"
                        },
                        {
                          "type": "codeVoice",
                          "code": "null"
                        },
                        {
                          "type": "text",
                          "text": ". To search all zones, set to "
                        },
                        {
                          "type": "codeVoice",
                          "code": "true"
                        },
                        {
                          "type": "text",
                          "text": ". To search the default zone only, set to "
                        },
                        {
                          "type": "codeVoice",
                          "code": "false"
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "numbersAsStrings"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "A Boolean value indicating whether number fields should be represented by strings. The default value is "
                        },
                        {
                          "type": "codeVoice",
                          "code": "false"
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                ]
              ],
              "metadata": {
                "anchor": "2556239",
                "title": "Table 1"
              }
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A "
            },
            {
              "type": "codeVoice",
              "code": "Promise"
            },
            {
              "type": "text",
              "text": " object that resolves to a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/queryresponse"
            },
            {
              "type": "text",
              "text": " object if the operation succeeds; otherwise, a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/ckerror"
            },
            {
              "type": "text",
              "text": " object."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To fetch records that match some criteria, create a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/query"
            },
            {
              "type": "text",
              "text": " dictionary."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "var query = {",
            "    recordType: 'Artwork',",
            "    filterBy: [{",
            "        comparator: 'EQUALS',",
            "        fieldName: 'address',",
            "        fieldValue: { value: 'Fort Bragg, CA' }",
            "    }]",
            "}"
          ],
          "syntax": "javascript",
          "metadata": {
            "anchor": "2556241",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Execute the query using the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/database/1628596-performquery"
            },
            {
              "type": "text",
              "text": " method. To get the records that were fetched, use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/recordsresponse/1628501-records"
            },
            {
              "type": "text",
              "text": " property in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/queryresponse"
            },
            {
              "type": "text",
              "text": " class. Otherwise, handle any errors appropriately."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "database.performQuery(query).then(function(response) {",
            "    if (response.hasErrors) {",
            "        // Insert error handling",
            "        throw response.errors[0];",
            "    } else {",
            "        // Insert successfully fetched record code",
            "    }",
            "});"
          ],
          "syntax": "javascript",
          "metadata": {
            "anchor": "2556244",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "inlineHead",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Creating Queries That Search by Location"
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can fetch records whose "
            },
            {
              "type": "codeVoice",
              "code": "Location"
            },
            {
              "type": "text",
              "text": " field lies within a circular region by specifying the center and radius of a circle in a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/query"
            },
            {
              "type": "text",
              "text": " dictionary. The "
            },
            {
              "type": "codeVoice",
              "code": "Location"
            },
            {
              "type": "text",
              "text": " field specified in the query must be indexed for the fetch to succeed."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "var query = {",
            "    recordType: 'Artwork',",
            "    filterBy: [{",
            "        fieldName: 'location',",
            "        distance: 100000,",
            "        fieldValue: {",
            "           latitude: 37.7749300,",
            "           longitude: -122.4194200",
            "       }",
            "    }]",
            "};"
          ],
          "syntax": "javascript",
          "metadata": {
            "anchor": "2556247",
            "title": "Listing 3"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Execute this query using the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/database/1628596-performquery"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "inlineHead",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Handling Large Numbers of Results"
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the results of a fetch exceeds the maximum number of records allowed in a response, perform the fetch again until all the records are fetched. You can also specify the results limit when performing a query."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To set a results limit, pass a dictionary as the "
            },
            {
              "type": "codeVoice",
              "code": "options"
            },
            {
              "type": "text",
              "text": " parameter to the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/database/1628596-performquery"
            },
            {
              "type": "text",
              "text": " method. In the dictionary, set the "
            },
            {
              "type": "codeVoice",
              "code": "resultsLimit"
            },
            {
              "type": "text",
              "text": " key to the number of records you want to fetch for each operation. If the "
            },
            {
              "type": "codeVoice",
              "code": "moreComing"
            },
            {
              "type": "text",
              "text": " property of the response object is "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": ", the results exceeded the limit and you should perform the fetch operation again until "
            },
            {
              "type": "codeVoice",
              "code": "moreComing"
            },
            {
              "type": "text",
              "text": " is false. The next time you perform the operation, pass the response object to the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/cloudkitjs/cloudkit/database/1628596-performquery"
            },
            {
              "type": "text",
              "text": " method. The response object contains the fetch options you passed the first time you called the method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, perform a query limiting the results to 10 records until all records matching the query are fetched. "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "database.performQuery(query, { resultsLimit: 10 }).then(function(response) {",
            "    if (response.hasErrors) {",
            "        // Insert error handling",
            "        throw response.errors[0];",
            "    } else {",
            "        // Insert successfully fetched record code",
            "        if(response.moreComing) {",
            "            return database.performQuery(response);",
            "        }",
            "    }",
            "});"
          ],
          "syntax": "javascript",
          "metadata": {
            "anchor": "2556250",
            "title": "Listing 4"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For the maximum number of records in a response and other data limits, see "
            },
            {
              "type": "link",
              "title": "Data Size Limits",
              "destination": "https://developer.apple.com/library/archive/documentation/DataManagement/Conceptual/CloudKitWebServicesReference/PropertyMetrics.html#//apple_ref/doc/uid/TP40015240-CH23"
            },
            {
              "type": "text",
              "text": " in "
            },
            {
              "type": "link",
              "title": "CloudKit Web Services Reference",
              "destination": "https://developer.apple.com/library/archive/documentation/DataManagement/Conceptual/CloudKitWebServicesReference/index.html#//apple_ref/doc/uid/TP40015240"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
