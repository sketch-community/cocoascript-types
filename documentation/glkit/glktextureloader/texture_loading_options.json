    "doc://com.apple.documentation/documentation/glkit/glktextureloaderapplypremultiplication": {
      "title": "GLKTextureLoaderApplyPremultiplication",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/glkit/glktextureloaderapplypremultiplication",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/glkit/glktextureloaderapplypremultiplication",
      "abstract": [
        {
          "type": "text",
          "text": "This key specifies whether the data in the image should be premultiplied before being loaded into the sharegroup. The value for this key is an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
        },
        {
          "type": "text",
          "text": " object that specifies a boolean value. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ", the data is loaded into the sharegroup without being modified. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": ", the red, green and blue components of each pixel are multiplied by the alpha value. If the key is not specified, the default value is "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ". Never specify "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": " for a texture that is in a compressed format."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/glkit/glktextureloadergeneratemipmaps": {
      "title": "GLKTextureLoaderGenerateMipmaps",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/glkit/glktextureloadergeneratemipmaps",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/glkit/glktextureloadergeneratemipmaps",
      "abstract": [
        {
          "type": "text",
          "text": "This key specifies whether mipmaps should be created for the texture. The value for this key is an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
        },
        {
          "type": "text",
          "text": " object that specifies a boolean value. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ", only the texture is loaded. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": ", a full set of mipmap levels are generated for the texture when the texture is created. The "
        },
        {
          "type": "codeVoice",
          "code": "GL_TEXTURE_MIN_FILTER"
        },
        {
          "type": "text",
          "text": " parameter for the texture is changed to "
        },
        {
          "type": "codeVoice",
          "code": "GL_LINEAR_MIPMAP_LINEAR"
        },
        {
          "type": "text",
          "text": " when mipmaps are generated. If the key is not specified, the default value is "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/glkit/glktextureloaderoriginbottomleft": {
      "title": "GLKTextureLoaderOriginBottomLeft",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/glkit/glktextureloaderoriginbottomleft",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/glkit/glktextureloaderoriginbottomleft",
      "abstract": [
        {
          "type": "text",
          "text": "This key specifies whether the image data should be vertically flipped to match OpenGLâ€™s coordinate system. The value for this key is an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
        },
        {
          "type": "text",
          "text": " object that specifies a boolean value. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ", the image data is not flipped. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": ", the image data is flipped before being loaded. If the key is not specified, the default value is "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/glkit/glktextureloadergrayscaleasalpha": {
      "title": "GLKTextureLoaderGrayscaleAsAlpha",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/glkit/glktextureloadergrayscaleasalpha",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/glkit/glktextureloadergrayscaleasalpha",
      "abstract": [
        {
          "type": "text",
          "text": "This key specifies whether the image data in a grayscale image should be treated as alpha information. The value for this key is an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
        },
        {
          "type": "text",
          "text": " object that specifies a boolean value. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ", the image data is treated as luminance data. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": ", the image data is treated as alpha data. The default value is "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ". This key is ignored if the source image is not a grayscale image."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/glkit/glktextureloadersrgb": {
      "title": "GLKTextureLoaderSRGB",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/glkit/glktextureloadersrgb",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/glkit/glktextureloadersrgb",
      "abstract": [
        {
          "type": "text",
          "text": "This key specifies whether the image data in the texture should be treated as sRGB data. The value for this key is an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
        },
        {
          "type": "text",
          "text": " object that specifies a boolean value. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": ", the image data is treated as linear pixel data. If "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": ", the image data is treated as sRGB pixel data. The default value is "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": "."
        }
