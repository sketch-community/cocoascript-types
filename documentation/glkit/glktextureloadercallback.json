          "inlineContent": [
            {
              "type": "text",
              "text": "The block parameters are defined as follows:"
            }
          ]
        },
        {
          "type": "termList",
          "items": [
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "emphasis",
