      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the value of this property is "
            },
            {
              "type": "codeVoice",
              "code": "GL_TRUE"
            },
            {
              "type": "text",
              "text": ", then fog calculations are performed each time a fragment is computed. If the value of this property is "
            },
            {
              "type": "codeVoice",
              "code": "GL_FALSE"
            },
            {
              "type": "text",
              "text": ", then fog calculations are skipped. The default value is "
            },
            {
              "type": "codeVoice",
              "code": "GL_TRUE"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
