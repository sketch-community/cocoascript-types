            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/security/1387147-cmsencode"
            },
            {
              "type": "text",
              "text": " function alone instead of this sequence of encoder functions."
            }
          ]
        }
