    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "keychainOrArray",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A keychain object for a single keychain to search, an array of keychain objects for a set of keychains to search, or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to search the userâ€™s default keychain search list."
                }
              ]
            }
          ]
        },
        {
          "name": "keyUsage",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A CSSM key use value as defined in "
                },
                {
                  "type": "codeVoice",
                  "code": "Security.framework/cssmtype.h"
                },
                {
                  "type": "text",
                  "text": ". (Note that, because key recovery is not implemented, the "
                },
                {
                  "type": "codeVoice",
                  "code": "SIGN_RECOVER"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "VERIFY_RECOVER"
                },
                {
                  "type": "text",
                  "text": " constants are not supported.) Use this parameter to filter the search by specifying the key use for the identity. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " if you want all identities returned by this search. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "CSSM_KEYUSE_ANY"
                },
                {
                  "type": "text",
                  "text": " to limit the identities returned to those that can be used for every operation."
                }
              ]
            }
          ]
        },
        {
          "name": "searchRef",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to the identity search object. Call the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are done with it."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code. See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1542001-security_framework_result_codes"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can OR "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYUSE"
            },
            {
              "type": "text",
              "text": " values together to set more than one value for key use. Use the returned search object in calls to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1396825-secidentitysearchcopynext"
            },
            {
              "type": "text",
              "text": " function to obtain identities that match the search criteria. "
            }
          ]
        }
      ]
    }
