          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to the data read. You must allocate this buffer before calling the function. The size of this buffer must be equal to or greater than the value in the "
                },
                {
                  "type": "codeVoice",
                  "code": "dataLength"
                },
                {
                  "type": "text",
                  "text": " parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "dataLength",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The amount of data you would like to read. "
                }
              ]
            }
          ]
        },
        {
          "name": "processed",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to the number of bytes actually read."
                }
              ]
            }
          ]
        }
      ]
    },
