          "tokens": [
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@CSSM_KEYATTR_FLAGS",
              "identifier": "symbol-1396959",
              "text": "CSSM_KEYATTR_FLAGS"
            },
            {
              "kind": "text",
              "text": " "
            },
            {
              "kind": "identifier",
              "text": "keyAttributes"
            },
            {
              "kind": "text",
              "text": ";"
            }
          ],
          "platforms": [
            "macOS"
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value is "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_SENSITIVE"
            },
            {
              "type": "codeVoice",
              "code": " | "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_EXTRACTABLE"
            },
            {
              "type": "text",
              "text": "; the "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_PERMANENT"
            },
            {
              "type": "text",
              "text": " bit is also added to the default if a non-NULL value is specified for the "
            },
            {
              "type": "codeVoice",
              "code": "importKeychain"
            },
            {
              "type": "text",
              "text": " parameter. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following are valid values for these flags: "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_PERMANENT"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_SENSITIVE"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_EXTRACTABLE"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_PERMANENT"
            },
            {
              "type": "text",
              "text": " bit is set, the "
            },
            {
              "type": "codeVoice",
              "code": "importKeychain"
            },
            {
              "type": "text",
              "text": " parameter is not valid, and if any keys are found in the external representation, then the error "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1542001-security_framework_result_codes/errsecinvalidkeychain"
            },
            {
              "type": "text",
              "text": " is returned."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_SENSITIVE"
            },
            {
              "type": "text",
              "text": " bit indicates that the key can only be extracted in wrapped form."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If you do not set the "
                },
                {
                  "type": "codeVoice",
                  "code": "CSSM_KEYATTR_EXTRACTABLE"
                },
                {
                  "type": "text",
                  "text": " bit, you cannot extract the imported key from the keychain in any form, including in wrapped form."
                }
              ]
            }
          ],
          "style": "important"
        },
        {
          "type": "paragraph",
          "inlineContent": []
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_FLAGS"
            },
            {
              "type": "text",
              "text": " enumeration is defined in "
            },
            {
              "type": "codeVoice",
              "code": "cssmtype.h"
            },
            {
              "type": "text",
              "text": ". Note that the "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_RETURN_xxx"
            },
            {
              "type": "text",
              "text": " bits are always forced to "
            },
            {
              "type": "codeVoice",
              "code": "CSSM_KEYATTR_RETURN_REF"
            },
            {
              "type": "text",
              "text": " regardless of how they are specified in the "
            },
            {
              "type": "codeVoice",
              "code": "keyAttributes"
            },
            {
              "type": "text",
              "text": " field."
            }
          ]
        }
      ]
    }
  ],
