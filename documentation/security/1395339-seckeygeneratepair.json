    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "parameters",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary of key-value pairs that specify the type of keys to be generated."
                }
              ]
            }
          ]
        },
        {
          "name": "publicKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to the keychain item object of the new public key. Call the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are finished with it."
                }
              ]
            }
          ]
        },
        {
          "name": "privateKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to the keychain item object of the new private key. Call the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are finished with it."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code. See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1542001-security_framework_result_codes"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In order to generate a key pair, the dictionary passed in the "
            },
            {
              "type": "codeVoice",
              "code": "parameters"
            },
            {
              "type": "text",
              "text": " parameter must contain at least the following key-value pairs:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecattrkeytype"
                    },
                    {
                      "type": "text",
                      "text": " key with a value of any key type defined in "
                    },
                    {
                      "type": "codeVoice",
                      "code": "SecItem.h"
                    },
                    {
                      "type": "text",
                      "text": " (see "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/keychain_services"
                    },
                    {
                      "type": "text",
                      "text": "), for example, "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecattrkeytypersa"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecattrkeysizeinbits"
                    },
                    {
                      "type": "text",
                      "text": " key with a value specifying the requested key size in bits. This can be specified as either a "
                    },
                    {
                      "type": "codeVoice",
                      "code": "CFNumberRef"
                    },
                    {
                      "type": "text",
                      "text": " or "
                    },
                    {
                      "type": "codeVoice",
                      "code": "CFStringRef"
                    },
                    {
                      "type": "text",
                      "text": " value. For example, RSA keys may have key size values of 512, 768, 1024, or 2048."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In addition, you can specify a number of other optional attributes for the public and private keys. The way you do this depends on whether you are writing code for macOS or iOS:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "In macOS, add the key-value pairs to the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "parameters"
                    },
                    {
                      "type": "text",
                      "text": " dictionary directly. The specified attributes are applied to both the public and private keys."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "In iOS, add dictionaries for the keys "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecpublickeyattrs"
                    },
                    {
                      "type": "text",
                      "text": " and "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecprivatekeyattrs"
                    },
                    {
                      "type": "text",
                      "text": " to the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "parameters"
                    },
                    {
                      "type": "text",
                      "text": " dictionary, and provide the attributes in those dictionaries. The attributes specified in these dictionaries are added to either the public or private key, respectively, allowing you to apply separate attributes to each key."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The possible attributes are as follows; for details on each attribute, see "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/keychain_services"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecattrlabel"
                    },
                    {
                      "type": "text",
                      "text": "—Default "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NULL"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/security/ksecattrispermanent"
                    },
                    {
                      "type": "text",
                      "text": "—If this key is present and has a Boolean value of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "true"
                    },
                    {
                      "type": "text",
