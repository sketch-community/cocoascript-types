    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An array, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if the trust object has not yet been evaluated. Call the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease"
            },
            {
              "type": "text",
              "text": " function to free this array's memory when you are done with it."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The result is an ordered array of dictionaries, one per certificate in the chain, beginning with the leaf node at index zero ("
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": ") and continuing up to the anchor (or the last certificate in the chain if no anchor was found)."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The property dictionary at index zero may also include general information about the entire chain's validity in the context of this trust evaluation. See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/certificate_key_and_trust_services/certificates/certificate_property_type_values"
            },
            {
              "type": "text",
              "text": " for a list of currently defined keys."
            }
          ]
        }
      ]
    }
