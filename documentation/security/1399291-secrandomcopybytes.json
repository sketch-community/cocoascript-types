          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of random bytes to return in the array pointed to by the "
                },
                {
                  "type": "codeVoice",
                  "code": "bytes"
                },
                {
                  "type": "text",
                  "text": " parameter. "
                }
              ]
            }
          ]
        },
        {
          "name": "bytes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an array that the function fills with cryptographically secure random bytes. Use an array that is large enough to hold at least "
                },
                {
                  "type": "codeVoice",
                  "code": "count"
                },
                {
                  "type": "text",
                  "text": " bytes."
                }
              ]
            }
          ]
