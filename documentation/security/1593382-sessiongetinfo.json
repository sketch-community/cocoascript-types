      "parameters": [
        {
          "name": "session",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The session you are asking about. You can use one of the special sessions given in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1593391-session_id_values"
                },
                {
                  "type": "text",
                  "text": ", for example to ask about your own session."
                }
              ]
            }
          ]
        },
        {
          "name": "sessionId",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/securitysessionid"
                },
                {
                  "type": "text",
                  "text": " value that the function populates with the actual session ID for the session you asked about. This value will not be one of the special values from "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1593391-session_id_values"
                },
                {
                  "type": "text",
                  "text": ", but will instead be an actual session ID."
                }
              ]
            }
          ]
        },
        {
          "name": "attributes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/sessionattributebits"
                },
                {
                  "type": "text",
                  "text": " structure that the function fills with the attribute bits for the session."
                }
              ]
            }
          ]
        }
      ]
    },
