    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can ask about any session whose identifier you know. Use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1593391-session_id_values/callersecuritysession"
            },
            {
              "type": "text",
              "text": " constant to ask about your own session (the one your process is in)."
            }
          ]
        }
      ]
