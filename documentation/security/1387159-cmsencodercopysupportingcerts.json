        {
          "name": "cmsEncoder",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The CMSEncoder reference returned by the "
                },
                {
                  "type": "codeVoice",
                  "code": "CMSEncoderCreate"
                },
                {
                  "type": "text",
                  "text": " function."
                }
              ]
            }
          ]
        },
        {
          "name": "certsOut",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to a CFArray of "
                },
                {
                  "type": "codeVoice",
                  "code": "SecCertificateRef"
                },
                {
                  "type": "text",
                  "text": " objects. You must use the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFRelease"
                },
                {
                  "type": "text",
                  "text": " function to free this reference when you are finished using it. "
                }
              ]
            }
          ]
        }
      ]
