          "inlineContent": [
            {
              "type": "text",
              "text": "A "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/secgrouptransformref"
            },
            {
              "type": "text",
              "text": " object that you can use for chaining calls to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/security/1401298-sectransformconnecttransforms"
            },
            {
              "type": "text",
