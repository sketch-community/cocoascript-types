          "inlineContent": [
            {
              "type": "text",
              "text": "A "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seccertificateref"
            },
            {
              "type": "text",
              "text": " object for a certificate that is stored in a keychain can be safely cast to a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seckeychainitemref"
            },
            {
              "type": "text",
              "text": " for manipulation as a keychain item. On the other hand, if the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seccertificateref"
            },
            {
              "type": "text",
              "text": " is not stored in a keychain, casting the object to a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seckeychainitemref"
            },
            {
              "type": "text",
              "text": " and passing it to Keychain Services functions returns errors."
            }
          ]
        }
