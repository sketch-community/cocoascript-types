          "inlineContent": [
            {
              "type": "text",
              "text": "Specifies if an attribute must have a non "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " value set or have an incoming connection before the transform starts to execute. This metadata has a default value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " for the input attribute, but "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " for all other attributes."
            }
          ]
        }
      ]
    }
