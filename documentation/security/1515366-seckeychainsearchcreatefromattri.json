      "parameters": [
        {
          "name": "keychainOrArray",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A reference to an array of keychains to search, a single keychain, or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to search the user’s current keychain search list. Use the function "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1402440-seckeychaincopysearchlist"
                },
                {
                  "type": "text",
                  "text": " to retrieve the user’s default search list."
                }
              ]
            }
          ]
        },
        {
          "name": "itemClass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The keychain item class. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/secitemclass"
                },
                {
                  "type": "text",
                  "text": " for valid constants."
                }
              ]
            }
          ]
        },
        {
          "name": "attrList",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a list of zero or more keychain attribute records to match. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to match any keychain attribute."
                }
              ]
            }
          ]
        },
        {
          "name": "searchRef",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, a pointer to the current search object. You must call the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are finished using it."
                }
              ]
            }
          ]
        }
      ]
