          "name": "cmsDecoder",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The CMSDecoder reference returned by the "
                },
                {
                  "type": "codeVoice",
                  "code": "CMSDecoderCreate"
                },
                {
                  "type": "text",
                  "text": " function."
                }
              ]
            }
          ]
        },
        {
          "name": "certsOut",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to an array of "
                },
                {
                  "type": "codeVoice",
                  "code": "SecCertificateRef"
                },
                {
                  "type": "text",
                  "text": " objects. Returns "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if the message does not contain any certificates (the message was encrypted but not signed); this is not considered an error. You must use the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFRelease"
                },
                {
                  "type": "text",
                  "text": " function to free this reference when you are finished using it."
                }
              ]
            }
          ]
        }
