          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A reference to the keychain item from which you wish to retrieve data or attributes."
                }
              ]
            }
          ]
        },
        {
          "name": "info",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a list of tags and formats of attributes to retrieve. You can call "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1392372-seckeychainattributeinfoforitemi"
                },
                {
                  "type": "text",
                  "text": " to obtain a list of all possible attribute tags and formats for the item's class. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you don’t wish to retrieve any attributes."
                }
              ]
            }
          ]
        },
        {
          "name": "itemClass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, the item’s class. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if not required. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/secitemclass"
                },
                {
                  "type": "text",
                  "text": " for valid constants."
                }
              ]
            }
          ]
        },
        {
          "name": "attrList",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, the retrieved attributes and their values .  Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if not required. You must call the function "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1397736-seckeychainitemfreeattributesand"
                },
                {
                  "type": "text",
                  "text": " when you no longer need the attributes and values."
                }
              ]
            }
          ]
        },
        {
          "name": "length",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, the actual length of the data returned in the "
                },
                {
                  "type": "codeVoice",
                  "code": "outData"
                },
                {
                  "type": "text",
                  "text": " parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "outData",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, the data in this item. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if not required. You must call the function "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1397736-seckeychainitemfreeattributesand"
                },
                {
                  "type": "text",
                  "text": " when you no longer need the data."
                }
              ]
            }
          ]
        }
