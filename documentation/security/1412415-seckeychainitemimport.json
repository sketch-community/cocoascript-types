      "parameters": [
        {
          "name": "importedData",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The external representation of the items to import."
                }
              ]
            }
          ]
        },
        {
          "name": "fileNameOrExtension",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name or extension of the file from which the external representation was obtained. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you don’t know the name or extension."
                }
              ]
            }
          ]
        },
        {
          "name": "inputFormat",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On entry, points to the format of the external representation. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "kSecFormatUnknown"
                },
                {
                  "type": "text",
                  "text": " if you do not know the exact format. On return, points to the format that the function has determined the external representation to be in. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you don’t know the format and don’t want the format returned to you. For a list of formats, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/secexternalformat"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "itemType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On entry, points to the item type of the item or items contained in the external representation. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/secexternalitemtype/ksecitemtypeunknown"
                },
                {
                  "type": "text",
                  "text": " if you do not know the item type. On return, points to the item type that the function has determined the external representation to contain. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you don’t know the item type and don’t want the type returned to you."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Unused; pass in "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "keyParams",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a structure containing a set of input parameters for the function. If no key items are being imported, these parameters are optional and you can set the "
                },
                {
                  "type": "codeVoice",
                  "code": "keyParams"
                },
                {
                  "type": "text",
                  "text": " parameter to "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". If you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " for the "
                },
                {
                  "type": "codeVoice",
                  "code": "importKeychain"
                },
                {
                  "type": "text",
                  "text": " parameter, the "
                },
                {
                  "type": "codeVoice",
                  "code": "kSecKeyImportOnlyOne"
                },
                {
                  "type": "text",
                  "text": " bit in the "
                },
                {
                  "type": "codeVoice",
                  "code": "flags"
                },
                {
                  "type": "text",
                  "text": " field of the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/seckeyimportexportparameters"
                },
                {
                  "type": "text",
                  "text": " structure is ignored. Otherwise, if the "
                },
                {
                  "type": "codeVoice",
                  "code": "kSecKeyImportOnlyOne"
                },
                {
                  "type": "text",
                  "text": " bit is set and there is more than one private key in the incoming external representation, no items are imported to the specified keychain and the error "
                },
                {
                  "type": "codeVoice",
                  "code": "errSecMultiplePrivKeys"
                },
                {
                  "type": "text",
                  "text": " is returned. The possible values for the "
                },
                {
                  "type": "codeVoice",
                  "code": "flags"
                },
                {
                  "type": "text",
                  "text": " field are described in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/seckeyimportexportflags"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "importKeychain",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A keychain object indicating the keychain to which the key or certificate should be imported. If you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", the item is not imported. Use the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1400743-seckeychaincopydefault"
                },
                {
                  "type": "text",
                  "text": " function to get a reference to the default keychain. "
                }
              ]
            }
          ]
        },
        {
          "name": "outItems",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, points to an array of "
                },
                {
                  "type": "codeVoice",
                  "code": "SecKeychainItemRef"
                },
                {
                  "type": "text",
                  "text": " objects for the imported items. You must provide a valid pointer to a "
                },
                {
                  "type": "codeVoice",
                  "code": "CFArrayRef"
                },
                {
                  "type": "text",
                  "text": " object to receive this information. If you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " for this parameter, the function does not return the imported items. You must call the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFRelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are finished using it."
                }
              ]
            }
          ]
        }
      ]
    },
