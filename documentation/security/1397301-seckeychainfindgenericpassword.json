          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The length of the "
                },
                {
                  "type": "codeVoice",
                  "code": "serviceName"
                },
                {
                  "type": "text",
                  "text": " character string."
                }
              ]
            }
          ]
        },
