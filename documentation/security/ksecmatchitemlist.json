    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To provide your own set of items to be filtered by a search query rather than searching the keychain, specify this search key in a call to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1398306-secitemcopymatching"
            },
            {
              "type": "text",
              "text": " function with a value that consists of an object of type "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfarrayref"
            },
            {
              "type": "text",
              "text": " where the array contains either "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seckeychainitemref"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seckeyref"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/seccertificateref"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/secidentityref"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfdataref"
            },
            {
              "type": "text",
              "text": " items. The objects in the provided array must all be of the same type. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To convert from persistent item references to normal item references, specify this search key in a call to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1398306-secitemcopymatching"
            },
            {
              "type": "text",
              "text": " function with a value of type "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfarrayref"
            },
            {
              "type": "text",
              "text": " where the array contains one or more "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfdataref"
            },
            {
              "type": "text",
              "text": " elements (the persistent references), and a return-type key of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksecreturnref"
            },
            {
              "type": "text",
              "text": " whose value is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfbooleantrue"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To delete an item identified by a transient reference, specify the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksecmatchitemlist"
            },
            {
              "type": "text",
              "text": " search key in a call to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1395547-secitemdelete"
            },
            {
              "type": "text",
              "text": " function with a reference returned by using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksecreturnref"
            },
            {
              "type": "text",
              "text": " return type key in a previous call to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1398306-secitemcopymatching"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1401659-secitemadd"
            },
            {
              "type": "text",
              "text": " functions."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To delete an item identified by a persistent reference, specify the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksecmatchitemlist"
            },
            {
              "type": "text",
              "text": " search key in a call to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1395547-secitemdelete"
            },
            {
              "type": "text",
              "text": " function with a persistent reference returned by using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksecreturnpersistentref"
            },
            {
              "type": "text",
              "text": " return type key to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1398306-secitemcopymatching"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1401659-secitemadd"
            },
            {
              "type": "text",
              "text": " functions."
            }
          ]
        }
      ]
    }
