    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Each item stored in the keychain contains data (such as a certificate), which is indexed by the itemâ€™s attributes. Use this function to create a keychain item from its attributes and data. To create keychain items that hold passwords, use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1393322-seckeychainaddinternetpassword"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1398366-seckeychainaddgenericpassword"
            },
            {
              "type": "text",
              "text": " functions."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A "
            },
            {
              "type": "codeVoice",
              "code": "SecKeychainItemRef"
            },
            {
              "type": "text",
              "text": " object for a certificate that is stored in a keychain can be safely cast to a "
            },
            {
              "type": "codeVoice",
              "code": "SecCertificateRef"
            },
            {
              "type": "text",
              "text": " for use with the Certificate, Key, and Trust API."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function automatically calls the function "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1400341-seckeychainunlock"
            },
            {
              "type": "text",
              "text": " to display the Unlock Keychain dialog box if the keychain is currently locked."
            }
          ]
        }
      ]
    }
