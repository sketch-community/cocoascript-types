              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A constant identifying the class of item to create. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/secitemclass"
                },
                {
                  "type": "text",
                  "text": " for valid constants."
                }
              ]
            }
          ]
        },
        {
          "name": "attrList",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the list of attributes for the item to create."
                }
              ]
            }
          ]
        },
        {
          "name": "length",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The length of the buffer pointed to by the "
                },
                {
                  "type": "codeVoice",
                  "code": "data"
                },
                {
                  "type": "text",
                  "text": " parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a buffer containing the data to store."
                }
              ]
            }
          ]
        },
        {
          "name": "keychainRef",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A reference to the keychain in which to add the item. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to specify the default keychain."
                }
              ]
            }
          ]
        },
        {
          "name": "initialAccess",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An access object for this keychain item. Use the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1393522-secaccesscreate"
                },
                {
                  "type": "text",
                  "text": " function to create an access object or the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/1397585-seckeychainitemcopyaccess"
                },
                {
                  "type": "text",
                  "text": " function to copy an access object from another keychain item. If you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " for this parameter, the access defaults to the application creating the item. "
                }
              ]
            }
          ]
        },
        {
          "name": "itemRef",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, a pointer to a reference to the newly created keychain item. This parameter is optional. You must call the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFRelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are finished using it."
                }
              ]
            }
          ]
        }
      ]
