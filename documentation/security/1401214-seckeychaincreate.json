        {
          "name": "pathName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A constant character string representing the POSIX path indicating where to store the keychain."
                }
              ]
            }
          ]
        },
        {
          "name": "passwordLength",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An unsigned 32-bit integer representing the length of the buffer pointed to by "
                },
                {
                  "type": "codeVoice",
                  "code": "password"
                },
                {
                  "type": "text",
                  "text": ". Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " if the value of "
                },
                {
                  "type": "codeVoice",
                  "code": "password"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " and the value of "
                },
                {
                  "type": "codeVoice",
                  "code": "promptUser"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "password",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the buffer containing the password which is used to protect the new keychain. The password must be in canonical UTF-8 encoding. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if the value of "
                },
                {
                  "type": "codeVoice",
                  "code": "passwordLength"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " and the value of "
                },
                {
                  "type": "codeVoice",
                  "code": "promptUser"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "promptUser",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value representing whether to display a password dialog to the user. Set this value to "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": " to display a password dialog or "
                },
                {
                  "type": "codeVoice",
                  "code": "FALSE"
                },
                {
                  "type": "text",
                  "text": " otherwise. If you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": ", any values passed for "
                },
                {
                  "type": "codeVoice",
                  "code": "passwordLength"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "password"
                },
                {
                  "type": "text",
                  "text": " are ignored, and a dialog for the user to enter a password is presented."
                }
              ]
            }
          ]
        },
        {
          "name": "initialAccess",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Ignored. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " for this parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "keychain",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, a pointer to a keychain object. You must call the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFRelease"
                },
                {
                  "type": "text",
                  "text": " function to release this object when you are finished using it."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code. See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1542001-security_framework_result_codes"
            },
            {
              "type": "text",
