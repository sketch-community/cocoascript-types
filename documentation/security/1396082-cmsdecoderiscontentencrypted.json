    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Note that if the message was encrypted and the decoding succeeded ("
            },
            {
              "type": "codeVoice",
              "code": "CMSDecoderFinalizeMessage"
            },
            {
              "type": "text",
              "text": " returned "
            },
            {
              "type": "codeVoice",
              "code": "noErr"
            },
            {
              "type": "text",
              "text": "), then the message was successfully decrypted. Call "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/security/1396553-cmsdecodercopycontent"
            },
            {
              "type": "text",
              "text": " to retrieve the decrypted content."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You cannot call this function until after you have called the "
            },
            {
              "type": "codeVoice",
              "code": "CMSDecoderFinalizeMessage"
            },
            {
              "type": "text",
              "text": " function."
            }
          ]
        }
      ]
    }
