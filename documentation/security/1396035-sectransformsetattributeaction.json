      "parameters": [
        {
          "name": "ref",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/sectransformimplementationref"
                },
                {
                  "type": "text",
                  "text": " that is bound to an instance of a custom transform."
                }
              ]
            }
          ]
        },
        {
          "name": "action",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The behavior to be set."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Use "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/ksectransformactionattributenotification"
                },
                {
                  "type": "text",
                  "text": " to add a block that is called when an attribute is set. If the name is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", then the supplied block is called for all set attributes except for ones that have a specific block as a handler."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Use "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/ksectransformactionattributevalidation"
                },
                {
                  "type": "text",
                  "text": " to add a block that is called to validate the input to an attribute."
                }
              ]
            }
          ]
        },
        {
          "name": "attribute",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the attribute that will be handled. An attribute reference may also be given here. A "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " value indicates that the supplied action is for all attributes."
                }
              ]
            }
          ]
        },
        {
          "name": "newAction",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/security/sectransformattributeactionblock"
                },
                {
                  "type": "text",
                  "text": " which implements the behavior."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An error on failure, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " on success. Call the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease"
            },
            {
              "type": "text",
              "text": " function to free the error's memory when you are done with it."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksectransformactionprocessdata"
            },
            {
              "type": "text",
              "text": " action used with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1399597-sectransformsetdataaction"
            },
            {
              "type": "text",
              "text": " function is a special case of a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1396035-sectransformsetattributeaction"
            },
            {
              "type": "text",
              "text": " action. If this is called on the input attribute then it will overwrite any "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/ksectransformactionprocessdata"
            },
            {
              "type": "text",
              "text": " action."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You may call this function multiple times for either a named attribute or for all attributes when the attribute parameter is "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": ". The last call takes precedence."
            }
          ]
        }
      ]
    }
