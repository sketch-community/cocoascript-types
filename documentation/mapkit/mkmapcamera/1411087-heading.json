      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " means that the top edge of the map view corresponds to true north. The value "
            },
            {
              "type": "codeVoice",
              "code": "90"
            },
            {
              "type": "text",
              "text": " means the top of the map is pointing due east. The value "
            },
            {
              "type": "codeVoice",
              "code": "180"
            },
            {
              "type": "text",
              "text": " means the top of the map points due south, and so on. "
            }
          ]
        }
      ]
    }
