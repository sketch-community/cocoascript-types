        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This point represents the coordinate on which the map should be centered. When the camera pitch is 0, this property also corresponds to the geographic position of the camera. Changing the pitch to a nonzero value moves the camera but does not affect this property."
            }
          ]
        }
      ]
