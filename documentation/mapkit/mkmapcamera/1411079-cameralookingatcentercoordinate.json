    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "centerCoordinate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The coordinate point on which the map should be centered."
                }
              ]
            }
          ]
        },
        {
          "name": "distance",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The line-of-sight distance from the camera to the center coordinate of the map."
                }
              ]
            }
          ]
        },
        {
          "name": "pitch",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The viewing angle of the camera, measured in degrees. A value of "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " results in a camera pointed straight down at the map. Angles greater than "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " result in a camera that is pitched toward the horizon by the specified number of degrees. "
                }
              ]
            }
          ]
        },
        {
          "name": "heading",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The heading of the camera (measured in degrees) relative to true north. The value "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " means that the top edge of the map view corresponds to true north. The value "
                },
                {
                  "type": "codeVoice",
                  "code": "90"
                },
                {
                  "type": "text",
                  "text": " means the top of the map is pointing due east. The value "
                },
                {
                  "type": "codeVoice",
                  "code": "180"
                },
                {
                  "type": "text",
                  "text": " means the top of the map points due south, and so on. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new camera object initialized with the specified information. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The altitude of the camera is obtained by multiplying "
            },
            {
              "type": "codeVoice",
              "code": "distance"
            },
            {
              "type": "text",
              "text": " by the cosine of the "
            },
            {
              "type": "codeVoice",
              "code": "pitch"
            },
            {
              "type": "text",
              "text": " value. "
            }
          ]
        }
      ]
    }
