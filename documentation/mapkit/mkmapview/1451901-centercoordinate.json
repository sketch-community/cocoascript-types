      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Changing the value in this property centers the map on the new coordinate without changing the current zoom level. It also updates the values in the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkit/mkmapview/1452709-region"
            },
            {
              "type": "text",
              "text": " property to reflect the new center coordinate and the new span values needed to maintain the current zoom level."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Changing the value of this property updates the map view immediately. If you want to animate the change, use the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkit/mkmapview/1451976-setcentercoordinate"
            },
            {
              "type": "text",
              "text": " method instead. "
            }
          ]
        }
      ]
    }
