        {
          "name": "overlay",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The overlay object to add. This object must conform to the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/mapkit/mkoverlay"
                },
                {
                  "type": "text",
                  "text": " protocol."
                }
              ]
            }
          ]
        },
        {
          "name": "level",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The map level at which to place the overlay. For a list of possible values for this parameter, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/mapkit/mkoverlaylevel"
