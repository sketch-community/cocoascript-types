      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value is "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Whenever possible, MapKit vectorizes overlay shapes by default so that they scale along with the map and remain sharp. In some case, you may wish to force an overlay to be rasterized and not vectorized. Set this variable to "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " to force the overlay to be rendered as a bitmap."
            }
          ]
        }
      ]
    }
