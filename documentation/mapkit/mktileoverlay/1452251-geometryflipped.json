    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When set to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": ", tile indexes start in the upper-left corner of the map and proceed down and to the right. Thus, the tile at "
            },
            {
              "type": "codeVoice",
              "code": "(0, 0)"
            },
            {
              "type": "text",
              "text": "is in the upper-left corner of the map, the tile at "
            },
            {
              "type": "codeVoice",
              "code": "(1, 0)"
            },
            {
              "type": "text",
              "text": " is to its immediate right and the tile at "
            },
            {
              "type": "codeVoice",
              "code": "(0, 1)"
            },
            {
              "type": "text",
              "text": " is immediately below it. Setting this property to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " causes the map to start indexes at the lower-left corner of the map and proceed up and to the right. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value of this property is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
