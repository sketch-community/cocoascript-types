        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If a search operation is currently in progress, this method attempts to cancels it. If cancellation is successful, the search completer does not notify its delegate. If no search operation is in progress, this method does nothing."
            }
          ]
        }
      ]
