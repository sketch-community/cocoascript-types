              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " dictionary of the specified "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsuseractivity"
            },
            {
              "type": "text",
              "text": " includes the corresponding "
            },
