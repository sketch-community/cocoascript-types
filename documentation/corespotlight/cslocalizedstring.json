    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "CSLocalizedString"
            },
            {
              "type": "text",
              "text": " class helps you localize text in searchable items. You can use a "
            },
            {
              "type": "codeVoice",
              "code": "CSLocalizedString"
            },
            {
              "type": "text",
              "text": " object in place of an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring"
            },
            {
              "type": "text",
              "text": " object to display localized text in search results related to your app."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, you might use the following code to define a "
            },
            {
              "type": "codeVoice",
              "code": "CSLocalizedString"
            },
            {
              "type": "text",
              "text": " object for a searchable item you want to identify as “Song” in English:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "CSSearchableItem *item = [CSSearchableItem new];",
            "    item.uniqueIdentifier = @\"song\";",
            " ",
            "    CSSearchableItemAttributeSet *attributes = [[CSSearchableItemAttributeSet alloc] initWithItemContentType:(NSString *)kUTTypeItem];",
            "    item.attributeSet = attributes;",
            " ",
            "    CSLocalizedString *displayName = [[CSLocalizedString alloc] initWithLocalizedStrings:@{@\"en\":@\"Song\", @\"fr\":@\"Chanson\"}];",
            "    attributes.displayName = displayName.localizedString;"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2556152",
            "title": "Listing 1"
          }
        }
      ]
    }
