          "name": "keyName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the custom attribute for use as a key in a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corespotlight/cssearchableitemattributeset"
                },
                {
                  "type": "text",
                  "text": ". The key name must be a string that contains only ASCII characters and no punctuation other than the underscore (that is “_”). The prefix "
                },
                {
                  "type": "codeVoice",
                  "code": "kMD"
                },
                {
                  "type": "text",
                  "text": " is reserved."
                }
              ]
            }
          ]
        }
      ]
    },
