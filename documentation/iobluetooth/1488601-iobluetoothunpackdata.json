          "type": "codeListing",
          "code": [
            " ",
            "   \tbytesUnpacked = UnpackData( 100, buffer, \"bwnsp\", &myByte1, &myWord, 50, buffer1, 128, buffer2, 255, pString );",
            " "
          ],
