      "parameters": [
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The request flags. The "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corenfc/nfciso15693requestflag/requestflagaddress"
                },
                {
                  "type": "text",
                  "text": " flag is enforced by default. However, using the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corenfc/nfciso15693requestflag/requestflagselect"
                },
                {
                  "type": "text",
                  "text": " flag disables the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corenfc/nfciso15693requestflag/requestflagaddress"
                },
                {
                  "type": "text",
                  "text": " flag."
                }
              ]
            }
          ]
        },
        {
          "name": "blockNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of the block to read. Blocks are numbered from 0 to 255 inclusively."
                }
              ]
            }
          ]
        },
        {
          "name": "completionHandler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A handler that the reader session invokes after the operation completes. The session calls completionHandler on the dispatch queue that you provided when creating the NFCTagReaderSession object."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The handler has the following parameters:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "data"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "A "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nsdata"
                          },
                          {
                            "type": "text",
                            "text": " object containing the blocks of data read from the tag. If the request flags include "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/corenfc/nfciso15693requestflag/requestflagoption"
                          },
                          {
                            "type": "text",
                            "text": ", the first byte of the data contains the associated block security status."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "error"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "codeVoice",
                            "code": "nil"
                          },
                          {
                            "type": "text",
                            "text": " when the operation is successful; otherwise, an "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nserror"
                          },
                          {
                            "type": "text",
                            "text": " object indicating that a problem occurred while communicating with the tag. "
                          }
                        ]
                      },
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "When the tag responds with a command error, the error's "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nserror/1411580-userinfo"
                          },
                          {
                            "type": "text",
                            "text": " directory contains the "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/corenfc/nfciso15693tagresponseerrorkey"
                          },
                          {
                            "type": "text",
                            "text": " and the error's "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nserror/1409165-code"
                          },
                          {
                            "type": "text",
                            "text": " property has a value defined in the ISO15693-3 specification."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": []
            }
          ]
        }
      ]
    },
