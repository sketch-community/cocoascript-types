    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method to send commands to tags that are from the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corenfc/nfcmifarefamily/nfcmifareultralight"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corenfc/nfcmifarefamily/nfcmifareplus"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corenfc/nfcmifarefamily/nfcmifaredesfire"
            },
            {
              "type": "text",
              "text": " product families."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method supports command chaining, passing the full response composed of the individual fragments to the "
            },
            {
              "type": "codeVoice",
              "code": "completionHandler"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This method doesn't support the Crypto1 protocol."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        },
