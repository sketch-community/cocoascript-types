          "name": "command",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A MIFARE command. For "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corenfc/nfcmifarefamily/nfcmifareultralight"
                },
                {
                  "type": "text",
                  "text": " commands, you must calculate a 2-byte CRC value and append it to the end of the "
                },
                {
                  "type": "codeVoice",
                  "code": "command"
                },
                {
                  "type": "text",
                  "text": " data."
                }
              ]
            }
          ]
        },
        {
          "name": "completionHandler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A handler that the reader session invokes after the operation completes. The session calls "
                },
                {
                  "type": "codeVoice",
                  "code": "completionHandler"
                },
                {
                  "type": "text",
                  "text": " on the dispatch queue that you provided when creating the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corenfc/nfctagreadersession"
                },
                {
                  "type": "text",
                  "text": " object."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The handler has the following parameters:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "response"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "An "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nsdata"
                          },
                          {
                            "type": "text",
                            "text": " object containing the tag's response data for the command."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "error"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "codeVoice",
                            "code": "nil"
                          },
                          {
                            "type": "text",
                            "text": " when the operation is successful; otherwise, an "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nserror"
                          },
                          {
                            "type": "text",
                            "text": " object indicating that a problem occurred while communicating with the tag."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": []
            }
