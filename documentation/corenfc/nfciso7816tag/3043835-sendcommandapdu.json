      "parameters": [
        {
          "name": "apdu",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An application protocol data unit to send to the tag."
                }
              ]
            }
          ]
        },
        {
          "name": "completionHandler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A handler that the reader session invokes after the operation completes. The handler has the following parameters:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "responseData"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The response data, which may be empty even if the operation completes successfully."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "sw1"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The SW1 command-processing status byte. This value is always valid."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "sw2"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The SW2 command-processing status byte. This value is always valid."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "error"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "codeVoice",
                            "code": "nil"
                          },
                          {
                            "type": "text",
                            "text": " when the operation completes successfully; otherwise, an "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nserror"
                          },
                          {
                            "type": "text",
                            "text": " object when there's a communication issue with the tag."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": []
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When you send a "
            },
            {
              "type": "codeVoice",
              "code": "SELECT"
            },
            {
              "type": "text",
              "text": " command with a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corenfc/nfciso7816apdu/3075558-p1parameter"
            },
            {
              "type": "text",
              "text": " value of "
            },
            {
              "type": "codeVoice",
              "code": "0x04"
            },
            {
              "type": "text",
              "text": ", your app must support one of the applications listed in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/bundleresources/information_property_list/select-identifiers"
            },
            {
              "type": "text",
              "text": " property list key. Otherwise, the "
            },
            {
              "type": "codeVoice",
              "code": "completionHandler"
            },
            {
              "type": "text",
              "text": " receives an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corenfc/nfcreadererror/nfcreadererrorsecurityviolation"
            },
            {
              "type": "text",
              "text": " error."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The session calls "
            },
            {
              "type": "codeVoice",
              "code": "completionHandler"
            },
            {
              "type": "text",
              "text": " on the dispatch queue that you provided when creating the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corenfc/nfctagreadersession"
            },
            {
              "type": "text",
              "text": " object."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
  ],
