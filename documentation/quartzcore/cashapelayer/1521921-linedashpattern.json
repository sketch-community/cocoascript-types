    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The dash pattern is specified as an array of "
            },
            {
              "type": "codeVoice",
              "code": "NSNumber"
            },
            {
              "type": "text",
              "text": " objects that specify the lengths of the painted segments and unpainted segments, respectively, of the dash pattern."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, passing an array with the values "
            },
            {
              "type": "codeVoice",
              "code": "[2,3]"
            },
            {
              "type": "text",
              "text": " sets a dash pattern that alternates between a 2-user-space-unit-long painted segment and a 3-user-space-unit-long unpainted segment. Passing the values "
            },
            {
              "type": "codeVoice",
              "code": "[10,5,5,5]"
            },
            {
              "type": "text",
              "text": " sets the pattern to a 10-unit painted segment, a 5-unit unpainted segment, a 5-unit painted segment, and a 5-unit unpainted segment."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Default is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", a solid line."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartzcore/cashapelayer/1521921-linedashpattern#2825197"
            },
            {
              "type": "text",
              "text": " shows how how you can create three shape layers using the dash patterns described above. Each shape layer contains a simple path that describes a horizontal line."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let layer = CALayer()",
            "      ",
            "let lineDashPatterns: [[NSNumber]?]  = [nil, [2,3], [10, 5, 5, 5]]",
            "     ",
            "for (index, lineDashPattern) in lineDashPatterns.enumerated() {",
            "    ",
            "    let shapeLayer = CAShapeLayer()",
            "    shapeLayer.strokeColor = UIColor.black.cgColor",
            "    shapeLayer.lineWidth = 5",
            "    shapeLayer.lineDashPattern = lineDashPattern",
            "    ",
            "    let path = CGMutablePath()",
            "    let y = CGFloat(index * 50)",
            "    path.addLines(between: [CGPoint(x: 0, y: y),",
            "                            CGPoint(x: 640, y: y)])",
            "    ",
            "    shapeLayer.path = path",
            "    ",
            "    layer.addSublayer(shapeLayer)",
            "}"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "2825197",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": true,
              "identifier": "link-media-2825198"
            },
            {
              "type": "text",
              "text": " shows three shape layers created with the code above. The top solid line has a "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/quartzcore/cashapelayer/1521921-linedashpattern"
            },
            {
              "type": "text",
              "text": ", the middle has "
            },
            {
              "type": "codeVoice",
              "code": "[2,3]"
            },
            {
              "type": "text",
              "text": " and the bottom has "
            },
            {
              "type": "codeVoice",
              "code": "[10,5,5,5]"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
