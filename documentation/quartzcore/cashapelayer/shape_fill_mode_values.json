    "doc://com.apple.documentation/documentation/quartzcore/kcafillrulenonzero": {
      "title": "kCAFillRuleNonZero",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartzcore/kcafillrulenonzero",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartzcore/kcafillrulenonzero",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies the non-zero winding rule. Count each left-to-right path as +1 and each right-to-left path as -1. If the sum of all crossings is 0, the point is outside the path. If the sum is nonzero, the point is inside the path and the region containing it is filled."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/quartzcore/kcafillruleevenodd": {
      "title": "kCAFillRuleEvenOdd",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartzcore/kcafillruleevenodd",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartzcore/kcafillruleevenodd",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies the even-odd winding rule. Count the total number of path crossings. If the number of crossings is even, the point is outside the path. If the number of crossings is odd, the point is inside the path and the region containing it should be filled."
        }
      ]
    },
