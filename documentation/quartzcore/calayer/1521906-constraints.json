        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "macOS apps can use this property to access their layer-based constraints. Before constraints can be applied, you must also assign a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartzcore/caconstraintlayoutmanager"
            },
            {
              "type": "text",
              "text": " object to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartzcore/calayer/1410749-layoutmanager"
            },
            {
              "type": "text",
              "text": " property of the layer. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "iOS apps do not support layer-based constraints."
            }
          ]
        }
