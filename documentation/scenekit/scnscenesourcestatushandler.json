    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You provide a block with this signature when using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scnscenesource/1522887-scenewithoptions"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The block takes four parameters:"
            }
          ]
        },
        {
          "type": "termList",
          "items": [
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "totalProgress"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "A floating-point number between "
                      },
                      {
                        "type": "codeVoice",
                        "code": "0.0"
                      },
                      {
                        "type": "text",
                        "text": " and "
                      },
                      {
                        "type": "codeVoice",
                        "code": "1.0"
                      },
                      {
                        "type": "text",
                        "text": " indicating the overall progress of loading the scene. A value of "
                      },
                      {
                        "type": "codeVoice",
                        "code": "0.0"
                      },
                      {
                        "type": "text",
                        "text": " indicates that the loading process has just begun, and a value of "
                      },
                      {
                        "type": "codeVoice",
                        "code": "1.0"
                      },
                      {
                        "type": "text",
                        "text": " indicates that the process has completed."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "status"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "A constant identifying one of the distinct phases of SceneKit’s loading procedure. See "
                      },
                      {
                        "type": "reference",
                        "isActive": true,
                        "identifier": "doc://com.apple.documentation/documentation/scenekit/scnscenesourcestatus"
                      },
                      {
                        "type": "text",
                        "text": " for possible values."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "error"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "An error object describing any error that has occurred during scene loading, or "
                      },
                      {
                        "type": "codeVoice",
                        "code": "nil"
                      },
                      {
                        "type": "text",
                        "text": " if no errors has been encountered."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "stopLoading"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "A reference to a Boolean value. Set "
                      },
                      {
                        "type": "codeVoice",
                        "code": "*stop"
                      },
                      {
                        "type": "text",
                        "text": " to "
                      },
                      {
                        "type": "reference",
                        "isActive": false,
                        "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                      },
                      {
                        "type": "text",
                        "text": " within the block to abort further processing of the scene source’s contents."
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
