      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "time"
            },
            {
              "type": "text",
              "text": " parameter is relative to the duration of the animation the event is attached to. For example, an event with a time of "
            },
            {
              "type": "codeVoice",
              "code": "0.5"
            },
            {
              "type": "text",
              "text": " triggers when the animation is halfway complete, and an event with a time of "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": " triggers when the animation ends."
            }
          ]
        }
      ]
