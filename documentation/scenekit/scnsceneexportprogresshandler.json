      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You specify a block with this signature when calling the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scnscene/1523577-writetourl"
            },
            {
              "type": "text",
              "text": " method in order to receive updates on the progress of the export operation. The block takes the following parameters:"
            }
          ]
        },
        {
          "type": "termList",
          "items": [
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "totalProgress"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "A number between "
                      },
                      {
                        "type": "codeVoice",
                        "code": "0.0"
                      },
                      {
                        "type": "text",
                        "text": " and "
                      },
                      {
                        "type": "codeVoice",
                        "code": "1.0"
                      },
                      {
                        "type": "text",
                        "text": " that indicates the progress of the export operation, with "
                      },
                      {
                        "type": "codeVoice",
                        "code": "0.0"
                      },
                      {
                        "type": "text",
                        "text": " indicating that the operation has just begun and "
                      },
                      {
                        "type": "codeVoice",
                        "code": "1.0"
                      },
                      {
                        "type": "text",
                        "text": " indicating the operation has completed."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "error"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "An error encountered during the export process, or "
                      },
                      {
                        "type": "codeVoice",
                        "code": "nil"
                      },
                      {
                        "type": "text",
                        "text": " if no errors have occurred."
                      }
                    ]
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "termList",
          "items": [
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "stop"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "Set "
                      },
                      {
                        "type": "codeVoice",
                        "code": "*stop"
                      },
                      {
                        "type": "text",
                        "text": " to "
                      },
                      {
                        "type": "reference",
                        "isActive": false,
                        "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                      },
                      {
                        "type": "text",
                        "text": " inside the block to cancel export."
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
