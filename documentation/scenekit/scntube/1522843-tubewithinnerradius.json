      "parameters": [
        {
          "name": "innerRadius",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The radius of the tube’s circular central hole in the x- and z-axes of its local coordinate space."
                }
              ]
            }
          ]
        },
        {
          "name": "outerRadius",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The radius of the tube’s circular cross section in the x- and z-axes of its local coordinate space."
                }
              ]
            }
          ]
        },
        {
          "name": "height",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The height of the tube along the y-axis of its local coordinate space."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new tube geometry."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The tube is centered in its local coordinate system. For example, if you create a tube whose outer radius is "
            },
            {
              "type": "codeVoice",
              "code": "5.0"
            },
            {
              "type": "text",
              "text": ", inner radius is "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": ", and height is "
            },
            {
              "type": "codeVoice",
              "code": "10.0"
            },
            {
              "type": "text",
              "text": ", its circular cross section extends from "
            },
            {
              "type": "codeVoice",
              "code": "-5.0"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "5.0"
            },
            {
              "type": "text",
              "text": " along the x- and z-axes, the y-coordinates of its base and top are "
            },
            {
              "type": "codeVoice",
              "code": "-5.0"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "5.0"
            },
            {
              "type": "text",
              "text": ", and the hole through its center extends from "
            },
            {
              "type": "codeVoice",
              "code": "-0.5"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "0.5"
            },
            {
              "type": "text",
              "text": " along the x- and z-axes."
            }
          ]
        }
      ]
    }
