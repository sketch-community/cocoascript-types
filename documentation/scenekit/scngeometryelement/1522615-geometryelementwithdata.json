      "parameters": [
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The data describing the element."
                }
              ]
            }
          ]
        },
        {
          "name": "primitiveType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The drawing primitive that connects vertices when rendering the geometry element. For possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/scenekit/scngeometryprimitivetype"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "primitiveCount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of primitives in the element."
                }
              ]
            }
          ]
        },
        {
          "name": "bytesPerIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of bytes that represent a single index value in the data."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new geometry element object."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An element’s data is an array of index values identifying vertices in a geometry source. SceneKit interprets the data as an array of unsigned integers (whose size is specified by the "
            },
            {
              "type": "codeVoice",
              "code": "bytesPerIndex"
            },
            {
              "type": "text",
              "text": " parameter), and then connects the vertices in the order specified by this array, arranged according to the "
            },
            {
              "type": "codeVoice",
              "code": "primitiveType"
            },
            {
              "type": "text",
              "text": " parameter."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To create a custom "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scngeometry"
            },
            {
              "type": "text",
              "text": " object from the geometry element, use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scngeometry/1522803-geometrywithsources"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        }
      ]
    }
