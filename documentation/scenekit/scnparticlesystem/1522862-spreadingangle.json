    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Setting a nonzero value for this property randomizes the direction specified by the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scnparticlesystem/1523600-emittingdirection"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scnparticlesystem/1523361-birthdirection"
            },
            {
              "type": "text",
              "text": " property. For example, at the default value of "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " degrees, all particles emit in the same direction. Increasing the spreading angle to "
            },
            {
              "type": "codeVoice",
              "code": "30.0"
            },
            {
              "type": "text",
              "text": " degrees allows particles to emit in any direction within a space shaped like a cone whose central angle is 30°."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property has no effect if the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scnparticlesystem/1523361-birthdirection"
            },
            {
              "type": "text",
              "text": " property value is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/scnparticlebirthdirection/scnparticlebirthdirectionrandom"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can animate changes to this property’s value. See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/scenekit/animation/animating_scenekit_content"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
