      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For a given display link, you must register a display link output callback using "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/corevideo/cvdisplaylinkoutputcallback"
            },
            {
              "type": "text",
              "text": " so that you can process and output the requested frame."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Your callback must retrieve the frame with the timestamp specified by the ("
            },
            {
              "type": "codeVoice",
              "code": "inOutputTime"
            },
            {
              "type": "text",
              "text": " parameter, manipulate it if desired (for example, apply color correction or map into onto a surface), and then output it to the display. "
            }
          ]
        }
      ]
    }
