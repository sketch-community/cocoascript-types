{
  "abstract": [
    {
      "type": "text",
      "text": "A Boolean value where "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfbooleantrue"
    },
    {
      "type": "text",
      "text": " indicates that the format contains alpha and some images may be considered transparent; "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfbooleanfalse"
    },
    {
      "type": "text",
      "text": " indicates that there is no alpha and images are always opaque."
    }
  ],
