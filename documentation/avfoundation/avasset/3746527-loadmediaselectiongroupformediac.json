      ],
      "parameters": [
        {
          "name": "mediaCharacteristic",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A media characteristic to load the available media selection options for. The supported characterisics are:"
                }
              ]
            },
            {
              "type": "unorderedList",
              "items": [
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmediacharacteristicaudible"
                        },
                        {
                          "type": "text",
                          "text": " to return the group of available options for audio media in various languages and for various purposes, such as descriptive audio"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmediacharacteristiclegible"
                        },
                        {
                          "type": "text",
                          "text": " to return the group of available options for subtitles in various languages and for various purposes"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmediacharacteristicvisual"
                        },
                        {
                          "type": "text",
                          "text": " to return the group of available options for video media"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "completionHandler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A callback that the system invokes after it finishes the loading request. It passes the completion handler the following parameters:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "mediaSelectionGroup"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The loaded media selection group, or "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
                          },
                          {
                            "type": "text",
                            "text": " if no group is available or if an error occurs."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "error"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "An error object if the request fails; otherwise, "
                          },
                          {
                            "type": "codeVoice",
                            "code": "nil"
                          },
                          {
                            "type": "text",
                            "text": "."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
