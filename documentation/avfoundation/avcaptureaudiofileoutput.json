            {
              "type": "codeVoice",
              "code": "AVCaptureAudioFileOutput"
            },
            {
              "type": "text",
              "text": " implements the complete file recording interface declared by "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturefileoutput"
            },
            {
              "type": "text",
              "text": " for writing media data to audio files. In addition, you can configure options specific to the audio file formats, including writing metadata collections to each file and specifying audio encoding options. "
            },
            {
              "type": "codeVoice",
              "code": "AVCaptureAudioFileOutput"
            },
            {
              "type": "text",
              "text": " does not, however, support "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturefileoutput/1387224-startrecordingtooutputfileurl"
            },
            {
              "type": "text",
              "text": "â€”use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcaptureaudiofileoutput/1387420-startrecordingtooutputfileurl"
            },
            {
              "type": "text",
              "text": " instead."
            }
