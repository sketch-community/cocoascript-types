          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When passed in the "
            },
            {
              "type": "codeVoice",
              "code": "options"
            },
            {
              "type": "text",
              "text": " parameter of the "
            },
            {
              "type": "reference",
