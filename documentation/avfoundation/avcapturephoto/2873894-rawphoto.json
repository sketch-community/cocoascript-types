        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When you request capture in RAW format, the capture output calls your delegate's "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturephotocapturedelegate/2873949-captureoutput"
            },
            {
              "type": "text",
              "text": " method one or more times, delivering both the RAW photo data and (if requested) equivalent processed photos. Use this property to distinguish between the RAW and processed results from the same capture."
            }
          ]
        }
      ]
    }
