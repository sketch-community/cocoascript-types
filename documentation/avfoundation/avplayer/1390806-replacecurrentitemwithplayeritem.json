      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The player item replacement occurs immediately and the item becomes the player’s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayer/1387569-currentitem"
            },
            {
              "type": "text",
              "text": ". Calling this method with the player’s current player item has no effect."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "You can only call this method on instances of "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayer"
                },
                {
                  "type": "text",
                  "text": ". Calling it on an instance of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avqueueplayer"
                },
                {
                  "type": "text",
                  "text": " results in the system throwing an exception."
                }
              ]
            }
          ],
          "style": "important"
        }
      ]
    }
