        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When route detection is enabled, "
            },
            {
              "type": "codeVoice",
              "code": "AVRouteDetector"
            },
            {
              "type": "text",
              "text": " reports whether multiple playback routes have been detected. If this is the case, AVKit's "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avkit/avroutepickerview"
            },
            {
              "type": "text",
              "text": " can be used to allow users to pick from the set of available routes. By default, route detection is disabled."
            }
          ]
        }
      ]
    }
