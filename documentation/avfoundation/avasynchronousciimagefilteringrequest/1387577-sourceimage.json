      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To apply a Core Image filter to this image, assign it to the "
            },
            {
              "type": "codeVoice",
              "code": "inputImage"
            },
            {
              "type": "text",
              "text": " parameter of a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter"
            },
            {
              "type": "text",
              "text": " object, or use a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage"
            },
            {
              "type": "text",
              "text": " convenience method such as the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage/1437589-imagebyapplyingfilter"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The pixel format for this image is the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/kciformatbgra8"
            },
            {
              "type": "text",
              "text": " format (of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corevideo/1563591-pixel_format_identifiers/kcvpixelformattype_32bgra"
            },
            {
              "type": "text",
              "text": " type). Unlike when processing video with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avasynchronousvideocompositionrequest"
            },
            {
              "type": "text",
              "text": " class, the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avasynchronousvideocompositionrequest/1389112-rendercontext"
            },
            {
              "type": "text",
              "text": " objectâ€™s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avvideocompositionrendercontext/1389831-rendertransform"
            },
            {
              "type": "text",
              "text": " property is already applied to this image."
            }
          ]
        }
      ]
