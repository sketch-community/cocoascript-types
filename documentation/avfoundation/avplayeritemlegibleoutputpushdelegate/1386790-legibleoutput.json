      "parameters": [
        {
          "name": "output",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayeritemlegibleoutput"
                },
                {
                  "type": "text",
                  "text": " source instance."
                }
              ]
            }
          ]
        },
        {
          "name": "strings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsattributedstring"
                },
                {
                  "type": "text",
                  "text": " objects, each containing both the run of text and the descriptive markup."
                }
              ]
            }
          ]
        },
        {
          "name": "nativeSamples",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coremedia/cmsamplebufferref"
                },
                {
                  "type": "text",
                  "text": " objects, for media subtypes included in the array passed to the "
                },
                {
                  "type": "codeVoice",
                  "code": "output"
                },
                {
                  "type": "text",
                  "text": " object’s "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayeritemlegibleoutput/1390500-initwithmediasubtypesfornativere"
                },
                {
                  "type": "text",
                  "text": " method."
                }
              ]
            }
          ]
        },
        {
          "name": "itemTime",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The item time at which the strings should be presented."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For each media subtype in the array passed in to the "
            },
            {
              "type": "codeVoice",
              "code": "output"
            },
            {
              "type": "text",
              "text": " object’s  "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayeritemlegibleoutput/1390500-initwithmediasubtypesfornativere"
            },
            {
              "type": "text",
              "text": " method, the delegate receives sample buffers carrying data in its native format via the "
            },
            {
              "type": "codeVoice",
              "code": "nativeSamples"
            },
            {
              "type": "text",
              "text": " parameter if there is media data of that subtype in the media resource.  "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For all other media subtypes present in the media resource, the delegate receives attributed strings in a common format via the "
            },
            {
              "type": "codeVoice",
              "code": "strings"
            },
            {
              "type": "text",
              "text": " parameter.  See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coremedia/cmtextmarkup"
            },
            {
              "type": "text",
              "text": " for the string attributes keys and values that are used in the attributed strings."
            }
          ]
        }
      ]
    }
