      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can set this value to true only if "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturephotooutput/3566331-contentawaredistortioncorrection"
            },
            {
              "type": "text",
              "text": " returns true. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Applying distortion correction to preserve natural-looking content may result in a small change in the field of view compared to what you see in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturevideopreviewlayer"
            },
            {
              "type": "text",
              "text": ". The amount lost or gained is content specific and varies from photo to photo. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Enabling this property requires a lengthy reconfiguration of the capture render pipeline, so set this property to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " before calling "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturesession/1388185-startrunning"
            },
            {
              "type": "text",
              "text": " on the capture session."
            }
          ]
        }
      ]
    }
