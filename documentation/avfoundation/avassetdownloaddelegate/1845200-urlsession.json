        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Unlike "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlsessiondownloaddelegate"
            },
            {
              "type": "text",
              "text": ", the delegate "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "should not"
                }
              ]
            },
            {
              "type": "text",
              "text": " move the file from this directory after it has been called. Downloaded assets must remain at the system-provided URL. Instead, you should save a persistent reference to this URL for future use."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)URLSession:(NSURLSession *)session assetDownloadTask:(AVAssetDownloadTask *)assetDownloadTask didFinishDownloadingToURL:(NSURL *)location {",
            "    NSUserDefaults *userDefaults = [NSUserDefaults standardUserDefaults];",
            "    // Save for later use",
            "    [userDefaults setURL:location forKey:@\"downloadURL\"];",
            "}"
          ],
          "syntax": "occ",
