    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the value in the "
            },
            {
              "type": "codeVoice",
              "code": "keySpace"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmetadatakeyspacecommon"
            },
            {
              "type": "text",
              "text": ", the value of the metadata item’s "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmetadataitem/1389864-commonkey"
            },
            {
              "type": "text",
              "text": " property must match the value in the "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " parameter for the item to be returned. If the "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", then all metadata items in the common key space are returned."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the value in the "
            },
            {
              "type": "codeVoice",
              "code": "keySpace"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " or is not the constant for the common key space, the value of the metadata item’s "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmetadataitem/1387843-key"
            },
            {
              "type": "text",
              "text": " property must match the value in the "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " parameter for the item to be returned. If the "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " parameter itself is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", then all metadata items in the specified key space are returned. "
            }
          ]
        }
