              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The array of "
                },
                {
                  "type": "codeVoice",
                  "code": "AVMetadataItem"
                },
                {
                  "type": "text",
                  "text": " objects to be filtered."
                }
