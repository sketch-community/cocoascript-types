            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time to which you would like to move the playback cursor."
                }
              ]
            }
          ]
        },
        {
          "name": "toleranceBefore",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The tolerance allowed before "
                },
                {
                  "type": "codeVoice",
                  "code": "time"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "toleranceAfter",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The tolerance allowed after "
                },
                {
                  "type": "codeVoice",
                  "code": "time"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The time seeked to will be within the range "
            },
            {
              "type": "codeVoice",
              "code": "[time-beforeTolerance, time+afterTolerance]"
            },
            {
              "type": "text",
              "text": ", and may differ from the specified time for efficiency. If you pass "
            },
            {
              "type": "codeVoice",
              "code": "kCMTimeZero"
            },
            {
              "type": "text",
              "text": " for both "
            },
            {
              "type": "codeVoice",
              "code": "toleranceBefore"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "toleranceAfter"
            },
            {
              "type": "text",
              "text": " (to request sample accurate seeking), you may incur additional decoding delay that impacts seeking performance."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Passing "
            },
            {
              "type": "codeVoice",
              "code": "kCMTimePositiveInfinity"
            },
            {
              "type": "text",
              "text": " for both "
            },
            {
              "type": "codeVoice",
              "code": "toleranceBefore"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "toleranceAfter"
            },
            {
              "type": "text",
              "text": " is the same as messaging "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayeritem/1390153-seektotime"
            },
            {
              "type": "text",
              "text": " directly."
            }
          ]
        }
      ]
    }
