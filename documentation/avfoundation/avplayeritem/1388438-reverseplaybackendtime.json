      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value indicated the time at which playback should end when the playback rate is negative (see "
            },
            {
              "type": "codeVoice",
              "code": "AVPlayer"
            },
            {
              "type": "text",
              "text": "â€™s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avplayer/1388846-rate"
            },
            {
              "type": "text",
              "text": " property)."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coremedia/kcmtimeinvalid"
            },
            {
              "type": "text",
              "text": ", which indicates that no end time for reverse playback is specified. In this case, the effective end time for reverse playback is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coremedia/kcmtimezero"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of this property has no effect on playback when the rate is positive."
            }
          ]
        }
      ]
    }
