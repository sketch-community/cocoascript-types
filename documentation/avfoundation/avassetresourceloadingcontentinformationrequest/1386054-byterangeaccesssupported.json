        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Before finishing loading an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetresourceloadingrequest"
            },
            {
              "type": "text",
              "text": " instance, if its "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetresourceloadingrequest/1390340-contentinformationrequest"
            },
            {
              "type": "text",
              "text": " property is not "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", set the value of this property to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " if it supports random access to arbitrary ranges of bytes of the resource. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If this property is not "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " for resources that must be loaded incrementally, loading of the resource may fail. Such resources include anything that contains media data."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If byte range access is supported  portions of the resource can be requested more than once."
            }
          ]
        }
      ]
