      "parameters": [
        {
          "name": "outputURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The location of the file to be written. The URL must be a file URL."
                }
              ]
            }
          ]
        },
        {
          "name": "outputFileType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The UTI-identified format of the file to be written. See "
                },
                {
                  "type": "codeVoice",
                  "code": "File Format UTIs"
                },
                {
                  "type": "text",
                  "text": " for values."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "For example, "
                },
                {
                  "type": "codeVoice",
                  "code": "AVFileTypeQuickTimeMovie"
                },
                {
                  "type": "text",
                  "text": " for a QuickTime movie file, "
                },
                {
                  "type": "codeVoice",
                  "code": "AVFileTypeMPEG4"
                },
                {
                  "type": "text",
                  "text": " for an MPEG-4 file, and "
                },
                {
                  "type": "codeVoice",
                  "code": "AVFileTypeAMR"
                },
                {
                  "type": "text",
                  "text": " for an adaptive multi-rate audio format file."
                }
              ]
            }
          ]
        },
        {
          "name": "outError",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If initialization of the asset writer fails, upon return contains an error object that describes the problem."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An asset writer for writing to the file identified by "
            },
            {
              "type": "codeVoice",
              "code": "URL"
            },
            {
              "type": "text",
              "text": " in the format specified by "
            },
            {
              "type": "codeVoice",
              "code": "outputFileType"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the writer could not be initialized."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Writing will fail if a file already exists at "
