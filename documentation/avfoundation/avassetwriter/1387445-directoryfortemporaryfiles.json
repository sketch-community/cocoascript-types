        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In some configurations, such as performing multipass encoding, an asset writer may need to write temporary files. Use this property value to set the file-system location where it writes temporary files. The system deletes all temporary files after it finishes writing successfully, fails, or you cancel the writing session."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can set this value after writing starts."
            }
          ]
        }
      ]
    }
