                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avaudiosessioncategoryoptions"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
