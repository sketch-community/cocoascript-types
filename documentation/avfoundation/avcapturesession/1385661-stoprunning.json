        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method is used to stop the flow of data from the inputs to the outputs connected to the "
            },
            {
              "type": "codeVoice",
              "code": "AVCaptureSession"
            },
            {
              "type": "text",
              "text": " instance that is the receiver. This method is synchronous and blocks until the receiver has completely stopped running."
            }
          ]
        }
      ]
