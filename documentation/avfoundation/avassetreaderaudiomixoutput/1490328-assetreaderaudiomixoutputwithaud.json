          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassettrack"
                },
                {
                  "type": "text",
                  "text": " objects from which the created object should read sample buffers to be mixed."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Each track must be one of the tracks owned by the target "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetreader"
                },
                {
                  "type": "text",
                  "text": " objectâ€™s asset and must be of media type "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmediatypeaudio"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "audioSettings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The audio settings to be used for audio output; the dictionary must contain values for keys in Linear PCM Format Settings."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you want to receive decoded samples in a convenient uncompressed format, with properties determined according to the properties of the specified audio tracks. "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfaudio/avsamplerateconverteraudioqualitykey"
                },
                {
                  "type": "text",
                  "text": " is not supported."
                }
              ]
            }
          ]
        }
      ]
