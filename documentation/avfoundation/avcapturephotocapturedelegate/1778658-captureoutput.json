                {
                  "type": "text",
                  "text": "An object describing the settings used for this capture. Match this object’s "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturephotosettings/1648767-uniqueid"
