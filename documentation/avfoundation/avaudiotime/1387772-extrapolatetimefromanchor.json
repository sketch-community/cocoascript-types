    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A newly created and initialized "
            },
            {
              "type": "codeVoice",
              "code": "AVAudioTime"
            },
            {
              "type": "text",
              "text": " instance with the extrapolated time."
            }
          ]
        }
