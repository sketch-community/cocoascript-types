    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "anchorTime"
            },
            {
              "type": "text",
              "text": " is an "
            },
            {
              "type": "codeVoice",
              "code": "AVAudioTime"
            },
            {
              "type": "text",
              "text": " instance where both host time and sample time are valid, and the receiver is another timestamp where only one of the two is valid, this method returns a new "
            },
            {
              "type": "codeVoice",
              "code": "AVAudioTime"
            },
            {
              "type": "text",
              "text": " instance copied from the receiver and where any additional valid fields provided by the anchor are also valid."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "// time0 has a valid audio sample representation, but no host time representation.",
            "AVAudioTime *time0 = [AVAudioTime timeWithSampleTime: 0.0 atRate: 44100.0];",
            "// anchor has a valid host time representation and sample time representation.",
            "AVAudioTime *anchor = [node currentTime];",
            "// fill in  valid host time representation",
            "AVAudioTime *fullTime = [sampleTime extrapolateTimeFromAnchor: sampleTime];"
          ],
          "syntax": "occ",
