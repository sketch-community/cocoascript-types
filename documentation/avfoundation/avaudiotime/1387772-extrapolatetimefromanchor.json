            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "AVAudioTime"
                },
                {
                  "type": "text",
                  "text": " instance with a more complete AudioTimeStamp than that of the receiver."
                }
              ]
            }
          ]
        }
      ]
