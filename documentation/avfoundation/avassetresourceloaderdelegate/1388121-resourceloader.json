      "parameters": [
        {
          "name": "resourceLoader",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The resource loader object that is making the request."
                }
              ]
            }
          ]
        },
        {
