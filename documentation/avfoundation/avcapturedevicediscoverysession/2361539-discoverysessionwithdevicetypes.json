          "name": "deviceTypes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A list of device types to search for, such as "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevicetypebuiltinmicrophone"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevicetypebuiltinwideanglecamera"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This array must contain at least one valid "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevicetype"
                },
                {
                  "type": "text",
                  "text": " value."
                }
              ]
            }
          ]
        },
        {
          "name": "mediaType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The media type to capture, such as "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmediatypeaudio"
                },
                {
                  "type": "text",
                  "text": " (see "
                },
                {
                  "type": "codeVoice",
                  "code": "Media Types"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to search for devices regardless of supported media types."
                }
              ]
            }
          ]
        },
        {
          "name": "position",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The position of capture device to search for, relative to system hardware (front- or back-facing)."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedeviceposition/avcapturedevicepositionunspecified"
                },
                {
                  "type": "text",
                  "text": " to search for devices regardless of position."
                }
              ]
            }
          ]
        }
      ]
