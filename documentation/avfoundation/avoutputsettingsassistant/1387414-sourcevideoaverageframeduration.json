      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Setting this property enables the output settings assistant to generate more complete video settings. After setting a value, requery the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avoutputsettingsassistant/1386880-videosettings"
            },
            {
              "type": "text",
              "text": " property to get the latest values."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value is "
            },
            {
              "type": "codeVoice",
              "code": "1/30"
            },
            {
              "type": "text",
              "text": ", which means the output settings assistant assumes that your source video has a frame rate of 30fps."
            }
          ]
        }
      ]
    }
