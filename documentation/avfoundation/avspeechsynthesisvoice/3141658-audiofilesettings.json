      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you want to generate speech and save it as an audio file to share or play later, use this dictionary to create an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avaudiofile"
            },
            {
              "type": "text",
              "text": " instance and pass it as the "
            },
            {
              "type": "codeVoice",
              "code": "settings"
            },
            {
              "type": "text",
              "text": " parameter."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can determine the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avaudiocommonformat"
            },
            {
              "type": "text",
              "text": " and interleaved properties of a voice from this dictionary. The format of this dictionary matches the data that "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avspeechsynthesizerbuffercallback"
            },
            {
              "type": "text",
              "text": " provides for the same voice."
            }
          ]
        }
      ]
    }
