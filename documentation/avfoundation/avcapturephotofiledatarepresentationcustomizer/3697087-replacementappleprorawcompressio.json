    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The system calls this method when writing an Apple ProRAW image to a DNG file. To configure the compression settings the system uses when writing the file, return a dictionary that contains values for the following keys:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/avfoundation/avvideoqualitykey"
                    },
                    {
                      "type": "text",
                      "text": ". A floating-point value from 0.0 to 1.0. Specify a value of 1.0 to use lossless compression, or less than 1.0 for lossy compression."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/avfoundation/avvideoappleprorawbitdepthkey"
                    },
                    {
                      "type": "text",
                      "text": ". An integer value from 8 to "
                    },
                    {
                      "type": "codeVoice",
                      "code": "maximumBitDepth"
                    },
                    {
                      "type": "text",
                      "text": ". Setting this key to a value less than the value specified in "
                    },
                    {
                      "type": "codeVoice",
                      "code": "defaultSettings"
                    },
                    {
                      "type": "text",
                      "text": " may result in quantization losses."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Any keys not specified in the returned dictionary use the values from the "
            },
            {
              "type": "codeVoice",
              "code": "defaultSettings"
            },
            {
              "type": "text",
              "text": " dictionary. If your delegate object doesn’t implement this method, the system uses the default compression settings for DNG files."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
