      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A track group describes a group of related alternate tracks, only one of which should play at a time. Groups of alternate tracks typically contain variations of the same content, like subtitles in multiple translations."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Apps can inspect an asset’s track groups by loading and retrieving the value of the asset’s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avasset/1390697-trackgroups"
            },
            {
              "type": "text",
              "text": " property."
            }
          ]
        }
      ]
    }
