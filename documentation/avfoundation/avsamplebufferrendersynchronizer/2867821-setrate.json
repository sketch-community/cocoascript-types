    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method first sets the new time and then the new rendering rate. A "
            },
            {
              "type": "codeVoice",
              "code": "rate"
            },
            {
              "type": "text",
              "text": " value of "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " means that playback has stopped while a "
            },
            {
              "type": "codeVoice",
              "code": "rate"
            },
            {
              "type": "text",
              "text": " value of "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": " indicates playback should be at the natural rate of the media."
            }
          ]
        }
      ]
    }
