        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this property for frequent vloume changes; for example, a volume knob or fader. A value of "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " silences all audio while a value of "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": " plays all audio at full volume."
            }
          ]
        }
      ]
