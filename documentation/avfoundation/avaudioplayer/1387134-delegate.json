          "inlineContent": [
            {
              "type": "text",
              "text": "The object that you assign to be an audio player’s delegate becomes the target of the notifications described in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avaudioplayerdelegate"
            },
            {
              "type": "text",
              "text": ". These notifications let you respond to decoding errors, audio interruptions (such as an incoming phone call), and playback completion."
            }
          ]
        }
      ]
