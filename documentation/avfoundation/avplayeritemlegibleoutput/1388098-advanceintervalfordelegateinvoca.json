          "inlineContent": [
            {
              "type": "text",
              "text": "If possible, an "
            },
            {
              "type": "codeVoice",
              "code": "AVPlayerItemLegibleOutput"
            },
            {
              "type": "text",
              "text": " instance messages its delegate "
            },
            {
              "type": "codeVoice",
              "code": "advanceIntervalForDelegateInvocation"
            },
            {
              "type": "text",
              "text": " seconds earlier than it otherwise would."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the value provided is large, the delegate methods are invoked as soon as possible."
            }
          ]
        }
