      "parameters": [
        {
          "name": "mediaType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of media that an input accepts."
                }
              ]
            }
          ]
        },
        {
          "name": "outputSettings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The settings to use for encoding the media you append to the output. Create an output settings dictionary manually, or use "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avoutputsettingsassistant"
                },
                {
                  "type": "text",
                  "text": " to create preset-based settings."
                }
              ]
            }
          ]
        },
        {
          "name": "sourceFormatHint",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A hint about the format of the media data to append. The input uses the source format hint to fill in missing output settings. If you specify a hint, you only need to specify "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfaudio/avformatidkey"
                },
                {
                  "type": "text",
                  "text": " for the audio output settings, and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avvideocodeckey"
                },
                {
                  "type": "text",
                  "text": " is the only required key for video output settings."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The system raises an error if the format description isn’t valid for the indicated media type."
                }
              ]
            }
          ]
        }
      ]
    },
