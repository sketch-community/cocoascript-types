    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "mediaType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of samples to be accepted by the input object. For a list of media types, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avfoundation_constants"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "outputSettings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The settings used for encoding the media appended to the output. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to specify that the appended samples should not be re-encoded."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Audio output settings keys are defined in "
                },
                {
                  "type": "codeVoice",
                  "code": "AV Foundation Audio Settings Constants"
                },
                {
                  "type": "text",
                  "text": ". Video output settings keys are defined in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avfoundation_constants"
                },
                {
                  "type": "text",
                  "text": ". Video output settings with keys from "
                },
                {
                  "type": "codeVoice",
                  "code": "<CoreVideo/CVPixelBuffer.h>"
                },
                {
                  "type": "text",
                  "text": " are not currently supported."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new writer input object that can accept samples of the specified media type and write them to the output file."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Each new input accepts data for a new track of the asset writerâ€™s output file. You add an input to an asset writer using the "
            },
            {
              "type": "codeVoice",
              "code": "AVAssetWriter"
            },
            {
              "type": "text",
              "text": " method "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetwriter/1390389-addinput"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Passing "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " for "
            },
            {
              "type": "codeVoice",
              "code": "outputSettings"
            },
            {
              "type": "text",
              "text": " instructs the input to pass through appended samples, doing no processing before they are written to the output file. This is useful if, for example, you are appending buffers that are already in a desirable compressed format. "
            }
          ]
        }
      ]
    }
