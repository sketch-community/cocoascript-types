      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Devices of this type are not necessarily “telephoto” by a traditional photography definition; rather, this type merely distinguishes the narrower-angle from the wider-angle device on hardware equipped with cameras of both types. To determine the actual focal length of a camera device, examine the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevice/format"
            },
            {
              "type": "text",
              "text": " objects in the device’s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevice/1388738-formats"
            },
            {
              "type": "text",
              "text": " array."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "To obtain a device of this type, use the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevice/2361508-default"
                },
                {
                  "type": "text",
                  "text": " method or the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevice/discoverysession"
                },
                {
                  "type": "text",
                  "text": " class. Other methods do not discover devices of this type."
                }
              ]
            }
          ],
          "style": "important"
        }
      ]
    }
