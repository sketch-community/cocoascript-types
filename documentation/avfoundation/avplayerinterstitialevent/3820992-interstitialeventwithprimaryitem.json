    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "primaryItem",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The player item that represents the primary content. The item must contain an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avasset"
                },
                {
                  "type": "text",
                  "text": " that provides intrinsic mappings from its timeline to real-time dates."
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An external identifier for the event."
                }
              ]
            }
          ]
        },
        {
          "name": "date",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A date within the date range of the primary item that playback of interstitial content begins."
                }
              ]
            }
          ]
        },
        {
          "name": "templateItems",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of player item configurations to use as templates for player items that play interstitial content."
                }
              ]
            }
          ]
        },
        {
          "name": "restrictions",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Restrictions on access to playback controls during the event."
                }
              ]
            }
          ]
        },
        {
          "name": "resumptionOffset",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time offset for resuming playback of the primary content after interstital content finishes. You can specify a definite time, or specify "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coremedia/kcmtimeindefinite"
                },
                {
                  "type": "text",
                  "text": " to indicate that the effective resumption time offset needs to align with time elapsed during interstitial playback."
                }
              ]
            }
          ]
        },
        {
          "name": "playoutLimit",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time offset from the beginning of the interstitial when interstitial playback needs to end, if interstitial assets are longer. Pass a positive numeric value, or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coremedia/kcmtimeinvalid"
                },
                {
                  "type": "text",
                  "text": " to indicate no playout limit."
                }
              ]
            }
          ]
        },
        {
          "name": "userDefinedAttributes  ",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Custom attributes to add to the event."
                }
              ]
            }
          ]
        }
      ]
    }
