    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The returned format description is suitable for use as the format hint parameter when creating an instance of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetwriterinput"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Each item referenced by the receiver must carry a non-"
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " value for its "
            },
            {
              "type": "codeVoice",
              "code": "dataType"
            },
            {
              "type": "text",
              "text": " property.  An exception will be thrown if any item does not have a data type."
            }
          ]
        }
      ]
