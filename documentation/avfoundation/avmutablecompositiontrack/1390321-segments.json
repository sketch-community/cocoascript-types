      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "timeMapping.target.start"
            },
            {
              "type": "text",
              "text": " of the first track segment must be "
            },
            {
              "type": "codeVoice",
              "code": "kCMTimeZero"
            },
            {
              "type": "text",
              "text": ", and the "
            },
            {
              "type": "codeVoice",
              "code": "timeMapping.target.start"
            },
            {
              "type": "text",
              "text": " of each subsequent track segment must equal "
            },
            {
              "type": "codeVoice",
              "code": "CMTimeRangeGetEnd(<#previousTrackSegment#>.timeMapping.target)"
            },
            {
              "type": "text",
              "text": ". You can use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avmutablecompositiontrack/1388746-validatetracksegments"
            },
            {
              "type": "text",
              "text": " to ensure that an array of track segments conforms to this rule."
            }
          ]
        }
      ]
    }
