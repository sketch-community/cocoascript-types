      "parameters": [
        {
          "name": "track",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The track from which the reader should source sample buffers."
                }
              ]
            }
          ]
        },
        {
          "name": "outputSettings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary of output settings to be used for sample output. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to receive samples as stored in the track."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "You use keys from one of AVAudioSettings.h, Video_Settings, or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corevideo/cvpixelbufferref"
                },
                {
                  "type": "text",
                  "text": ", depending on the media type and the output format required.  "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A value of "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " configures the output to vend samples in their original format as stored by the specified track."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Initialization fails if the output settings cannot be used with the specified track."
                }
              ]
            }
          ]
        }
      ]
    },
