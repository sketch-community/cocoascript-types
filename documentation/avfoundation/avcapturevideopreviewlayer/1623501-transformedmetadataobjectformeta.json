        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A metadata object whose visual properties have been converted into layer coordinates, or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the "
            },
            {
              "type": "codeVoice",
              "code": "metadataObject"
            },
            {
              "type": "text",
              "text": " originates from an input source other than that of the preview layer."
            }
          ]
