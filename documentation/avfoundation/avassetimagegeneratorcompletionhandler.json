    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Pass a block of this type to "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetimagegenerator/1388100-generatecgimagesasynchronouslyfo"
            },
            {
              "type": "text",
              "text": ", and use that block to receive the generated images. The "
            },
            {
              "type": "codeVoice",
              "code": "generateCGImagesAsynchronously(forTimes:completionHandler:)"
            },
            {
              "type": "text",
              "text": " method executes this handler once for each time value you specified."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The block takes five arguments:"
            }
          ]
        },
        {
          "type": "termList",
          "items": [
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "requestedTime"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "The time for which you requested an image."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "image"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "The image that was generated, or "
                      },
                      {
                        "type": "codeVoice",
                        "code": "NULL"
                      },
                      {
                        "type": "text",
                        "text": " if the image could not be generated."
                      }
                    ]
                  },
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "This parameter follows "
                      },
                      {
                        "type": "link",
                        "title": "The Get Rule",
                        "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-SW1"
                      },
                      {
                        "type": "text",
                        "text": "."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "actualTime"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "The time at which the image was actually generated."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "result"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "A result code indicating whether the image generation process succeeded, failed, or was cancelled."
                      }
                    ]
                  }
                ]
              }
            },
            {
              "term": {
                "inlineContent": [
                  {
                    "type": "text",
                    "text": "error"
                  }
                ]
              },
              "definition": {
                "content": [
                  {
                    "type": "paragraph",
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "If "
                      },
                      {
                        "type": "codeVoice",
                        "code": "result"
                      },
                      {
                        "type": "text",
                        "text": " is "
                      },
                      {
                        "type": "reference",
                        "isActive": true,
                        "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassetimagegeneratorresult/avassetimagegeneratorfailed"
                      },
                      {
                        "type": "text",
                        "text": ", an error object that describes the problem."
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
