          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The connection through which the still image bracket should be captured."
                }
              ]
            }
          ]
        },
        {
          "name": "settings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturebracketedstillimagesettings"
                },
                {
                  "type": "text",
                  "text": " objects. All the array items must be of the same "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturebracketedstillimagesettings"
                },
                {
                  "type": "text",
                  "text": " subclass, or an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsinvalidargumentexception"
                },
                {
                  "type": "text",
                  "text": " exception is thrown."
                }
              ]
            }
          ]
        },
        {
          "name": "handler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A user provided block that will be called asynchronously as each still image in the bracket is captured."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block has three parameters:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "sampleBuffer"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "If the capture request is successful,  contains a valid CMSampleBuffer."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "stillImageSettings"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "Contains the "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturebracketedstillimagesettings"
                          },
                          {
                            "type": "text",
                            "text": " object corresponding to this still image."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "error"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "If the bracketed capture fails, "
                          },
                          {
                            "type": "codeVoice",
                            "code": "sampleBuffer"
                          },
                          {
                            "type": "text",
                            "text": " is "
                          },
                          {
                            "type": "codeVoice",
                            "code": "NULL"
                          },
                          {
                            "type": "text",
                            "text": " and error is non-"
                          },
                          {
                            "type": "codeVoice",
                            "code": "nil"
                          },
                          {
                            "type": "text",
                            "text": "."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If the count of the "
                },
                {
                  "type": "codeVoice",
                  "code": "settings"
                },
                {
                  "type": "text",
                  "text": " parameter exceeds "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturestillimageoutput/1616294-maxbracketedcapturestillimagecou"
                },
                {
                  "type": "text",
                  "text": ", then "
                },
                {
                  "type": "codeVoice",
                  "code": "AVErrorMaximumStillImageCaptureRequestsExceeded"
                },
                {
                  "type": "text",
                  "text": " is returned."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "You should not assume that the completion handler will be called on a specific thread."
                }
              ]
            }
          ]
        }
      ]
    },
