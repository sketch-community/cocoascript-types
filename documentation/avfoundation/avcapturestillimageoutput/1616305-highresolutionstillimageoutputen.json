      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, "
            },
            {
              "type": "codeVoice",
              "code": "AVCaptureStillImageOutput"
            },
            {
              "type": "text",
              "text": " emits images with the same dimensions as its source "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedevice"
            },
            {
              "type": "text",
              "text": " instance’s "
            },
            {
              "type": "codeVoice",
              "code": "activeFormat.formatDescription"
            },
            {
              "type": "text",
              "text": ".  However, if you set this property to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ", the receiver emits still images at the capture device’s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcapturedeviceformat/1624620-highresolutionstillimagedimensio"
            },
            {
              "type": "text",
              "text": " value.  "
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If you enable video stabilization by setting "
                },
                {
                  "type": "codeVoice",
                  "code": "preferredVideoStabilizationMode"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " for any output, the high resolution still images emitted by "
                },
                {
                  "type": "codeVoice",
                  "code": "AVCaptureStillImageOutput"
                },
                {
                  "type": "text",
                  "text": " may be smaller by 10% or more."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        }
      ]
    }
