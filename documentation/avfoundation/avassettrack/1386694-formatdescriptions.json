    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The array contains "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coremedia/cmformatdescriptionref"
            },
            {
              "type": "text",
              "text": " objects that indicate the format of media samples referenced by the track."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Asset tracks commonly present uniform media (for example, media encoded using the same encoding settings) and contain a single format description. However, in some cases an asset track may contain multiple format descriptions. For example, an H.264-encoded video track may have some of its segments encoded using the Main profile and others using the High profile. Also, an individual "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avcompositiontrack"
            },
            {
              "type": "text",
              "text": ", which is a subclass of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/avfoundation/avassettrack"
            },
            {
              "type": "text",
              "text": ", may contain audio or video segments using different codecs."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can use "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/coremedia/cmformatdescriptionref"
            },
            {
              "type": "text",
              "text": " to access low-level details about the media referenced by the track. For example, you could retrieve the details of track’s media type and subtype as shown below:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "@implementation AVAssetTrack (CoreMediaExtensions)",
            " ",
            "- (NSString *)mediaFormat {",
            "    NSMutableString *format = [[NSMutableString alloc] init];",
            "    for (int i = 0; i < self.formatDescriptions.count; i++) {",
            "        CMFormatDescriptionRef desc =",
            "            (__bridge CMFormatDescriptionRef)self.formatDescriptions[i];",
            "        // Get String representation of media type (vide, soun, sbtl, etc.)",
            "        NSString *type = FourCCString(CMFormatDescriptionGetMediaType(desc));",
            "        // Get String representation media subtype (avc1, aac, tx3g, etc.)",
            "        NSString *subType = FourCCString(CMFormatDescriptionGetMediaSubType(desc));",
            "        // Format string as type/subType",
            "        [format appendFormat:@\"%@/%@\", type, subType];",
            "        // Comma separate if more than one format description",
            "        if (i < self.formatDescriptions.count - 1) {",
            "            [format appendString:@\",\"];",
            "        }",
            "    }",
            "    return format;",
            "}",
            " ",
            "static NSString * FourCCString(FourCharCode code) {",
            "    NSString *result = [NSString stringWithFormat:@\"%c%c%c%c\",",
            "                        (code >> 24) & 0xff,",
            "                        (code >> 16) & 0xff,",
            "                        (code >> 8) & 0xff,",
            "                        code & 0xff];",
            "    NSCharacterSet *characterSet = [NSCharacterSet whitespaceCharacterSet];",
            "    return [result stringByTrimmingCharactersInSet:characterSet];",
            "}",
            " ",
            "@end"
          ],
          "syntax": "occ",
          "metadata": {
