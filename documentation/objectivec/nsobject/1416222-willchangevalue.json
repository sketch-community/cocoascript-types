        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method when implementing key-value observer compliance manually to inform the observed object that the value at "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " is about to change."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The change type of this method is "
            },
            {
              "type": "codeVoice",
              "code": "NSKeyValueChangeSetting"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "After the values have been changed, a corresponding "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1411809-didchangevalue"
                },
                {
                  "type": "text",
                  "text": " must be invoked with the same parameter. "
                }
              ]
            }
          ],
          "style": "important"
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Special Considerations",
          "anchor": "1891993"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You rarely need to override this method in subclasses, but if you do, be sure to call "
            },
            {
              "type": "codeVoice",
              "code": "super"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
