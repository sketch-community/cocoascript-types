          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If an error occurs during the commit operation, upon returns contains an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSError"
                },
                {
                  "type": "text",
                  "text": " object that describes the problem."
                }
              ]
            }
          ]
        }
      ]
    },
