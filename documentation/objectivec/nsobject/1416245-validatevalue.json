    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "ioValue",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a new value for the property identified by "
                },
                {
                  "type": "codeVoice",
                  "code": "inK"
                },
                {
                  "type": "codeVoice",
                  "code": "eyPath"
                },
                {
                  "type": "text",
                  "text": ". This method may modify or replace the value in order to make it valid."
                }
              ]
            }
          ]
        },
        {
          "name": "inKeyPath",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of one of the receiver's properties. The key path must specify an attribute or a to-one relationship. The key path has the form "
                },
                {
                  "type": "codeVoice",
                  "code": "relationship"
                },
                {
                  "type": "codeVoice",
                  "code": "."
                },
                {
                  "type": "codeVoice",
                  "code": "property"
                },
                {
                  "type": "text",
                  "text": " (with one or more relationships); for example "
                },
                {
                  "type": "codeVoice",
                  "code": "department.name"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "department.manager.lastName"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "outError",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If validation is necessary and "
                },
                {
                  "type": "codeVoice",
                  "code": "ioValue"
                },
                {
                  "type": "text",
                  "text": " is not transformed into a valid value, upon return contains an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nserror"
                },
                {
                  "type": "text",
                  "text": " object that describes the reason that "
                },
                {
                  "type": "codeVoice",
                  "code": "ioValue"
                },
                {
                  "type": "text",
                  "text": " is not a valid value."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A Boolean that is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " if the value pointed at by "
            },
            {
              "type": "codeVoice",
              "code": "ioValue"
            },
            {
              "type": "text",
              "text": " is valid for the property identified by "
            },
            {
              "type": "codeVoice",
              "code": "inKeyPath"
            },
            {
              "type": "text",
              "text": ", or if the method is able to modify the value at "
            },
            {
              "type": "codeVoice",
              "code": "ioValue"
            },
            {
              "type": "text",
              "text": " to make it valid; otherwise "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default implementation of this method gets the destination object for each relationship using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1412591-valueforkey"
            },
            {
              "type": "text",
              "text": " and returns the result of calling the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1416754-validatevalue"
            },
            {
              "type": "text",
              "text": " method for the property."
            }
          ]
        }
      ]
    }
