    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The version number assigned to the class."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If no version has been set, the default is "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Version numbers are needed for decoding or unarchiving, so older versions of an object can be detected and decoded correctly."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Caution should be taken when obtaining the version from within an "
            },
            {
              "type": "codeVoice",
              "code": "NSCoding"
            },
            {
              "type": "text",
              "text": " protocol or other methods. Use the class name explicitly when getting a class version number:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "version = [MyClass version];"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555786",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Don’t simply send "
            },
            {
              "type": "codeVoice",
              "code": "version"
            },
            {
              "type": "text",
              "text": " to the return value of class—a subclass version number may be returned instead."
            }
          ]
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Special Considerations",
          "anchor": "1968712"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The version number applies to "
            },
            {
              "type": "codeVoice",
              "code": "NSArchiver"
            },
            {
              "type": "text",
              "text": "/"
            },
            {
              "type": "codeVoice",
              "code": "NSUnarchiver"
            },
            {
              "type": "text",
              "text": ", but not to "
            },
            {
              "type": "codeVoice",
              "code": "NSKeyedArchiver"
            },
            {
              "type": "text",
              "text": "/"
            },
            {
              "type": "codeVoice",
              "code": "NSKeyedUnarchiver"
            },
            {
              "type": "text",
              "text": ".  A keyed archiver does not encode class version numbers."
            }
          ]
        }
      ]
    }
