          "name": "error",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nserror"
                },
                {
                  "type": "text",
                  "text": " object that describes the error, including error recovery options."
                }
              ]
            }
          ]
        },
        {
          "name": "recoveryOptionIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index of the user selected recovery option in "
                },
                {
                  "type": "codeVoice",
                  "code": "error"
                },
                {
                  "type": "text",
                  "text": "’s localized recovery array."
                }
              ]
            }
          ]
        },
        {
          "name": "delegate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An object that is the modal delegate."
                }
              ]
            }
          ]
        },
        {
          "name": "didRecoverSelector",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A selector identifying the method implemented by the modal delegate."
                }
              ]
            }
          ]
        },
        {
          "name": "contextInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Arbitrary data associated with the attempt at error recovery, to be passed to "
                },
                {
                  "type": "codeVoice",
                  "code": "delegate"
                },
                {
                  "type": "text",
                  "text": " in "
                },
                {
                  "type": "codeVoice",
                  "code": "didRecoverSelector"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Invoked when an error alert is presented to the user in a document-modal sheet, and the user has selected an error recovery option specified by "
            },
            {
              "type": "codeVoice",
              "code": "error"
            },
            {
              "type": "text",
              "text": ". After recovery is attempted, your implementation should send "
            },
            {
              "type": "codeVoice",
              "code": "delegate"
            },
            {
              "type": "text",
              "text": " the message specified in "
            },
            {
              "type": "codeVoice",
              "code": "didRecoverSelector"
            },
            {
              "type": "text",
              "text": ", passing the provided "
            },
            {
              "type": "codeVoice",
              "code": "contextInfo"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "didRecoverSelector"
            },
            {
              "type": "text",
              "text": " should have the following signature:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)didPresentErrorWithRecovery:(BOOL)didRecover contextInfo:(void *)contextInfo;"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555934",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "where "
            },
            {
              "type": "codeVoice",
              "code": "didRecover"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " if the error recovery attempt was successful; otherwise it is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
