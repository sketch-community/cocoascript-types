      ],
      "parameters": [
        {
          "name": "keyPath",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key path, relative to "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": ", to the value that has changed."
                }
              ]
            }
          ]
        },
        {
          "name": "object",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The source object of the key path "
                },
                {
                  "type": "codeVoice",
                  "code": "keyPath"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "change",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary that describes the changes that have been made to the value of the property at the key path "
                },
                {
                  "type": "codeVoice",
                  "code": "keyPath"
                },
                {
                  "type": "text",
                  "text": " relative to "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": ". Entries are described in "
                },
                {
                  "type": "codeVoice",
                  "code": "Change Dictionary Keys"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value that was provided when the observer was registered to receive key-value observation notifications."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For an "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " to begin sending change notification messages for the value at "
            },
            {
              "type": "codeVoice",
              "code": "keyPath"
            },
            {
              "type": "text",
              "text": ", you send it an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1412787-addobserver"
            },
            {
              "type": "text",
              "text": " message, naming the observing object that should receive the messages. When you are done observing, and in particular before the observing object is deallocated, you send the observed object a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1408054-removeobserver"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1410691-removeobserver"
            },
            {
              "type": "text",
              "text": " message to unregister the observer, and stop sending change notification messages."
            }
          ]
        }
      ]
    }
