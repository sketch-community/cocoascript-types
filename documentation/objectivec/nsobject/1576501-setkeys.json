          "name": "keys",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The names of the properties upon which the value of the property identified by "
                },
                {
                  "type": "codeVoice",
                  "code": "dependentKey"
                },
                {
                  "type": "text",
                  "text": " depends."
                }
              ]
            }
          ]
        },
        {
          "name": "dependentKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of a property whose value depends on the properties specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "keys"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
