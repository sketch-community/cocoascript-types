      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When you create an instance of a particular class, the allocated memory contains an "
            },
            {
              "type": "codeVoice",
              "code": "objc_object"
            },
            {
              "type": "text",
              "text": " data structure, which is directly followed by the data for the instance variables of the class. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "alloc"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "allocWithZone:"
            },
            {
              "type": "text",
              "text": " methods of the Foundation framework class NSObject use the function "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/1441565-class_createinstance"
            },
            {
              "type": "text",
              "text": " to create "
            },
            {
              "type": "codeVoice",
              "code": "objc_object"
            },
            {
              "type": "text",
              "text": " data structures."
            }
          ]
        }
      ]
    }
