      "abstract": [
        {
          "type": "text",
          "text": "An array of pointers to "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/method"
        },
        {
          "type": "text",
          "text": " data structures. This array may contain no more than "
        },
        {
          "type": "codeVoice",
          "code": "mask + 1"
        },
