              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The memory allocator to use. If "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", the default allocator is used."
                }
              ]
