    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new object. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "protocolFamily",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The protocol family for the socket. If negative or 0 is passed, the socket defaults to "
                },
                {
                  "type": "codeVoice",
                  "code": "PF_INET"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "socketType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The socket type to create. If "
                },
                {
                  "type": "codeVoice",
                  "code": "protocolFamily"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "PF_INET"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "socketType"
                },
                {
                  "type": "text",
                  "text": " is negative or 0, the socket type defaults to "
                },
                {
                  "type": "codeVoice",
                  "code": "SOCK_STREAM"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "protocol",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The protocol for the socket. If "
                },
                {
                  "type": "codeVoice",
                  "code": "protocolFamily"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "PF_INET"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "protocol"
                },
                {
                  "type": "text",
                  "text": " is negative or 0, the socket protocol defaults to "
                },
                {
                  "type": "codeVoice",
                  "code": "IPPROTO_TCP"
                },
                {
                  "type": "text",
                  "text": " if "
                },
                {
                  "type": "codeVoice",
                  "code": "socketType"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "SOCK_STREAM"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "IPPROTO_UDP"
                },
                {
                  "type": "text",
                  "text": " if "
                },
                {
                  "type": "codeVoice",
                  "code": "socketType"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "SOCK_DGRAM"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "callBackTypes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bitwise-OR combination of the types of socket activity that should cause "
                },
                {
                  "type": "codeVoice",
                  "code": "callout"
                },
                {
                  "type": "text",
                  "text": " to be called. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype"
                },
                {
                  "type": "text",
                  "text": " for the possible activity values. "
                }
              ]
            }
          ]
        },
        {
          "name": "callout",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The function to call when one of the activities indicated by "
                },
                {
                  "type": "codeVoice",
                  "code": "callBackTypes"
                },
                {
                  "type": "text",
                  "text": " occurs. "
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A structure holding contextual information for the CFSocket object. The function copies the information out of the structure, so the memory pointed to by "
                },
                {
                  "type": "codeVoice",
                  "code": "context"
                },
                {
                  "type": "text",
                  "text": " does not need to persist beyond the function call. Can be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The new CFSocket object, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if an error occurred. Ownership follows the "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
