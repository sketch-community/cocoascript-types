      "parameters": [
        {
          "name": "applicationID",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The ID of the application whose preferences to write to storage, typically "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencescurrentapplication"
                },
                {
                  "type": "text",
                  "text": ". Do not pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencesanyapplication"
                },
                {
                  "type": "text",
                  "text": ". Takes the form of a Java package name, "
                },
                {
                  "type": "codeVoice",
                  "code": "com.foosoft"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if synchronization was successful, otherwise "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Calling the function "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1515528-cfpreferencessetappvalue"
            },
            {
              "type": "text",
              "text": " is not in itself sufficient for storing preferences. The "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1515510-cfpreferencesappsynchronize"
            },
            {
              "type": "text",
              "text": " function writes to permanent storage all pending preference changes for the application. Typically you would call this function after multiple calls to "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1515528-cfpreferencessetappvalue"
            },
            {
              "type": "text",
              "text": ". Conversely, preference data is cached after it is first read. Changes made externally are not automatically incorporated. The "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1515510-cfpreferencesappsynchronize"
            },
            {
              "type": "text",
              "text": " function reads the latest preferences from permanent storage."
            }
          ]
        }
      ]
    }
  ],
