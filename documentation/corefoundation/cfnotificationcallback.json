      ],
      "parameters": [
        {
          "name": "center",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The notification center handling the notification. "
                }
              ]
            }
          ]
        },
        {
          "name": "observer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An arbitrary value, other than "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", that identifies the observer."
                }
              ]
            }
          ]
        },
        {
          "name": "name",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the notification being posted. "
                }
              ]
            }
          ]
        },
        {
          "name": "object",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An arbitrary value that identifies the object posting the notification. For distributed notifications, "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": " is always a CFString object. This value could be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "userInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary containing additional information regarding the notification. This value could be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If the notification center is a Darwin notification center, this value must be ignored."
                }
              ]
            }
          ]
        }
      ]
    }
