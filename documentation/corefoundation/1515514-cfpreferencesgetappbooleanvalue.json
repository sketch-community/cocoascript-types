      "parameters": [
        {
          "name": "key",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The preference key whose value to obtain. The key must specify a preference whose value is of type "
                },
                {
                  "type": "codeVoice",
                  "code": "Boolean"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "applicationID",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The identifier of the application whose preferences are searched, typically "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencescurrentapplication"
                },
                {
                  "type": "text",
                  "text": ". Do not pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencesanyapplication"
                },
                {
                  "type": "text",
                  "text": ". Takes the form of a Java package name, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "com.foosoft"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "keyExistsAndHasValidFormat",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " if the preference value for the specified key was located and found to be of type "
                },
                {
                  "type": "codeVoice",
                  "code": "Boolean"
                },
                {
                  "type": "text",
                  "text": ", otherwise "
                },
                {
                  "type": "codeVoice",
                  "code": "false"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The preference data for the specified key and application, or if no value was located, "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
