              "inlineContent": [
                {
                  "type": "text",
                  "text": "A CFType object to release. This value must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
