      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new "
                },
                {
                  "type": "codeVoice",
                  "code": "CFData"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "CFDictionary"
                },
                {
                  "type": "text",
                  "text": " objects returned in "
                },
                {
                  "type": "codeVoice",
                  "code": "resourceData"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "properties"
                },
                {
                  "type": "text",
                  "text": ". Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or kCFAllocatorDefault to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "url",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL referring to the data and/or properties you wish to load."
                }
              ]
            }
          ]
        },
        {
          "name": "resourceData",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, contains a "
                },
                {
                  "type": "codeVoice",
                  "code": "CFData"
                },
                {
                  "type": "text",
                  "text": " object containing the data referred to by "
                },
                {
                  "type": "codeVoice",
                  "code": "url"
                },
                {
                  "type": "text",
                  "text": ". Ownership follows the "
                },
                {
                  "type": "link",
                  "title": "The Create Rule",
                  "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "properties",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, a pointer to a "
                },
                {
                  "type": "codeVoice",
                  "code": "CFDictionary"
                },
                {
                  "type": "text",
                  "text": " object containing the resource properties referred to by "
                },
                {
                  "type": "codeVoice",
                  "code": "url"
                },
                {
                  "type": "text",
                  "text": ". Ownership follows the "
                },
                {
                  "type": "link",
                  "title": "The Create Rule",
                  "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "desiredProperties",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A list of the properties you wish to obtain and return in "
                },
                {
                  "type": "codeVoice",
                  "code": "properties"
                },
                {
                  "type": "text",
                  "text": ". See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/core_foundation_url_access_utilities/file_url_properties"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/core_foundation_url_access_utilities/http_url_properties"
                },
                {
                  "type": "text",
                  "text": " for the list of available properties."
                }
              ]
            }
          ]
        },
        {
          "name": "errorCode",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " if successful, otherwise an error code indicating the nature of the problem. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfurlerror"
                },
                {
                  "type": "text",
                  "text": " for a list of possible error codes."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if successful, "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " otherwise. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you are interested in loading only the resource data or the resource's properties, pass "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " for the one you don't want. If "
            },
            {
              "type": "codeVoice",
              "code": "properties"
            },
            {
              "type": "text",
              "text": " is non-"
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "desiredProperties"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " then all properties are fetched. Note that as much work as possible is done even if "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " is returned. For instance, if one property is not available, the others are fetched anyway. This function is intended for convenience, not performance."
            }
          ]
        }
      ]
    }
