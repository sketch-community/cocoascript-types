        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if "
            },
            {
              "type": "codeVoice",
              "code": "stream"
            },
            {
              "type": "text",
              "text": " was successfully opened, "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " otherwise. If "
            },
            {
              "type": "codeVoice",
              "code": "stream"
            },
            {
              "type": "text",
              "text": " is not in the "
            },
            {
