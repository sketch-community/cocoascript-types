          "name": "alloc",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to be used to allocate memory for the new attributed string. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or kCFAllocatorDefault to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "maxLength",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The limit on the length of the new attributed string. The string starts empty and can grow to this length (it can be shorter)."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " to specify that the maximum length is not limited. If non-"
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "maxLength"
                },
                {
                  "type": "text",
                  "text": " must be greater than or equal to the length of "
                },
                {
                  "type": "codeVoice",
                  "code": "aStr"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "aStr",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The attributed string to copy. "
                }
              ]
            }
          ]
        }
      ]
    },
