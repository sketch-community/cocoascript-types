          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the structure into which the context information for "
                },
                {
                  "type": "codeVoice",
                  "code": "port"
                },
                {
                  "type": "text",
                  "text": " is to be copied. The information being returned is usually the same information you passed to "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1400934-cfmachportcreate"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1400924-cfmachportcreatewithport"
                },
                {
                  "type": "text",
                  "text": " when creating "
                },
                {
                  "type": "codeVoice",
                  "code": "port"
                },
                {
                  "type": "text",
                  "text": ". However, if "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1400924-cfmachportcreatewithport"
                },
                {
                  "type": "text",
                  "text": " returned a cached CFMachPort object instead of creating a new object, "
                },
                {
                  "type": "codeVoice",
                  "code": "context"
                },
                {
                  "type": "text",
                  "text": " is filled with information from the original CFMachPort object instead of the information you passed to the function."
                }
              ]
            }
          ]
        }
      ]
