    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new set and its storage for values. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "capacity",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum number of values that can be contained by the new set. The set starts with the same number of values as "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": " and can grow to this number of values (and it can have less)."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " to specify that the maximum capacity is not limited. If non-"
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "capacity"
                },
                {
                  "type": "text",
                  "text": " must be greater than or equal to the count of "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "theSet",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The set to copy. The pointer values from "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": " are copied into the new set. The values are also retained by the new set. The count of the new set is the same as the count of "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": ". The new set uses the same callbacks as "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new mutable set that contains the same values as "
            },
            {
              "type": "codeVoice",
              "code": "theSet"
            },
            {
              "type": "text",
              "text": ". Ownership follows the "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
