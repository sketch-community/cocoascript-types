        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You must set this value before opening the writable file stream. The default value is "
            },
            {
              "type": "codeVoice",
              "code": "kCFBooleanFalse"
            },
            {
              "type": "text",
              "text": ", indicating that the stream should replace any pre-existing file. You cannot read this value."
            }
          ]
        }
