    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "timer"
            },
            {
              "type": "text",
              "text": " repeats, the run loop automatically schedules the next firing time after calling this function, unless you manually update the firing time within this callback by calling "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1542501-cfrunlooptimersetnextfiredate"
            },
            {
              "type": "text",
              "text": ". If "
            },
            {
              "type": "codeVoice",
              "code": "timer"
            },
            {
              "type": "text",
              "text": " does not repeat, the run loop invalidates "
            },
            {
              "type": "codeVoice",
              "code": "timer"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You specify this callback when you create the timer with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1543570-cfrunlooptimercreate"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
