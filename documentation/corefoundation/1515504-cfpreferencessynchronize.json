      ],
      "parameters": [
        {
          "name": "applicationID",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The ID of the application whose preferences you wish to modify. Takes the form of a Java package name, "
                },
                {
                  "type": "codeVoice",
                  "code": "com.foosoft"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "userName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencescurrentuser"
                },
                {
                  "type": "text",
                  "text": " to modify the current user’s preferences, otherwise "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencesanyuser"
                },
                {
                  "type": "text",
                  "text": " to modify the preferences of all users. "
                }
              ]
            }
          ]
        },
        {
          "name": "hostName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencescurrenthost"
                },
                {
                  "type": "text",
                  "text": " to search the current-host domain, otherwise "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencesanyhost"
                },
                {
                  "type": "text",
                  "text": " to search the any-host domain."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if synchronization was successful, "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " if an error occurred. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function is the primitive synchronize mechanism for the higher level preference function "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1515510-cfpreferencesappsynchronize"
            },
            {
              "type": "text",
              "text": "; it writes updated preferences to permanent storage, and reads the latest preferences from permanent storage. Only the exact domain specified is modified. Note that to modify “Any User” preferences requires root privileges (or Admin privileges prior to OS X v10.6)—see "
            },
            {
              "type": "link",
              "title": "Authorization Services Programming Guide",
              "destination": "https://developer.apple.com/library/archive/documentation/Security/Conceptual/authorization_concepts/01introduction/introduction.html#//apple_ref/doc/uid/TP30000995"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Do not use this function directly unless you have a specific need. All arguments must be non- "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": ". Do not use arbitrary user and host names, instead pass the pre-defined constants. "
            }
          ]
        }
      ]
    }
