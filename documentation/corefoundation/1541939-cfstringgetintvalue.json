      ],
      "parameters": [
        {
          "name": "str",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string that represents a signed integer value. The only allowed characters are the ASCII digit characters (ASCII "
                },
                {
                  "type": "codeVoice",
                  "code": "0x30"
                },
                {
                  "type": "text",
                  "text": " - "
                },
                {
                  "type": "codeVoice",
                  "code": "0x39"
                },
                {
                  "type": "text",
                  "text": "), the plus sign (ASCII "
                },
                {
                  "type": "codeVoice",
                  "code": "0x2B"
                },
                {
                  "type": "text",
                  "text": "), the minus sign (ASCII "
                },
                {
                  "type": "codeVoice",
                  "code": "0x2D"
                },
                {
                  "type": "text",
                  "text": "), and the period character (ASCII "
                },
                {
                  "type": "codeVoice",
                  "code": "0x2E"
                },
                {
                  "type": "text",
                  "text": "). "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The signed integer value represented by "
            },
            {
              "type": "codeVoice",
              "code": "str"
            },
            {
              "type": "text",
              "text": ". The result is "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " if there is a scanning error (if the string contains disallowed characters or does not represent an integer value) or "
            },
            {
              "type": "codeVoice",
              "code": "INT_MAX"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "INT_MIN"
            },
            {
              "type": "text",
              "text": " if there is an overflow error. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Consider the following example: "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "SInt32 val = CFStringGetIntValue(CFSTR(\"-123\"));"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2556097",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The variable "
            },
            {
              "type": "codeVoice",
              "code": "val"
            },
            {
              "type": "text",
              "text": " in this example would contain the value "
            },
            {
              "type": "codeVoice",
              "code": "-123"
            },
            {
              "type": "text",
              "text": " after the function is called."
            }
          ]
        }
      ]
    }
