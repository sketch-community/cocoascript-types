            },
            {
              "kind": "text",
              "text": "("
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@CFAllocatorRef",
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfallocatorref",
              "text": "CFAllocatorRef"
            },
            {
              "kind": "text",
              "text": " allocator, "
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@CFAllocatorContext",
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfallocatorcontext",
