    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new "
                },
                {
                  "type": "codeVoice",
                  "code": "CFString"
                },
                {
                  "type": "text",
                  "text": " object. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "originalString",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "CFString"
                },
                {
                  "type": "text",
                  "text": " object to copy."
                }
              ]
            }
          ]
        },
        {
          "name": "charactersToLeaveUnescaped",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Characters whose percent escape sequences you want to leave intact. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to specify that all illegal characters be escaped."
                }
              ]
            }
          ]
        },
        {
          "name": "legalURLCharactersToBeEscaped",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Legal characters to be escaped. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to specify that no legal characters be replaced."
                }
              ]
            }
          ]
        },
        {
          "name": "encoding",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The encoding to use for the translation. If you are uncertain of the correct encoding, you should use UTF-8 ("
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringbuiltinencodings/kcfstringencodingutf8"
                },
                {
                  "type": "text",
                  "text": "), which is the encoding designated by RFC 2396 as the correct encoding for use in URLs."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A copy of "
            },
            {
              "type": "codeVoice",
              "code": "originalString"
            },
            {
              "type": "text",
              "text": " replacing certain characters. If it does not need to be modified (no percent escape sequences are missing), this function may merely return "
            },
            {
              "type": "codeVoice",
              "code": "originalString"
            },
            {
              "type": "text",
              "text": " with its reference count incremented. Ownership follows the create rule. See "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The characters escaped are all characters that are not legal URL characters (based on RFC 2396), plus any characters in "
            },
            {
              "type": "codeVoice",
              "code": "legalURLCharactersToBeEscaped"
            },
            {
              "type": "text",
              "text": ", less any characters in "
            },
            {
              "type": "codeVoice",
              "code": "charactersToLeaveUnescaped"
            },
            {
              "type": "text",
              "text": ". To simply correct any non-URL characters in an otherwise correct URL string, pass "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " for the "
            },
            {
              "type": "codeVoice",
              "code": "allocator"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "charactersToLeaveEscaped"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "legalURLCharactersToBeEscaped"
            },
            {
              "type": "text",
              "text": " parameters, and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringbuiltinencodings/kcfstringencodingutf8"
            },
            {
              "type": "text",
              "text": " as the "
            },
            {
              "type": "codeVoice",
              "code": "encoding"
            },
            {
              "type": "text",
              "text": " parameter."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "It may be difficult to use this function to \"clean up\" unescaped or partially escaped URL strings where sequences are unpredictable and you cannot specify "
            },
            {
              "type": "codeVoice",
              "code": "charactersToLeaveUnescaped"
            },
            {
              "type": "text",
              "text": ". Instead, you can \"pre-process\" a URL string using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1541974-cfurlcreatestringbyreplacingperc"
            },
            {
              "type": "text",
              "text": " then add the escape characters using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1542665-cfurlcreatestringbyaddingpercent"
            },
            {
              "type": "text",
              "text": ", as shown in the following code fragment. "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "CFStringRef originalURLString = CFSTR(\"http://online.store.com/storefront/?request=get-document&doi=10.1175%2F1520-0426(2005)014%3C1157:DODADSS%3E2.0.CO%3B2\");",
            "CFStringRef preprocessedString =",
            "    CFURLCreateStringByReplacingPercentEscapesUsingEncoding(kCFAllocatorDefault, originalURLString, CFSTR(\"\"), kCFStringEncodingUTF8);",
            "CFStringRef urlString =",
            "    CFURLCreateStringByAddingPercentEscapes(kCFAllocatorDefault, preprocessedString, NULL, NULL, kCFStringEncodingUTF8);",
            "url = CFURLCreateWithString(kCFAllocatorDefault, urlString, NULL);"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2556100",
            "title": "Listing 1"
          }
        }
      ]
    }
