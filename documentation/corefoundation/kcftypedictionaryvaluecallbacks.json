      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": " The retain callback is "
            },
            {
              "type": "codeVoice",
              "code": "CFRetain"
            },
            {
              "type": "text",
              "text": ", the release callback is "
            },
            {
              "type": "codeVoice",
              "code": "CFRelease"
            },
            {
              "type": "text",
              "text": ", the copy callback is "
            },
            {
              "type": "codeVoice",
              "code": "CFCopyDescription"
            },
            {
              "type": "text",
              "text": ", and the equal callback is "
            },
            {
              "type": "codeVoice",
              "code": "CFEqual"
            },
            {
              "type": "text",
              "text": ". Therefore, if you use a pointer to this constant when creating the dictionary, values are automatically retained when added to the collection, and released when removed from the collection."
            }
          ]
        }
      ]
    }
