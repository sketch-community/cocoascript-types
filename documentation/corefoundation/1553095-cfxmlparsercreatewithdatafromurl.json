      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new object. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or kCFAllocatorDefault to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "dataSource",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL from which to load the XML data. The URL is used to resolve any relative references found in XML Data. It must be a valid CFURL object; "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " is an unacceptable value. "
                }
              ]
            }
          ]
        },
        {
          "name": "parseOptions",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Flags which control how the XML data will be parsed. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfxmlparseroptions"
                },
                {
                  "type": "text",
                  "text": " for the list of available options."
                }
              ]
            }
          ]
        },
        {
          "name": "versionOfNodes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Determines which version of CFXMLNode objects are produced by the parser. "
                }
              ]
            }
          ]
        },
        {
          "name": "callBacks",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Callbacks called by the parser as the XML is processed. The callbacks are called as each XML tag is encountered, when an external entity needs to be resolved, and when an error occurs. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfxmlparsercallbacks"
                },
                {
                  "type": "text",
                  "text": " and the individual callbacks for more details. Do not pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Determines what, if any, information pointer is passed to the callbacks as the parse progresses; may be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
