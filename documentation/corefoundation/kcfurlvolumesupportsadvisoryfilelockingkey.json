{
  "abstract": [
    {
      "type": "text",
      "text": "Key for determining whether the volume implements whole-file advisory locks in the style of flock, along with the "
    },
    {
      "type": "codeVoice",
      "code": "O_EXLOCK"
    },
    {
      "type": "text",
      "text": " and "
    },
    {
      "type": "codeVoice",
      "code": "O_SHLOCK"
    },
    {
      "type": "text",
      "text": " flags of the open function, returned as a "
    },
    {
      "type": "codeVoice",
      "code": "CFBoolean"
    },
    {
      "type": "text",
      "text": " object."
    }
  ],
