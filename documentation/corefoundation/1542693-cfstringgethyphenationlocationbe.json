        {
          "name": "string",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The string to be hyphenated. If this parameter is not a valid CFString object, the behavior is undefined."
                }
              ]
            }
          ]
        },
        {
          "name": "location",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An index in the string. If a valid hyphen index is returned, it will be before this index."
                }
              ]
            }
          ]
        },
        {
          "name": "limitRange",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range of characters within the string to search. If the range location or end point (defined by the location plus length minus 1) are outside the index space of the string (0 to N-1 inclusive, where N is the length of the string), the behavior is undefined. If the range length is negative, the behavior is undefined. The range may be empty (length 0), in which case no hyphen location is generated."
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Reserved for future use."
                }
              ]
            }
          ]
        },
        {
          "name": "locale",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A valid locale that specifies which language's hyphenation conventions to use. Hyphenation data is not available for all locales. You can use "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1543237-cfstringishyphenationavailablefo"
                },
                {
                  "type": "text",
                  "text": " to test for availability of hyphenation data."
                }
              ]
            }
          ]
        },
        {
          "name": "character",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The suggested hyphen character to insert. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you do not need this information."
                }
              ]
            }
          ]
        }
      ]
