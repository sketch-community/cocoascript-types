              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "CFURL"
                },
                {
                  "type": "text",
                  "text": " object to examine. "
                }
              ]
            }
          ]
