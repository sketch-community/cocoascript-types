      ],
      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new set and its storage for values. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "theSet",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The set to copy. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new set that contains the same values as "
            },
            {
              "type": "codeVoice",
              "code": "theSet"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if there was a problem creating the object. Ownership follows the "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The pointer values from "
            },
            {
              "type": "codeVoice",
              "code": "theSet"
            },
            {
              "type": "text",
              "text": " are copied into the new set, and the values are retained by the new set. The count of the new set is the same as the count of "
            },
            {
              "type": "codeVoice",
              "code": "theSet"
            },
            {
              "type": "text",
              "text": ". The new set uses the same callbacks as "
            },
            {
              "type": "codeVoice",
              "code": "theSet"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        }
      ]
    }
