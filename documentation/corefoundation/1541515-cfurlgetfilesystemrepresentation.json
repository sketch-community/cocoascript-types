      "parameters": [
        {
          "name": "url",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "CFURL"
                },
                {
                  "type": "text",
                  "text": " object whose native file system representation you want to obtain. "
                }
              ]
            }
          ]
        },
        {
          "name": "resolveAgainstBase",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " to return an absolute path name. "
                }
              ]
            }
          ]
        },
        {
          "name": "buffer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a character buffer. On return, the buffer holds the native file system's representation of "
                },
                {
                  "type": "codeVoice",
                  "code": "url"
                },
                {
                  "type": "text",
                  "text": ". The buffer is null-terminated. This parameter must be at least "
                },
                {
                  "type": "codeVoice",
                  "code": "maxBufLen"
                },
                {
                  "type": "text",
                  "text": " in size for the file system in question to avoid failures for insufficiently large buffers."
                }
              ]
            }
          ]
        },
        {
          "name": "maxBufLen",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum number of characters that can be written to "
                },
                {
                  "type": "codeVoice",
                  "code": "buffer"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if successful, "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " if an error occurred. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "No more than "
            },
            {
              "type": "codeVoice",
              "code": "maxBufLen"
            },
            {
              "type": "text",
              "text": " bytes are written to "
            },
            {
              "type": "codeVoice",
              "code": "buffer"
            },
            {
              "type": "text",
              "text": ". If "
            },
            {
              "type": "codeVoice",
              "code": "url"
            },
            {
              "type": "text",
              "text": " requires more than "
            },
            {
              "type": "codeVoice",
              "code": "maxBufLen"
            },
            {
              "type": "text",
              "text": " bytes to represent itself, including the terminating null byte, this function returns "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": ". To avoid this possible failure, you should pass a buffer with size of at least the maximum path length for the file system in question."
            }
          ]
        }
      ]
    }
