      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new "
                },
                {
                  "type": "codeVoice",
                  "code": "CFString"
                },
                {
                  "type": "text",
                  "text": " object. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "originalString",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "CFString"
                },
                {
                  "type": "text",
                  "text": " object to be copied and modified."
                }
              ]
            }
          ]
        },
        {
          "name": "charactersToLeaveEscaped",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Characters whose percent escape sequences, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "%20"
                },
                {
                  "type": "text",
                  "text": " for a space character, you want to leave intact. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to specify that no percent escapes be replaced, or the empty string ("
                },
                {
                  "type": "codeVoice",
                  "code": "CFSTR(\"\")"
                },
                {
                  "type": "text",
                  "text": ") to specify that all be replaced."
                }
              ]
            }
          ]
        },
        {
          "name": "encoding",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies the encoding to use when interpreting percent escapes. If you are uncertain of the correct encoding, you should use UTF-8 ("
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringbuiltinencodings/kcfstringencodingutf8"
                },
                {
                  "type": "text",
                  "text": "), which is the encoding designated by RFC 3986 as the correct encoding for use in URLs."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new "
            },
            {
              "type": "codeVoice",
              "code": "CFString"
            },
            {
              "type": "text",
              "text": " object, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if the percent escapes cannot be converted to characters, assuming the encoding given by "
            },
            {
              "type": "codeVoice",
              "code": "encoding"
            },
            {
              "type": "text",
              "text": ". If no characters need to be replaced, this function returns the original string with its reference count incremented. Ownership follows the create rule. See "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
