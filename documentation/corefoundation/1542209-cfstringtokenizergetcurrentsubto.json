      ],
      "parameters": [
        {
          "name": "tokenizer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A CFStringTokenizer object."
                }
              ]
            }
          ]
        },
        {
          "name": "ranges",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Upon return, an array of CFRanges containing the ranges of subtokens. The ranges are relative to the string specified to CFStringTokenizerCreate. This parameter can be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "maxRangeLength",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum number of ranges to return."
                }
              ]
            }
          ]
        },
        {
          "name": "derivedSubTokens",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A CFMutableArray to which the derived subtokens are to be added. This parameter can be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The number of ranges returned."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If token type is "
            },
            {
              "type": "codeVoice",
              "code": "kCFStringTokenizerTokenNone"
            },
            {
              "type": "text",
              "text": ", the "
            },
            {
              "type": "codeVoice",
              "code": "ranges"
            },
            {
              "type": "text",
              "text": " array and "
            },
            {
              "type": "codeVoice",
              "code": "derivedSubTokens"
            },
            {
              "type": "text",
              "text": " array are untouched and the return value is "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If token type is "
            },
            {
              "type": "codeVoice",
              "code": "kCFStringTokenizerTokenNormal"
            },
            {
              "type": "text",
              "text": ", the "
            },
            {
              "type": "codeVoice",
              "code": "ranges"
            },
            {
              "type": "text",
              "text": " array has one item filled in with the entire range of the token (if "
            },
            {
              "type": "codeVoice",
              "code": "maxRangeLength"
            },
            {
              "type": "text",
              "text": " >= 1) and a string taken from the entire token range is added to the "
            },
            {
              "type": "codeVoice",
              "code": "derivedSubTokens"
            },
            {
              "type": "text",
              "text": " array and the return value is "
            },
            {
              "type": "codeVoice",
              "code": "1"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If token type is "
            },
            {
              "type": "codeVoice",
              "code": "kCFStringTokenizerTokenHasSubTokensMask"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "kCFStringTokenizerTokenHasDerivedSubTokensMask"
            },
            {
              "type": "text",
              "text": ", the ranges array is filled in with as many items as there are subtokens (up to a limit of "
            },
            {
              "type": "codeVoice",
              "code": "maxRangeLength"
            },
            {
              "type": "text",
              "text": ")."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "derivedSubTokens"
            },
            {
              "type": "text",
              "text": " array will have sub tokens added even when the sub token is a substring of the token. If token type is "
            },
            {
              "type": "codeVoice",
              "code": "kCFStringTokenizerTokenHasSubTokensMask"
            },
            {
              "type": "text",
              "text": ", the ordinary non-derived subtokens are added to the "
            },
            {
              "type": "codeVoice",
              "code": "derivedSubTokens"
            },
            {
              "type": "text",
              "text": " array. "
            }
          ]
        }
      ]
    }
