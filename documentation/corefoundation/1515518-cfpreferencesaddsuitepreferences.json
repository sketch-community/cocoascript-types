          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The ID of the application to which to add suite preferences, typically "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencescurrentapplication"
                },
                {
                  "type": "text",
                  "text": ". Do not pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencesanyapplication"
                },
                {
                  "type": "text",
                  "text": ". Takes the form of a Java package name, "
                },
                {
                  "type": "codeVoice",
                  "code": "com.foosoft"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "suiteID",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The ID of the application suite preferences to add. Takes the form of a Java package name, "
                },
                {
                  "type": "codeVoice",
                  "code": "com.foosoft"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
