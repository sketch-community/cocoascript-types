    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new set and its storage for values. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "values",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A C array of the pointer-sized values to be in the new set. This parameter may be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if the "
                },
                {
                  "type": "codeVoice",
                  "code": "numValues"
                },
                {
                  "type": "text",
                  "text": " parameter is 0. The C array is not changed or freed by this function. "
                },
                {
                  "type": "codeVoice",
                  "code": "values"
                },
                {
                  "type": "text",
                  "text": " must be a pointer to a C array of at least "
                },
                {
                  "type": "codeVoice",
                  "code": "numValues"
                },
                {
                  "type": "text",
                  "text": " elements. "
                }
              ]
            }
          ]
        },
        {
          "name": "numValues",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of values to copy from the "
                },
                {
                  "type": "codeVoice",
                  "code": "values"
                },
                {
                  "type": "text",
                  "text": " C array in the new set. "
                }
              ]
            }
          ]
        },
        {
          "name": "callBacks",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsetcallbacks"
                },
                {
                  "type": "text",
                  "text": " structure initialized with the callbacks to use to retain, release, describe, and compare values in the collection. A copy of the contents of the callbacks structure is made, so that a pointer to a structure on the stack can be passed in or can be reused for multiple collection creations."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This value may be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", which is treated as a valid structure of version "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " with all fields "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". If the collection contains only CFType objects, then pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcftypesetcallbacks"
                },
                {
                  "type": "text",
                  "text": " to use the default callback functions."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new immutable set, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if there was a problem creating the object. Ownership follows the "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If any value put into the collection is not one understood by one of the callback functions, the behavior when that callback function is used is undefined."
            }
          ]
        }
      ]
    }
