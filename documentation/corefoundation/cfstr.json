      "parameters": [
        {
          "name": "cStr",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A constant C string (that is, text enclosed in double-quotation marks) from which the string is to be created."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An immutable string, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if there was a problem creating the object. The returned object is a constant. You may retain and release it, similar to other immutable CFString objects, but are not required to do so—it will remain valid until the program terminates."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "CFSTR"
            },
            {
              "type": "text",
              "text": " macro is a convenient way to create CFString representations of constant compile-time strings."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A value returned by "
            },
            {
              "type": "codeVoice",
              "code": "CFSTR"
            },
            {
              "type": "text",
              "text": " has the following semantics:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Values returned from "
                    },
                    {
                      "type": "codeVoice",
                      "code": "CFSTR"
                    },
                    {
                      "type": "text",
                      "text": " are not released by CFString—they are guaranteed to be valid until the program terminates. "
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "You can retain and release values returned from "
                    },
                    {
                      "type": "codeVoice",
                      "code": "CFSTR"
                    },
                    {
                      "type": "text",
                      "text": " in a balanced fashion, like any other CFString, but you are not required to do so. "
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "When using this macro as an initializer, you must build using the "
                },
                {
                  "type": "codeVoice",
                  "code": "-fconstant-cfstrings"
                },
                {
                  "type": "text",
                  "text": " compiler flag."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        }
      ]
    }
