        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to use to allocate memory for the new "
                },
                {
                  "type": "codeVoice",
                  "code": "CFData"
                },
                {
                  "type": "text",
                  "text": " object. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault"
                },
                {
                  "type": "text",
                  "text": " to use the current default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "url",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL to convert into a "
                },
                {
                  "type": "codeVoice",
                  "code": "CFData"
                },
                {
                  "type": "text",
                  "text": " object. "
                }
              ]
            }
          ]
        },
        {
          "name": "encoding",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The string encoding to use when converting "
                },
                {
                  "type": "codeVoice",
                  "code": "url"
                },
                {
                  "type": "text",
                  "text": " into a "
                },
                {
                  "type": "codeVoice",
                  "code": "CFData"
                },
                {
                  "type": "text",
                  "text": " object."
                }
              ]
            }
          ]
        },
        {
          "name": "escapeWhitespace",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " if you want to escape whitespace characters in the URL, "
                },
                {
                  "type": "codeVoice",
                  "code": "false"
                },
                {
                  "type": "text",
                  "text": " otherwise. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new "
            },
            {
              "type": "codeVoice",
              "code": "CFData"
            },
            {
              "type": "text",
              "text": " object containing the content of "
            },
            {
              "type": "codeVoice",
              "code": "url"
            },
            {
              "type": "text",
              "text": ". Ownership follows the create rule. See "
            },
            {
              "type": "link",
              "title": "The Create Rule",
              "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function escapes any character that is not 7-bit ASCII with the byte-code for the given encoding. If "
            },
            {
              "type": "codeVoice",
              "code": "escapeWhitespace"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": ", whitespace characters (' ', '\\t', '\\r', '\\n') will be escaped as well. This is desirable if you want to embed the URL into a larger text stream like HTML. "
            }
          ]
        }
      ]
    }
