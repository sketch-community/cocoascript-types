    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcompareanchored": {
      "title": "kCFCompareAnchored",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcompareanchored",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcompareanchored",
      "abstract": [
        {
          "type": "text",
          "text": "Performs searching only on characters at the beginning or end of the range."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparebackwards": {
      "title": "kCFCompareBackwards",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparebackwards",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparebackwards",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that the comparison should start at the last elements of the entities being compared (for example, strings or arrays)."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparecaseinsensitive": {
      "title": "kCFCompareCaseInsensitive",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparecaseinsensitive",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparecaseinsensitive",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that the comparison should ignore differences in case between alphabetical characters."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparediacriticinsensitive": {
      "title": "kCFCompareDiacriticInsensitive",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparediacriticinsensitive",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparediacriticinsensitive",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that the comparison should ignore diacritic markers."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcompareforcedordering": {
      "title": "kCFCompareForcedOrdering",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcompareforcedordering",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcompareforcedordering",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that the comparison is forced to return either "
        },
        {
          "type": "codeVoice",
          "code": "kCFCompareLessThan"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "codeVoice",
          "code": "kCFCompareGreaterThan"
        },
        {
          "type": "text",
          "text": " if the strings are equivalent but not strictly equal."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparelocalized": {
      "title": "kCFCompareLocalized",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparelocalized",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparelocalized",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that the comparison should take into account differences related to locale, such as the thousands separator character."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparenonliteral": {
      "title": "kCFCompareNonliteral",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparenonliteral",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparenonliteral",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that loose equivalence is acceptable, especially as pertains to diacritical marks. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparenumerically": {
      "title": "kCFCompareNumerically",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparenumerically",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparenumerically",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that represented numeric values should be used as the basis for comparison and not the actual character values."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparewidthinsensitive": {
      "title": "kCFCompareWidthInsensitive",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstringcompareflags/kcfcomparewidthinsensitive",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfstringcompareflags/kcfcomparewidthinsensitive",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that the comparison should ignore width differences."
        }
      ]
    },
