    "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketnocallback": {
      "title": "kCFSocketNoCallBack",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketnocallback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfsocketcallbacktype/kcfsocketnocallback",
      "abstract": [
        {
          "type": "text",
          "text": "No callback should be made for any activity."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketreadcallback": {
      "title": "kCFSocketReadCallBack",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketreadcallback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfsocketcallbacktype/kcfsocketreadcallback",
      "abstract": [
        {
          "type": "text",
          "text": "The callback is called when data is available to be read or a new connection is waiting to be accepted. The data is not automatically read; the callback must read the data itself."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketacceptcallback": {
      "title": "kCFSocketAcceptCallBack",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketacceptcallback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfsocketcallbacktype/kcfsocketacceptcallback",
      "abstract": [
        {
          "type": "text",
          "text": "New connections will be automatically accepted and the callback is called with the data argument being a pointer to a "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketnativehandle"
        },
        {
          "type": "text",
          "text": " of the child socket. This callback is usable only with listening sockets."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketdatacallback": {
      "title": "kCFSocketDataCallBack",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketdatacallback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfsocketcallbacktype/kcfsocketdatacallback",
      "abstract": [
        {
          "type": "text",
          "text": "Incoming data will be read in chunks in the background and the callback is called with the data argument being a CFData object containing the read data."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketconnectcallback": {
      "title": "kCFSocketConnectCallBack",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketconnectcallback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfsocketcallbacktype/kcfsocketconnectcallback"
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketwritecallback": {
      "title": "kCFSocketWriteCallBack",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype/kcfsocketwritecallback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfsocketcallbacktype/kcfsocketwritecallback",
      "abstract": [
        {
          "type": "text",
          "text": "The callback is called when the socket is writable. This callback type may be useful when large amounts of data are being sent rapidly over the socket and you want a notification when there is space in the kernel buffers for more data."
        }
      ]
    },
