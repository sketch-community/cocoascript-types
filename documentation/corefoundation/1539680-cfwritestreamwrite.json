      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The number of bytes successfully written, "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " if the stream has been filled to capacity (for fixed-length streams), or "
            },
            {
              "type": "codeVoice",
              "code": "-1"
            },
            {
              "type": "text",
              "text": " if either the stream is not open or an error occurs."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "stream"
            },
            {
              "type": "text",
              "text": " is in the process of opening, this function waits until it has completed. If the stream is not full, this call blocks until at least one byte is written; it does not block until all the bytes in "
            },
            {
              "type": "codeVoice",
              "code": "buffer"
            },
            {
              "type": "text",
              "text": " is written. To avoid blocking, call this function only if "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539684-cfwritestreamcanacceptbytes"
            },
            {
              "type": "text",
              "text": " returns "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " or after the stream’s client (set with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539678-cfwritestreamsetclient"
            },
            {
              "type": "text",
              "text": ") is notified of a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstreameventtype/kcfstreameventcanacceptbytes"
            },
            {
              "type": "text",
              "text": " event."
            }
          ]
        }
      ]
    }
