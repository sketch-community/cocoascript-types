          ]
        },
        {
          "name": "bufferLength",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of bytes from "
                },
                {
                  "type": "codeVoice",
                  "code": "buffer"
                },
                {
                  "type": "text",
                  "text": " to write."
                }
              ]
            }
          ]
