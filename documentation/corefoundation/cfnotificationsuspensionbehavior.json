    "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviordrop": {
      "title": "CFNotificationSuspensionBehaviorDrop",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviordrop",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviordrop",
      "abstract": [
        {
          "type": "text",
          "text": "The server will not queue any notifications of the specified name and object while the receiving application is in the background."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviorcoalesce": {
      "title": "CFNotificationSuspensionBehaviorCoalesce",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviorcoalesce",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviorcoalesce",
      "abstract": [
        {
          "type": "text",
          "text": "The server will only queue the last notification of the specified name and object; earlier notifications are dropped."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviorhold": {
      "title": "CFNotificationSuspensionBehaviorHold",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviorhold",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviorhold",
      "abstract": [
        {
          "type": "text",
          "text": "The server will hold all matching notifications until the queue has been filled (queue size determined by the server) at which point the server may flush queued notifications."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviordeliverimmediately": {
      "title": "CFNotificationSuspensionBehaviorDeliverImmediately",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviordeliverimmediately",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/corefoundation/cfnotificationsuspensionbehavior/cfnotificationsuspensionbehaviordeliverimmediately",
      "abstract": [
        {
          "type": "text",
          "text": "The server will deliver notifications of the specified name and object whether or not the application is in the background. When a notification with this suspension behavior is matched, it has the effect of first flushing any queued notifications."
        }
      ]
    },
