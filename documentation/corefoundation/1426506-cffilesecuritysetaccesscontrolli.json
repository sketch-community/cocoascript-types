    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "fileSec",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "CFFileSecurityRef"
                },
                {
                  "type": "text",
                  "text": " object to modify."
                }
              ]
            }
          ]
        },
        {
          "name": "accessControlList",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The access control list to set, or "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFFileSecurityRemoveACL"
                },
                {
                  "type": "text",
                  "text": " to indicate that the access control list should be removed from a file, or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to unset the access control list property in the object."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Returns "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if the access control list was successfully set, or "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " otherwise."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To remove the access control list from a file system object, pass "
            },
            {
              "type": "codeVoice",
              "code": "kCFFileSecurityRemoveACL"
            },
            {
              "type": "text",
              "text": " as the "
            },
            {
              "type": "codeVoice",
              "code": "accessControlList"
            },
            {
              "type": "text",
              "text": " parameter. Then, call "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1541607-cfurlsetresourcepropertyforkey"
            },
            {
              "type": "text",
              "text": " to set "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfurlfilesecuritykey"
            },
            {
              "type": "text",
              "text": " to the resulting "
            },
            {
              "type": "codeVoice",
              "code": "fileSec"
            },
            {
              "type": "text",
              "text": " object."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Setting the "
            },
            {
              "type": "codeVoice",
              "code": "accessControlList"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " unsets the ACL property of the "
            },
            {
              "type": "codeVoice",
              "code": "CFFileSecurityRef"
            },
            {
              "type": "text",
              "text": " object. By doing this, the access control list of the file will be unchanged if you subsequently use this object to set permissions on an actual file system object."
            }
          ]
        }
      ]
    }
  ],
