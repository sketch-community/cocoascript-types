        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator to examine. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to obtain the context of the default allocator. "
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, contains the context of "
                },
                {
                  "type": "codeVoice",
                  "code": "allocator"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An allocator's context, a structure of type "
            },
            {
              "type": "codeVoice",
              "code": "CFAllocatorContext"
            },
            {
              "type": "text",
              "text": ", holds pointers to various function callbacks (particularly those that allocate, reallocate, and deallocate memory for an object). The context also contains a version number and the "
            },
            {
              "type": "codeVoice",
              "code": "info"
            },
            {
              "type": "text",
              "text": " field for program-defined data. To obtain the value of the "
            },
            {
              "type": "codeVoice",
              "code": "info"
            },
            {
              "type": "text",
              "text": " field you usually first have to get an allocator's context. "
            }
          ]
        }
      ]
    }
