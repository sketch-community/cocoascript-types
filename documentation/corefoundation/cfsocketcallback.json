    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "s",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The CFSocket object that experienced some activity. "
                }
              ]
            }
          ]
        },
        {
          "name": "callbackType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of activity detected. "
                }
              ]
            }
          ]
        },
        {
          "name": "address",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A CFData object holding the contents of a "
                },
                {
                  "type": "codeVoice",
                  "code": "struct sockaddr"
                },
                {
                  "type": "text",
                  "text": " appropriate for the protocol family of "
                },
                {
                  "type": "codeVoice",
                  "code": "s"
                },
                {
                  "type": "text",
                  "text": " ("
                },
                {
                  "type": "codeVoice",
                  "code": "struct sockaddr_in"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "struct sockaddr_in6"
                },
                {
                  "type": "text",
                  "text": ", for example), identifying the remote address to which "
                },
                {
                  "type": "codeVoice",
                  "code": "s"
                },
                {
                  "type": "text",
                  "text": " is connected. This value is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " except for "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFSocketAcceptCallBack"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFSocketDataCallBack"
                },
                {
                  "type": "text",
                  "text": " callbacks. "
                }
              ]
            }
          ]
        },
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Data appropriate for the callback type. For a "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFSocketConnectCallBack"
                },
                {
                  "type": "text",
                  "text": " that failed in the background, it is a pointer to an "
                },
                {
                  "type": "codeVoice",
                  "code": "SInt32"
                },
                {
                  "type": "text",
                  "text": " error code; for a "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFSocketAcceptCallBack"
                },
                {
                  "type": "text",
                  "text": ", it is a pointer to a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketnativehandle"
                },
                {
                  "type": "text",
                  "text": "; or for a "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFSocketDataCallBack"
                },
                {
                  "type": "text",
                  "text": ", it is a CFData object containing the incoming data. In all other cases, it is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "info",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "info"
                },
                {
                  "type": "text",
                  "text": " member of the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcontext"
                },
                {
                  "type": "text",
                  "text": " structure that was used when creating the CFSocket object."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You specify this callback when you create the CFSocket object with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1543527-cfsocketcreate"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1542283-cfsocketcreateconnectedtosockets"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1543295-cfsocketcreatewithnative"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1542862-cfsocketcreatewithsocketsignatur"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
