    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "theSet",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The set to operate upon. "
                }
              ]
            }
          ]
        },
        {
          "name": "applier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The callback function to call once for each value in the "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": ". If this parameter is not a pointer to a function of the correct prototype, the behavior is undefined. The "
                },
                {
                  "type": "codeVoice",
                  "code": "applier"
                },
                {
                  "type": "text",
                  "text": " function must be able to work with all values in "
                },
                {
                  "type": "codeVoice",
                  "code": "theSet"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer-sized program-defined value, which is passed as the second parameter to the "
                },
                {
                  "type": "codeVoice",
                  "code": "applier"
                },
                {
                  "type": "text",
                  "text": " function, but is otherwise unused by this function."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "theSet"
            },
            {
              "type": "text",
              "text": " is mutable, it is unsafe for the "
            },
            {
              "type": "codeVoice",
              "code": "applier"
            },
            {
              "type": "text",
              "text": " function to change the contents of the collection."
            }
          ]
        }
      ]
    }
