      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Once invalidated, "
            },
            {
              "type": "codeVoice",
              "code": "timer"
            },
            {
              "type": "text",
              "text": " will never fire and call its callback function again. This function automatically removes "
            },
            {
              "type": "codeVoice",
              "code": "timer"
            },
            {
              "type": "text",
              "text": " from all run loop modes in which it had been added. The memory is not deallocated unless the run loop held the only reference to "
            },
            {
              "type": "codeVoice",
              "code": "timer"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
