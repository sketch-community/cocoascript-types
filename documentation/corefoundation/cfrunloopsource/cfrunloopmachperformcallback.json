    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "msg",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The Mach message received on the Mach port. The pointer is to a "
                },
                {
                  "type": "codeVoice",
                  "code": "mach_msg_header_t"
                },
                {
                  "type": "text",
                  "text": " structure. A version 0 format trailer ("
                },
                {
                  "type": "codeVoice",
                  "code": "mach_msg_format_0_trailer_t"
                },
                {
                  "type": "text",
                  "text": ") is at the end of the Mach message."
                }
              ]
            }
          ]
        },
        {
          "name": "size",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Size of the Mach message in "
                },
                {
                  "type": "codeVoice",
                  "code": "msg"
                },
                {
                  "type": "text",
                  "text": ", excluding the message trailer."
                }
              ]
            }
          ]
        },
        {
          "name": "allocator",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allocator object that should be used to allocate a reply message. "
                }
              ]
            }
          ]
        },
        {
          "name": "info",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "info"
                },
                {
                  "type": "text",
                  "text": " member of the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfrunloopsourcecontext1"
                },
                {
                  "type": "text",
                  "text": " structure that was used when creating the run loop source."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An optional Mach message to be sent in response to the received message. The message must be allocated using "
            },
            {
              "type": "codeVoice",
              "code": "allocator"
            },
            {
              "type": "text",
              "text": ". Return "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if you want an empty reply returned to the sender."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You only need to provide this callback if you create your own version 1 run loop source. CFMachPort and CFMessagePort run loop sources already implement this callback to forward the received message to the CFMachPort’s or CFMessagePort’s own callback function, which you do need to implement."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You specify this callback in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfrunloopsourcecontext1"
            },
            {
              "type": "text",
              "text": " structure when creating the run loop source."
            }
          ]
        }
      ]
    }
