      "parameters": [
        {
          "name": "remote",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The message port to which "
                },
                {
                  "type": "codeVoice",
                  "code": "data"
                },
                {
                  "type": "text",
                  "text": " should be sent."
                }
              ]
            }
          ]
        },
        {
          "name": "msgid",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An arbitrary integer value that you can send with the message."
                }
              ]
            }
          ]
        },
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The data to send to "
                },
                {
                  "type": "codeVoice",
                  "code": "remote"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "sendTimeout",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time to wait for "
                },
                {
                  "type": "codeVoice",
                  "code": "data"
                },
                {
                  "type": "text",
                  "text": " to be sent."
                }
              ]
            }
          ]
        },
        {
          "name": "rcvTimeout",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time to wait for a reply to be returned."
                }
              ]
            }
          ]
        },
        {
          "name": "replyMode",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The run loop mode in which the function should wait for a reply. If the message is a "
                },
                {
                  "type": "codeVoice",
                  "code": "oneway"
                },
                {
                  "type": "text",
                  "text": " (so no response is expected), then "
                },
                {
                  "type": "codeVoice",
                  "code": "replyMode"
                },
                {
                  "type": "text",
                  "text": " should be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "codeVoice",
                  "code": "replyMode"
                },
                {
                  "type": "text",
                  "text": " is non-"
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", the function runs the run loop waiting for a reply, in that mode. "
                },
                {
                  "type": "codeVoice",
                  "code": "replyMode"
                },
                {
                  "type": "text",
                  "text": " can be any string name of a run loop mode, but it should be one with input sources installed. You should use the "
                },
                {
                  "type": "codeVoice",
                  "code": "kCFRunLoopDefaultMode"
                },
                {
                  "type": "text",
                  "text": " constant unless you have a specific reason to use a different mode."
                }
              ]
            }
          ]
        },
        {
          "name": "returnData",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Upon return, contains a CFData object containing the reply data. Ownership follows the "
                },
                {
                  "type": "link",
                  "title": "The Create Rule",
                  "destination": "https://developer.apple.com/library/archive/documentation/CoreFoundation/Conceptual/CFMemoryMgmt/Concepts/Ownership.html#//apple_ref/doc/uid/20001148-103029"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
