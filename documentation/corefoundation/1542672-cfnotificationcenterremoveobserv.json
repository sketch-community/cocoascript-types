      ],
      "parameters": [
        {
          "name": "center",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The notification center to modify. "
                }
              ]
            }
          ]
        },
        {
          "name": "observer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The observer. This value must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "name",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the notification to stop observing. If "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "observer"
                },
                {
                  "type": "text",
                  "text": " stops receiving callbacks for all notifications posted by "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "object",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object to stop observing. For distributed notifications, "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": " must be a CFString object. If "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "observer"
                },
                {
                  "type": "text",
                  "text": " stops receiving callbacks for all objects posting notifications named "
                },
                {
                  "type": "codeVoice",
                  "code": "name"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "codeVoice",
                  "code": "center"
                },
                {
                  "type": "text",
                  "text": " is a Darwin notification center, this value is ignored."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If both "
            },
            {
              "type": "codeVoice",
              "code": "name"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " are "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": ", this function unregisters "
            },
            {
              "type": "codeVoice",
              "code": "observer"
            },
            {
              "type": "text",
              "text": " from all the notifications for which it had previously registered with "
            },
            {
              "type": "codeVoice",
              "code": "center"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
