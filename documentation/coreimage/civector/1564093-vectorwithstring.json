          "name": "representation",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string that is in one of the formats returned by the "
                },
                {
                  "type": "codeVoice",
                  "code": "stringRepresentation"
                },
                {
                  "type": "text",
                  "text": " method."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Some typical string representations for vectors are:"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "@\"[1.0 0.5 0.3]\" "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "which specifies a "
            },
            {
              "type": "codeVoice",
              "code": "vec3"
            },
            {
              "type": "text",
              "text": " vector whose components are "
            },
            {
              "type": "codeVoice",
              "code": "X = 1.0"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "Y = 0.5"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "Z = 0.3"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "@\"[10.0 23.0]"
            },
            {
              "type": "text",
              "text": " "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "which specifies a "
            },
            {
              "type": "codeVoice",
              "code": "vec2"
            },
            {
              "type": "text",
              "text": " vector show components are "
            },
            {
              "type": "codeVoice",
              "code": "X = 10.0"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "Y = 23.0"
            }
          ]
        }
      ]
    }
