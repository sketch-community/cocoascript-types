        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value for this key is an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
            },
            {
              "type": "text",
              "text": " object containing a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciformat"
            },
            {
              "type": "text",
              "text": " value. The default working format is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/kciformatrgba8"
            },
            {
              "type": "text",
              "text": " for CPU rendering and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/kciformatrgbaf"
            },
            {
              "type": "text",
              "text": " for GPU rendering. For greater color precision, GPU rendering also supports the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/kciformatrgbah"
            },
            {
              "type": "text",
              "text": " format, but this format requires twice as much memory and can be used only with color management enabled."
            }
          ]
        }
      ]
    }
