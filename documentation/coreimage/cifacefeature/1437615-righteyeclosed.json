      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "“Right” is relative to the original (non-mirrored) image orientation, not to the owner of the eye."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For closed eyes to be detected, the key "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cidetectoreyeblink"
            },
            {
              "type": "text",
              "text": " must be present with a value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " in the dictionary passed to a detector’s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cidetector/1438189-featuresinimage"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        }
      ]
    }
