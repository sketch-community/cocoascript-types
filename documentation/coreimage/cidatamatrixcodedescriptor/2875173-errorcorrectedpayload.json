        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The error-corrected payload contains the de-interleaved bits of the message."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Data Matrix symbols are specified by ISO/IEC 16022:2006(E).  ECC 200-type symbols will always have an even number of rows and columns."
            }
          ]
        }
      ]
