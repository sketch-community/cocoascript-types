        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreimage/kciinputnoisereductionamountkey": {
      "title": "kCIInputNoiseReductionAmountKey",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreimage/kciinputnoisereductionamountkey",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreimage/kciinputnoisereductionamountkey",
      "abstract": [
        {
          "type": "text",
          "text": "A key for the amount to reduce noise in the image. The associated value must be an "
        },
        {
          "type": "reference",
