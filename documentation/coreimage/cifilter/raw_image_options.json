    "doc://com.apple.documentation/documentation/coreimage/kcisupporteddecoderversionskey": {
      "title": "kCISupportedDecoderVersionsKey",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreimage/kcisupporteddecoderversionskey",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreimage/kcisupporteddecoderversionskey",
      "abstract": [
        {
          "type": "text",
          "text": "A key for the supported decoder versions. The associated value is  an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsarray"
        },
        {
          "type": "text",
          "text": " object that contains all supported decoder versions for the given image type, sorted in increasingly newer order. Each entry is an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsdictionary"
        },
        {
          "type": "text",
          "text": " object that contains key-value pairs. All entries represent a valid version identifier that can be passed as the "
        },
        {
          "type": "codeVoice",
          "code": "kCIDecoderVersion"
        },
        {
          "type": "text",
          "text": " value for the key "
        },
        {
          "type": "codeVoice",
          "code": "kCIDecoderMethodKey"
        },
        {
          "type": "text",
          "text": ". Version values are read-only; attempting to set this value raises an exception. Currently, the only defined key is "
        },
        {
          "type": "codeVoice",
          "code": "@\"version\""
        },
        {
          "type": "text",
          "text": " which has as its value an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring"
        },
        {
          "type": "text",
          "text": " that uniquely describing a given decoder version. This string might not be suitable for user interface display.. "
        }
      ]
    },
