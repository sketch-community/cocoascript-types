                {
                  "type": "text",
                  "text": "A program in the Core Image Kernel Language that contains a single routine marked using the "
                },
                {
                  "type": "codeVoice",
                  "code": "kernel"
                },
                {
                  "type": "text",
                  "text": " keyword."
                }
              ]
            }
          ]
