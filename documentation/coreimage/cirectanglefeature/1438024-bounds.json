        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property identifies the rectangular region "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "of the image"
                }
              ]
            },
            {
              "type": "text",
              "text": " containing the detected rectangle, not necessarily the shape of the rectangle. A detected feature is rectangular in space, but may appear in perspective in the image. Use the properties listed in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cirectanglefeature"
            },
            {
              "type": "text",
              "text": " to find the corners of the rectangle as it appears in perspective."
            }
          ]
        }
      ]
    }
