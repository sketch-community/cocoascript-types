        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value for this key must be an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber"
            },
            {
              "type": "text",
              "text": " object containing a supported OpenGL texture target constant, either "
            },
            {
              "type": "codeVoice",
              "code": "GL_TEXTURE_2D"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "GL_TEXTURE_RECTANGLE_ARB"
            },
            {
              "type": "text",
              "text": ". You may only use this key when initializing an image using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage/1437880-initwithtexture"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        }
