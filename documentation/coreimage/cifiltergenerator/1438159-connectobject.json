      "parameters": [
        {
          "name": "sourceObject",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter"
                },
                {
                  "type": "text",
                  "text": " object, a  "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage"
                },
                {
                  "type": "text",
                  "text": " object, or the path (an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurl"
                },
                {
                  "type": "text",
                  "text": " object) to an image."
                }
              ]
            }
          ]
        },
        {
          "name": "sourceKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key that specifies the source object. For example, if the source is the output image of a filter, pass the "
                },
                {
                  "type": "codeVoice",
                  "code": "outputImage"
                },
                {
                  "type": "text",
                  "text": " key. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if the source object is used directly. "
                }
              ]
            }
          ]
        },
        {
          "name": "targetObject",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object to which the source object links."
                }
              ]
            }
          ]
        },
        {
          "name": "targetKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key that specifies the target for the source. For example, if you are connecting the source to the input image of a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter"
                },
                {
                  "type": "text",
                  "text": " object, you would pass the "
                },
                {
                  "type": "codeVoice",
                  "code": "inputImage"
                },
                {
                  "type": "text",
                  "text": " key. "
                }
              ]
            }
          ]
        }
      ]
    }
