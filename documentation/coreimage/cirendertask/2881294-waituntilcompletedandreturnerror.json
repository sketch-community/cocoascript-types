      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Synchronously blocks execution until the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cirendertask"
            },
            {
              "type": "text",
              "text": " either completes or fails (with error).  Calling this method after "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cicontext/2875429-starttasktorender"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cicontext/2875448-starttasktorender"
            },
            {
              "type": "text",
              "text": " makes the render task behave synchronously, as if the CPU and GPU were operating as a single unit."
            }
          ]
        }
      ]
    }
