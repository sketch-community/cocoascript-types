            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1396821-secidentitysearchcreate"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/security/1396825-secidentitysearchcopynext"
            },
            {
              "type": "text",
              "text": " return the preferred identity for this domain first."
            }
