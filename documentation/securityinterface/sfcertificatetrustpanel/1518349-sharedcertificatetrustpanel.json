            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1571958-alloc"
            },
            {
              "type": "text",
              "text": " class method inherited from "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject"
            },
            {
              "type": "text",
              "text": ") and initialize (using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1418641-init"
            },
            {
              "type": "text",
              "text": " instance method, also inherited from "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject"
            },
            {
              "type": "text",
              "text": ") instead of using this class method."
            }
          ]
        }
      ]
