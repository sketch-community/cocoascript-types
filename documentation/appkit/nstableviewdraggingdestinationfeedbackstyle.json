    "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylesourcelist": {
      "title": "NSTableViewDraggingDestinationFeedbackStyleSourceList",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylesourcelist",
