      "role": "symbol",
      "url": "/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylesourcelist",
      "abstract": [
        {
          "type": "text",
          "text": "Draws an outline on drop target rows, and an insertion marker between rows. This style will automatically be set for source lists when the table’s "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstableview/1527447-unhiderowsatindexes"
        },
        {
          "type": "text",
          "text": " is set to "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylesourcelist"
        },
        {
          "type": "text",
          "text": ". This is the standard look for Source Lists, but may be used in other areas as needed."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsview": {
