    "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylenone": {
      "title": "NSTableViewDraggingDestinationFeedbackStyleNone",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylenone",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylenone",
      "abstract": [
        {
          "type": "text",
          "text": "Provides no feedback when the user drags over the table view. This option exists to allow subclasses to implement their dragging destination highlighting, or to make it not show anything all."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstyleregular": {
      "title": "NSTableViewDraggingDestinationFeedbackStyleRegular",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstyleregular",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstyleregular",
      "abstract": [
        {
          "type": "text",
          "text": "Draws a solid round-rect background on drop target rows, and an insertion marker between rows. This style should be used in most cases."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylegap": {
      "title": "NSTableViewDraggingDestinationFeedbackStyleGap",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylegap",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstableviewdraggingdestinationfeedbackstyle/nstableviewdraggingdestinationfeedbackstylegap",
      "abstract": [
        {
          "type": "text",
          "text": "Provides a gap insertion when dragging over the table. Note that this style is only officially supported for "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsview"
        },
        {
          "type": "text",
          "text": "-based table views, but may partially work in Cell Based TableViews. The decision to use the gap style (compared to another style) can be made in "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstableviewdatasource/1528890-tableview"
        },
        {
          "type": "text",
          "text": ", or it can dynamically be changed."
        }
      ]
    },
