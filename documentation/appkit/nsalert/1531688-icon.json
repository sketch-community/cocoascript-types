      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, the image used in an alert is the app icon. If you set this property’s value, your specified custom image is used in place of the app icon."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you’ve set a custom alert icon, you can clear it by setting this property’s value to "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", which restores use of the app icon for the alert."
            }
          ]
        }
      ]
    }
