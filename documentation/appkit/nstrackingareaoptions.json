    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingmouseenteredandexited": {
      "title": "NSTrackingMouseEnteredAndExited",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingmouseenteredandexited",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingmouseenteredandexited",
      "abstract": [
        {
          "type": "text",
          "text": "The owner of the tracking area receives "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1529306-mouseentered"
        },
        {
          "type": "text",
          "text": " when the mouse cursor enters the area and "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1527561-mouseexited"
        },
        {
          "type": "text",
          "text": " events when the mouse leaves the area. This value specifies a type of tracking area."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingmousemoved": {
      "title": "NSTrackingMouseMoved",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingmousemoved",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingmousemoved",
      "abstract": [
        {
          "type": "text",
          "text": "The owner of the tracking area receives "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1525114-mousemoved"
        },
        {
          "type": "text",
          "text": " messages while the mouse cursor is within the area. This value specifies a type of tracking area."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingcursorupdate": {
      "title": "NSTrackingCursorUpdate",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingcursorupdate",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingcursorupdate",
      "abstract": [
        {
          "type": "text",
          "text": "The owner of the tracking area receives "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1525066-cursorupdate"
        },
        {
          "type": "text",
          "text": " messages when the mouse cursor enters the area; when the mouse leaves the area, the cursor is appropriately reset. This value specifies a type of tracking area."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactivewhenfirstresponder": {
      "title": "NSTrackingActiveWhenFirstResponder",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactivewhenfirstresponder",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingactivewhenfirstresponder",
      "abstract": [
        {
          "type": "text",
          "text": "The owner receives messages when the view is the first responder. This value specifies when the tracking area defined by an "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea"
        },
        {
          "type": "text",
          "text": " object is active."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactiveinkeywindow": {
      "title": "NSTrackingActiveInKeyWindow",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactiveinkeywindow",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingactiveinkeywindow",
      "abstract": [
        {
          "type": "text",
          "text": "The owner receives messages when the view is in the key window. This value specifies when the tracking area defined by an "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea"
        },
        {
          "type": "text",
          "text": " object is active."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactiveinactiveapp": {
      "title": "NSTrackingActiveInActiveApp",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactiveinactiveapp",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingactiveinactiveapp",
      "abstract": [
        {
          "type": "text",
          "text": "The owner receives messages when the application is active. This value specifies when the tracking area defined by an "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea"
        },
        {
          "type": "text",
          "text": " object is active."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactivealways": {
      "title": "NSTrackingActiveAlways",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingactivealways",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingactivealways",
      "abstract": [
        {
          "type": "text",
          "text": "The owner receives messages regardless of first-responder status, window status, or application status. The "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1525066-cursorupdate"
        },
        {
          "type": "text",
          "text": " message is "
        },
        {
          "type": "emphasis",
          "inlineContent": [
            {
              "type": "text",
              "text": "not"
            }
          ]
        },
        {
          "type": "text",
          "text": " sent when the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingcursorupdate"
        },
        {
          "type": "text",
          "text": " option is specified along with this constant. This value specifies when the tracking area defined by an "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea"
        },
        {
          "type": "text",
          "text": " object is active."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingassumeinside": {
      "title": "NSTrackingAssumeInside",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingassumeinside",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingassumeinside",
      "abstract": [
        {
          "type": "text",
          "text": "The first event is generated when the cursor leaves the tracking area, regardless if the cursor is inside the area when the "
        },
        {
          "type": "codeVoice",
          "code": "NSTrackingArea"
        },
        {
          "type": "text",
          "text": " is added to a view.  If this option is not specified, the first event is generated when the cursor leaves the tracking area if the cursor is initially inside the area, or when the cursor enters the area if the cursor is initially outside it. Generally, you do not want to request this behavior. This value specifies a behavior of the tracking area defined by the "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackinginvisiblerect": {
      "title": "NSTrackingInVisibleRect",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackinginvisiblerect",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackinginvisiblerect",
      "abstract": [
        {
          "type": "text",
          "text": "Mouse tracking occurs only in the visible rectangle of the view—in other words, that region of the tracking rectangle that is unobscured. Otherwise, the entire tracking area is active regardless of overlapping views. The "
        },
        {
          "type": "codeVoice",
          "code": "NSTrackingArea"
        },
        {
          "type": "text",
          "text": " object is automatically synchronized with changes in the view’s visible area ("
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483446-visiblerect"
        },
        {
          "type": "text",
          "text": ") and the value returned from "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea/1525874-rect"
        },
        {
          "type": "text",
          "text": " is ignored. This value specifies a behavior of the tracking area defined by the "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingenabledduringmousedrag": {
      "title": "NSTrackingEnabledDuringMouseDrag",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions/nstrackingenabledduringmousedrag",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstrackingareaoptions/nstrackingenabledduringmousedrag",
      "abstract": [
        {
          "type": "text",
          "text": "The owner receives "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsmouseentered"
        },
        {
          "type": "text",
          "text": " events when the mouse cursor is dragged into the tracking area. If this option is not specified, the owner receives mouse-entered events when the mouse is moved (no buttons pressed) into the tracking area and on "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsleftmouseup"
        },
        {
          "type": "text",
          "text": " events after a mouse drag."
        }
      ]
    },
