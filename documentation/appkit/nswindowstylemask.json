    "doc://com.apple.documentation/documentation/appkit/nswindowstylemask/nswindowstylemaskborderless": {
      "title": "NSWindowStyleMaskBorderless",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nswindowstylemask/nswindowstylemaskborderless",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nswindowstylemask/nswindowstylemaskborderless",
      "abstract": [
        {
          "type": "text",
          "text": "The window displays none of the usual peripheral elements. Useful only for display or caching purposes. A window that uses "
        },
        {
          "type": "codeVoice",
          "code": "NSWindowStyleMaskBorderless"
        },
        {
          "type": "text",
          "text": " can’t become key or main, unless the value of "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow/1419543-canbecomekeywindow"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow/1419162-canbecomemainwindow"
        },
        {
          "type": "text",
          "text": " is "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
        },
        {
          "type": "text",
          "text": ". Note that you can set a window’s or panel’s style mask to "
        },
        {
          "type": "codeVoice",
          "code": "NSWindowStyleMaskBorderless"
        },
        {
          "type": "text",
          "text": " in Interface Builder by deselecting Title Bar in the Appearance section of the Attributes inspector.  "
        }
      ]
    },
