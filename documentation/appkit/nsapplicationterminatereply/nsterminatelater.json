      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "It may be OK to proceed with termination later. Returning this value causes Cocoa to run the run loop in the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsmodalpanelrunloopmode"
            },
            {
              "type": "text",
              "text": " until your app subsequently calls "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsapplication/1428594-replytoapplicationshouldterminat"
            },
            {
              "type": "text",
              "text": " with the value "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": ". This return value is for delegates that need to provide document modal alerts (sheets) in order to decide whether to quit."
            }
          ]
        }
      ]
    }
