      "parameters": [
        {
          "name": "pixelBuffer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The pixel buffer to attach."
                }
              ]
            }
          ]
        },
        {
          "name": "face",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "For pixel buffers with a texture target of "
                },
                {
                  "type": "codeVoice",
                  "code": "GL_CUBE_MAP"
                },
                {
                  "type": "text",
                  "text": ", this parameter should be zero or one of the following values:"
                }
              ]
            },
            {
              "type": "unorderedList",
              "items": [
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "GL_TEXTURE_CUBE_MAP_POSITIVE_X"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "GL_TEXTURE_CUBE_MAP_POSITIVE_Y"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "GL_TEXTURE_CUBE_MAP_POSITIVE_Z"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "GL_TEXTURE_CUBE_MAP_NEGATIVE_X"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "GL_TEXTURE_CUBE_MAP_NEGATIVE_Y"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "GL_TEXTURE_CUBE_MAP_NEGATIVE_Z"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "level",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The desired mipmap level for rendering. This value must be less than or equal to the maximum texture mipmap level of "
                },
                {
                  "type": "codeVoice",
                  "code": "pixelBuffer"
                },
                {
                  "type": "text",
                  "text": " (accessible through an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSOpenGLPixelBuffer"
                },
                {
                  "type": "text",
                  "text": " objectâ€™s "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsopenglpixelbuffer/1436120-texturemaxmipmaplevel"
                },
                {
                  "type": "text",
                  "text": " method). "
                }
              ]
            }
          ]
        },
        {
          "name": "screen",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The virtual screen of the receiver (if applicable) should be set to the same value as the current virtual screen you are using for rendering onscreen"
                }
              ]
            }
          ]
        }
      ]
    },
