          "inlineContent": [
            {
              "type": "text",
              "text": "Copies the pixels in the rectangle specified by "
            },
            {
              "type": "codeVoice",
              "code": "srcRect"
            },
            {
              "type": "text",
              "text": " to the location specified by "
            },
            {
              "type": "codeVoice",
              "code": "destPoint"
            },
            {
              "type": "text",
              "text": ". The source rectangle is defined in the graphics state designated by "
            },
            {
              "type": "codeVoice",
              "code": "srcGState"
            },
            {
              "type": "text",
              "text": ". If "
            },
            {
              "type": "codeVoice",
              "code": "srcGState"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "NSNullObject"
            },
            {
              "type": "text",
              "text": ", the current graphics state is assumed. The "
            },
            {
              "type": "codeVoice",
              "code": "destPoint"
            },
            {
              "type": "text",
              "text": " destination is defined in the current graphics state."
            }
          ]
