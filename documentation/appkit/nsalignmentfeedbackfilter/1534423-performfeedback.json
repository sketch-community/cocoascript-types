          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One or more feedback tokens prepared for specific alignment locations by calling "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsalignmentfeedbackfilter/1535346-alignmentfeedbacktokenformovemen"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsalignmentfeedbackfilter/1534596-alignmentfeedbacktokenforhorizon"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsalignmentfeedbackfilter/1531563-alignmentfeedbacktokenforvertica"
                },
                {
                  "type": "text",
                  "text": ". Typically, no more than one feedback token per dimension (horizontal/vertical) should be provided."
                }
              ]
            }
          ]
        },
        {
          "name": "performanceTime",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time, of type "
                },
                {
                  "type": "codeVoice",
                  "code": "NSHapticFeedbackPerformanceTime"
                },
                {
                  "type": "text",
                  "text": ", when the feedback should be provided to the user."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Call this method to initiate haptic feedback to the user. Pass it one or more alignment feedback tokens of type "
            },
            {
              "type": "codeVoice",
              "code": "NSAlignmentFeedbackToken"
            },
            {
              "type": "text",
              "text": " and a time to execute of type "
            },
            {
              "type": "codeVoice",
              "code": "NSHapticFeedbackPerformanceTime"
            },
            {
              "type": "text",
              "text": ". Call this method immediately before moving the object to its new aligned position."
            }
          ]
        }
      ]
    }
