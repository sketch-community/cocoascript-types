      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method redraws the receiver. An active color well will have its color updated when the current color of the "
            },
            {
              "type": "codeVoice",
              "code": "NSColorPanel"
            },
            {
              "type": "text",
              "text": " changes. Any color well that shows its border highlights the border when it’s active."
            }
          ]
        }
      ]
