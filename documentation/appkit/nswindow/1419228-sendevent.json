      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Never invoke this method directly. A right mouse-down event in a window of an inactive application is not delivered to the corresponding "
            },
            {
              "type": "codeVoice",
              "code": "NSWindow"
            },
            {
              "type": "text",
              "text": " object. It is instead delivered to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsapplication"
            },
            {
              "type": "text",
              "text": " object through a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsapplication/1428359-sendevent"
            },
            {
              "type": "text",
              "text": " message with a window number of 0."
            }
          ]
        }
      ]
    }
