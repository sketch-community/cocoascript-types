        {
          "name": "image",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object to be dragged."
                }
              ]
            }
          ]
        },
        {
          "name": "baseLocation",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Location of the image’s bottom-left corner in the window’s coordinate system. It determines the placement of the dragged image under the pointer."
                }
              ]
            }
          ]
        },
        {
          "name": "initialOffset",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The pointer’s location relative to the mouse-down location. Not used in macOS 10.4 and later."
                }
              ]
            }
          ]
        },
        {
          "name": "event",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The left-mouse down event that triggered the dragging operation."
                }
              ]
            }
          ]
        },
        {
          "name": "pboard",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The pasteboard that holds the data to be transferred to the destination."
                }
              ]
            }
          ]
        },
        {
          "name": "sourceObj",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object serving as the controller of the dragging operation. It must conform to the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource"
                },
                {
                  "type": "text",
                  "text": " protocol."
                }
              ]
            }
          ]
        },
        {
          "name": "slideFlag",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether the drag image should slide back to "
                },
                {
                  "type": "codeVoice",
                  "code": "baseLocation"
                },
                {
                  "type": "text",
                  "text": " if it's rejected by the drag destination. Pass "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " to specify slide back behavior or "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": " to specify that it should not."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method should be invoked only from within a view’s implementation of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1524634-mousedown"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1527420-mousedragged"
            },
            {
              "type": "text",
              "text": " methods (which overrides the version defined in "
            },
            {
              "type": "codeVoice",
              "code": "NSResponder"
            },
            {
              "type": "text",
              "text": " class). Essentially the same as the "
            },
            {
              "type": "codeVoice",
              "code": "NSView"
            },
            {
              "type": "text",
              "text": " method of the same name, except that "
            },
            {
              "type": "codeVoice",
              "code": "baseLocation"
            },
            {
              "type": "text",
              "text": " is given in the "
            },
            {
              "type": "codeVoice",
              "code": "NSWindow"
            },
            {
              "type": "text",
              "text": " object’s base coordinate system."
            }
          ]
        }
      ]
    }
