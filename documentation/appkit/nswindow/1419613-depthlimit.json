      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of this property can be examined with the Application Kit functions "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/1473820-nsplanarfromdepth"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/1473594-nscolorspacefromdepth"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/1473674-nsbitspersamplefromdepth"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/1473660-nsbitsperpixelfromdepth"
            },
            {
              "type": "text",
              "text": ". In addition, the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/1473812-nsbestdepth"
            },
            {
              "type": "text",
              "text": " function provides the best depth limit based on a set of parameters."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Setting this property to "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " sets the depth limit to the window’s default depth limit. A depth limit of "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " can be useful for reverting a window object to its initial depth. You can also use one of the explicit bit depths defined in "
            },
            {
              "type": "codeVoice",
              "code": "Explicit Window Depth Limits"
            },
            {
              "type": "text",
              "text": " ("
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindowdepth/nswindowdepthtwentyfourbitrgb"
            },
            {
              "type": "text",
              "text": " is the default)."
            }
          ]
        }
      ]
    }
