            },
            {
              "kind": "text",
              "text": " "
            },
            {
              "kind": "identifier",
              "text": "NSCollectionViewItem"
            },
            {
              "kind": "text",
              "text": " : "
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:objc(cs)NSViewController",
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsviewcontroller",
              "text": "NSViewController"
            }
          ],
          "platforms": [
