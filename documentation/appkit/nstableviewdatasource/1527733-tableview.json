    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The data source should incorporate the data from the dragging pasteboard in the implementation of this method. You can use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdragginginfo/1416092-draggingpasteboard"
            },
            {
              "type": "text",
              "text": " method to get the data for the drop operation from "
            },
            {
              "type": "codeVoice",
              "code": "info"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To accept a drop on the second row, "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": " would be 2 and "
            },
            {
              "type": "codeVoice",
              "code": "operation"
            },
            {
              "type": "text",
              "text": " would be "
            },
            {
              "type": "codeVoice",
              "code": "NSTableViewDropOn"
            },
            {
              "type": "text",
              "text": ". To accept a drop below the last row, "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": " would be "
            },
            {
              "type": "codeVoice",
              "code": "[aTableView numberOfRows]"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "operation"
            },
            {
              "type": "text",
              "text": " would be "
            },
            {
              "type": "codeVoice",
              "code": "NSTableViewDropAbove"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
