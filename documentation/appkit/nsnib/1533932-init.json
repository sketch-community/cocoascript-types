      ],
      "parameters": [
        {
          "name": "nibName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the nib file, without any leading path information. Inclusion of the "
                },
                {
                  "type": "codeVoice",
                  "code": ".nib"
                },
                {
                  "type": "text",
                  "text": " extension on the nib file name is optional. "
                }
              ]
            }
          ]
        },
        {
          "name": "bundle",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The bundle in which to search for the nib file. If you specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", this method looks for the nib file in the main bundle."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The initialized "
            },
            {
              "type": "codeVoice",
              "code": "NSNib"
            },
            {
              "type": "text",
              "text": " object or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if there were errors during initialization or the nib file could not be located.  "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "NSNib"
            },
            {
              "type": "text",
              "text": " object looks for the nib file in the bundle's language-specific project directories first, followed by the "
            },
            {
              "type": "codeVoice",
              "code": "Resources"
            },
            {
              "type": "text",
              "text": " directory. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "After the nib file has been loaded, the "
            },
            {
              "type": "codeVoice",
              "code": "NSNib"
            },
            {
              "type": "text",
              "text": " object uses the bundle’s resource map to locate additional resources referenced by the nib. If you specified "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " for the "
            },
            {
              "type": "codeVoice",
              "code": "bundle"
            },
            {
              "type": "text",
              "text": " parameter, the "
            },
            {
              "type": "codeVoice",
              "code": "NSNib"
            },
            {
              "type": "text",
              "text": " object looks for those resources in the bundle associated with the class of the nib file’s owner instead. If the nib file does not have an owner, the "
            },
            {
              "type": "codeVoice",
              "code": "NSNib"
            },
            {
              "type": "text",
              "text": " object looks for additional resources in the application’s main bundle."
            }
          ]
        }
      ]
    }
