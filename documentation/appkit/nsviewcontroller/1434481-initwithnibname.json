          "name": "nibNameOrNil",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the nib file, without any leading path information."
                }
              ]
            }
          ]
        },
        {
          "name": "nibBundleOrNil",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The bundle in which to search for the nib file. If you specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", this method looks for the nib file in the main bundle."
                }
              ]
            }
          ]
        }
      ]
