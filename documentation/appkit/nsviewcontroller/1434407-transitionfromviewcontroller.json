    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "fromViewController",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A child view controller whose view is visible in the view controller’s view hierarchy."
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The view of this view controller must have a superview, or else this method raises an exception."
                    }
                  ]
                }
              ],
              "style": "note",
              "name": "Note"
            }
          ]
        },
        {
          "name": "toViewController",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A child view controller whose view is not in the view controller’s view hierarchy."
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bitmask of options that specify how you want to perform the transition animation. For the options, see the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsviewcontrollertransitionoptions"
                },
                {
                  "type": "text",
                  "text": " enumeration."
                }
              ]
            }
          ]
        },
        {
          "name": "completion",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A block called immediately after the transition animation completes."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method to transition between sibling child view controllers owned by a parent view controller (which is the receiver of this method)."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method adds the view in the "
            },
            {
              "type": "codeVoice",
              "code": "toViewController"
            },
            {
              "type": "text",
              "text": " view controller to the superview of the view in the "
            },
            {
              "type": "codeVoice",
              "code": "fromViewController"
            },
            {
              "type": "text",
              "text": " view controller. Likewise, this method removes the "
            },
            {
              "type": "codeVoice",
              "code": "fromViewController"
            },
            {
              "type": "text",
              "text": " view from the parent view controller’s view hierarchy at the appropriate time. It is important to allow this method to add and remove these views."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The receiver of this method must be the parent view controller of the "
                },
                {
                  "type": "codeVoice",
                  "code": "fromViewController"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "toViewController"
                },
                {
                  "type": "text",
                  "text": " view controllers, or else this method raises an exception."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To create a parent/child relationship between view controllers, use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsviewcontroller/1434501-addchildviewcontroller"
            },
            {
              "type": "text",
              "text": " method or the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsviewcontroller/1434437-insertchildviewcontroller"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        }
      ]
    }
