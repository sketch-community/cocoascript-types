      "abstract": [
        {
          "type": "text",
          "text": "The deselection highlight state. During interactive selection, this state is used to indicate that the item will become deselected when interactions end. After interactions end, the highlight state returns to "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nscollectionviewitemhighlightstate/nscollectionviewitemhighlightnone"
