    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the document has been saved, the display name is the last component of the directory location of the saved file (for example, “"
            },
            {
              "type": "codeVoice",
              "code": "MyDocument"
            },
            {
              "type": "text",
              "text": "” if the path is “"
            },
            {
              "type": "codeVoice",
              "code": "/tmp/MyDocument.rtf"
            },
            {
              "type": "text",
              "text": "”). If the document is new, "
            },
            {
              "type": "codeVoice",
              "code": "NSDocument"
            },
            {
              "type": "text",
              "text": " makes the display name “Untitled "
            },
            {
              "type": "text",
              "text": "n"
            },
            {
              "type": "text",
              "text": ",” where "
            },
            {
              "type": "text",
              "text": "n"
            },
            {
              "type": "text",
              "text": " is a number in a sequence of new and unsaved documents. The displayable name also takes into account whether the document’s filename extension should be hidden. Subclasses of "
            },
            {
              "type": "codeVoice",
              "code": "NSWindowController"
            },
            {
              "type": "text",
              "text": " can override "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindowcontroller/1528112-windowtitlefordocumentdisplaynam"
            },
            {
              "type": "text",
              "text": " to modify the display name as it appears in window titles."
            }
          ]
        }
      ]
    }
