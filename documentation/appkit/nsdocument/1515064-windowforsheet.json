      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method searches the document's window controllers for the most suitable window to use when displaying the sheet. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of this property may be "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", in which case the sender should present an app-modal panel. The "
            },
            {
              "type": "codeVoice",
              "code": "NSDocument"
            },
            {
              "type": "text",
              "text": " implementation of this property sets the value to the window of the first window controller, or "
            },
            {
              "type": "codeVoice",
              "code": "[NSApp mainWindow]"
            },
            {
              "type": "text",
              "text": " if there are no window controllers or if the first window controller has no window."
            }
          ]
        }
      ]
    }
