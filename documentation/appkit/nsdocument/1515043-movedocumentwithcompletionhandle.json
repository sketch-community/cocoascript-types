    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method presents the user with a move panel if "
            },
            {
              "type": "codeVoice",
              "code": "[self fileURL]"
            },
            {
              "type": "text",
              "text": " is non-nil and then tries to save the document to the new location by invoking the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515057-movetourl"
            },
            {
              "type": "text",
              "text": " method if the user accepts the location presented by the panel. If a file with the same name already exists at that location, the user will be asked to choose between replacing the pre-existing file, renaming the current document, or canceling the move process. If "
            },
            {
              "type": "codeVoice",
              "code": "[self fileURL]"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", then the "
            },
            {
              "type": "codeVoice",
              "code": "[self runModalSavePanelForSaveOperation:NSSaveAsOperation delegate:didSaveSelector:contextInfo:]"
            },
            {
              "type": "text",
              "text": " message is sent instead."
            }
          ]
        }
      ]
    }
