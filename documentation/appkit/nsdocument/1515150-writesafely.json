      "parameters": [
        {
          "name": "absoluteURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The location to which the document contents are written."
                }
              ]
            }
          ]
        },
        {
          "name": "typeName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
