    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the user dismisses the alert and any recovery possible for the error and chosen by the user has been attempted, sends the message "
            },
            {
              "type": "codeVoice",
              "code": "didPresentSelector"
            },
            {
              "type": "text",
              "text": " to the specified "
            },
            {
              "type": "codeVoice",
              "code": "delegate"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "NSDocument"
            },
            {
              "type": "text",
              "text": " default implementation of this method is equivalent to that of "
            },
            {
              "type": "codeVoice",
              "code": "NSResponder"
            },
            {
              "type": "text",
              "text": " and treats the shared "
            },
            {
              "type": "codeVoice",
              "code": "NSDocumentController"
            },
            {
              "type": "text",
              "text": " object as the next responder and forwards these messages to it. The default implementations of several "
            },
            {
              "type": "codeVoice",
              "code": "NSDocument"
            },
            {
              "type": "text",
              "text": " methods invoke this method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default implementation of this method invokes "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515229-willpresenterror"
            },
            {
              "type": "text",
              "text": " to give subclasses an opportunity to customize error presentation. You should not override this method but should instead override "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515229-willpresenterror"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The method selected by "
            },
            {
              "type": "codeVoice",
              "code": "didPresentSelector"
            },
            {
              "type": "text",
              "text": " must have the same signature as:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)didPresentErrorWithRecovery:(BOOL)didRecover contextInfo:(void  *)contextInfo"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555663",
            "title": "Listing 1"
          }
        }
      ]
    }
