            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " otherwise, without changing "
            },
            {
              "type": "codeVoice",
              "code": "effectiveCharRange"
            },
