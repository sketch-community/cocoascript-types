        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "items"
            },
            {
              "type": "text",
              "text": " represent the objects to be shared and must conform to the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboardwriting"
            },
            {
              "type": "text",
              "text": " protocol, for example, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsimage"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurl"
            },
            {
              "type": "text",
              "text": ", etc. If the one of the items is an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurl"
            },
            {
              "type": "text",
              "text": " instance is a file URL (pointing to a video for example), then the content of the file will be shared. If the URL is remote, then the URL itself will be shared."
            }
          ]
        }
