{
  "abstract": [
    {
      "type": "text",
      "text": "A meta-locale identifier representing the set of Roman input sources available. You can pass "
    },
    {
      "type": "codeVoice",
      "code": "[NSArray arrayWithObject: NSAllRomanInputSourcesLocaleIdentifier]"
    },
    {
      "type": "text",
      "text": " to the "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstextview/1449370-allowedinputsourcelocales"
    },
    {
      "type": "text",
      "text": " property to restrict allowed input sources to Roman only."
    }
  ],
