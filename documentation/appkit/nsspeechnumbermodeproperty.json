      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring"
            },
            {
              "type": "text",
              "text": " object that specifies whether the synthesizer is currently in normal or literal number-processing mode. The supported values are listed in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsspeechmode"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the number-processing mode is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsspeechmodenormal"
            },
            {
              "type": "text",
              "text": ", the synthesizer assembles digits into numbers (so that “12” is spoken as “twelve”). When the mode is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsspeechmodeliteral"
            },
            {
              "type": "text",
              "text": ", each digit is spoken literally (so that “12” is spoken as “one, two”)."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property is used with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsspeechsynthesizer/1448425-setobject"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsspeechsynthesizer/1448436-objectforproperty"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
