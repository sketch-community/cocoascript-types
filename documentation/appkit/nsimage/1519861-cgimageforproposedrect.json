    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "proposedDestRect",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On input, the proposed destination rectangle for drawing the image. If "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", it defaults to the smallest pixel-integral rectangle containing {{0,0}, [self size]}.  The "
                },
                {
                  "type": "codeVoice",
                  "code": "proposedDestRect"
                },
                {
                  "type": "text",
                  "text": " is in user space in the reference context."
                }
              ]
            }
          ]
        },
        {
          "name": "referenceContext",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A graphics context."
                }
              ]
            }
          ]
        },
        {
          "name": "hints",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary of hints that provide more context for selecting or generating a "
                },
                {
                  "type": "codeVoice",
                  "code": "CGImage"
                },
                {
                  "type": "text",
                  "text": ", and may override properties of the "
                },
                {
                  "type": "codeVoice",
                  "code": "referenceContext"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A "
            },
            {
              "type": "codeVoice",
              "code": "CGImageRef"
            },
            {
              "type": "text",
              "text": ". This may be an existing "
            },
            {
              "type": "codeVoice",
              "code": "CGImage"
            },
            {
              "type": "text",
              "text": " if one is available. If not, a new "
            },
            {
              "type": "codeVoice",
              "code": "CGImage"
            },
            {
              "type": "text",
              "text": " is created."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "codeVoice",
              "code": "NSImage"
            },
            {
              "type": "text",
              "text": " is potentially resolution independent, and may have representations that allow it to draw well in many contexts.  A "
            },
            {
              "type": "codeVoice",
              "code": "CGImage"
            },
            {
              "type": "text",
              "text": " is more like a single pixel-based representation.   This method produces a snapshot of how the "
            },
            {
              "type": "codeVoice",
              "code": "NSImage"
            },
            {
              "type": "text",
              "text": " would draw if it was asked to draw in the proposed rectangle in the graphics context.  "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": " All input parameters are optional.  They provide hints for how to choose among existing "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgimageref"
            },
            {
              "type": "text",
              "text": " objects, or how to create one if there isn't already a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgimageref"
            },
            {
              "type": "text",
              "text": " available.  The parameters are only hints."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method is typically called, not overridden."
            }
          ]
        }
      ]
    }
