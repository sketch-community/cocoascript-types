      "parameters": [
        {
          "name": "rowIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index at which the new row should be inserted. "
                },
                {
                  "type": "codeVoice",
                  "code": "rowIndex"
                },
                {
                  "type": "text",
                  "text": " must be greater than "
                },
                {
                  "type": "codeVoice",
                  "code": "parentRow"
                },
                {
                  "type": "text",
                  "text": ", and much specify a row that does not fall amongst the children of some other parent."
                }
              ]
            }
          ]
        },
        {
          "name": "rowType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of the new row."
                }
              ]
            }
          ]
        },
        {
          "name": "parentRow",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index of the row of which the new row is a child. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "-1"
                },
                {
                  "type": "text",
                  "text": " to indicate that the new row should be a root row."
                }
              ]
            }
          ]
        },
        {
          "name": "shouldAnimate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " if creation of the new row should be animated, otherwise "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
