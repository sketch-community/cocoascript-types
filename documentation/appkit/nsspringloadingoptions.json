    "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingdisabled": {
      "title": "NSSpringLoadingDisabled",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingdisabled",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadingoptions/nsspringloadingdisabled",
      "abstract": [
        {
          "type": "text",
          "text": "Spring-loading on the destination object is disabled. No spring-loading operations can occur."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingenabled": {
      "title": "NSSpringLoadingEnabled",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingenabled",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadingoptions/nsspringloadingenabled",
      "abstract": [
        {
          "type": "text",
          "text": "Spring-loading on the destination object is enabled. The user can drag an object over a destination object and hover or force click to initiate spring-loading and activate the destination object. When initiated by a force click, spring-loading is invoked once the force click is released."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingcontinuousactivation": {
      "title": "NSSpringLoadingContinuousActivation",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingcontinuousactivation",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadingoptions/nsspringloadingcontinuousactivation",
      "abstract": [
        {
          "type": "text",
          "text": "Spring-loading on the destination object is enabled. The user can drag an object over a destination object and hover or force click to initiate spring-loading and activate the destination object. When initiated by a force click, spring-loading is invoked once the force click begins and deactivated when the force click is released. When initiated by hovering, spring-loading is invoked at the hover timeout and deactivated when the drag exits the destination object. Use this constant sparingly."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingnohover": {
      "title": "NSSpringLoadingNoHover",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadingoptions/nsspringloadingnohover",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadingoptions/nsspringloadingnohover",
      "abstract": [
        {
          "type": "text",
          "text": "Spring-loading on the destination object is enabled, but cannot be invoked by hovering. The user can drag an object over a destination object and force click to initiate spring-loading and activate the destination object. This option may be useful in situations where a long hover, such as dragging across a large destination object, initiates undesired spring-loading. Use this constant sparingly."
        }
      ]
    },
