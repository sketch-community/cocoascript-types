        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": " The notification object is the notifying "
            },
            {
              "type": "codeVoice",
              "code": "NSText"
            },
            {
              "type": "text",
              "text": " object. The "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " dictionary contains the following information:"
            }
