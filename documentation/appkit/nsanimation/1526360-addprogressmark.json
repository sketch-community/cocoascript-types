    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A progress mark represents a percentage of the animation completed. When the animation reaches a progress mark, an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsanimationdelegate/1535100-animation"
            },
            {
              "type": "text",
              "text": " message is sent to the delegate and an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsanimationprogressmarknotification"
            },
            {
              "type": "text",
              "text": " is broadcast to all observers. You might receive multiple notifications of progress advances over multiple marks."
            }
          ]
        }
      ]
    }
