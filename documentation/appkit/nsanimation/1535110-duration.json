          "inlineContent": [
            {
              "type": "text",
              "text": "The value of this property must be greater than or equal to "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": ". Setting the duration to a negative value raises an exception. "
            }
