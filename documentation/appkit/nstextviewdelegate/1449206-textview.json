      "parameters": [
        {
          "name": "textView",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The text view sending the message. This is the first text view in a series shared by a layout manager, not necessarily the text view displaying the selected text."
                }
              ]
            }
          ]
        },
        {
          "name": "affectedRanges",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The array of ranges of characters to be replaced. This array must be a non-nil, non-empty array of objects responding to the NSValue "
                },
                {
                  "type": "codeVoice",
                  "code": "rangeValue"
                },
                {
                  "type": "text",
                  "text": " method, and in addition its elements must be sorted, non-overlapping, non-contiguous, and (except for the case of a single range) have non-zero-length."
                }
              ]
            }
          ]
        },
        {
          "name": "replacementStrings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The array of strings that will replace the characters in "
                },
                {
                  "type": "codeVoice",
                  "code": "affectedRanges"
                },
                {
                  "type": "text",
                  "text": ", one string for each range; "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if only text attributes are being changed."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
