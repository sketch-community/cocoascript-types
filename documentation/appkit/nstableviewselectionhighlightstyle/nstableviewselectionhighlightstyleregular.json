          "inlineContent": [
            {
              "type": "text",
              "text": "The regular highlight style of NSTableView. In OS X v10.7 a light blue (returned by sending "
            },
            {
              "type": "codeVoice",
              "code": "NSColor"
            },
            {
              "type": "text",
              "text": " a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscolor/1533135-alternateselectedcontrolcolor"
            },
            {
              "type": "text",
              "text": " message) or light gray color (returned by sending NSColor a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscolor/1535258-secondaryselectedcontrolcolor"
            },
            {
              "type": "text",
              "text": " message)."
            }
          ]
        }
      ]
