          "inlineContent": [
            {
              "type": "text",
              "text": "Counts the number of onscreen windows belonging to a particular application, identified by "
            },
            {
              "type": "codeVoice",
              "code": "context"
            },
            {
              "type": "text",
              "text": ", which is a window server connection ID. The function returns the number by reference in "
            },
            {
              "type": "codeVoice",
              "code": "count"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
