          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsopenglpixelbuffer"
            },
            {
              "type": "text",
