{
  "abstract": [
    {
      "type": "text",
      "text": "An NSNumber object containing a Boolean value specifying whether disabled fonts should be included in the list of matching descriptors; "
    },
    {
      "type": "reference",
      "isActive": false,
      "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
    },
    {
      "type": "text",
      "text": " if they should be included, "
    },
    {
      "type": "reference",
      "isActive": false,
      "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
    },
    {
      "type": "text",
      "text": " otherwise.. When unspecified, CoreText assumes "
    },
    {
      "type": "reference",
      "isActive": false,
      "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
    },
    {
      "type": "text",
      "text": ". This option is intended only for font management applications. This option will make descriptor matching slower."
    }
  ],
