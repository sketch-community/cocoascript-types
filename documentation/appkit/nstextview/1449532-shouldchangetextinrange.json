          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range of characters affected by the proposed change."
                }
              ]
            }
          ]
        },
        {
          "name": "replacementString",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The characters that will replace those in "
                },
                {
                  "type": "codeVoice",
                  "code": "affectedCharRange"
                },
                {
                  "type": "text",
                  "text": ". If only text attributes are being changed, "
                },
                {
                  "type": "codeVoice",
                  "code": "replacementString"
                },
