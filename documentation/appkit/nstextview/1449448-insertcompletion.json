    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method has two effects, text substitution and changing of the selection:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "It replaces the text between "
                    },
                    {
                      "type": "codeVoice",
                      "code": "charRange.start"
                    },
                    {
                      "type": "text",
                      "text": " and the current insertion point with "
                    },
                    {
                      "type": "codeVoice",
                      "code": "word"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If "
                    },
                    {
                      "type": "codeVoice",
                      "code": "flag"
                    },
                    {
                      "type": "text",
                      "text": " is "
                    },
                    {
                      "type": "reference",
                      "isActive": false,
                      "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                    },
                    {
                      "type": "text",
                      "text": " it changes the selection to be the last "
                    },
                    {
                      "type": "emphasis",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "n"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " characters of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "word"
                    },
                    {
                      "type": "text",
                      "text": " where "
                    },
                    {
                      "type": "emphasis",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "n"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " is equal to "
                    },
                    {
                      "type": "codeVoice",
                      "code": "[word length]"
                    },
                    {
                      "type": "text",
                      "text": " minus "
                    },
                    {
                      "type": "codeVoice",
                      "code": "charRange.length"
                    },
                    {
                      "type": "text",
                      "text": ", that is, the potential completion."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": " If "
                    },
                    {
                      "type": "codeVoice",
                      "code": "flag"
                    },
                    {
                      "type": "text",
                      "text": " is "
                    },
                    {
                      "type": "reference",
                      "isActive": false,
                      "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                    },
                    {
                      "type": "text",
                      "text": " it makes the selection empty and puts the insertion point just after "
                    },
                    {
                      "type": "codeVoice",
                      "code": "word"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
