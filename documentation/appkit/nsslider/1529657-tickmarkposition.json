      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For horizontal sliders, use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstickmarkposition/nstickmarkpositionbelow"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstickmarkposition/nstickmarkpositionabove"
            },
            {
              "type": "text",
              "text": ". For vertical sliders, use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstickmarkposition/nstickmarkpositionleading"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstickmarkposition/nstickmarkpositiontrailing"
            },
            {
              "type": "text",
              "text": ". The default positions are "
            },
            {
              "type": "codeVoice",
              "code": "below"
            },
            {
              "type": "text",
              "text": " for horizontal and "
            },
            {
              "type": "codeVoice",
              "code": "leading"
            },
            {
              "type": "text",
              "text": " for vertical. This property has no effect if "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsslider/1524268-numberoftickmarks"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": ", or if the slider is circular. In its implementation of this property, the receiving "
            },
            {
              "type": "codeVoice",
              "code": "NSSlider"
            },
            {
              "type": "text",
              "text": " instance invokes the method of the same name of its "
            },
            {
              "type": "codeVoice",
              "code": "NSSliderCell"
            },
            {
              "type": "text",
              "text": " instance."
            }
          ]
        }
      ]
    }
