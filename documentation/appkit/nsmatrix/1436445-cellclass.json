      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of this property should be the id of a subclass of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscell"
            },
            {
              "type": "text",
              "text": ", which can be obtained by sending the "
            },
            {
              "type": "codeVoice",
              "code": "class"
            },
            {
              "type": "text",
              "text": " message to either the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscell"
            },
            {
              "type": "text",
              "text": " subclass object or to an instance of that subclass. The default cell class is that set with the class method "
            },
            {
              "type": "codeVoice",
              "code": "setCellClass:"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsactioncell"
            },
            {
              "type": "text",
              "text": " if no other default cell class has been specified."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You need to use this property only with matrices initialized with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsmatrix/1436428-initwithframe"
            },
            {
              "type": "text",
              "text": ", because the other initializers allow you to specify an instance-specific cell class or cell prototype."
            }
          ]
        }
      ]
    }
