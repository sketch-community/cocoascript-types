    },
    "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstyletoolbar": {
      "title": "NSTabViewControllerTabStyleToolbar",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstyletoolbar",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstyletoolbar",
      "abstract": [
        {
          "type": "text",
          "text": "A style that automatically adds any tabs to the window’s toolbar. The tab view controller takes control of the window’s toolbar and sets itself as the toolbar’s delegate. Customization of the toolbar is handled using the methods in "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontroller#1660994"
        },
        {
          "type": "text",
          "text": ". "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstyleunspecified": {
      "title": "NSTabViewControllerTabStyleUnspecified",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstyleunspecified",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstyleunspecified",
      "abstract": [
        {
          "type": "text",
          "text": "A style that indicates the tab view controller does not provide the tab selection UI. Your app provides the control (such as an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nssegmentedcontrol"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nspopupbutton"
        },
        {
          "type": "text",
          "text": ") for navigating between tabs. You can bind an existing control to the tab view controller object so that interactions with the control automatically change tabs."
        }
      ]
    },
