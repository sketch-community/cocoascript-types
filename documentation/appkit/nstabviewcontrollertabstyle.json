    "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstylesegmentedcontrolontop": {
      "title": "NSTabViewControllerTabStyleSegmentedControlOnTop",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstylesegmentedcontrolontop",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstylesegmentedcontrolontop",
      "abstract": [
        {
          "type": "text",
          "text": "A style that displays a segmented control along the top edge of the tab view interface. Access the configuration of the tab items through the tab view, which you can get from the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontroller/1428241-tabview"
        },
        {
          "type": "text",
          "text": " property. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstylesegmentedcontrolonbottom": {
      "title": "NSTabViewControllerTabStyleSegmentedControlOnBottom",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstylesegmentedcontrolonbottom",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nstabviewcontrollertabstyle/nstabviewcontrollertabstylesegmentedcontrolonbottom",
      "abstract": [
        {
          "type": "text",
          "text": "A style that displays a segmented control along the bottom edge of the tab view interface. Access the configuration of the tab items through the tab view, which you can get from the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nstabviewcontroller/1428241-tabview"
        },
        {
          "type": "text",
          "text": " property. "
        }
