          "inlineContent": [
            {
              "type": "text",
              "text": "The set of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsindexpath"
            },
            {
              "type": "text",
              "text": " objects corresponding to the currently visible items."
