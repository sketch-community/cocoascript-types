      "parameters": [
        {
          "name": "allowedTypes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of strings representing either file extensions or UTIs. Can be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the default value, allowing all types."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "allowedTypes"
            },
            {
              "type": "text",
              "text": " array can contain file extensions (without the period that begins the extension) or UTIs. To allow folders, include the "
            },
            {
              "type": "codeVoice",
              "code": "public.folder"
            },
            {
              "type": "text",
              "text": " identifier. To allow any type, use "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ". If the value of "
            },
            {
              "type": "codeVoice",
              "code": "allowedTypes"
            },
            {
              "type": "text",
              "text": " is an empty array, nothing is allowed. The default value is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", allowing all types."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the cell is editable and its type is "
            },
            {
              "type": "codeVoice",
              "code": "NSPathStylePopUp"
            },
            {
              "type": "text",
              "text": ", a Choose item is included to enable selection of a different path by invoking an Open panel. The allowed types are passed to the Open panel to filter out other types. The allowed types are also used with drag and drop to indicate if a drop is allowed."
            }
          ]
        }
      ]
    }
