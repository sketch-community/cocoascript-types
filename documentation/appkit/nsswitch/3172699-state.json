        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The values "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscontrolstatevalueoff"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscontrolstatevalueon"
            },
            {
              "type": "text",
              "text": " indicate that the switch is in the off or on position. The switch treats any value other than "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscontrolstatevalueoff"
            },
            {
              "type": "text",
              "text": " as on."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Setting this property through the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsanimatablepropertycontainer/1530511-animator"
            },
            {
              "type": "text",
              "text": " proxy animates the switch to the new value."
            }
          ]
        },
