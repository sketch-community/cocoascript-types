          "inlineContent": [
            {
              "type": "text",
              "text": "Do not use highlighting as a means to determine whether spring-loading has actually been activated or deactivated. The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadingdestination/1416106-springloadingactivated"
            },
            {
              "type": "text",
              "text": " method alerts your app when spring-loading activation occurs."
            }
          ]
        }
      ]
