    "doc://com.apple.documentation/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightnone": {
      "title": "NSSpringLoadingHighlightNone",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightnone",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightnone",
      "abstract": [
        {
          "type": "text",
          "text": "Don’t apply any highlighting style to the destination’s user interface. The destination is probably not spring-loadable."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightstandard": {
      "title": "NSSpringLoadingHighlightStandard",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightstandard",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightstandard",
      "abstract": [
        {
          "type": "text",
          "text": "Apply light highlighting to the destination’s user interface. This highlighting style typically provides a subtle visual cue to the user that the destination supports spring-loading and that spring-loading is about to be fully engaged. A brief period of highlighting with this style provides the user with an opportunity to disengage and prevent the spring-loading from occurring."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightemphasized": {
      "title": "NSSpringLoadingHighlightEmphasized",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightemphasized",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsspringloadinghighlight/nsspringloadinghighlightemphasized",
      "abstract": [
        {
          "type": "text",
          "text": "Apply darker highlighting to the destination’s user interface to visually indicate to the user that spring-loading was engaged on the destination."
        }
      ]
    },
