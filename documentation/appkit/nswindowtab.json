            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindowtab"
            },
            {
              "type": "text",
              "text": " are configurable at any time, but only take effect when the associated "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow"
            },
            {
              "type": "text",
              "text": " displays in a tab."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "AppKit automatically creates an instance of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindowtab"
            },
            {
              "type": "text",
              "text": " for each "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow"
            },
            {
              "type": "text",
              "text": ". You can access a windowâ€™s tab object using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow/2870102-tab"
            },
            {
              "type": "text",
              "text": " property."
            }
          ]
        }
