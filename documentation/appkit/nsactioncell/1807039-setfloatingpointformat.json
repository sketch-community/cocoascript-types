    },
    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "autoRange",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": " if you want the receiver to places digits to the right and left of the decimal point as specified (in "
                },
                {
                  "type": "codeVoice",
                  "code": "leftDigits"
                },
                {
                  "type": "text",
                  "text": " and "
                },
