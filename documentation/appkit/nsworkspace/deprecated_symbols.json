  "primaryContentSections": [
    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsworkspace/deprecated_symbols#3737326"
            },
            {
              "type": "text",
              "text": " describes keys for an "
            },
            {
              "type": "codeVoice",
              "code": "NSDictionary"
            },
            {
              "type": "text",
              "text": " object containing information about an app. This dictionary is returned by "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsworkspace/1530411-activeapplication"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsworkspace/1579272-launchedapplications"
            },
            {
              "type": "text",
              "text": ", and is also provided in the "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " of "
            },
            {
              "type": "codeVoice",
              "code": "NSWorkspace"
            },
            {
              "type": "text",
              "text": " notifications for app launch and termination."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Note that these constants are considered legacy."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "It is strongly suggested that you use the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSWorkspace"
                },
                {
                  "type": "text",
                  "text": " class’s "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsworkspace/1534059-runningapplications"
                },
                {
                  "type": "text",
                  "text": " method and the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsrunningapplication"
                },
                {
                  "type": "text",
                  "text": " class to retrieve this information in apps target macOS 10.6 and later, rather than the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsworkspace/1530411-activeapplication"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsworkspace/1579272-launchedapplications"
                },
                {
                  "type": "text",
                  "text": " methods."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        },
