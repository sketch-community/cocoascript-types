      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The method specified by the "
            },
            {
              "type": "codeVoice",
              "code": " didRunSelector"
            },
            {
              "type": "text",
              "text": " parameter must have the following signature:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)printOperationDidRun:(NSPrintOperation *)printOperation  success:(BOOL)success  contextInfo:(void *)contextInfo"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555787",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of "
            },
            {
              "type": "codeVoice",
              "code": "success"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " if the print operation ran to completion without cancellation or error, and "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " otherwise."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you send "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsprintoperation/1532487-canspawnseparatethread"
            },
            {
              "type": "text",
              "text": " to an "
            },
            {
              "type": "codeVoice",
              "code": "NSPrintOperation"
            },
            {
              "type": "text",
              "text": " object with an argument of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ", then the delegate specified in a subsequent invocation of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsprintoperation/1532065-runoperationmodalforwindow"
            },
            {
              "type": "text",
              "text": " may be messaged in that spawned, non-main thread."
            }
          ]
        }
      ]
    }
