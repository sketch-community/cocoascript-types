      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsmultilevelacceleratorbutton",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsmultilevelacceleratorbutton",
      "abstract": [
        {
          "type": "text",
          "text": "A multilevel accelerator button is a variation of a normal accelerator button that allows for a configurable numberÂ of stepped pressure levels. As each one is reached, the user receives light tactile feedback and an action is sent."
        }
      ],
      "deprecated": true
    },
