    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A file promise is a possible future file of a specified type.  When you're working with drag and drop, use promises to indicate intent for future action.  Avoid loading or performing any actions on the file until the promise completes."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseprovider"
            },
            {
              "type": "text",
              "text": " class when creating file promises. Instantiate one "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseprovider"
            },
            {
              "type": "text",
              "text": " for each file promised. Set the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseprovider/1644738-filetype"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseprovider/1644726-delegate"
            },
            {
              "type": "text",
              "text": " properties before writing any "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseprovider"
            },
            {
              "type": "text",
              "text": " to the pasteboard. The file type must be a Uniform Type Identifier (UTI) that ultimately conforms to "
            },
            {
              "type": "codeVoice",
              "code": "kUTTypeData"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "kUTTypeDirectory"
            },
            {
              "type": "text",
              "text": ". The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseproviderdelegate"
            },
            {
              "type": "text",
              "text": " will write the promised file to the destination directory."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Optionally, you may attach a "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " object of your choosing to the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseprovider"
            },
            {
              "type": "text",
              "text": " to determine which promise is being referenced when promising multiple files under the same "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsfilepromiseproviderdelegate"
            },
            {
              "type": "text",
              "text": " instance."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
