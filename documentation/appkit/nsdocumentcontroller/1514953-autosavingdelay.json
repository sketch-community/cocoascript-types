        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A value of 0 indicates that periodic autosaving should not be done at all. The "
            },
            {
              "type": "codeVoice",
              "code": "NSDocumentController"
            },
            {
              "type": "text",
              "text": " object uses this number as the amount of time to wait between detecting that a document has unautosaved changes and sending the document an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515225-autosavedocumentwithdelegate"
            },
            {
              "type": "text",
              "text": " message. The default value is "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
