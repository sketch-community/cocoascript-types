              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One or more logically ORed color mode masks described in "
                },
                {
                  "type": "codeVoice",
                  "code": "Color Picker Mode Masks"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
