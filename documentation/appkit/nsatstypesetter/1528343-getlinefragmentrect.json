      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The method returns the calculated line fragment rectangle in "
            },
            {
              "type": "codeVoice",
              "code": "lineFragmentRect"
            },
            {
              "type": "text",
              "text": ", and it returns the used rectangle (the portion of the line fragment rectangle that actually contains marks) in "
            },
            {
              "type": "codeVoice",
              "code": "lineFragmentUsedRect"
            },
            {
              "type": "text",
              "text": ". The "
            },
            {
              "type": "codeVoice",
              "code": "paragraphSeparatorGlyphRange"
            },
            {
              "type": "text",
              "text": " is the range of glyphs under consideration, and "
            },
            {
              "type": "codeVoice",
              "code": "lineOrigin"
            },
            {
              "type": "text",
              "text": " is the origin point of the line fragment rectangle. A "
            },
            {
              "type": "codeVoice",
              "code": "paragraphSeparatorGlyphRange"
            },
            {
              "type": "text",
              "text": " with length 0 indicates an extra line fragment (which occurs if the last character in the paragraph is a line separator.)"
            }
          ]
        }
      ]
    }
