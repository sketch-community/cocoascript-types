    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsatstypesetter"
            },
            {
              "type": "text",
              "text": " calls this method to determine the number of points of space to include below the descenders in the used rectangle for the proposed line fragment rectangle "
            },
            {
              "type": "codeVoice",
              "code": "rect"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Line spacing, also called leading, is an attribute of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/nsparagraphstyle"
            },
            {
              "type": "text",
              "text": ", which you can set on an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/nsmutableparagraphstyle"
            },
            {
              "type": "text",
              "text": " object. A font typically includes a default minimum line spacing metric used if none is set in the paragraph style."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the typesetter behavior specified in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/nslayoutmanager"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstypesetterbehavior/nstypesetteroriginalbehavior"
            },
            {
              "type": "text",
              "text": ", the text system uses the original, private typesetter "
            },
            {
              "type": "codeVoice",
              "code": "NSSimpleHorizontalTypesetter"
            },
            {
              "type": "text",
              "text": ", which adds the line spacing above the ascender. Similarly, "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsatstypesetter"
            },
            {
              "type": "text",
              "text": " adds the line spacing above the ascender if the value is negative."
            }
          ]
        }
      ]
    }
