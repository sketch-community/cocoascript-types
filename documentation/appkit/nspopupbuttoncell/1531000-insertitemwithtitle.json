    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "title",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The title of the new item. If an item with the same title already exists in the menu, the existing item is removed and the new one is added "
                }
              ]
            }
          ]
        },
        {
          "name": "index",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The zero-based index at which to insert the item. Specifying "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " inserts the item at the top of the menu. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value in "
            },
            {
              "type": "codeVoice",
              "code": "index"
            },
            {
              "type": "text",
              "text": " must represent a valid position in the array. The menu item at "
            },
            {
              "type": "codeVoice",
              "code": "index"
            },
            {
              "type": "text",
              "text": " and all those that follow it are shifted down one slot to make room for the new menu item. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method assigns the pop-up button’s default action and target to the new menu item. Use the menu item’s setAction: and setTarget: methods to assign a new action and target."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Because this method searches for duplicate items, it should not be used if you are adding an item to an already populated menu with more than a few hundred items. In a situation like this, add items directly to the button's menu instead."
            }
          ]
        }
      ]
    }
