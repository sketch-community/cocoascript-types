    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the developer creates an "
            },
            {
              "type": "codeVoice",
              "code": "NSDraggingItem"
            },
            {
              "type": "text",
              "text": " instance , it is for use with the view method "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483791-begindraggingsessionwithitems"
            },
            {
              "type": "text",
              "text": " During the invocation of that method, the "
            },
            {
              "type": "codeVoice",
              "code": "pasteboardWriter"
            },
            {
              "type": "text",
              "text": " is placed onto the dragging pasteboard for the "
            },
            {
              "type": "codeVoice",
              "code": "NSDraggingSession"
            },
            {
              "type": "text",
              "text": " that contains the dragging item instance."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The designated initializer."
            }
          ]
        }
      ]
