      "parameters": [
        {
          "name": "window",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The window being presented in a version browser."
                }
              ]
            }
          ]
        },
        {
          "name": "maxPreferredSize",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum size the version browser would prefer the window to be."
                }
              ]
            }
          ]
        },
        {
          "name": "maxAllowedSize",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum allowed size for the window (the full-screen frame minus the margins required to ensure the Versions controls are still visible)."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
