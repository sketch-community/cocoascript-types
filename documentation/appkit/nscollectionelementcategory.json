      "abstract": [
        {
          "type": "text",
          "text": "The element is an item. Items represent the main content of your collection view. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nscollectionelementcategory/nscollectionelementcategorysupplementaryview": {
      "title": "NSCollectionElementCategorySupplementaryView",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nscollectionelementcategory/nscollectionelementcategorysupplementaryview",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nscollectionelementcategory/nscollectionelementcategorysupplementaryview",
      "abstract": [
        {
          "type": "text",
          "text": "The element is a supplementary view. Use supplementary views for single views that contain some data but are associated with an entire section. For example, use them to specify header or footer views for a section. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nscollectionelementcategory/nscollectionelementcategorydecorationview": {
      "title": "NSCollectionElementCategoryDecorationView",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nscollectionelementcategory/nscollectionelementcategorydecorationview",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nscollectionelementcategory/nscollectionelementcategorydecorationview",
      "abstract": [
        {
          "type": "text",
          "text": "The element is a decoration view. Decoration views represent visual adornments that do not contain any data of their own. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nscollectionelementcategory/nscollectionelementcategoryinteritemgap": {
      "title": "NSCollectionElementCategoryInterItemGap",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nscollectionelementcategory/nscollectionelementcategoryinteritemgap",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nscollectionelementcategory/nscollectionelementcategoryinteritemgap",
      "abstract": [
        {
          "type": "text",
          "text": "The element is an inter-item gap. An inter-item gap element is a custom visual indicator that is displayed between items when dropping items into the collection view."
        }
      ]
    },
