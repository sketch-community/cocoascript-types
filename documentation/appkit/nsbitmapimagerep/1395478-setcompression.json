              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "enum"
                },
                {
                  "type": "text",
                  "text": " constant that identifies one of the supported compression types as described in "
