        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value and minimum value are "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": ". The default maximum value is dependent on "
            },
            {
              "type": "codeVoice",
              "code": "levelIndicatorStyle"
            },
            {
              "type": "text",
              "text": ". For continuous styles, the default maximum value is "
            },
            {
              "type": "codeVoice",
              "code": "100.0"
            },
            {
              "type": "text",
              "text": ". For discrete styles, the default maximum value is "
            },
            {
              "type": "codeVoice",
              "code": "5.0"
            },
            {
              "type": "text",
              "text": "."
            }
