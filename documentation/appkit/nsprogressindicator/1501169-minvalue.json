        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, a determinate progress indicator goes from "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "100.0"
            },
            {
              "type": "text",
              "text": ", so the default value of this property is "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An indeterminate progress indicator does not use this value."
            }
          ]
        }
