      ],
      "parameters": [
        {
          "name": "absoluteDirectoryPath",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The directory whose files the panel displays. When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the directory is the same directory used in the previous invocation of the panel; this is probably the best choice for most situations."
                }
              ]
            }
          ]
        },
        {
          "name": "filename",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies a particular file in "
                },
                {
                  "type": "codeVoice",
                  "code": "absoluteDirectoryPath"
                },
                {
                  "type": "text",
                  "text": " that is selected when the Open panel is presented to the user. When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", no file is initially selected."
                }
              ]
            }
          ]
        },
        {
          "name": "fileTypes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of file extensions and/or HFS file types. Specifies the files the panel allows the user to select. nil makes all files in "
                },
                {
                  "type": "codeVoice",
                  "code": "absoluteDirectoryPath"
                },
                {
                  "type": "text",
                  "text": " selectable by the user. An array of types passed in here will override one set using "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nssavepanel/1534419-allowedfiletypes"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "docWindow",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The window to open the sheet on."
                }
              ]
            }
          ]
        },
        {
          "name": "modalDelegate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This is not the same as a delegate assigned to the panel. This delegate is temporary and the relationship only lasts until the panel is dismissed."
                }
              ]
            }
          ]
        },
        {
          "name": "didEndSelector",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The message sent to "
                },
                {
                  "type": "codeVoice",
                  "code": "modalDelegate"
                },
                {
                  "type": "text",
                  "text": " after the modal session has ended, but before dismissing the Open panel. "
                },
                {
                  "type": "codeVoice",
                  "code": "didEndSelector"
                },
                {
                  "type": "text",
                  "text": " may dismiss the Open panel itself; otherwise, it will be dismissed on return from the method. The corresponding method should have the following signature:"
                }
              ]
            },
            {
              "type": "codeListing",
              "code": [
                "- (void)openPanelDidEnd:(NSOpenPanel *)panel returnCode:(int)returnCode  contextInfo:(void  *)contextInfo"
              ],
              "syntax": "occ",
              "metadata": {
                "anchor": "2555745",
                "title": "Listing 1"
              }
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value passed as "
                },
                {
                  "type": "codeVoice",
                  "code": "returnCode"
                },
                {
                  "type": "text",
                  "text": " will be either "
                },
                {
                  "type": "codeVoice",
                  "code": "NSCancelButton"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "NSOKButton"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "contextInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Any context information passed to "
                },
                {
                  "type": "codeVoice",
                  "code": "modalDelegate"
                },
                {
                  "type": "text",
                  "text": " in the "
                },
                {
                  "type": "codeVoice",
                  "code": "didEndSelector"
                },
                {
                  "type": "text",
                  "text": " message."
                }
              ]
            }
          ]
        }
      ]
    }
