      "parameters": [
        {
          "name": "propertyList",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A property list containing data to initialize the receiver."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "By default, the property list object is an instance of "
                },
                {
                  "type": "codeVoice",
                  "code": "NSData"
                },
                {
                  "type": "text",
                  "text": ". If you implement "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboardreading/1533559-readingoptionsfortype"
                },
                {
                  "type": "text",
                  "text": " and specify an option other than "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboardreadingoptions/nspasteboardreadingasdata"
                },
                {
                  "type": "text",
                  "text": ", the "
                },
                {
                  "type": "codeVoice",
                  "code": "propertyList"
                },
                {
                  "type": "text",
                  "text": " may be any other property list object."
                }
              ]
            }
          ]
        },
        {
          "name": "type",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A UTI supported by the receiver for reading (one of the types returned by "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboardreading/1532019-readabletypesforpasteboard"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An object initialized using the data in "
            },
            {
              "type": "codeVoice",
              "code": "propertyList"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method is considered optional because, if "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboardreading/1532019-readabletypesforpasteboard"
            },
            {
              "type": "text",
              "text": " returns just a single type, and that type uses the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboardreadingoptions/nspasteboardreadingaskeyedarchive"
            },
            {
              "type": "text",
              "text": " reading option, then instances are initialized using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoding/1416145-initwithcoder"
            },
            {
              "type": "text",
              "text": " instead of this method."
            }
          ]
        }
      ]
    }
