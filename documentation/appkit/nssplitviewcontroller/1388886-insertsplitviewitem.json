      "parameters": [
        {
          "name": "splitViewItem",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The split view item to add. "
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Before you add a split view item, it must be non-"
                    },
                    {
                      "type": "codeVoice",
                      "code": "nil"
                    },
                    {
                      "type": "text",
                      "text": " and it must have an associated view controller, or the system throws an exception."
                    }
                  ]
                }
              ],
              "style": "important"
            }
          ]
        },
        {
          "name": "index",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index position for adding the split view item in the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nssplitviewcontroller/1388876-splitviewitems"
                },
                {
                  "type": "text",
                  "text": " array."
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the index value is out of bounds (less than "
                    },
                    {
                      "type": "codeVoice",
                      "code": "0"
                    },
                    {
                      "type": "text",
                      "text": " or greater than the count of the array), the system throws an exception."
                    }
                  ]
                }
              ],
              "style": "important"
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the split view controller’s view finishes loading, and the split view item that you add is visible, the system loads the split view item’s view controller’s view and adds it to the split view."
            }
          ]
        }
      ]
