        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The frame of the outline cell for the row at index "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": ", considering the current indentation and the value in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsoutlineview/1531707-indentationmarkerfollowscell"
            },
            {
              "type": "text",
              "text": " property. If the row at index "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": " is not an expandable row, returns "
            },
            {
              "type": "codeVoice",
              "code": "NSZeroRect"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
