            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Identifier of the voice to set as the current voice. When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the default voice is used. Passing in a specific voice means the initial speaking rate is determined by the synthesizer’s default speaking rate; passing "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " means the speaking rate is automatically set to the rate the user specifies in Speech preferences."
                }
              ]
            }
          ]
        }
