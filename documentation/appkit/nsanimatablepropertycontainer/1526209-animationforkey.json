    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the action specified by "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " is triggered for an object, this method is consulted to find the animation, if any, that should be performed in response."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Like its Core Animation "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartzcore/calayer"
            },
            {
              "type": "text",
              "text": " counterpart, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartzcore/calayer/1410844-actionforkey"
            },
            {
              "type": "text",
              "text": ", this method is a funnel point that defines the order in which the search for an animation proceeds.It first checks the receiver's "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsanimatablepropertycontainer#1663306"
            },
            {
              "type": "text",
              "text": " dictionary for a value matching "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": ", then falls back to  "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsanimatablepropertycontainer/1530511-animator"
            },
            {
              "type": "text",
              "text": " for the receiver's class."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": " Subclasses should not typically need to override this method."
            }
          ]
        }
      ]
    }
