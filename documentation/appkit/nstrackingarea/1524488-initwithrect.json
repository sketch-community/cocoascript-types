    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "rect",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A rectangle that defines a region of a target view, in the view’s coordinate system, for tracking events related to mouse tracking and cursor updating. The specified rectangle should not exceed the view’s bounds rectangle. "
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One or more constants that specify the type of tracking area, the situations when the area is active, and special behaviors of the tracking area. See the description of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingareaoptions"
                },
                {
                  "type": "text",
                  "text": " and related constants for details. You must specify one or more options for the initialized object for the type of tracking area and for when the tracking area is active; zero is not a valid value."
                }
              ]
            }
          ]
        },
        {
          "name": "owner",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object to receive the requested mouse-tracking, mouse-moved, or cursor-update messages. It does not necessarily have to be the view associated with the created "
                },
                {
                  "type": "codeVoice",
                  "code": "NSTrackingArea"
                },
                {
                  "type": "text",
                  "text": " object, but should be an object capable of responding to the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSResponder"
                },
                {
                  "type": "text",
                  "text": " methods "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1529306-mouseentered"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1527561-mouseexited"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1525114-mousemoved"
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1525066-cursorupdate"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "userInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary containing arbitrary data for each mouse-entered, mouse-exited, and cursor-update event. When handling such an event you can obtain the dictionary by sending "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsevent/1526810-userdata"
                },
                {
                  "type": "text",
                  "text": " to the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSEvent"
                },
                {
                  "type": "text",
                  "text": " object. (The dictionary is not available for mouse-moved events.) This parameter may be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The newly-initialized tracking area object."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "After creating and initializing an "
            },
            {
              "type": "codeVoice",
              "code": "NSTrackingArea"
            },
            {
              "type": "text",
              "text": " object with this method, you must add it to a target view using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483489-addtrackingarea"
            },
            {
              "type": "text",
              "text": " method. When changes in the view require changes in the geometry of its tracking areas, the Application Kit invokes "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483719-updatetrackingareas"
            },
            {
              "type": "text",
              "text": ". The view should implement this method to replace the current "
            },
            {
              "type": "codeVoice",
              "code": "NSTrackingArea"
            },
            {
              "type": "text",
              "text": " object with one with a recomputed area."
            }
          ]
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Special Considerations",
          "anchor": "1674232"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Beginning with OS X v10.5, the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingarea/1524488-initwithrect"
            },
            {
              "type": "text",
              "text": ", along with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483489-addtrackingarea"
            },
            {
              "type": "text",
              "text": " method of "
            },
            {
              "type": "codeVoice",
              "code": "NSView"
            },
            {
              "type": "text",
              "text": ", replace the "
            },
            {
              "type": "codeVoice",
              "code": "NSView"
            },
            {
              "type": "text",
              "text": " method "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483668-addtrackingrect"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
