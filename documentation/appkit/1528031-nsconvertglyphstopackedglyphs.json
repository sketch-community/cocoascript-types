      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function takes a buffer of glyphs, specified in the "
            },
            {
              "type": "codeVoice",
              "code": "glBuf"
            },
            {
              "type": "text",
              "text": " parameter, and packs them into a condensed character array. The character array is returned in the "
            },
            {
              "type": "codeVoice",
              "code": "packedGlyphs"
            },
            {
              "type": "text",
              "text": " parameter, which should have enough space for at least ((4 * count) + 1) bytes to guarantee that the packed glyphs fit. "
            },
            {
              "type": "codeVoice",
              "code": "count"
            },
            {
              "type": "text",
              "text": " specifies the number of glyphs in "
            },
            {
              "type": "codeVoice",
              "code": "glBuf"
            },
            {
              "type": "text",
              "text": ". "
            },
            {
              "type": "codeVoice",
              "code": "packing"
            },
            {
              "type": "text",
              "text": " specifies how the glyphs are currently packed."
            }
          ]
        }
      ]
    }
