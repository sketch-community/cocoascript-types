          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The string to match. If "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nscomboboxcelldatasource/1410250-comboboxcell"
                },
                {
                  "type": "text",
                  "text": " is implemented, "
