    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfieldcell"
            },
            {
              "type": "text",
              "text": " class adds to the text display capabilities of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscell"
            },
            {
              "type": "text",
              "text": " class by allowing you to set the color of both the text and its background. You can also specify whether the cell draws its background at all."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "All of the methods declared by this class are also declared by the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfield"
            },
            {
              "type": "text",
              "text": " class, which uses "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfieldcell"
            },
            {
              "type": "text",
              "text": " objects to draw and edit text. The "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfield"
            },
            {
              "type": "text",
              "text": " cover methods call the corresponding "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfieldcell"
            },
            {
              "type": "text",
              "text": " methods."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Placeholder strings, set using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfieldcell/1447165-placeholderstring"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstextfieldcell/1447153-placeholderattributedstring"
            },
            {
              "type": "text",
              "text": " property, appear in the text field cell if the actual string is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " or an empty string. They’re drawn in gray on the cell and aren’t archived in the “pre-10.2” nib format."
            }
          ]
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Designated Initializers",
          "anchor": "1665110"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When subclassing "
            },
            {
              "type": "codeVoice",
              "code": "NSTextFieldCell"
            },
            {
              "type": "text",
              "text": " you must implement the designated initializers "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoding/1416145-initwithcoder"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscell/1530851-inittextcell"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
