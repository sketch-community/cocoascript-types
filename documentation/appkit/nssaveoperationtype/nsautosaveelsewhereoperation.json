        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For an "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument"
            },
            {
              "type": "text",
              "text": " subclass that overrides "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515106-autosavesinplace"
            },
            {
              "type": "text",
              "text": " to have the value "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ", this is used during autosaving of documents that have never been saved and therefore do not yet have a document file that can be overwritten during autosaving. "
            },
            {
              "type": "codeVoice",
              "code": "NSDocument"
            },
            {
              "type": "text",
              "text": " may also pass "
            },
            {
              "type": "codeVoice",
              "code": "NSAutosaveElsewhereOperation"
            },
            {
              "type": "text",
              "text": " when invoking "
            },
            {
              "type": "codeVoice",
              "code": "writeSafelyToURL:ofType:forSaveOperation:error:"
            },
            {
              "type": "text",
              "text": " while duplicating or reverting a document."
            }
          ]
        }
