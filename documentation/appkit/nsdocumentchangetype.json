      "abstract": [
        {
          "type": "text",
          "text": "The document’s contents have been autosaved. For example, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515187-savetourl"
        },
        {
          "type": "text",
          "text": " passes this value for a successful "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nssaveoperationtype/nsautosaveoperation"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
