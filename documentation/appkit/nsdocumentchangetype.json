    "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangedone": {
      "title": "NSChangeDone",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangedone",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsdocumentchangetype/nschangedone",
      "abstract": [
        {
          "type": "text",
          "text": "Increment change count. Pass this value to the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515223-updatechangecount"
        },
        {
          "type": "text",
          "text": " method to indicate that a single change has been done. For example, the built-in undo support in the "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument"
        },
        {
          "type": "text",
          "text": " class passes this value whenever a document receives an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsundomanagerdidcloseundogroupnotification"
        },
        {
          "type": "text",
          "text": " notification from its own undo manager."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangeundone": {
      "title": "NSChangeUndone",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangeundone",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsdocumentchangetype/nschangeundone",
      "abstract": [
        {
          "type": "text",
          "text": "Decrement change count. A single change has been undone. For example, the built-in undo support of "
        },
        {
          "type": "codeVoice",
          "code": "NSDocument"
        },
        {
          "type": "text",
          "text": " passes this value whenever a document receives an "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsundomanagerdidundochangenotification"
        },
        {
          "type": "text",
          "text": " from its own undo manager."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangecleared": {
      "title": "NSChangeCleared",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangecleared",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsdocumentchangetype/nschangecleared",
      "abstract": [
        {
          "type": "text",
          "text": "Set change count to 0. The document has been synchronized with its file or file package. For example, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515187-savetourl"
        },
        {
          "type": "text",
          "text": " passes this value for a successful "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nssaveoperationtype/nssaveoperation"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nssaveoperationtype/nssaveasoperation"
        },
        {
          "type": "text",
          "text": ". The "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515059-revertdocumenttosaved"
        },
        {
          "type": "text",
          "text": " method does too."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangereadothercontents": {
      "title": "NSChangeReadOtherContents",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangereadothercontents",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/appkit/nsdocumentchangetype/nschangereadothercontents",
      "abstract": [
        {
          "type": "text",
          "text": "The document has been initialized with the contents of a file or file package other than the one whose location is in the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515038-fileurl"
        },
        {
          "type": "text",
          "text": " property, and therefore can’t possibly be synchronized with its persistent representation. For example, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/appkit/nsdocument/1515041-initforurl"
        },
        {
          "type": "text",
          "text": " passes this value when the two passed-in URLs are not equal to indicate that an autosaved document is being reopened."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/appkit/nsdocumentchangetype/nschangeautosaved": {
