        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Sends "
            },
            {
              "type": "codeVoice",
              "code": "notification"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " to any assistive apps that register to receive the notification from the UI object "
            },
            {
              "type": "codeVoice",
              "code": "element"
            },
            {
              "type": "text",
              "text": " in your app. The system restricts the "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " dictionary values to the same values as it restricts the accessibility attributes. The "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " dictionary can also be "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " (most accessibility notifications don’t require it). "
            }
          ]
        }
