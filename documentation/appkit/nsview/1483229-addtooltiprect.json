              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The view maintains a weak reference to "
                    },
                    {
                      "type": "codeVoice",
                      "code": "anObject"
                    },
                    {
                      "type": "text",
                      "text": ". You are responsible for ensuring that "
                    },
                    {
                      "type": "codeVoice",
                      "code": "anObject"
                    },
                    {
                      "type": "text",
                      "text": " remains valid for as long as it may be needed."
