      "parameters": [
        {
          "name": "items",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The dragging items. The frame property of each "
                },
                {
                  "type": "codeVoice",
                  "code": "NSDraggingItem"
                },
                {
                  "type": "text",
                  "text": " must be in the view's coordinate system."
                }
              ]
            }
          ]
        },
        {
          "name": "event",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The mouse-down event object from which to initiate the drag operation. In particular, its mouse location is used for the offset of the icon being dragged."
                }
              ]
            }
          ]
        },
        {
          "name": "source",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An object that serves as the controller of the dragging operation. It must conform to the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource"
                },
                {
                  "type": "text",
                  "text": " protocol and is typically the view itself or its "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow"
                },
                {
                  "type": "text",
                  "text": " object."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The dragging session for the drag."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A basic drag starts by calling "
            },
            {
              "type": "codeVoice",
              "code": "beginDraggingSessionWithItems:event:source:"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The caller can take the returned "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsession"
            },
            {
              "type": "text",
              "text": " and continue to modify its properties. When the drag actually starts, the source is sent a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource/1415960-draggingsession"
            },
            {
              "type": "text",
              "text": " message followed by multiple "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource/1416079-draggingsession"
            },
            {
              "type": "text",
              "text": " messages as the user drags. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Once the drag is ended or cancelled, the source receives a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource/1416017-draggingsession"
            },
            {
              "type": "text",
              "text": " method and the drag is complete."
            }
          ]
        }
      ]
    }
