    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "anImage",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsimage"
                },
                {
                  "type": "text",
                  "text": " object to be dragged."
                }
              ]
            }
          ]
        },
        {
          "name": "imageLoc",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The location of the image’s lower-left corner, in the view’s coordinate system. It determines the placement of the dragged image under the cursor. When determining the image location you should use the mouse down coordinate, provided in "
                },
                {
                  "type": "codeVoice",
                  "code": "theEvent"
                },
                {
                  "type": "text",
                  "text": ", rather than the current mouse location."
                }
              ]
            }
          ]
        },
        {
          "name": "mouseOffset",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This parameter is ignored."
                }
              ]
            }
          ]
        },
        {
          "name": "theEvent",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The left mouse-down event that triggered the dragging operation (see discussion below)."
                }
              ]
            }
          ]
        },
        {
          "name": "pboard",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The pasteboard that holds the data to be transferred to the destination (see discussion below)."
                }
              ]
            }
          ]
        },
        {
          "name": "sourceObject",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An object that serves as the controller of the dragging operation. It must conform to the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource"
                },
                {
                  "type": "text",
                  "text": " protocol and is typically the view itself or its "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nswindow"
                },
                {
                  "type": "text",
                  "text": " object."
                }
              ]
            }
          ]
        },
        {
          "name": "slideBack",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean that determines whether the drag image should slide back if it’s rejected. The image slides back to "
                },
                {
                  "type": "codeVoice",
                  "code": "imageLoc"
                },
                {
                  "type": "text",
                  "text": " if "
                },
                {
                  "type": "codeVoice",
                  "code": "slideBack"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " and the image isn’t accepted by the dragging destination. If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": " the image doesn't slide back."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method must be invoked only within an implementation of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1524634-mousedown"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsresponder/1527420-mousedragged"
            },
            {
              "type": "text",
              "text": " methods."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Before invoking this method, you must place the data to be transferred on "
            },
            {
              "type": "codeVoice",
              "code": "pboard"
            },
            {
              "type": "text",
              "text": ". To do this, get the drag pasteboard object ("
            },
            {
              "type": "codeVoice",
              "code": "NSDragPboard"
            },
            {
              "type": "text",
              "text": "), declare the types of the data, and then put the data on the pasteboard. This code fragment initiates a dragging operation on an image itself (that is, the image is the data to be transferred):"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)mouseDown:(NSEvent *)theEvent",
            "{",
            "    NSSize dragOffset = NSMakeSize(0.0, 0.0);",
            "    NSPasteboard *pboard;",
            " ",
            "    pboard = [NSPasteboard pasteboardWithName:NSDragPboard];",
            "    [pboard declareTypes:[NSArray arrayWithObject:NSTIFFPboardType]  owner:self];",
            "    [pboard setData:[[self image] TIFFRepresentation] forType:NSTIFFPboardType];",
            " ",
            "    [self dragImage:[self image] at:[self imageLocation] offset:dragOffset",
            "        event:theEvent pasteboard:pboard source:self slideBack:YES];",
            " ",
            "    return;",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555900",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "See the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingsource"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdragginginfo"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdraggingdestination"
            },
            {
              "type": "text",
              "text": " protocol specifications for more information on dragging operations."
            }
          ]
        }
      ]
    }
