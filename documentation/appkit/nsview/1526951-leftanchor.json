              "code": "leftAnchor"
            },
            {
              "type": "text",
              "text": " with another "
            },
            {
              "type": "codeVoice",
              "code": "leftAnchor"
            },
            {
              "type": "text",
              "text": ", a "
            },
            {
              "type": "codeVoice",
              "code": "rightAnchor"
            },
            {
              "type": "text",
              "text": ", or a "
            },
