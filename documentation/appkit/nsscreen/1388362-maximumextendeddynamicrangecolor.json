      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If no content on screen provides extended dynamic range (EDR) values, the value of this property is "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": ". If any content onscreen has requested EDR, the value may be greater than "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": ", depending on the capabilities of the display hardware and other conditions. Only rendering contexts that support EDR can use values greater than "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the value changes, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsapplicationdidchangescreenparametersnotification"
            },
            {
              "type": "text",
              "text": " is posted."
            }
          ]
        }
      ]
