          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The created "
            },
            {
              "type": "codeVoice",
              "code": "NSEvent"
            },
            {
              "type": "text",
              "text": " instance or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the instance could not be created."
            }
          ]
        }
      ]
