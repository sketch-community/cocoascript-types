    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "name": "type",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One of the modifier key masks described in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsevent/eventtype"
                },
                {
                  "type": "text",
                  "text": ", or an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSInternalInconsistencyException"
                },
                {
                  "type": "text",
                  "text": " is raised."
                }
              ]
            }
          ]
        },
        {
          "name": "location",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The cursor location in the base coordinate system of the window specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "windowNum"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer bit field containing any of the modifier key masks described in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsevent#1652237"
                },
                {
                  "type": "text",
                  "text": ", combined using the C bitwise OR operator."
                }
              ]
            }
          ]
        },
        {
          "name": "time",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time the event occurred in seconds since system startup."
                }
              ]
            }
          ]
        },
        {
          "name": "windowNum",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer that identifies the window device associated with the event, which is associated with the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSWindow"
                },
                {
                  "type": "text",
                  "text": " that will receive the event."
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The display graphics context of the event."
                }
              ]
            }
          ]
        },
        {
          "name": "eventNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An identifier for the new event. It’s normally taken from a counter for mouse events, which continually increases as the application runs."
                }
              ]
            }
          ]
        },
        {
          "name": "clickNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of mouse clicks associated with the mouse event."
                }
              ]
            }
          ]
        },
        {
          "name": "pressure",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A value from "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "codeVoice",
                  "code": "1.0"
                },
                {
                  "type": "text",
                  "text": " indicating the pressure applied to the input device on a mouse event, used for an appropriate device such as a graphics tablet. For devices that aren’t pressure-sensitive, the value should be either "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "1.0"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
