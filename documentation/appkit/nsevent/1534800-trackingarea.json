      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If accessed on an event that is not a mouse-tracking event (that is, an event of type "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsmouseentered"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsmouseexited"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscursorupdate"
            },
            {
              "type": "text",
              "text": "), this property raises an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsinternalinconsistencyexception"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If no "
            },
            {
              "type": "codeVoice",
              "code": "NSTrackingArea"
            },
            {
              "type": "text",
              "text": " object is associated with the event because the event corresponds to a tracking rectangle installed with the "
            },
            {
              "type": "codeVoice",
              "code": "NSView"
            },
            {
              "type": "text",
              "text": " method "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsview/1483668-addtrackingrect"
            },
            {
              "type": "text",
              "text": ", this method returns "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ". Note that the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsevent/1533974-trackingnumber"
            },
            {
              "type": "text",
              "text": " property returns either an "
            },
            {
              "type": "codeVoice",
              "code": "NSTrackingArea"
            },
            {
              "type": "text",
              "text": " object or the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nstrackingrecttag"
            },
            {
              "type": "text",
              "text": " constant depending on how the event was generated."
            }
          ]
        }
      ]
    }
