      "parameters": [
        {
          "name": "type",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One of the following event-type constants: "
                },
                {
                  "type": "codeVoice",
                  "code": "NSKeyDown"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "NSKeyUp"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "NSFlagsChanged"
                },
                {
                  "type": "text",
                  "text": ". If anything else is specified, an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSInternalInconsistencyException"
                },
                {
                  "type": "text",
                  "text": " is raised."
                }
              ]
            }
          ]
        },
        {
          "name": "location",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The cursor location in the base coordinate system of the window specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "windowNum"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer bit field containing any of the modifier key masks described in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsevent#1652237"
                },
                {
                  "type": "text",
                  "text": ", combined using the C bitwise OR operator."
                }
              ]
            }
          ]
        },
        {
          "name": "time",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time the event occurred in seconds since system startup."
                }
              ]
            }
          ]
        },
        {
          "name": "windowNum",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer that identifies the window device associated with the event, which is associated with the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSWindow"
                },
                {
                  "type": "text",
                  "text": " that will receive the event."
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The display graphics context of the event."
                }
              ]
            }
          ]
        },
        {
          "name": "characters",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string of characters associated with the key event. Though most key events contain only one character, it is possible for a single keypress to generate a series of characters."
                }
              ]
            }
          ]
        },
        {
          "name": "unmodCharacters",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The string of characters generated by the key event as if no modifier key had been pressed (except for Shift). This argument is useful for getting the “basic” key value in a hardware-independent manner."
                }
              ]
            }
          ]
        },
        {
          "name": "repeatKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/swift/true"
                },
                {
                  "type": "text",
                  "text": " if the key event is a repeat caused by the user holding the key down, "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/swift/false"
                },
                {
                  "type": "text",
                  "text": " if the key event is new."
                }
              ]
            }
          ]
        },
        {
          "name": "code",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A number that identifies the keyboard key associated with the key event. Its value is hardware-independent."
                }
              ]
            }
          ]
        }
      ]
    },
