    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "type",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One of the following event-type constants:"
                }
              ]
            },
            {
              "type": "unorderedList",
              "items": [
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "NSAppKitDefined"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "NSSystemDefined"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "NSApplicationDefined"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "NSPeriodic"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "codeVoice",
                  "code": "type"
                },
                {
                  "type": "text",
                  "text": " is anything else, an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSInternalInconsistencyException"
                },
                {
                  "type": "text",
                  "text": " is raised. Your code should only create events of type "
                },
                {
                  "type": "codeVoice",
                  "code": "NSApplicationDefined"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "location",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The cursor location in the base coordinate system of the window specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "windowNum"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer bit field containing any of the modifier key masks described in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsevent#1652237"
                },
                {
                  "type": "text",
                  "text": ", combined using the C bitwise OR operator."
                }
              ]
            }
          ]
        },
        {
          "name": "time",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time the event occurred in seconds since system startup."
                }
              ]
            }
          ]
        },
        {
          "name": "windowNum",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer that identifies the window device associated with the event, which is associated with the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSWindow"
                },
                {
                  "type": "text",
                  "text": " that will receive the event."
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The display graphics context of the event."
                }
              ]
            }
          ]
        },
        {
          "name": "subtype",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A numeric identifier that further differentiates custom events of types "
