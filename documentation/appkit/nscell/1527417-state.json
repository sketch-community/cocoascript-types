      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsoffstate"
            },
            {
              "type": "text",
              "text": " state indicates the normal or unpressed state. The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsonstate"
            },
            {
              "type": "text",
              "text": " state indicates the alternate or pressed state. The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsmixedstate"
            },
            {
              "type": "text",
              "text": " state indicates that the feature represented by the control is in effect somewhere. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Although using the enumerated constants is preferred, you can also assign an integer value to this property. If the cell has two states, "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " is treated as "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsoffstate"
            },
            {
              "type": "text",
              "text": ", and a nonzero value is treated as "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsonstate"
            },
            {
              "type": "text",
              "text": ". If the cell has three states, "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " is treated as "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsoffstate"
            },
            {
              "type": "text",
              "text": ", a negative value is treated as "
            },
            {
              "type": "codeVoice",
              "code": "NSMixedState"
            },
            {
              "type": "text",
              "text": ", and a positive value is treated as "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsonstate"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
