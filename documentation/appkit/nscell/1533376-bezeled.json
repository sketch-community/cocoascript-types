        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of this property is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " if the cell has a bezeled border or "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " if it does not. This property is mutually exclusive with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nscell/1525990-bordered"
            },
            {
              "type": "text",
              "text": " property—that is, a cell’s border can be plain or bezeled but not both. Changing the value of this property automatically removes any border that has been set, regardless of the value you assign to the property. "
            }
          ]
        }
