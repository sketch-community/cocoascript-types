          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": ", this method was invoked because the user released the mouse button; otherwise, if "
                },
