      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Together, "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasImageOnLeftOrBottom"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasImageHorizontal"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasOverlappingImage"
            },
            {
              "type": "text",
              "text": " control the position of the cell’s image and text. To place the image above, set none of them. To place the image below, set "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasImageOnLeftOrBottom"
            },
            {
              "type": "text",
              "text": ". To place the image to the right, set "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasImageHorizontal"
            },
            {
              "type": "text",
              "text": ". To place the image to the left, set "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasImageHorizontal"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasImageOnLeftOrBottom"
            },
            {
              "type": "text",
              "text": ". To place the image directly over, set "
            },
            {
              "type": "codeVoice",
              "code": "NSCellHasOverlappingImage"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
