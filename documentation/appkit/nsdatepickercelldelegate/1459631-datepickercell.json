      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When returning a new "
            },
            {
              "type": "codeVoice",
              "code": "proposedDateValue"
            },
            {
              "type": "text",
              "text": ", the "
            },
            {
              "type": "codeVoice",
              "code": "NSDate"
            },
            {
              "type": "text",
              "text": " instance should be autoreleased, and the "
            },
            {
              "type": "codeVoice",
              "code": "proposedDateValue"
            },
            {
              "type": "text",
              "text": " should not be released by the delegate."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "proposedDateValue"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "proposedTimeInterval"
            },
            {
              "type": "text",
              "text": " are guaranteed to lie between the dates returned by "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdatepickercell/1459619-mindate"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsdatepickercell/1459640-maxdate"
            },
            {
              "type": "text",
              "text": ". If you modify these values, you should ensure that the new values are within the appropriate range."
            }
          ]
        }
      ]
    }
