      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Uses the value of "
            },
            {
              "type": "codeVoice",
              "code": "flag"
            },
            {
              "type": "text",
              "text": " to specify whether the app may have protected content. Protected content is identified by a value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " for "
            },
            {
              "type": "codeVoice",
              "code": "NSAccessibilityContainsProtectedContentAttribute"
            },
            {
              "type": "text",
              "text": ", but if "
            },
            {
              "type": "codeVoice",
              "code": "NSAccessibilitySetMayContainProtectedContent"
            },
            {
              "type": "text",
              "text": " returns "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": ", the value of "
            },
            {
              "type": "codeVoice",
              "code": "NSAccessibilityContainsProtectedContentAttribute"
            },
            {
              "type": "text",
              "text": " is ignored. This function returns "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " on success."
            }
          ]
        }
      ]
    }
