        {
          "name": "aURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL pointing to the image data."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An array of image representation objects. The array contains one object for each image in the data at the specified URL."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If sent to the "
            },
            {
              "type": "codeVoice",
              "code": "NSImageRep"
            },
            {
              "type": "text",
              "text": " class object, this method returns an array of objects (all newly allocated instances of a subclass of "
            },
            {
              "type": "codeVoice",
              "code": "NSImageRep"
            },
            {
              "type": "text",
              "text": ") that have been initialized with the contents of the specified URL. If sent to a subclass of "
            },
            {
              "type": "codeVoice",
              "code": "NSImageRep"
            },
            {
              "type": "text",
              "text": " that recognizes the data at the specified URL, it returns an array of objects (all instances of that subclass) that have been initialized with the contents of that URL."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method returns "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " in any of the following cases:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The message is sent to the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSImageRep"
                    },
                    {
                      "type": "text",
                      "text": " class object and there are no subclasses in the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSImageRep"
                    },
                    {
                      "type": "text",
                      "text": " class registry that handle data in the specified URL."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The message is sent to a subclass of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSImageRep"
                    },
                    {
                      "type": "text",
                      "text": " and that subclass cannot handle data in the specified URL."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSImageRep"
                    },
                    {
                      "type": "text",
                      "text": " subclass is unable to initialize itself with the contents of the specified URL."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "NSImageRep"
            },
            {
              "type": "text",
              "text": " subclass is initialized by creating an "
            },
            {
              "type": "codeVoice",
              "code": "NSData"
            },
            {
              "type": "text",
              "text": " object based on the contents of the specified URL and passing it to the "
            },
            {
              "type": "codeVoice",
              "code": "imageRepsWithData:"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        }
      ]
    }
