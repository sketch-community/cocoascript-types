      "parameters": [
        {
          "name": "dstSpacePortionRect",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The rectangle in which to draw the image, specified in the current coordinate system. "
                }
              ]
            }
          ]
        },
        {
          "name": "srcSpacePortionRect",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The source rectangle specifying the portion of the image you want to draw. The coordinates of this rectangle must be specified using the image's own coordinate system. If you pass in "
                },
                {
                  "type": "codeVoice",
                  "code": "NSZeroRect"
                },
                {
                  "type": "text",
                  "text": ", the entire image is drawn. "
                }
              ]
            }
          ]
        },
        {
          "name": "op",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The compositing operation to use when drawing the image. See the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nscompositingoperation"
                },
                {
                  "type": "text",
                  "text": " constants."
                }
              ]
            }
          ]
        },
        {
          "name": "requestedAlpha",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The opacity of the image, specified as a value from 0.0 to 1.0. Specifying a value of 0.0 draws the image as fully transparent while a value of 1.0 draws the image as fully opaque. Values greater than 1.0 are interpreted as 1.0."
                }
              ]
            }
          ]
        },
        {
          "name": "respectContextIsFlipped",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " if the flipped context of the receiver should be respected, otherwise "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "hints",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An optional dictionary of hints that provide more context for selecting or generating the image. See "
                },
                {
                  "type": "codeVoice",
                  "code": "Image Hint Dictionary Keys"
                },
                {
                  "type": "text",
                  "text": " for possible values."
                }
              ]
            }
          ]
        }
      ]
    },
