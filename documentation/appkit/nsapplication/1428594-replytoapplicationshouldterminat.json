          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If your app delegate returns "
            },
            {
              "type": "codeVoice",
              "code": "NSTerminateLater"
            },
            {
              "type": "text",
              "text": " from its "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nsapplicationdelegate/1428642-applicationshouldterminate"
            },
            {
              "type": "text",
              "text": " method, your code must subsequently call this method to let the "
            },
            {
              "type": "codeVoice",
              "code": "NSApplication"
            },
            {
              "type": "text",
              "text": " object know whether it can actually terminate itself."
            }
          ]
        }
