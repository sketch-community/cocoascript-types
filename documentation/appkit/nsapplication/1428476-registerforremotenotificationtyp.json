              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bit mask specifying the types of notifications the app accepts. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/appkit/nsremotenotificationtype"
                },
                {
                  "type": "text",
                  "text": " for valid bit-mask values."
                }
              ]
            }
          ]
