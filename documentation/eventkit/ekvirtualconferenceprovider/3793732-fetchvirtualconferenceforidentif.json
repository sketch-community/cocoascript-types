              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A completion handler that you call after you determine the available rooms. If you provide a virtual conference descriptor, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " for the error parameter. Conversely, if you provide an error object, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " for the virtual conference descriptor."
                }
              ]
            }
