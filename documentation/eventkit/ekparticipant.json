      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Do not create "
            },
            {
              "type": "codeVoice",
              "code": "EKParticipant"
            },
            {
              "type": "text",
              "text": " objects directly. Instead, use the property attendees on "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/eventkit/ekcalendaritem"
            },
            {
              "type": "text",
              "text": " to return an array of "
            },
            {
              "type": "codeVoice",
              "code": "EKParticipant"
            },
            {
              "type": "text",
              "text": " objects."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "EventKit cannot add participants to an event nor change participant information. Use the properties in this class to get information about a participant."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A participant can be a person, group, room, or other resource."
            }
          ]
        }
      ]
    }
