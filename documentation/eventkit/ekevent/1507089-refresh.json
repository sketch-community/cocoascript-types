            {
              "type": "text",
              "text": "If the event was successfully refreshed, "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": "; otherwise, "
            },
