      "parameters": [
        {
          "name": "Mb",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of rows in terms of blocks of the matrix.  Must be greater than 0."
                }
              ]
            }
          ]
        },
        {
          "name": "Nb",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of columns in terms of blocks of the matrix.  Must be greater than 0."
                }
              ]
            }
          ]
        },
        {
          "name": "K",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Array containing row dimensions of the blocks.  The "
                },
                {
                  "type": "codeVoice",
                  "code": "i"
                },
                {
                  "type": "text",
                  "text": "'th row in terms of blocks will have a dimension "
                },
                {
                  "type": "codeVoice",
                  "code": "K[i]"
                },
                {
                  "type": "text",
                  "text": ". "
                },
                {
                  "type": "codeVoice",
                  "code": "K"
                },
                {
                  "type": "text",
                  "text": " is expected to hold "
                },
                {
                  "type": "codeVoice",
                  "code": "Mb"
                },
                {
                  "type": "text",
                  "text": " elements. All values of "
                },
                {
                  "type": "codeVoice",
                  "code": "K"
                },
                {
                  "type": "text",
                  "text": " are expected to be greater than 0."
                }
              ]
            }
          ]
        },
        {
          "name": "L",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Array containing column dimensions of the blocks.  The "
                },
                {
                  "type": "codeVoice",
                  "code": "j"
                },
                {
                  "type": "text",
                  "text": "'th column in terms of blocks will have a dimension "
                },
                {
                  "type": "codeVoice",
                  "code": "L[j]"
                },
                {
                  "type": "text",
                  "text": ". "
                },
                {
                  "type": "codeVoice",
                  "code": "L"
                },
                {
                  "type": "text",
                  "text": " is expected to hold Nb elements. All values of "
                },
                {
                  "type": "codeVoice",
                  "code": "L"
                },
                {
                  "type": "text",
                  "text": " are expected to be greater than 0."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A matrix object that is ready for receiving entries.  If an error occurs, "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " is returned."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Create a sparse matrix object that is stored in block-entry format and is ready to receive values from the various block insert routines. Blocks are are of variable dimension where the "
            },
            {
              "type": "codeVoice",
              "code": "i,j"
            },
            {
              "type": "text",
              "text": "'th block index has a dimension "
            },
            {
              "type": "codeVoice",
              "code": "K[i] * L[j]"
            },
            {
              "type": "text",
              "text": ". Block-entry format means blocks of dense regions will be stored at block indices "
            },
            {
              "type": "codeVoice",
              "code": "i,j"
            },
            {
              "type": "text",
              "text": ". For point wise format use non block version of create. See the various insert routines for details on inserting values into this matrix object."
            }
          ]
        }
      ]
    }
