    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the products of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " complex (optionally conjugates) of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " by the corresponding complex elements of "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", writing the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "With "
            },
            {
              "type": "codeVoice",
              "code": "Conjugate"
            },
            {
              "type": "text",
              "text": " set to "
            },
            {
              "type": "codeVoice",
              "code": "+1"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3213997",
              "metadata": {
                "anchor": "3213997",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "With "
            },
            {
              "type": "codeVoice",
              "code": "Conjugate"
            },
            {
              "type": "text",
              "text": " set to -"
            },
            {
              "type": "codeVoice",
              "code": "1"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3214013",
              "metadata": {
                "anchor": "3214013",
                "title": "Figure 2"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "If Conjugate is +1:",
            "",
            "    for (n = 0; n < N; ++n)",
            "        C[n] = A[n] * B[n];",
            "",
            "If Conjugate is -1:",
            "",
            "    for (n = 0; n < N; ++n)",
            "        C[n] = conj(A[n]) * B[n];"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3214002",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
