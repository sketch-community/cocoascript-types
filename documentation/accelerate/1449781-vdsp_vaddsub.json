    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the sums and differences of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", writing the results to "
            },
            {
              "type": "codeVoice",
              "code": "O0"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "O1"
            },
            {
              "type": "text",
              "text": " respectively:"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110539",
              "metadata": {
                "anchor": "3110539",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            " for (i = 0; i < N; ++i)",
            " {",
            "     i1 = I1[i*I1S];",
            "     i0 = I0[i*I0S];",
            "     O0[i*O0S] = i1 + i0;",
            "     O1[i*O1S] = i1 - i0;",
            " }"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108780",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1449781-vdsp_vaddsub"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let i0: [Float] = [10, 20, 30, 40, 50]",
            "let i1: [Float] = [1, 2, 3, 4, 5]",
            "",
            "let n = vDSP_Length(i0.count)",
            "",
            "var o0 = [Float](repeating: 0,",
            "                count: i0.count)",
            "",
            "var o1 = [Float](repeating: 0,",
            "                 count: i0.count)",
            "",
            "vDSP_vaddsub(i0, stride,",
            "             i1, stride,",
            "             &o0, stride,",
            "             &o1, stride,",
            "             n)",
            "",
            "// Prints \"additions [11.0, 22.0, 33.0, 44.0, 55.0]\"",
            "print(\"additions\", o0)",
            "",
            "// Prints \"subtractions [-9.0, -18.0, -27.0, -36.0, -45.0]\"",
            "print(\"subtractions\", o1)"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108781",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "strong",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "In-place operation:"
                }
              ]
            },
            {
              "type": "text",
              "text": " Output vectors may occupy the same space as input vectors. O0 and O1 may not occupy the same space"
            }
          ]
        }
      ]
    }
