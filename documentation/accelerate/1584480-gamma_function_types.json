    "doc://com.apple.documentation/documentation/accelerate/1584480-gamma_function_types/kvimagegamma_bt709_forward_half_precision": {
      "title": "kvImageGamma_BT709_forward_half_precision",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/accelerate/1584480-gamma_function_types/kvimagegamma_bt709_forward_half_precision",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/accelerate/1584480-gamma_function_types/kvimagegamma_bt709_forward_half_precision",
      "abstract": [
        {
          "type": "text",
          "text": "ITU-R BT.709 standard. This is like kvImageGamma_sRGB_forward_half_precision above but without the 1.125 viewing gamma for computer graphics: "
        },
        {
          "type": "codeVoice",
          "code": "x<0.081? x/4.5: pow((x+0.099)/1.099, 1/0.45)"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/accelerate/1584480-gamma_function_types/kvimagegamma_bt709_reverse_half_precision": {
