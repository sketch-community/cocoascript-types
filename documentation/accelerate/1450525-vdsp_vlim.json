    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Compares values from vector "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " to limit scalar "
            },
            {
              "type": "codeVoice",
              "code": "*B"
            },
            {
              "type": "text",
              "text": ". For inputs greater than or equal to "
            },
            {
              "type": "codeVoice",
              "code": "*B"
            },
            {
              "type": "text",
              "text": ", scalar "
            },
            {
              "type": "codeVoice",
              "code": "*C"
            },
            {
              "type": "text",
              "text": " is written to "
            },
            {
              "type": "codeVoice",
              "code": "D"
            },
            {
              "type": "text",
              "text": " . For inputs  less than "
            },
            {
              "type": "codeVoice",
              "code": "*B"
            },
            {
              "type": "text",
              "text": ", the negated value of scalar "
            },
            {
              "type": "codeVoice",
              "code": "*C"
            },
            {
              "type": "text",
              "text": " is written to vector  "
            },
            {
              "type": "codeVoice",
              "code": "D"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This calculates the following:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "    for (n = 0; n < N; ++n)",
            "        if (*B <= A[n*IA])",
            "            D[n*ID] = *C;",
            "        else",
            "            D[n*ID] = -(*C);"
          ],
          "syntax": "occ",
          "metadata": {
