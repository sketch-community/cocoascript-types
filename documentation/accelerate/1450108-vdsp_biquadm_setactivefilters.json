      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For each filter configured in "
            },
            {
              "type": "codeVoice",
              "code": "setup"
            },
            {
              "type": "text",
              "text": ", the corresponding value in "
            },
            {
              "type": "codeVoice",
              "code": "filter_states"
            },
            {
              "type": "text",
              "text": " sets the filter to its active or inactive state. Any inactive filters are removed from the biquad processing chain.  Their state is retained for future use should they be activated later."
            }
          ]
        }
      ]
