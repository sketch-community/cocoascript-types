          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An input array of "
                },
                {
                  "type": "codeVoice",
                  "code": "bool"
                },
                {
                  "type": "text",
                  "text": " values. The length of this array should be the number of filters configured in "
                },
                {
