        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In the following example, the "
            },
            {
              "type": "codeVoice",
              "code": "employees"
            },
            {
              "type": "text",
              "text": " set is disjoint with the "
            },
            {
              "type": "codeVoice",
              "code": "visitors"
            },
            {
              "type": "text",
              "text": " set because no name appears in both sets."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let employees: Set = [\"Alicia\", \"Bethany\", \"Chris\", \"Diana\", \"Eric\"]",
            "let visitors: Set = [\"Marcia\", \"Nathaniel\", \"Olivia\"]",
            "print(employees.isDisjoint(with: visitors))",
            "// Prints \"true\""
          ],
