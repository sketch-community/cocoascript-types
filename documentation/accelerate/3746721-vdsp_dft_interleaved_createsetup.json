    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "Previous",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An existing "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp_dft_interleaved_setupd"
                },
                {
                  "type": "text",
                  "text": " structure that shares memory with the setup structure that this function returns. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to create an object with newly initialized and allocated memory."
                }
              ]
            }
          ]
        },
        {
          "name": "Length",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of complex elements. The supported values are "
                },
                {
                  "type": "codeVoice",
                  "code": "f * 2ⁿ"
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "codeVoice",
                  "code": "f"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "2"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "3"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "5"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "3 * 3"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "3 * 5"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "codeVoice",
                  "code": "5 * 5"
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "codeVoice",
                  "code": "n >= 2"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "Direction",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A flag that specifies the transform direction. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp_dft_direction/vdsp_dft_forward"
                },
                {
                  "type": "text",
                  "text": " to transform from the time domain to the frequency domain. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp_dft_direction/vdsp_dft_inverse"
                },
                {
                  "type": "text",
                  "text": " to transform from the frequency domain to the time domain."
                }
              ]
            }
          ]
        },
        {
          "name": "RealtoComplex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A flag that specifies the transform type. To transform from complex to complex, pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp_dft_realtocomplex/vdsp_dft_interleaved_complextocomplex"
                },
                {
                  "type": "text",
                  "text": ". To transform from real to complex, pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp_dft_realtocomplex/vdsp_dft_interleaved_realtocomplex"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Returns a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp_dft_interleaved_setupd"
            },
            {
              "type": "text",
              "text": " object, or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the function fails, either from insufficient memory or because "
            },
            {
              "type": "codeVoice",
              "code": "Length"
            },
            {
              "type": "text",
              "text": " doesn’t satisfy the above requirements."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "To prevent potential memory leaks, if the "
                },
                {
                  "type": "codeVoice",
                  "code": "Previous"
                },
                {
                  "type": "text",
                  "text": " parameter isn’t "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the "
                },
                {
                  "type": "codeVoice",
                  "code": "Previous"
                },
                {
                  "type": "text",
                  "text": " parameter must not point to the same address as the return value. "
                }
              ]
            }
          ],
          "style": "important"
        },
