    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function multiplies "
            },
            {
              "type": "codeVoice",
              "code": "A * B"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "B * A"
            },
            {
              "type": "text",
              "text": " (depending on the value of "
            },
            {
              "type": "codeVoice",
              "code": "Side"
            },
            {
              "type": "text",
              "text": ") and multiplies the resulting matrix by "
            },
            {
              "type": "codeVoice",
              "code": "alpha"
            },
            {
              "type": "text",
              "text": ". It then multiplies matrix "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " by "
            },
            {
              "type": "codeVoice",
              "code": "beta"
            },
            {
              "type": "text",
              "text": ". It stores the sum of these two products in matrix "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Thus, it calculates either"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "C←αAB + βC"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "or"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "C←αBA + βC"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "where"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "A = AT"
            }
          ]
        }
