            },
            {
              "type": "codeVoice",
              "code": "columnCount * blockSize"
            },
            {
              "type": "text",
              "text": " columns. For each "
            },
            {
              "type": "codeVoice",
              "code": "i in 0..<blockCount"
            },
            {
              "type": "text",
              "text": ", there’s a structurally nonzero block at block position "
            },
            {
              "type": "codeVoice",
              "code": "(row[i], column[i])"
            },
            {
              "type": "text",
              "text": " with numerical values "
            },
            {
              "type": "codeVoice",
              "code": "data[i * blockSize * blockSize:(i + 1) * blockSize * blockSize - 1]"
            },
            {
              "type": "text",
              "text": ". The system interprets the block’s values as the elements of a dense column-major matrix with "
            },
            {
              "type": "codeVoice",
              "code": "blockSize"
            },
            {
              "type": "text",
              "text": " rows and columns."
            }
          ]
        },
