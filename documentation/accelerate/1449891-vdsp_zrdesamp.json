    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Performs finite impulse response (FIR) filtering at selected positions of the input vector "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ", with the filter "
            },
            {
              "type": "codeVoice",
              "code": "F"
            },
            {
              "type": "text",
              "text": ", using the decimation factor "
            },
            {
              "type": "codeVoice",
              "code": "DF"
            },
            {
              "type": "text",
              "text": ". Results are left in the output vector "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function can run in place, but "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " cannot be in place with "
            },
            {
              "type": "codeVoice",
              "code": "F"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function’s operation is indicated by the following pseudocode:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for (n = 0; n < N; ++n)",
            "{",
            "    sum = 0;",
            "    for (p = 0; p < P; ++p)",
            "        sum += A[n*DF+p] * F[p];",
            "    C[n] = sum;",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557703",
            "title": "Listing 1"
          }
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This function may rearrange the order of operations and use various floating-point precisions, so actual results may not be calculated in exactly the same way as the pseudocode above."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        }
      ]
