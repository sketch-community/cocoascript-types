      ],
      "parameters": [
        {
          "name": "srcPlanarBuffers",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of vImage buffer structures, each of which contains image data in Planar8 format. Each structure must have the same width and height values, but may have different row byte values."
                }
              ]
            }
          ]
        },
        {
          "name": "destChannels",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of pointers to channels of the destination image. Each pointer points to the start of the data for one destination channel. The function fills the pixel values of each channel, using the corresponding source image for each channel."
                }
              ]
            }
          ]
        },
        {
          "name": "channelCount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of vImage buffer structures in the "
                },
                {
                  "type": "codeVoice",
                  "code": "srcPlanarBuffers"
                },
                {
                  "type": "text",
                  "text": " array and the number of channels in the destination image."
                }
              ]
            }
          ]
        },
        {
          "name": "destStrideBytes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of bytes from one pixel value in a given channel to the next pixel of that channel (within a row). This value is used for all channels."
                }
              ]
            }
          ]
        },
        {
          "name": "destWidth",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of pixels in a row. This value is used for all channels. It must be the same as the width of each of the planar source images."
                }
              ]
            }
          ]
        },
        {
          "name": "destHeight",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of rows. This value will be used for all channels. It must be the same as the height of each of the planar source images."
                }
              ]
            }
          ]
        },
        {
          "name": "destRowBytes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of bytes from the beginning of a channel row to the beginning of the next row in that channel. This value is used for all channels. (It does not have to be the same as the "
                },
                {
                  "type": "codeVoice",
                  "code": "rowBytes"
                },
                {
                  "type": "text",
                  "text": " values of the source buffers. Each source buffer can have its own "
                },
                {
                  "type": "codeVoice",
                  "code": "rowBytes"
                },
                {
                  "type": "text",
                  "text": " value.)"
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The options to use when performing the operation. If your code implements its own tiling or its own multithreading, pass "
