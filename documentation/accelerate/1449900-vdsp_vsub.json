    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the differences of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of input vectors "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", writing the result to output vector "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110544",
              "metadata": {
                "anchor": "3110544",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = A[n] - B[n];"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108767",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1449900-vdsp_vsub"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [1, 2, 3, 4, 5]",
            "let b: [Float] = [10, 20, 30, 40, 50]",
            "",
            "let n = vDSP_Length(a.count)",
            "",
            "var c = [Float](repeating: 0,",
            "                count: a.count)",
            "",
            "vDSP_vsub(b, stride,",
            "          a, stride,",
            "          &c, stride,",
            "          n)",
            "",
            "// Prints \"[-9.0, -18.0, -27.0, -36.0, -45.0]\"",
            "print(c)"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108766",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
