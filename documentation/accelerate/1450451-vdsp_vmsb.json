    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the products of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", subtracts the corresponding value in "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " from each product, and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "D"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110551",
              "metadata": {
                "anchor": "3110551",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            " for (n = 0; n < N; ++n)",
            "    D[n] = A[n] * B[n] - C[n];"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108797",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450451-vdsp_vmsb"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [10, 20, 30, 40, 50]",
            "let b: [Float] = [ 1,  2,  3,  4,  5]",
            "",
            "let c: [Float] = [1, 2, 3, 4, 5]",
            "",
            "let n = vDSP_Length(a.count)",
            "",
            "var d = [Float](repeating: 0,",
            "                count: a.count)",
            "",
            "vDSP_vmsb(a, stride,",
            "         b, stride,",
            "         c, stride,",
            "         &d, stride,",
            "         n)",
            "",
            "// Prints \"[9.0, 38.0, 87.0, 156.0, 245.0]\"",
            "print(d)"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108795",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
