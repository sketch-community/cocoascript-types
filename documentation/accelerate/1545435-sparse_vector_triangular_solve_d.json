      "parameters": [
        {
          "name": "transt",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether to perform the operation with T or the transpose of T. Must be one of "
                },
                {
                  "type": "codeVoice",
                  "code": "CblasNoTrans"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "CblasTrans"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "alpha",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": " Scalar multiplier of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "T"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "T",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The sparse triangular matrix.  Must be upper or lower triangular matrix. Will return "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
                },
                {
                  "type": "text",
                  "text": " if not a triangular matrix."
                }
              ]
            }
          ]
        },
        {
          "name": "x",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pointer to the dense vector "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "x"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". The dimension must match the dimension of the triangular matrix "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "T"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Behavior undefined if this is not met.  Negative strides are supported.  Note, unlike dense BLAS routines, the pointer points to the last element when stride is negative.  On exit holds the solution to the system of equations."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": " "
                }
              ]
            }
          ]
        },
        {
          "name": "incx",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Increment between valid values in the dense vector "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "x"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Negative strides are supported."
                }
              ]
            }
          ]
        }
      ]
    },
