      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The sparsity structure is in a "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "block compressed sparse column"
                }
              ]
            },
            {
              "type": "text",
              "text": " format. The matrix consists of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsematrixstructure/2868121-rowcount"
            },
            {
              "type": "text",
              "text": " x "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsematrixstructure/2868155-columncount"
            },
            {
              "type": "text",
              "text": " blocks, each of which is a square matrix of some fixed size (the system stores this block size separately from the sparsity because the graph algorithms that operate on the structure don’t need it)."
            }
          ]
        }
      ]
    }
