    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the products of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " complex elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", the products of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " compex elements of "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "D"
            },
            {
              "type": "text",
              "text": ", adds the corresponding products, adds the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " complex elements of "
            },
            {
              "type": "codeVoice",
              "code": "E"
            },
            {
              "type": "text",
              "text": ", and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "F"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110564",
              "metadata": {
                "anchor": "3110564",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for (n = 0; n < N; ++n)",
            "    F[n] = A[n] * B[n] + C[n] * D[n] + E[n];"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108867",
            "title": "Listing 1"
          }
        },
