        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Performs the following operation:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "vDSP_Length p = 0;",
            "for (n = 0; n < N; ++n){",
            "    if (B[n*IB] != 0)",
            "        C[p++ * IC] = A[n*IA];",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557673",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Compresses vector "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " based on the nonzero values of gating vector "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ". For nonzero elements of "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", corresponding elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " are sequentially copied to output vector "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        }
      ]
    }
