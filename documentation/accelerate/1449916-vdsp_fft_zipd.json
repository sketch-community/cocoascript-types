      "parameters": [
        {
          "name": "__Setup",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The FFT setup structure for this transform. The setup’s structure "
                },
                {
                  "type": "codeVoice",
                  "code": "Log2N"
                },
                {
                  "type": "text",
                  "text": " must be greater than or equal to this function’s "
                },
                {
                  "type": "codeVoice",
                  "code": "Log2N"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "__C",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the input-output data."
                }
              ]
            }
          ]
        },
        {
          "name": "__IC",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The stride between the elements in "
                },
                {
                  "type": "codeVoice",
                  "code": "C"
                },
                {
                  "type": "text",
                  "text": ", set to 1 for best performance. "
                }
              ]
            }
          ]
        },
        {
          "name": "__Log2N",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The base 2 exponent of the number of elements to process. For example, to process 1024 elements, specify "
                },
                {
                  "type": "codeVoice",
                  "code": "10"
                },
                {
                  "type": "text",
                  "text": " for parameter "
                },
                {
                  "type": "codeVoice",
                  "code": "Log2N"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "__Direction",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A flag that specifies the transform direction. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/kernel/1645049-anonymous/kfftdirection_forward"
                },
                {
                  "type": "text",
                  "text": " to transform from the time domain to the frequency domain. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/kernel/1645049-anonymous/kfftdirection_inverse"
                },
                {
                  "type": "text",
                  "text": " to transform from the frequency domain to the time domain."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": []
            }
          ]
        }
      ]
