    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the bitwise logical equivalence of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", writing the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3111142",
              "metadata": {
                "anchor": "3111142",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for (n = 0; n < N; ++n)",
            "    C[n] = ~(A[n] ^ B[n]);"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3109206",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450585-vdsp_veqvi"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Int32] = [0, 0, 1, 1]",
            "let b: [Int32] = [0, 1, 0, 1]",
            "",
            "let n = vDSP_Length(a.count)",
            "",
            "var c = [Int32](repeating: 0,",
            "                count: a.count)",
            "",
            "vDSP_veqvi(b, stride,",
            "           a, stride,",
            "           &c, stride,",
            "           n)",
            "",
            "// Prints \"[-1, -2, -2, -1]\"",
            "print(c)"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3109208",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
