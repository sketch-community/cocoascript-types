    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the mean value of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110585",
              "metadata": {
                "anchor": "3110585",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is: "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "C[0] = sum(A[n], 0 <= n < N) / N;"
          ],
          "syntax": "other",
          "metadata": {
            "anchor": "3110120",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1449980-vdsp_meanv"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [-1.5, 2.25, 3.6,",
            "                  0.2, -0.1, -4.3]",
            "let n = vDSP_Length(a.count)",
            "",
            "var c: Float = .nan",
            "",
            "vDSP_meanv(a,",
            "           stride,",
            "           &c,",
            "           n)",
            "",
            "print(String(format: \"mean %.4f\", c)) // Prints \"mean 0.0250\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3110123",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
