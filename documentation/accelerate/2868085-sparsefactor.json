          ]
        },
        {
          "name": "factorStorage",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to space used to store the factorization of size at least "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/sparseopaquesymbolicfactorization/2879481-factorsize_float"
                },
                {
                  "type": "text",
                  "text": " bytes. This storage should not be altered by the user during the lifetime of the return value."
                }
              ]
            }
          ]
        },
        {
          "name": "workspace",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a workspace of size at least "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/sparseopaquesymbolicfactorization/2879480-workspacesize_float"
                },
                {
                  "type": "text",
                  "text": " bytes. This workspace may be reused or destroyed by the user as soon as the function returns."
                }
              ]
            }
          ]
        }
