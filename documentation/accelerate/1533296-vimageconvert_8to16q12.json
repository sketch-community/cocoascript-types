    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The conversion from 8-bit float to 16-bit format with 12 fractional bits is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "uint16_t dest = ((src << 12) + 127)/255;"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2935233",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Source pixel values of 0 are mapped to 0, and source pixel values of 255 are mapped to "
            },
            {
              "type": "codeVoice",
              "code": "0x1000"
            },
            {
              "type": "text",
              "text": " (4096).  No larger values are produced by this conversion, which provides some headroom to help subsequent operations avoid overflow or clipping."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This routine doesn't work in place."
            }
          ]
        }
      ]
    }
