      "parameters": [
        {
          "name": "buf",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The source "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vimage_buffer"
                },
                {
                  "type": "text",
                  "text": " structure."
                }
              ]
            }
          ]
        },
        {
          "name": "format",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The image format of "
                },
                {
                  "type": "codeVoice",
                  "code": "buf"
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "codeVoice",
                  "code": "format->colorspace"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", sRGB is used. The colorspace is retained as needed by the new Core Graphics image."
                }
              ]
            }
          ]
        },
        {
          "name": "callback",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A callback that destorys the "
                },
                {
                  "type": "codeVoice",
                  "code": "buf->data"
                },
                {
                  "type": "text",
                  "text": " when the returned image no longer needs it. This callback is used in no-copy mode; that is, when "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimagenoallocate"
                },
                {
                  "type": "text",
                  "text": " is passed to "
                },
                {
                  "type": "codeVoice",
                  "code": "flags"
                },
                {
                  "type": "text",
                  "text": ". It can be called at any time, from any thread, and can be called before "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1399036-vimagecreatecgimagefrombuffer"
                },
                {
                  "type": "text",
                  "text": " returns."
                }
              ]
            }
          ]
        },
        {
          "name": "userData",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value to pass to the "
                },
                {
                  "type": "codeVoice",
                  "code": "callback"
                },
                {
                  "type": "text",
                  "text": " function's "
                },
                {
                  "type": "codeVoice",
                  "code": "userData"
                },
                {
                  "type": "text",
                  "text": " parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The options to use when performing this operation. The following flags are supported:"
                }
              ]
            },
            {
              "type": "table",
              "header": "none",
              "rows": [
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimagenoallocate"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Instructs the function to run in no-copy mode. Ownership of the memory that "
                        },
                        {
                          "type": "codeVoice",
                          "code": "buf->data"
                        },
                        {
                          "type": "text",
                          "text": " points to is transferred to the returned "
                        },
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgimageref"
                        },
                        {
                          "type": "text",
                          "text": ". In this mode, you need to specify the "
                        },
                        {
                          "type": "codeVoice",
                          "code": "callback"
                        },
                        {
                          "type": "text",
                          "text": " and "
                        },
                        {
                          "type": "codeVoice",
                          "code": "userData"
                        },
                        {
                          "type": "text",
                          "text": " parameters."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimageprintdiagnosticstoconsole"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Prints a debug message if the operation fails."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimagehighqualityresampling"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Determines whether Lanczos3 or Lanczos5 resampling is used if the image is resampled during creation."
                        }
                      ]
                    }
                  ]
                ],
                [
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "reference",
                          "isActive": true,
                          "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimagedonottile"
                        }
                      ]
                    }
                  ],
                  [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Disables multithreading in any conversions needed."
                        }
                      ]
                    }
                  ]
                ]
              ],
