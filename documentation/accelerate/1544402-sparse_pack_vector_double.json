        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Pack the first "
            },
            {
              "type": "codeVoice",
              "code": "nz"
            },
            {
              "type": "text",
              "text": " nonzero values and indices from the dense vector "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "x"
                }
              ]
            },
            {
              "type": "text",
              "text": " and place them in "
            },
            {
              "type": "codeVoice",
              "code": "y"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "indy"
            },
            {
              "type": "text",
              "text": ". If less than "
            },
            {
              "type": "codeVoice",
              "code": "nz"
            },
            {
              "type": "text",
              "text": " nonzero elements are found in the "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "x"
            },
            {
              "type": "text",
              "text": ", then the last "
            },
            {
              "type": "codeVoice",
              "code": "nz - actual_nonzero_count"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "y"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "indy"
            },
