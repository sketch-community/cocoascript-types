    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "Order",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies row-major (C) or column-major (Fortran) data ordering."
                }
              ]
            }
          ]
        },
        {
          "name": "Uplo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether to use the upper or lower triangle from the matrix. Valid values are "
                },
                {
                  "type": "codeVoice",
                  "code": "'U'"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "'L'"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "TransA",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether to use matrix A ("
                },
                {
                  "type": "codeVoice",
                  "code": "'N'"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "'n'"
                },
                {
                  "type": "text",
                  "text": ") or the transpose of A ("
                },
                {
                  "type": "codeVoice",
                  "code": "'T'"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "'t'"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "'C'"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "codeVoice",
                  "code": "'c'"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            }
          ]
        },
        {
          "name": "Diag",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether the matrix is unit triangular. Possible values are "
                },
                {
                  "type": "codeVoice",
                  "code": "'U'"
                },
                {
                  "type": "text",
                  "text": " (unit triangular) or "
                },
                {
                  "type": "codeVoice",
                  "code": "'N'"
                },
                {
                  "type": "text",
                  "text": " (not unit triangular)."
                }
              ]
            }
          ]
        },
        {
          "name": "N",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Order of matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "K",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Number of superdiagonals or subdiagonals of matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": " (depending on the value of "
                },
                {
                  "type": "codeVoice",
                  "code": "Uplo"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            }
          ]
        },
        {
          "name": "A",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "lda",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The leading dimension of matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "X",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Contains vector "
                },
                {
                  "type": "codeVoice",
                  "code": "B"
                },
                {
                  "type": "text",
                  "text": " on entry. Overwritten with vector "
                },
                {
                  "type": "codeVoice",
                  "code": "X"
                },
                {
                  "type": "text",
                  "text": " on return."
                }
              ]
            }
          ]
        },
        {
          "name": "incX",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Stride within "
                },
                {
                  "type": "codeVoice",
                  "code": "X"
                },
                {
                  "type": "text",
                  "text": ". For example, if "
                },
                {
                  "type": "codeVoice",
                  "code": "incX"
                },
                {
                  "type": "text",
                  "text": " is 7, every 7th element is used."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Solves the system of equations "
            },
            {
              "type": "codeVoice",
              "code": "A*X=B"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "A'*X=B"
            },
            {
              "type": "text",
              "text": ", depending on the value of "
            },
            {
              "type": "codeVoice",
              "code": "TransA"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
