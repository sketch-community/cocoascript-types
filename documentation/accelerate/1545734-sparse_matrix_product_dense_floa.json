      "parameters": [
        {
          "name": "order",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The storage order for the dense matrices "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "B"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "C"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Must be one of "
                },
                {
                  "type": "codeVoice",
                  "code": "CblasRowMajor"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "CblasColMajor"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "transa",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether to perform the operation with "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " or the transpose of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Must be one of "
                },
                {
                  "type": "codeVoice",
                  "code": "CblasNoTrans"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "CblasTrans"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "n",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of columns of the matrices "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "B"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "C"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "alpha",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Scalar multiplier of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "A",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The sparse matrix, "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "B",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pointer to the dense matrix "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "B"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". The number of rows must be equal to the number of columns of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and the number of columns is "
                },
                {
                  "type": "codeVoice",
                  "code": "n"
                },
                {
                  "type": "text",
                  "text": ". Behavior undefined if this is not met. The parameter "
                },
                {
                  "type": "codeVoice",
                  "code": "ldb"
                },
                {
                  "type": "text",
                  "text": " describes how many elements to move between one row (row major) or column (column major)."
                }
              ]
            }
          ]
        },
        {
          "name": "ldb",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Increment in elements between rows (row major) or columns (column major) of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "B"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Must be greater than or equal to "
                },
                {
                  "type": "codeVoice",
                  "code": "n"
                },
                {
                  "type": "text",
                  "text": " when row major, or number of columns of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " when column major."
                }
              ]
            }
          ]
        },
        {
          "name": "C",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pointer to the dense matrix "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "C"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". The number of rows must be equal to the number of rows of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and the number of columns is "
                },
                {
                  "type": "codeVoice",
                  "code": "n"
                },
                {
                  "type": "text",
                  "text": ". Behavior undefined if this is not met. The argument "
                },
                {
                  "type": "codeVoice",
                  "code": "ldc"
                },
                {
                  "type": "text",
                  "text": " describes how many elements to move between one row (row major) or column (column major). "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "C"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is updated with the result of the operation."
                }
              ]
            }
          ]
        },
        {
          "name": "ldc",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Increment in elements between rows (row major) or columns (column major) of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "C"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Must be greater than or equal to "
                },
                {
                  "type": "codeVoice",
                  "code": "n"
                },
                {
                  "type": "text",
                  "text": " when row major, or number of rows of "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " when column major."
                }
              ]
            }
          ]
