      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsenumericfactoroptions"
            },
            {
              "type": "text",
              "text": " allows for the following types of scaling to be performed:"
            }
          ]
        },
        {
          "type": "table",
          "header": "none",
          "rows": [
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsescaling_t/sparsescalingdefault"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Default scaling—at present, "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsescaling_t/sparsescalingequilibriationinf"
                    },
                    {
                      "type": "text",
                      "text": " if "
                    },
                    {
                      "type": "emphasis",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "LDLᵀ"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", or no scaling if Cholesky."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsescaling_t/sparsescalinguser"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "User scaling if "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsenumericfactoroptions/2868158-scaling"
                    },
                    {
                      "type": "text",
                      "text": " is nonnull, otherwise no scaling."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsescaling_t/sparsescalingequilibriationinf"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Norm equilibration scaling using infinity norm."
                    }
                  ]
                }
              ]
            ]
          ],
          "metadata": {
            "anchor": "2878972",
            "title": "Table 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Note that "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsenumericfactoroptions/2868089-pivottolerance"
            },
            {
              "type": "text",
              "text": " is clamped to range "
            },
            {
              "type": "codeVoice",
              "code": "[0,0.5]"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
