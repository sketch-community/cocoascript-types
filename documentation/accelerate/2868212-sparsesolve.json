          "name": "xb",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On input, the vector "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "b"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". On return it is overwritten with the vector "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "x"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " has dimension "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "m x n"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", then this parameter must have length "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "k"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "k = max(m,n)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "workspace",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Scratch space of size "
                },
