        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The function calculates values for "
            },
            {
              "type": "codeVoice",
              "code": "Mean"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "StandardDeviation"
            },
            {
              "type": "text",
              "text": ", then calculates new values for "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " to have a zero mean and unit standard deviation."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For iOS 9.0 and later or macOS 10.11 and later, the production of new elements may be omitted by passing "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " for "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ". In this case "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " remains unchanged."
            }
          ]
        }
