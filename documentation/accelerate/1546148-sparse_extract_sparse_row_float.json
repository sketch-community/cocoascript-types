    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "A",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The sparse matrix, "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", which must have been created with "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1546996-sparse_matrix_create_float"
                },
                {
                  "type": "text",
                  "text": ". "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
                },
                {
                  "type": "text",
                  "text": " is returned if not met."
                }
              ]
            }
          ]
        },
        {
          "name": "row",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The row for value extraction."
                }
              ]
            }
          ]
        },
        {
          "name": "column_start",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index of the column to start extraction."
                }
              ]
            }
          ]
        },
        {
          "name": "column_end",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, holds the column index of the next nonzero value."
                }
              ]
            }
          ]
        },
        {
          "name": "nz",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of values to extract from "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": ".  Each of "
                },
                {
                  "type": "codeVoice",
                  "code": "jndx"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": " are of size "
                },
                {
                  "type": "codeVoice",
                  "code": "nz"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "val",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pointer to array to hold the values extracted from the sparse matrix.  The value is extracted from the location specified by the corresponding indices of "
                },
                {
                  "type": "codeVoice",
                  "code": "row"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "jndx"
                },
                {
                  "type": "text",
                  "text": ".  Must be of size "
                },
                {
                  "type": "codeVoice",
                  "code": "nz"
                },
                {
                  "type": "text",
                  "text": " elements.  If less than "
                },
                {
                  "type": "codeVoice",
                  "code": "nz"
                },
                {
                  "type": "text",
                  "text": " nonzero values are found, then the last "
                },
                {
                  "type": "codeVoice",
                  "code": "nz - actual_nonzero_count"
                },
                {
                  "type": "text",
                  "text": " elements of val are untouched."
                }
              ]
            }
          ]
        },
        {
          "name": "jndx",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array to hold the extracted column indices that correspond to the values in "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": ". Note that these indices are relative to the matrix row and not the starting column index specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "column_start"
                },
                {
                  "type": "text",
                  "text": ".  Returned indices are 0 based (first element of pointer is "
                },
                {
                  "type": "codeVoice",
                  "code": "ptr[0]"
                },
                {
                  "type": "text",
                  "text": ").  Must be of size "
                },
                {
                  "type": "codeVoice",
                  "code": "nz"
                },
                {
                  "type": "text",
                  "text": " elements."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "On success "
            },
            {
              "type": "codeVoice",
              "code": "val"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "jndx"
            },
            {
              "type": "text",
              "text": " have been updated with the nonzero values of the "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": "'th row, "
            },
            {
              "type": "codeVoice",
              "code": "column_end"
            },
            {
              "type": "text",
              "text": " holds the column index of the next nonzero value, and the number of nonzero values written are returned.  If "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " creation requirements are not met, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
            },
            {
              "type": "text",
              "text": " is returned and "
            },
            {
              "type": "codeVoice",
              "code": "val"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "jndx"
            },
            {
              "type": "text",
              "text": " are unchanged."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Extract the first "
            },
            {
              "type": "codeVoice",
              "code": "nz"
            },
            {
              "type": "text",
              "text": " values of the row beginning at "
            },
            {
              "type": "codeVoice",
              "code": "A[row,column_start]"
            },
            {
              "type": "text",
              "text": " for the sparse matrix "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": ".  The number of nonzero values extracted is limited by "
            },
            {
              "type": "codeVoice",
              "code": "nz"
            },
            {
              "type": "text",
              "text": ", and the number of nonzero's written to "
            },
            {
              "type": "codeVoice",
              "code": "jndx"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "val"
            },
            {
              "type": "text",
              "text": " are returned.  Additionally, the column index of the next nonzero value is returned in "
            },
            {
              "type": "codeVoice",
              "code": "column_end"
            },
            {
              "type": "text",
              "text": ". For example if "
            },
            {
              "type": "codeVoice",
              "code": "nz"
            },
            {
              "type": "text",
              "text": " is returned, not all nonzero values have been extracted, and a second extract can start from "
            },
            {
              "type": "codeVoice",
              "code": "column_end"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
  ],
