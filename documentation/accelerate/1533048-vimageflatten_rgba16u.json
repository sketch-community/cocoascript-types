    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The per-pixel operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            " resultAlpha = (pixelAlpha * 65535 + (65535 - pixelAlpha) * backgroundAlpha + 32767) / 65535",
            " if(isImagePremultiplied)",
            " {",
            "     resultColor = (pixelColor * 65535 + (65535 - pixelAlpha) * backgroundColor + 32767) / 65535",
            " }",
            " else",
            " {",
            "     resultColor = (pixelColor * pixelAlpha + (65535 - pixelAlpha) * backgroundColor + 32767) / 65535",
            " }",
            ""
          ],
          "syntax": "other",
          "metadata": {
            "anchor": "2935218",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The source and destination buffers must have the same height and width."
            }
          ]
        }
      ]
    }
