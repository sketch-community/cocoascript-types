    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "src",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A valid pointer to a vImage buffer struct that describes an 16-bit-per-component, four-channel buffer. The pixels don't have to be ARGB; they can be BGRA, RGBA, CMYK, etc. "
                }
              ]
            }
          ]
        },
        {
          "name": "dest",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A valid pointer to a vImage buffer structure that describes an 16-bit-per-component, one-channel buffer. You allocate the buffer that dest points to, and the buffer is overwritten by the channel specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "channelIndex"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "channelIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index of the channel to extract. For alpha in an ARGB image, this is 0.  For alpha in a BGRA image, this value is 3."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The options to use when performing the operation. If your code implements its own tiling or its own multithreading, pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimagedonottile"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1578972-error_codes/kvimagenoerror"
            },
            {
              "type": "text",
              "text": "; otherwise, one of the error codes described in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/vimage/data_types_and_constants"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The data written to "
            },
            {
              "type": "codeVoice",
              "code": "dest"
            },
            {
              "type": "text",
              "text": " will be in the same format ("
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/kernel/uint16_t"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/kernel/int16_t"
            },
            {
              "type": "text",
              "text": ", half-float, etc.) as the data provided in the "
            },
            {
              "type": "codeVoice",
              "code": "src"
            },
            {
              "type": "text",
              "text": " format, except only a single channel is present. Data must be at least 2-byte aligned."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function can work in place, provided that "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/vimage_buffer/1545333-rowbytes"
            },
            {
              "type": "text",
              "text": " is the same for "
            },
            {
              "type": "codeVoice",
              "code": "src"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "dest"
            },
            {
              "type": "text",
              "text": " images, and the start address matches. The data written to "
            },
            {
              "type": "codeVoice",
              "code": "dest"
            },
            {
              "type": "text",
              "text": " will be in the same format as the data provided in the "
            },
            {
              "type": "codeVoice",
              "code": "src"
            },
            {
              "type": "text",
              "text": " format, except only a single channel is present."
            }
          ]
        }
      ]
    }
