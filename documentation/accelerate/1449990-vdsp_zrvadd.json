    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the sums of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " complex elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " to the corresponding real elements of "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", writing the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110566",
              "metadata": {
                "anchor": "3110566",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = A[n] + B[n];"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108846",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1449990-vdsp_zrvadd"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let n = vDSP_Length(4)",
            "let stride = vDSP_Stride(1)",
            "",
            "var realA: [Float] = [2, 4, 8, 16]",
            "var imagA: [Float] = [10, 11, 12, 13]",
            "",
            "var b: [Float] = [100, 200, 300, 400]",
            "",
            "var realC = [Float](repeating: .nan, count: Int(n))",
            "var imagC = [Float](repeating: .nan, count: Int(n))",
            "",
            "realA.withUnsafeMutableBufferPointer { realAPtr in",
            "    imagA.withUnsafeMutableBufferPointer { imagAPtr in",
            "        realC.withUnsafeMutableBufferPointer { realCPtr in",
            "            imagC.withUnsafeMutableBufferPointer { imagCPtr in",
            "                var a = DSPSplitComplex(realp: realAPtr.baseAddress!,",
            "                                        imagp: imagAPtr.baseAddress!)",
            "                ",
            "                var c = DSPSplitComplex(realp: realCPtr.baseAddress!,",
            "                                        imagp: imagCPtr.baseAddress!)",
            "                ",
            "                vDSP_zrvadd(&a, stride,",
            "                            &b, stride,",
            "                            &c, stride,",
            "                            n)",
            "            }",
            "        }",
            "    }",
            "}",
            "",
            "print(\"real\", realC) // Prints \"real [102.0, 204.0, 308.0, 416.0]\"",
            "print(\"imag\", imagC) // Prints \"imag [10.0, 11.0, 12.0, 13.0]\" (imagp is unchanged)"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108847",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
