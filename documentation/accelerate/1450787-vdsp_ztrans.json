    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " divided by the corresponding element in "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", writing the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110560",
              "metadata": {
                "anchor": "3110560",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = B[n] / A[n];"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108782",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450787-vdsp_ztrans"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let a: [Float] = [10, 20, 30, 40]",
            "",
            "let n = vDSP_Length(a.count)",
            "",
            "var realB: [Float] = [100, 200, 300, 400]",
            "var imagB: [Float] = [500, 800, 600, 800]",
            "",
            "var realC = [Float](repeating: .nan,",
            "                    count: Int(n))",
            "var imagC = [Float](repeating: .nan,",
            "                    count: Int(n))",
            "",
            "realB.withUnsafeMutableBufferPointer { realBPtr in",
            "    imagB.withUnsafeMutableBufferPointer { imagBPtr in",
            "        realC.withUnsafeMutableBufferPointer { realCPtr in",
            "            imagC.withUnsafeMutableBufferPointer { imagCPtr in",
            "                ",
            "                let b = DSPSplitComplex(realp: realBPtr.baseAddress!,",
            "                                        imagp: imagBPtr.baseAddress!)",
            "                ",
            "                var c = DSPSplitComplex(realp: realCPtr.baseAddress!,",
            "                                        imagp: imagCPtr.baseAddress!)",
            "                ",
            "                vDSP_ztrans(a,",
            "                            b,",
            "                            &c,",
            "                            n)",
            "            }",
            "            ",
            "        }",
            "    }",
            "}",
            "",
            "print(\"real\", realC) // Prints \"real [10, 10, 10, 10]\"",
            "print(\"imag\", imagC) // Prints \"imag [50, 40, 20, 20]\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108784",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
