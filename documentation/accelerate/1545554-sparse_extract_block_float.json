    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "A",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The sparse matrix, "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", which must have been created with "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1544775-sparse_matrix_block_create_float"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1546234-sparse_matrix_variable_block_cre"
                },
                {
                  "type": "text",
                  "text": ". "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
                },
                {
                  "type": "text",
                  "text": " is returned if not met. "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": " holds block dimensions (fixed or variable) set with matrix object creation routine."
                }
              ]
            }
          ]
        },
        {
          "name": "bi",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block row index for value extraction. Indices are 0 based (first block of matrix is "
                },
                {
                  "type": "codeVoice",
                  "code": "A[0,0]"
                },
                {
                  "type": "text",
                  "text": ").  Indices expected to be in the bounds of matrix dimensions, undefined behavior if not met."
                }
              ]
            }
          ]
        },
        {
          "name": "bj",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block column index for value extraction. Indices are 0 based (first block of matrix is "
                },
                {
                  "type": "codeVoice",
                  "code": "A[0,0]"
                },
                {
                  "type": "text",
                  "text": ").  Indices expected to be in the bounds of matrix dimensions, undefined behavior if not met."
                }
              ]
            }
          ]
        },
        {
          "name": "row_stride",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The row stride in number of elements to move from one row to the next for the block "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "col_stride",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The column stride in number of elements to move from one column to the next for the block "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "val",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pointer to dense block to place the extracted values.  Expected to be of size "
                },
                {
                  "type": "codeVoice",
                  "code": "K * L"
                },
                {
                  "type": "text",
                  "text": " where "
                },
                {
                  "type": "codeVoice",
                  "code": "K * L"
                },
                {
                  "type": "text",
                  "text": " is the block size for the matrix object at block index "
                },
                {
                  "type": "codeVoice",
                  "code": "bi,bj"
                },
                {
                  "type": "text",
                  "text": ". This dimensions is set at matrix object creation time."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "On success "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_success"
            },
            {
              "type": "text",
              "text": " is return and val has been updated with the block from block index "
            },
            {
              "type": "codeVoice",
              "code": "bi,bj"
            },
            {
              "type": "text",
              "text": ". If "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " creation requirements are not met, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
            },
            {
              "type": "text",
              "text": " is returned and "
            },
            {
              "type": "codeVoice",
              "code": "val"
            },
            {
              "type": "text",
              "text": " is unchanged."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Extract the "
            },
            {
              "type": "codeVoice",
              "code": "bi,bj"
            },
            {
              "type": "text",
              "text": "'th block from the sparse matrix "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        }
      ]
    }
