    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For best performance, "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "Z.realp"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "Z.imagp"
            },
            {
              "type": "text",
              "text": " should be 16-byte aligned. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This performs the following operations:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for (n = 0; n < N; ++n)",
            "{",
            "  C[n*IC/2].real = Z->realp[n*IZ];",
            "  C[n*IC/2].imag = Z->imagp[n*IZ];",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557708",
            "title": "Listing 1"
          }
        },
