    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If this array is not "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparsesymbolicfactoroptions/2868115-ignorerowsandcolumns"
            },
            {
              "type": "text",
              "text": " provides a list of rows and columns to ignore, terminated by a negative index. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The row and column indices are for the actual matrix, not its block structure, so 0 indicates the first row, not the first "
            },
            {
              "type": "codeVoice",
              "code": "blockSize"
            },
            {
              "type": "text",
              "text": " rows."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In the symmetric case (Cholesky, "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "LDL"
                }
              ]
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "ᵀ"
                }
              ]
            },
            {
              "type": "text",
              "text": "), each entry indicates that the matching row and column should be ignored."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In the unsymmetric case (QR, Cholesky "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "AᵀA"
                }
              ]
            },
            {
              "type": "text",
              "text": "), consider the matrix, "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": ", given the value "
            },
            {
              "type": "codeVoice",
              "code": "m"
            },
            {
              "type": "text",
              "text": ", that is defined as either of the following:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "m = A.structure.rowCount * A.blockSize"
                    },
                    {
                      "type": "text",
                      "text": " if "
                    },
                    {
                      "type": "codeVoice",
                      "code": "A"
                    },
                    {
                      "type": "text",
                      "text": " is not transposed"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "m = A.structure.columnCount * A.blockSize"
                    },
                    {
                      "type": "text",
                      "text": " if "
                    },
                    {
                      "type": "codeVoice",
                      "code": "A"
                    },
                    {
                      "type": "text",
                      "text": " is transposed"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this case, an index less than "
            },
            {
              "type": "codeVoice",
              "code": "m"
            },
            {
              "type": "text",
              "text": " indicates that row "
            },
            {
              "type": "codeVoice",
              "code": "m"
            },
            {
              "type": "text",
              "text": " should be ignored. An index "
            },
            {
              "type": "codeVoice",
              "code": "i"
            },
            {
              "type": "text",
              "text": ", greater than "
            },
            {
              "type": "codeVoice",
              "code": "m"
            },
            {
              "type": "text",
              "text": ", indicates that column "
            },
            {
              "type": "codeVoice",
              "code": "i - m"
            },
            {
              "type": "text",
              "text": " should be ignored."
            }
          ]
        }
      ]
    }
  ],
