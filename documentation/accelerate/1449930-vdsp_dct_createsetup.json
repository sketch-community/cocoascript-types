      "parameters": [
        {
          "name": "__Previous",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Either "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", or an existing structure of type "
                },
                {
                  "type": "codeVoice",
                  "code": "vDSP_DFT_Setup"
                },
                {
                  "type": "text",
                  "text": " â€“ i.e. a previous result from any setup function in the single-precision DFT/DCT family: "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1450061-vdsp_dft_zop_createsetup"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1449739-vdsp_dft_zrop_createsetup"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "vDSP_DFT_CreateSetup"
                },
                {
                  "type": "text",
                  "text": " (deprecated), or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1449930-vdsp_dct_createsetup"
                },
                {
                  "type": "text",
                  "text": ". Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " to create an initial setup object."
                }
              ]
            }
          ]
        },
        {
          "name": "__Length",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Number of real elements to be transformed. The supported values are "
                },
                {
                  "type": "codeVoice",
                  "code": "Length"
                },
                {
                  "type": "text",
                  "text": " = "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "f"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " * 2**"
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "n"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "f"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is 1, 3, 5, or 15 and "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "n"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is at least 4."
                }
              ]
            }
          ]
        },
        {
          "name": "__Type",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Which DCT variant to perform.  At present, the supported DCT types are II and III (which are mutual inverses, up to scaling) and IV (which is its own inverse).  These are specified with symbol names "
                },
                {
                  "type": "codeVoice",
                  "code": "vDSP_DCT_II"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "vDSP_DCT_III"
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "codeVoice",
                  "code": "vDSP_DCT_IV"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Returns a "
            },
            {
              "type": "codeVoice",
              "code": "vDSP_DFT_Setup"
            },
            {
              "type": "text",
              "text": " object, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if the function fails, either from lack of memory or because "
            },
            {
              "type": "codeVoice",
              "code": "Length"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "Type"
            },
            {
              "type": "text",
              "text": " does not satisfy the requirements given above."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The returned setup object may be used only with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450016-vdsp_dct_execute"
            },
            {
              "type": "text",
              "text": " using an input vector of length "
            },
            {
              "type": "codeVoice",
              "code": "Length"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Do not call this function while any DFT or DCT routine sharing the "
            },
            {
              "type": "codeVoice",
              "code": "Previous"
            },
            {
              "type": "text",
              "text": " setup data might be executing."
            }
          ]
        }
      ]
    }
