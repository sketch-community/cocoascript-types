    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function applies a cascaded biquad filter to the input values in "
            },
            {
              "type": "codeVoice",
              "code": "X"
            },
            {
              "type": "text",
              "text": ", and places the results in "
            },
            {
              "type": "codeVoice",
              "code": "Y"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The state contained in the "
            },
            {
              "type": "codeVoice",
              "code": "setup"
            },
            {
              "type": "text",
              "text": " object is updated upon return."
            }
          ]
