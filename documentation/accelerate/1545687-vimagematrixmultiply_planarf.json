      "parameters": [
        {
          "name": "srcs",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an array of vImage buffer structures, one buffer for each source plane."
                }
              ]
            }
          ]
        },
        {
          "name": "dests",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an array of pointers to vImage buffer data structures, one buffer structure for each destination plane. You're responsible for filling out the "
                },
                {
                  "type": "codeVoice",
                  "code": "height"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "width"
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "codeVoice",
                  "code": "rowBytes"
                },
                {
                  "type": "text",
                  "text": " fields of these structures, and for allocating data buffers of the appropriate size.  On return, the data buffers in these structures contains  the destination image data for each plane. When you no longer need the data buffers, you must deallocate the memory."
                }
              ]
            }
          ]
        },
        {
          "name": "src_planes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of source planes."
                }
              ]
            }
          ]
        },
        {
          "name": "dest_planes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of destination planes."
                }
              ]
            }
          ]
        },
        {
          "name": "matrix",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A 1D array whose values represent a matrix with the dimensions "
                },
                {
                  "type": "codeVoice",
                  "code": "dest_planes"
                },
                {
                  "type": "codeVoice",
                  "code": "*"
                },
                {
                  "type": "codeVoice",
                  "code": "src_planes"
                },
                {
                  "type": "text",
                  "text": ". vImage multiplies each source pixel by this matrix to produce a destination pixel."
                }
              ]
            }
          ]
        },
        {
          "name": "divisor",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A divisor for normalization after performing the matrix multiplication."
                }
              ]
            }
          ]
        },
        {
          "name": "pre_bias",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A packed array of bias values, one for each source plane. vImage adds the appropriate bias value to each source value before matrix multiplication. If you don't want to apply a preprocessing bias value, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "post_bias",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A packed array of bias values, one for each destination plane. vImage adds the appropriate bias value to each destination value after matrix multiplication. If you don't want to apply a preprocessing bias value, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "flags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The options to use when performing the operation. If your code implements its own tiling or its own multithreading, pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags/kvimagedonottile"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
