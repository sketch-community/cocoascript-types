    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The transfer function here is defined in the style of ITU-R BT.709 and is the inverse operation of what appears in an ICC color profile."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The conversion is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "if (R >= cutoff) {",
            "    R' = c0 * pow( c1 * R + c2, gamma ) + c3",
            "} ",
            "else {",
            "    R' = c4 * R + c5                             ",
            "}"
          ],
          "syntax": "other",
          "metadata": {
            "anchor": "2939992",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
