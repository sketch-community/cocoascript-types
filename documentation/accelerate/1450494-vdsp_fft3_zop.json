    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The number of input and output values processed equals 3 times the power of 2 specified by parameter "
            },
            {
              "type": "codeVoice",
              "code": "Log2N"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This performs the following operation:"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-2557819",
              "metadata": {
                "anchor": "2557819",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "where F is "
            },
            {
              "type": "codeVoice",
              "code": "Direction"
            },
            {
              "type": "text",
              "text": ", C is "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": ", j is the square root of "
            },
            {
              "type": "codeVoice",
              "code": "-1"
            },
            {
              "type": "text",
              "text": ", and N is two raised to the power of "
            },
            {
              "type": "codeVoice",
              "code": "Log2N"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "See also functions "
            },
