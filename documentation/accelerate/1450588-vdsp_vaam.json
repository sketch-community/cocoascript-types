    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the sums of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ", the sums of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "D"
            },
            {
              "type": "text",
              "text": ", multiplies the corresponding sums, and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "E"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3110553",
              "metadata": {
                "anchor": "3110553",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for (n = 0; n < N; ++n)",
            "    E[n] = (A[n]+B[n]) * (C[n]+D[n]);"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108824",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450588-vdsp_vaam"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [10, 20, 30, 40, 50]",
            "let b: [Float] = [ 1,  2,  3,  4,  5]",
            "",
            "let c: [Float] = [0.25, 1, 1.5, 2, 2]",
            "let d: [Float] = [0.75, 1, 0.5, 1, 2]",
            "",
            "let n = vDSP_Length(a.count)",
            "",
            "var e = [Float](repeating: 0,",
            "                count: a.count)",
            "",
            "vDSP_vaam(a, stride,",
            "          b, stride,",
            "          c, stride,",
            "          d, stride,",
            "          &e, stride,",
            "          n)",
            "",
            "// Prints \"[11.0, 44.0, 66.0, 132.0, 220.0]\"",
            "print(e)"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3108821",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
