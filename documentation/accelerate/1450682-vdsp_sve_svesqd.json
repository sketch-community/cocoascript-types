    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the sum of values and the sum of squares of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "Sum"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "SumOfSquares"
            },
            {
              "type": "text",
              "text": " respectively:"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3226729",
              "metadata": {
                "anchor": "3226729",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is: "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "Sum          = sum(A[n],      0 <= n < N);",
            "SumOfSquares = sum(A[n] ** 2, 0 <= n < N);"
          ],
          "syntax": "other",
          "metadata": {
            "anchor": "3226736",
            "title": "Listing 1"
          }
        },
