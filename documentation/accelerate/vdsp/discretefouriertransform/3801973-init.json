      "parameters": [
        {
          "name": "previous",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An existing "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/vdsp/discretefouriertransform"
                },
                {
                  "type": "text",
                  "text": " structure that shares memory with the discrete Fourier transform instance that this function returns. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to create an object with newly initialized and allocated memory."
                }
              ]
            }
          ]
        },
        {
          "name": "count",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of complex elements. For split-complex real-to-complex, the value of count must be "
                },
                {
                  "type": "codeVoice",
                  "code": "2ⁿ"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "f * 2ⁿ"
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "codeVoice",
                  "code": "f"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "3"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "5"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "codeVoice",
                  "code": "15"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "n >= 4"
                },
                {
                  "type": "text",
                  "text": ". For split-complex complex-to-complex, the value of count must be "
                },
                {
                  "type": "codeVoice",
                  "code": "2ⁿ"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "f * 2ⁿ"
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "codeVoice",
                  "code": "f"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "3"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "5"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "codeVoice",
                  "code": "15"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "n >= 3"
                },
                {
                  "type": "text",
                  "text": ". And for interleaved, the value of count must be "
                },
                {
                  "type": "codeVoice",
                  "code": "f * 2ⁿ"
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "codeVoice",
                  "code": "f"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "2"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "3"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "5"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "3 * 3"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "3 x 5"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "codeVoice",
                  "code": "5 * 5"
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "codeVoice",
                  "code": "n>=2"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "direction",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A flag that specifies the transform direction."
                }
              ]
            }
          ]
        },
        {
          "name": "transformType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A flag that specifies whether the forward transform is real-to-complex or complex-to-complex."
                }
              ]
            }
          ]
        },
        {
          "name": "ofType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The data type for the discrete Fourier transform operation. For split-complex operations, this must be either "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/swift/float"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/swift/double"
                },
                {
                  "type": "text",
                  "text": ". For interleaved operations, this must be either "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/dspcomplex"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/dspdoublecomplex"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
