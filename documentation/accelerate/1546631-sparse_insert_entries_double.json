          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of values to insert into "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": ". Each of "
                },
