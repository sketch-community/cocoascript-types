      "parameters": [
        {
          "name": "Order",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies row-major (C) or column-major (Fortran) data ordering."
                }
              ]
            }
          ]
        },
        {
          "name": "Uplo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies whether to use the upper or lower triangle from the matrix. Valid values are "
                },
                {
                  "type": "codeVoice",
                  "code": "'U'"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "'L'"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "N",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Order of matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": " and the number of elements in vectors "
                },
                {
                  "type": "codeVoice",
                  "code": "x"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "y"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "alpha",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Scaling factor that matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": " is multiplied by."
                }
              ]
            }
          ]
        },
        {
          "name": "Ap",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Matrix "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": " (in packed storage format)."
                }
              ]
            }
          ]
        },
        {
          "name": "X",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Vector "
                },
                {
                  "type": "codeVoice",
                  "code": "x"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "incX",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Stride within "
                },
                {
                  "type": "codeVoice",
                  "code": "X"
                },
                {
                  "type": "text",
                  "text": ". For example, if "
                },
                {
                  "type": "codeVoice",
                  "code": "incX"
                },
                {
                  "type": "text",
                  "text": " is 7, every 7th element is used."
                }
              ]
            }
          ]
        },
        {
          "name": "beta",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Scaling factor that vector "
                },
                {
                  "type": "codeVoice",
                  "code": "y"
                },
                {
                  "type": "text",
                  "text": " is multiplied by."
                }
              ]
            }
          ]
        },
        {
          "name": "Y",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Vector "
                },
                {
                  "type": "codeVoice",
                  "code": "y"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "incY",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Stride within "
                },
                {
                  "type": "codeVoice",
                  "code": "Y"
                },
                {
                  "type": "text",
                  "text": ". For example, if "
                },
                {
                  "type": "codeVoice",
                  "code": "incY"
                },
                {
                  "type": "text",
                  "text": " is 7, every 7th element is used."
                }
              ]
            }
          ]
        }
      ]
