    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "A",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The sparse matrix, "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ","
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": " "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "which must have been created with "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1546690-sparse_matrix_block_create_doubl"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/1546650-sparse_matrix_variable_block_cre"
                },
                {
                  "type": "text",
                  "text": ". "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
                },
                {
                  "type": "text",
                  "text": " is returned if not met. "
                },
                {
                  "type": "codeVoice",
                  "code": "A"
                },
                {
                  "type": "text",
                  "text": " holds block dimensions (fixed or variable) set with matrix object creation routine."
                }
              ]
            }
          ]
        },
        {
          "name": "val",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Pointer to block to be inserted at block index location "
                },
                {
                  "type": "codeVoice",
                  "code": "A[bi,bj]"
                },
                {
                  "type": "text",
                  "text": ". The block is of dimension "
                },
                {
                  "type": "codeVoice",
                  "code": "k * l"
                },
                {
                  "type": "text",
                  "text": " where "
                },
                {
                  "type": "codeVoice",
                  "code": "k"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "l"
                },
                {
                  "type": "text",
                  "text": " are set for "
                },
                {
                  "type": "codeVoice",
                  "code": "bi,bj"
                },
                {
                  "type": "text",
                  "text": " at object creation time. The strides between elements for rows and columns are provided in "
                },
                {
                  "type": "codeVoice",
                  "code": "row_stride"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "col_stride"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "row_stride",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The row stride in number of elements to move from one row to the next for the block "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "col_stride",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The column stride in number of elements to move from one column to the next for the block "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "bi",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block row index where "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": " is to be inserted. Indexing is zero based, the first block is located at "
                },
                {
                  "type": "codeVoice",
                  "code": "0,0"
                },
                {
                  "type": "text",
                  "text": ".  Index is assumed to be within the bounds of the matrix object, undefined behavior if not met."
                }
              ]
            }
          ]
        },
        {
          "name": "bj",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block column index where "
                },
                {
                  "type": "codeVoice",
                  "code": "val"
                },
                {
                  "type": "text",
                  "text": " is to be inserted. Indexing is zero based, the first block is located at "
                },
                {
                  "type": "codeVoice",
                  "code": "0,0"
                },
                {
                  "type": "text",
                  "text": ".  Index is assumed to be within the bounds of the matrix object, undefined behavior if not met."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "On successful insertion, "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " has been updated with the value and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_success"
            },
            {
              "type": "text",
              "text": " is returned. If "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " creation requirements are not met, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/sparse_status/sparse_illegal_parameter"
            },
            {
              "type": "text",
              "text": " is returned and "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " is unchanged."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use to build a sparse matrix by providing a dense block for entry at block location "
            },
            {
              "type": "codeVoice",
              "code": "A[bi,bj]"
            },
            {
              "type": "text",
              "text": ". Block size is determined at object creation time. Given a block dimension of "
            },
            {
              "type": "codeVoice",
              "code": "k * l"
            },
            {
              "type": "text",
              "text": " and for location "
            },
            {
              "type": "codeVoice",
              "code": "bi,bj"
            },
            {
              "type": "text",
              "text": ", update as: "
            },
            {
              "type": "codeVoice",
              "code": "A[bi,bj][i,j] = val[i*row_stride + j*col_stride]"
            },
            {
              "type": "text",
              "text": " for each "
            },
            {
              "type": "codeVoice",
              "code": "i"
            },
            {
              "type": "text",
              "text": " in "
            },
            {
              "type": "codeVoice",
              "code": "k"
            },
            {
              "type": "text",
              "text": " and each "
            },
            {
              "type": "codeVoice",
              "code": "j"
            },
            {
              "type": "text",
              "text": " in "
            },
            {
              "type": "codeVoice",
              "code": "l"
            },
            {
              "type": "text",
              "text": ".  "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Note that matrix properties cannot be modified after value insertion begins.This includes properties such as specifying a triangular matrix.Insertion can be expensive, generally speaking it is best to do a batch update. Inserted values may be temporarily held internally within the object and only inserted into the sparse format when a later computation triggers a need to insert."
            }
          ]
        }
      ]
    }
  ],
