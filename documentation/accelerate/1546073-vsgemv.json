        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Vector "
            },
            {
              "type": "codeVoice",
              "code": "y"
            },
            {
              "type": "text",
              "text": " is multiplied by "
            },
            {
              "type": "codeVoice",
              "code": "beta"
            },
            {
              "type": "text",
              "text": ". Matrix "
            },
            {
              "type": "codeVoice",
              "code": "a"
            },
            {
              "type": "text",
              "text": " is multiplied by "
            },
            {
              "type": "codeVoice",
              "code": "alpha"
            },
            {
              "type": "text",
              "text": ". Then if "
            },
            {
              "type": "codeVoice",
              "code": "forma"
            },
            {
              "type": "text",
              "text": " = ‘N’, "
            },
            {
              "type": "codeVoice",
              "code": "a"
            },
            {
              "type": "text",
              "text": " is multiplied by vector "
            },
            {
              "type": "codeVoice",
              "code": "x"
            },
            {
              "type": "text",
              "text": "; if "
            },
            {
              "type": "codeVoice",
              "code": "forma"
            },
            {
              "type": "text",
              "text": " = ‘T’, the transpose of "
            },
            {
              "type": "codeVoice",
              "code": "a"
            },
            {
              "type": "text",
              "text": " is multiplied by "
            },
            {
              "type": "codeVoice",
              "code": "x"
            },
            {
              "type": "text",
              "text": ". The resulting vector is added to vector "
            },
            {
              "type": "codeVoice",
              "code": "y"
            },
            {
              "type": "text",
              "text": ", and the results are stored in "
            },
