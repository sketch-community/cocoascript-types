      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/accelerate/1578972-error_codes/kvimageinvalidedgestyle",
      "abstract": [
        {
          "type": "text",
          "text": "The edge style specified is invalid. This usually means that a particular function requires you to set at least one edge option flag ("
        },
        {
          "type": "codeVoice",
          "code": "kvImageCopyInPlace"
        },
        {
          "type": "text",
          "text": ", "
        },
        {
          "type": "codeVoice",
          "code": "kvImageBackgroundColorFill"
        },
        {
          "type": "text",
          "text": ", or "
        },
        {
          "type": "codeVoice",
          "code": "kvImageEdgeExtend"
        },
        {
          "type": "text",
          "text": "), but you did not specify one. See "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/accelerate/1578976-processing_flags"
        },
        {
          "type": "text",
          "text": " for more information about these flags."
        }
      ]
    },
