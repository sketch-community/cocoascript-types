            },
            {
              "kind": "text",
              "text": " "
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@CGFloat",
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgfloat",
              "text": "CGFloat"
            },
            {
              "kind": "text",
              "text": " *backgroundColor, "
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@vImage_Flags",
              "identifier": "doc://com.apple.documentation/documentation/accelerate/vimage_flags",
              "text": "vImage_Flags"
            },
