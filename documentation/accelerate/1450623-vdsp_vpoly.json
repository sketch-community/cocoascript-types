        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Evaluates a polynomial using vector "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": " as independent  variables and vector "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " as coefficients. A polynomial of degree p requires p+1  coefficients, so vector "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " should contain "
            },
            {
              "type": "codeVoice",
              "code": "P"
            },
            {
              "type": "text",
              "text": "+1 values. "
            }
          ]
