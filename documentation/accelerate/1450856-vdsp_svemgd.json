        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the sum of magnitudes of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
