    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Performs the following operation:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for (n = 0; n < N; ++n){",
            "    float b = B[n*IB];",
            "    float index = trunc(b]); //int part of B value",
            "    float alpha = b - index; //frac part of B value",
            " ",
            "    float a0 = A[(int)index];     //indexed A value",
            "    float a1 = A[(int)index + 1]; //next A value",
            " ",
            "    C[n*IC] = a0 + (alpha * (a1 -a0)); //interpolated value",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557677",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Generates vector "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " by interpolating between neighboring values of  vector "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": " as controlled by vector "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": ". The integer portion of each element in "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": " is the zero-based index of the first element of a pair of adjacent values in vector "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of the corresponding element of "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " is derived from these two values by linear interpolation, using the fractional part of the value in B. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Argument "
            },
            {
              "type": "codeVoice",
              "code": "M"
            },
            {
              "type": "text",
              "text": " is not used in the calculation. However, the integer parts of the values in "
            },
            {
              "type": "codeVoice",
              "code": "B"
            },
            {
              "type": "text",
              "text": " must be greater than or equal to zero  and less than or equal to "
            },
            {
              "type": "codeVoice",
              "code": "M"
            },
            {
              "type": "text",
              "text": " - 2."
            }
          ]
        }
      ]
    }
