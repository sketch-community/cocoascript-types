      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Performs the following operation:"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-2557732",
              "metadata": {
                "anchor": "2557732",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The \"function\" truncate(x) is the integer farthest from 0 but not farther than x. Thus, for example, "
            },
            {
              "type": "codeVoice",
              "code": "vDSP_vFrac(-3.25)"
            },
            {
              "type": "text",
              "text": " produces the result -0.25."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Sets each element of vector "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": " to the signed fractional part of the corresponding element of "
            },
            {
              "type": "codeVoice",
              "code": "A"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        }
      ]
    }
