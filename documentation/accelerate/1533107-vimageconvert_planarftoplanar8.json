    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The minimum and maximum value determine the mapping of intensity values to the destination image. The mapping is:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "    if oldPixel < minFloat",
            "    newPixel = 0"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557603",
            "title": "Listing 1"
          }
        },
        {
          "type": "codeListing",
          "code": [
            "    if minfloat <= oldPixel <= maxFloat",
            "    newPixel = (oldPixel - minFloat) * 255.0f / (maxFloat - minFloat)"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557609",
            "title": "Listing 2"
          }
        },
        {
          "type": "codeListing",
          "code": [
            "    if oldPixel > maxFloat",
            "    newPixel = 255"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557617",
            "title": "Listing 3"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The source and destination buffers must have the same height and width."
            }
          ]
        }
      ]
    }
