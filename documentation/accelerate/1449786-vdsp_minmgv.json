    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function calculates the minimum magnitude of the first "
            },
            {
              "type": "codeVoice",
              "code": "N"
            },
            {
              "type": "text",
              "text": " elements of "
            },
            {
              "type": "codeVoice",
              "code": "A "
            },
            {
              "type": "text",
              "text": "and writes the result to "
            },
            {
              "type": "codeVoice",
              "code": "C"
            },
            {
              "type": "text",
              "text": ":"
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "image",
              "identifier": "media-3111266",
              "metadata": {
                "anchor": "3111266",
                "title": "Figure 1"
              }
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The operation is: "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "*C = INFINITY;",
            "for (n = 0; n < N; ++n)",
            "    if (*C > |A[n*I]|)",
            "        *C = |A[n*I]|;"
          ],
          "syntax": "other",
          "metadata": {
            "anchor": "3110615",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following code shows an example of using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1449786-vdsp_minmgv"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [-1.5, 2.25, 3.6,",
            "                  0.2, -0.1, -4.3]",
            "let n = vDSP_Length(a.count)",
            "",
            "var c: Float = .nan",
            "",
            "vDSP_minmgv(a,",
            "            stride,",
            "            &c,",
            "            n)",
            "",
            "print(\"min magnitude\", c) // Prints \"min magnitude 0.1\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3110611",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
