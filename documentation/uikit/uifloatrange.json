    "doc://com.apple.documentation/documentation/uikit/uifloatrange/1621305-maximum": {
      "title": "maximum",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uifloatrange/1621305-maximum",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uifloatrange/1621305-maximum",
      "abstract": [
        {
          "type": "text",
          "text": "The maximum range of motion for sliding and pin attachments. For sliding attachments, it represents the number of points to move along the axis of translation in the positive direction. For pin attachments, it represents the number of radians to rotate in the clockwise direction. This value must be greater than or equal to "
        },
        {
          "type": "codeVoice",
          "code": "0"
        },
        {
          "type": "text",
          "text": ". "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uifloatrange/1621300-minimum": {
      "title": "minimum",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uifloatrange/1621300-minimum",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uifloatrange/1621300-minimum",
      "abstract": [
        {
          "type": "text",
          "text": "The minimum range of motion for sliding and pin attachments. For sliding attachments, it represents the number of points to move along the axis of translation in the negative direction. For pin attachments, it represents the number of radians to rotate in the counter-clockwise direction. This value must be less than or equal to "
        },
        {
          "type": "codeVoice",
          "code": "0"
        },
        {
          "type": "text",
          "text": ". "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/1621299-uifloatrangeisinfinite": {
