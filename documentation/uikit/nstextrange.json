      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "codeVoice",
              "code": "NSTextRange"
            },
            {
              "type": "text",
              "text": " consists of the starting and terminating locations. There the two basic properties: "
            },
            {
              "type": "codeVoice",
              "code": "location"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "endLocation"
            },
            {
              "type": "text",
              "text": ", respectively. The terminating "
            },
            {
              "type": "codeVoice",
              "code": "location"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "endLocation"
            },
            {
              "type": "text",
              "text": ", is directly following the last location in the range. For example, a location contains a range if "
            },
            {
              "type": "codeVoice",
              "code": "(range.location <= location) && (location < range.endLocation)"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
