          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This keyboard type prominently features the numbers "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " through "
            },
            {
              "type": "codeVoice",
              "code": "9"
            },
            {
              "type": "text",
              "text": " and the “"
            },
            {
              "type": "codeVoice",
              "code": "*"
            },
            {
              "type": "text",
              "text": "” and “"
            },
            {
              "type": "codeVoice",
              "code": "#"
            },
            {
              "type": "text",
              "text": "” characters. This keyboard type does not support auto-capitalization."
            }
          ]
        }
