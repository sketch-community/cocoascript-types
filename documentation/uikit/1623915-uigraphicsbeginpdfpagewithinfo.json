    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "bounds",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A rectangle that specifies the size and location of the new PDF page. This rectangle corresponds to the media box rectangle for the page."
                }
              ]
            }
          ]
        },
        {
          "name": "pageInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary that specifies additional page-related information, such as the boxes that define different parts of the page. For a list of keys you can include in this dictionary, see Box Dictionary Keys in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfcontext"
                },
                {
                  "type": "text",
                  "text": ". The dictionary is retained by the new page, so you may release it after this function returns."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to associate any additional information with the page. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function ends any previous page before beginning a new one. It sets the media box of the new page to the value in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgpdfcontextmediabox"
            },
            {
              "type": "text",
              "text": " key of the "
            },
            {
              "type": "codeVoice",
              "code": "pageInfo"
            },
            {
              "type": "text",
              "text": " dictionary, or to the value in the "
            },
            {
              "type": "codeVoice",
              "code": "bounds"
            },
            {
              "type": "text",
              "text": " parameter if the dictionary does not contain the key."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the current graphics context is not a PDF context, this function does nothing."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You must call this function or the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/1623915-uigraphicsbeginpdfpagewithinfo"
            },
            {
              "type": "text",
              "text": " function before you issue any drawing commands."
            }
          ]
        }
      ]
    }
  ],
