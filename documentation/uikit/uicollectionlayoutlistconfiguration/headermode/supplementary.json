          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Choose this header mode to use supplementary views with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uicollectionview/1617700-elementkindsectionheader"
            },
            {
              "type": "text",
              "text": " as the section header. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, lists that use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uicollectionlayoutlistconfiguration/appearance/plain"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uicollectionlayoutlistconfiguration/appearance/sidebarplain"
            },
            {
              "type": "text",
              "text": " list appearances use pinned headers. You must use this header mode if you want to opt into this default pinning behavior. "
            }
          ]
        }
      ]
