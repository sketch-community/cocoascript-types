    "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstatenormal": {
      "title": "UIDocumentStateNormal",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstatenormal",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidocumentstate/uidocumentstatenormal",
      "abstract": [
        {
          "type": "text",
          "text": "The document is open, editing is enabled, and there are no conflicts or errors associated with it."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateclosed": {
      "title": "UIDocumentStateClosed",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateclosed",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidocumentstate/uidocumentstateclosed",
      "abstract": [
        {
          "type": "text",
          "text": "There was an error in reading the document: the document has either not been successfully opened, or has been since closed. The document properties might not be valid."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateinconflict": {
      "title": "UIDocumentStateInConflict",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateinconflict",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidocumentstate/uidocumentstateinconflict",
      "abstract": [
        {
          "type": "text",
          "text": "Conflicts exist for the document file located at "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uidocument/1619990-fileurl"
        },
        {
          "type": "text",
          "text": ". You can access these conflicting document versions by calling the otherVersionsOfItemAtURL: class method of the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsfileversion"
        },
        {
          "type": "text",
          "text": " class.  This method returns an array of "
        },
        {
          "type": "codeVoice",
          "code": "NSFileVersion"
        },
        {
          "type": "text",
          "text": " objects. You can then resolve the conflicting versionsâ€”for example, programmatically attempt to merge the versions or present the document versions to the user and request him or her to pick one."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstatesavingerror": {
      "title": "UIDocumentStateSavingError",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstatesavingerror",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidocumentstate/uidocumentstatesavingerror",
      "abstract": [
        {
          "type": "text",
          "text": "There was an error in saving or reverting the document."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateeditingdisabled": {
      "title": "UIDocumentStateEditingDisabled",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateeditingdisabled",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidocumentstate/uidocumentstateeditingdisabled",
      "abstract": [
        {
          "type": "text",
          "text": "The document is busy and it is not currently safe for user edits. This state is set just before "
        },
        {
          "type": "codeVoice",
          "code": "UIDocument"
        },
        {
          "type": "text",
          "text": " calls the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uidocument/1619958-disableediting"
        },
        {
          "type": "text",
          "text": " method. It calls "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uidocument/1619987-enableediting"
        },
        {
          "type": "text",
          "text": " when it becomes safe to edit again. "
        },
        {
          "type": "codeVoice",
          "code": "UIDocument"
        },
        {
          "type": "text",
          "text": " also sets this state when an error prevents the reverting of a document. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateprogressavailable": {
      "title": "UIDocumentStateProgressAvailable",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentstate/uidocumentstateprogressavailable",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidocumentstate/uidocumentstateprogressavailable",
      "abstract": [
        {
          "type": "text",
          "text": "The document is being downloaded or uploaded and progress information is available. When this state is set, you can use the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogressreporting/1412781-progress"
        },
        {
          "type": "text",
          "text": " property of the document to monitor the current operation. "
        }
      ]
    },
