          "name": "aLayoutManager",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The layout manager doing the layout."
                }
              ]
            }
          ]
        },
        {
          "name": "aTextContainer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The text container in which layout is complete. If "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", if there aren’t enough containers to hold all the text; the delegate can use this information as a cue to add another text container."
                }
              ]
            }
          ]
        },
        {
          "name": "flag",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "aLayoutManager"
                },
                {
                  "type": "text",
                  "text": " is finished laying out its text—this also means that "
                },
                {
                  "type": "codeVoice",
                  "code": "aTextContainer"
                },
                {
                  "type": "text",
                  "text": " is the final text container used by the layout manager. Delegates can use this information to show an indicator or background or to enable or disable a button that forces immediate layout of text."
                }
              ]
            }
          ]
        }
