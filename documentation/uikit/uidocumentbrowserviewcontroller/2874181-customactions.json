        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, this property contains an empty array. Assign an array of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentbrowseraction"
            },
            {
              "type": "text",
              "text": " objects to add custom document browser actions."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Document browser actions can be accessed in two ways:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "strong",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Navigation bar"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " actions appear in the Navigation bar when the user places the browser into the Select mode."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "strong",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Menu"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " actions appear when the user long presses on a document or folder."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": " When triggered, these actions are passed the URLs of the currently selected items."
            }
          ]
        }
      ]
    }
