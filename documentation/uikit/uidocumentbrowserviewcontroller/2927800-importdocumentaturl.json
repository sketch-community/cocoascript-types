    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method to import a document into the same file provider and directory as an existing document. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, to duplicate a document that's already managed by a file provider:"
            }
          ]
        },
        {
          "type": "orderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Create a duplicate of the original file in the user's temporary directory. Be sure to give it a unique name."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Call "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/uikit/uidocumentbrowserviewcontroller/2927800-importdocumentaturl"
                    },
                    {
                      "type": "text",
                      "text": ", passing in the temporary file's URL as the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "documentURL"
                    },
                    {
                      "type": "text",
                      "text": " parameter and the original file's URL as the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "neighborURL"
                    },
                    {
                      "type": "text",
                      "text": " parameter."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The system imports the duplicate into the same file provider (the local file provider, the iCloud file provider, or a third-party file provider) and the same directory as the original file."
            }
          ]
        }
      ]
    }
