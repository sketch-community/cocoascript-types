        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/nslayoutconstraint"
            },
            {
              "type": "text",
              "text": " object that defines the attribute represented by this layout anchor as equal to the attribute represented by the "
            },
            {
              "type": "codeVoice",
              "code": "anchor"
            },
            {
              "type": "text",
              "text": " parameter multiplied by the "
            },
