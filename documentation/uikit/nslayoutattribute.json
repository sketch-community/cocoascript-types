      "abstract": [
        {
          "type": "text",
          "text": "A placeholder value that is used to indicate that the constraint’s second item and second attribute are not used in any calculations. Use this value when creating a constraint that assigns a constant to an attribute. For example, "
        },
        {
          "type": "codeVoice",
          "code": "item1.height >= 40"
        },
        {
          "type": "text",
          "text": ". If a constraint only has one item, set the second item to "
        },
        {
          "type": "codeVoice",
          "code": "nil"
        },
        {
          "type": "text",
          "text": ", and set the second attribute to "
        },
        {
          "type": "codeVoice",
          "code": "NSLayoutAttributeNotAnAttribute"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
