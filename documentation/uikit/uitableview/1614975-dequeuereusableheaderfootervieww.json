              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string identifying the header or footer view to be reused. This parameter must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
