      "parameters": [
        {
          "name": "cellClass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The class of a cell that you want to use in the table (must be a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewcell"
                },
                {
                  "type": "text",
                  "text": " subclass)."
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The reuse identifier for the cell. This parameter must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " and must not be an empty string. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Prior to dequeueing any cells, call this method or the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableview/1614937-registernib"
            },
            {
              "type": "text",
              "text": " method to tell the table view how to create new cells. If a cell of the specified type is not currently in a reuse queue, the table view uses the provided information to create a new cell object automatically."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you previously registered a class or nib file with the same reuse identifier, the class you specify in the "
            },
            {
              "type": "codeVoice",
              "code": "cellClass"
            },
            {
              "type": "text",
              "text": " parameter replaces the old entry. You may specify "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " for "
            },
            {
              "type": "codeVoice",
              "code": "cellClass"
            },
            {
              "type": "text",
              "text": " if you want to unregister the class from the specified reuse identifier. "
            }
          ]
        }
      ]
    }
