    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Table views with a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableview/1614913-style"
            },
            {
              "type": "text",
              "text": " of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewstyle/uitableviewstyleplain"
            },
            {
              "type": "text",
              "text": " can display filler rows, empty rows that appear below the last row when there isn’t enough content to fill the table view. Set this property to adjust the height of each filler row:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Set "
                    },
                    {
                      "type": "codeVoice",
                      "code": "0.0"
                    },
                    {
                      "type": "text",
                      "text": " to not display filler rows. This behavior is the default in iOS 15 and later."
                    }
                  ]
                },
                {
                  "type": "paragraph",
                  "inlineContent": []
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Set "
                    },
                    {
                      "type": "reference",
                      "isActive": false,
                      "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewautomaticdimension"
                    },
                    {
                      "type": "text",
                      "text": " to display filler rows that use an automatic height, matching the height of the last row in the table view. This behavior is the default in versions of iOS earlier than iOS 15."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Set any other positive value to display filler rows of that specified height."
                    }
                  ]
                },
                {
                  "type": "paragraph",
                  "inlineContent": []
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A table view with a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableview/1614913-style"
            },
            {
              "type": "text",
              "text": " other than "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewstyle/uitableviewstyleplain"
            },
            {
              "type": "text",
              "text": " doesn’t show filler rows, so it ignores any value other than "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " for this property. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
