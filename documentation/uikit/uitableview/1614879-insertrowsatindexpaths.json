      "parameters": [
        {
          "name": "indexPaths",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of index path objects, each representing a row index and section index that together identify a row in the table view."
                }
              ]
            }
          ]
        },
        {
          "name": "animation",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A constant that either specifies the kind of animation to perform when inserting the cell or requests no animation. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewrowanimation"
                },
                {
                  "type": "text",
                  "text": " for descriptions of the constants."
                }
              ]
            }
          ]
        }
      ]
