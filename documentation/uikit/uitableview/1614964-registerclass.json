      "parameters": [
        {
          "name": "aClass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The class of the header or footer view that you want to register. You must specify either "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewheaderfooterview"
                },
                {
                  "type": "text",
                  "text": " or a subclass of it."
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The reuse identifier for the header or footer view. This parameter must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " and must not be an empty string. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Before dequeueing any header or footer views, call this method or the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableview/1614921-registernib"
            },
            {
              "type": "text",
              "text": " method to tell the table view how to create new instances of your views. If a view of the specified type is not currently in a reuse queue, the table view uses the provided information to create a one automatically."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you previously registered a class or nib file with the same reuse identifier, the class you specify in the "
            },
            {
              "type": "codeVoice",
              "code": "aClass"
            },
            {
              "type": "text",
              "text": " parameter replaces the old entry. You may specify "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " for "
            },
            {
              "type": "codeVoice",
              "code": "aClass"
            },
            {
              "type": "text",
              "text": " if you want to unregister the class from the specified reuse identifier. "
            }
          ]
        }
      ]
    }
