      "abstract": [
        {
          "type": "text",
          "text": "A simple style for a cell with a text label (black and left-aligned) and an optional image view. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uitableviewcellstyle/uitableviewcellstylevalue1": {
      "title": "UITableViewCellStyleValue1",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewcellstyle/uitableviewcellstylevalue1",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uitableviewcellstyle/uitableviewcellstylevalue1",
      "abstract": [
        {
          "type": "text",
          "text": "A style for a cell with a label on the left side of the cell with left-aligned and black text; on the right side is a label  that has smaller blue text and is right-aligned. The Settings application uses cells in this style."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uitableviewcellstyle/uitableviewcellstylevalue2": {
      "title": "UITableViewCellStyleValue2",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewcellstyle/uitableviewcellstylevalue2",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uitableviewcellstyle/uitableviewcellstylevalue2",
      "abstract": [
        {
          "type": "text",
          "text": "A style for a cell with a label on the left side of the cell with text that is right-aligned and blue; on the right side of the cell is another label with smaller text that is left-aligned and black. The Phone/Contacts application uses cells in this style."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uitableviewcellstyle/uitableviewcellstylesubtitle": {
      "title": "UITableViewCellStyleSubtitle",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewcellstyle/uitableviewcellstylesubtitle",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uitableviewcellstyle/uitableviewcellstylesubtitle",
      "abstract": [
        {
          "type": "text",
          "text": "A style for a cell with a left-aligned label across the top and a left-aligned label below it in smaller gray text."
        }
      ]
    },
