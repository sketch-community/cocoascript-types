    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "animation",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A block containing the animations to add to the animator object. This block has no return value and takes no parameters. "
                }
              ]
            }
          ]
        },
        {
          "name": "delayFactor",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The factor to use for delaying the start of the animations. The value must be between "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "1.0"
                },
                {
                  "type": "text",
                  "text": ". Multiply this value by the animator’s remaining duration to determine the actual delay in seconds. For example, if the value "
                },
                {
                  "type": "codeVoice",
                  "code": "0.5"
                },
                {
                  "type": "text",
                  "text": " and the animator’s duration is "
                },
                {
                  "type": "codeVoice",
                  "code": "2.0"
                },
                {
                  "type": "text",
                  "text": ", delay the start of the animations by one second. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method to add new animation blocks to your custom animator object. The animations in the new block should run alongside any previously configured animations, starting after the specified delay and finishing at the same time as any original animations. Your implementation must be able to handle multiple calls to this method."
            }
          ]
        }
      ]
    }
