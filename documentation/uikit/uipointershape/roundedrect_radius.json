        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you don't specify a radius, the rounded rectangle uses the "
            },
            {
              "type": "codeVoice",
              "code": "defaultCornerRadius"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If used alongside a content effect, this rectangle must be in the view coordinate space of the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uipointereffect/3543901-preview"
                },
                {
                  "type": "text",
                  "text": ". Otherwise, it’s centered around the pointer's current location, and the rectangle's origin is interpreted as an offset."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        }
      ]
