        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This value represents the bounds of every Core Graphics context that the associated graphics renderer creates."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the graphics renderer itself creates a format object, the bounds are set to those provided to the renderer as part of the initializer."
            }
          ]
        }
      ]
    }
