          "inlineContent": [
            {
              "type": "text",
              "text": "Invalidation of an app pasteboard frees up all resources used by it. Once a pasteboard is invalidated, you cannot use the it; "
            },
            {
              "type": "codeVoice",
              "code": "UIPasteboard"
            },
            {
              "type": "text",
              "text": " ignores any calls to it. The method has no effect if called with the name of a system pasteboard."
            }
          ]
