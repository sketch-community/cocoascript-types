              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string or string constant that identifies (or should identify) the pasteboard. To create a pasteboard with a "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " name, specify a "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " value for this parameter."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": []
            }
          ]
        },
        {
          "name": "create",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value that specifies whether to create the pasteboard if it doesn’t already exist. Specify "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/swift/false"
                },
                {
                  "type": "text",
                  "text": " for system pasteboards or if you want to use an existing app pasteboard."
                }
              ]
            }
          ]
        }
      ]
