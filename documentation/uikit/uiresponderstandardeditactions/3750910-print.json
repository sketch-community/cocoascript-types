    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Implement this method on the responder responsible for printing the contents of the window scene; for instance, the root view controller of a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiwindow"
            },
            {
              "type": "text",
              "text": ". In your implementation, prepare the print job and present an instance of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiprintinteractioncontroller"
            },
            {
              "type": "text",
              "text": " to show a Print dialog."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)print:(id)sender {",
            "    UIPrintInfo *info = UIPrintInfo.printInfo;",
            "    info.outputType = UIPrintInfoOutputPhoto;",
            "    info.orientation = UIPrintInfoOrientationPortrait;",
            "    info.jobName = _modelItem.title;",
            "    ",
            "    UIPrintInteractionController *printInteractionController = [[UIPrintInteractionController alloc] init];",
            "    printInteractionController.printInfo = info;",
            "    printInteractionController.printingItem = _modelItem.image;",
            "    ",
            "    UIPrintInteractionCompletionHandler completionHandler = ^(UIPrintInteractionController *printController, BOOL completed, NSError *error) {",
            "        if (error != nil) {",
            "            NSLog(@\"Print failed due to an error in domain %@ with error code %lu.\", error.domain, (long)error.code);",
            "        }",
            "    };",
            "    ",
            "    if (self.traitCollection.userInterfaceIdiom == UIUserInterfaceIdiomPad) {",
            "        UIBarButtonItem *printButton = self.navigationItem.rightBarButtonItem;",
            "        [printInteractionController presentFromBarButtonItem:printButton animated:YES completionHandler:completionHandler];",
            "    } else {",
            "        [printInteractionController presentAnimated:YES completionHandler: completionHandler];",
            "    }",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "3807683",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If your app includes the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/bundleresources/information_property_list/uiapplicationsupportsprintcommand"
            },
            {
              "type": "text",
              "text": " key in its "
            },
            {
              "type": "codeVoice",
              "code": "Info.plist"
            },
            {
              "type": "text",
              "text": " file, people can print from your app using the keyboard shortcut Command-P, which calls "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiresponderstandardeditactions/3750910-print"
            },
            {
              "type": "text",
              "text": ". You can also set "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiresponderstandardeditactions/3750910-print"
            },
            {
              "type": "text",
              "text": " as the action on other print-related controls such as a print button on a toolbar."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For more information about printing from your app, see "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/printing"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
