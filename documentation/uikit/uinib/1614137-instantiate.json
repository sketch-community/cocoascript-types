        {
          "name": "ownerOrNil",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object to use as the owner of the nib file. If the nib file has an owner, you must specify a valid object for this parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "optionsOrNil",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary containing the options to use when opening the nib file. For a list of available keys for this dictionary, see "
                },
                {
                  "type": "codeVoice",
                  "code": "NSBundle UIKit Additions"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
