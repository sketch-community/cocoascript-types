            {
              "type": "codeVoice",
              "code": "UITableViewCell"
            },
            {
              "type": "text",
              "text": " creates the image-view object when you create the cell. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property is mutually exclusive with a content configuration. Setting a non-"
            },
