    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When you call this method with the value of "
            },
            {
              "type": "codeVoice",
              "code": "editing"
            },
            {
              "type": "text",
              "text": " set to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ", and the "
            },
            {
              "type": "codeVoice",
              "code": "UITableViewCell"
            },
            {
              "type": "text",
              "text": " object is configured to have controls, the cell shows an insertion (green plus) or deletion control (red minus) on the left side of each cell and a reordering control on the right side. This method is called on each visible cell when the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitableview/1614876-setediting"
            },
            {
              "type": "text",
              "text": " method of "
            },
            {
              "type": "codeVoice",
              "code": "UITableView"
            },
            {
              "type": "text",
              "text": " is invoked. Calling this method with "
            },
            {
              "type": "codeVoice",
              "code": "editing"
            },
            {
              "type": "text",
              "text": " set to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " removes the controls from the cell."
            }
          ]
        }
      ]
    }
