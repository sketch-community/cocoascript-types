          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A target drag preview to show during the drop animation, or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " to show the default preview."
            }
