{
  "abstract": [
    {
      "type": "text",
      "text": "Center items in the available space. With this option, the tab bar uses the "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/uikit/uitabbar/1623465-itemwidth"
    },
    {
      "type": "text",
      "text": " and "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/uikit/uitabbar/1623446-itemspacing"
    },
    {
      "type": "text",
      "text": " properties to set the width of items and the spacing between items, positioning those items in the center of the available space When the "
    },
    {
      "type": "reference",
      "isActive": false,
      "identifier": "doc://com.apple.documentation/documentation/uikit/uitabbaritempositioning/uitabbaritempositioningautomatic"
    },
    {
      "type": "text",
      "text": " option is selected, the tab bar uses this behavior in horizontally regular environments. "
    }
