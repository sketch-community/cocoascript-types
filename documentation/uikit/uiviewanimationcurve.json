    "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeaseinout": {
      "title": "UIViewAnimationCurveEaseInOut",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeaseinout",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeaseinout",
      "abstract": [
        {
          "type": "text",
          "text": "An ease-in ease-out curve causes the animation to begin slowly, accelerate through the middle of its duration, and then slow again before completing. This is the default curve for most animations."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeasein": {
      "title": "UIViewAnimationCurveEaseIn",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeasein",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeasein",
      "abstract": [
        {
          "type": "text",
          "text": "An ease-in curve causes the animation to begin slowly, and then speed up as it progresses."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeaseout": {
      "title": "UIViewAnimationCurveEaseOut",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeaseout",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimationcurve/uiviewanimationcurveeaseout",
      "abstract": [
        {
          "type": "text",
          "text": "An ease-out curve causes the animation to begin quickly, and then slow down as it completes."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurvelinear": {
      "title": "UIViewAnimationCurveLinear",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimationcurve/uiviewanimationcurvelinear",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimationcurve/uiviewanimationcurvelinear",
      "abstract": [
        {
          "type": "text",
          "text": "A linear animation curve causes an animation to occur evenly over its duration."
        }
      ]
