          "inlineContent": [
            {
              "type": "text",
              "text": "This property determines whether the regular or highlighted images are used. When "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview/1621060-highlighted"
            },
            {
              "type": "text",
              "text": " is set to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ", a non-animated image will use the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview/1621066-highlightedimage"
            },
            {
              "type": "text",
              "text": " property and an animated image will use the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview/1621065-highlightedanimationimages"
            },
            {
              "type": "text",
              "text": ". If both of those properties are set to "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " or if "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview/1621060-highlighted"
            },
            {
              "type": "text",
              "text": " is set to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": ", it will use the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview/1621069-image"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview/1621068-animationimages"
            },
            {
              "type": "text",
