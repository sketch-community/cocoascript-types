      "parameters": [
        {
          "name": "touches",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A set of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uitouch"
                },
                {
                  "type": "text",
                  "text": " instances in the event represented by "
                },
                {
                  "type": "codeVoice",
                  "code": "event"
                },
                {
                  "type": "text",
                  "text": " that represent the touches in the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uitouchphase/uitouchphasebegan"
                },
