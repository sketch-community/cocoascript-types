            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitouchtype"
            },
            {
              "type": "text",
              "text": " enumeration in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitouch"
            },
            {
              "type": "text",
              "text": ". The default value of this property contains all touch types. "
            }
          ]
