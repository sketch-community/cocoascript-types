                },
                {
                  "type": "codeVoice",
                  "code": "Ö"
                },
                {
                  "type": "text",
                  "text": "\", and if "
                },
                {
                  "type": "codeVoice",
                  "code": "charRange"
                },
                {
                  "type": "text",
                  "text": " encloses only the first or second character, then "
                },
                {
                  "type": "codeVoice",
                  "code": "actualCharRange"
                },
                {
                  "type": "text",
                  "text": " is set to enclose both characters."
