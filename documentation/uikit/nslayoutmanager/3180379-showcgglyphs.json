          "name": "glyphs",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The glyphs to draw, which may include embedded "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " bytes."
                }
              ]
            }
          ]
        },
        {
          "name": "positions",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The positions at which to draw the glyphs in the user space coordinate system."
                }
              ]
            }
          ]
        },
        {
          "name": "glyphCount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of glyphs to draw."
                }
              ]
            }
          ]
        },
        {
          "name": "font",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The font to apply to the graphics state. This value can be different from the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/nsfontattributename"
                },
                {
                  "type": "text",
                  "text": " value in the "
                },
                {
                  "type": "codeVoice",
                  "code": "attributes"
                },
                {
                  "type": "text",
                  "text": " argument because of various font substitutions that the system automatically executes."
                }
              ]
            }
          ]
        },
        {
          "name": "textMatrix",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The affine transform mapping the text space coordinate system to the user space coordinate system. The "
                },
                {
                  "type": "codeVoice",
                  "code": "tx"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "ty"
                },
                {
                  "type": "text",
                  "text": " components of "
                },
                {
                  "type": "codeVoice",
                  "code": "textMatrix"
                },
                {
                  "type": "text",
                  "text": " are ignored because Quartz overrides them with the glyph positions."
                }
              ]
            }
          ]
        },
