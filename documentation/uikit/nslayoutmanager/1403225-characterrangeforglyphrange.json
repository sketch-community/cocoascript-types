                {
                  "type": "codeVoice",
                  "code": "Ö"
                },
                {
                  "type": "text",
                  "text": "” and the glyph cache contains the two atomic glyphs “"
                },
                {
                  "type": "codeVoice",
                  "code": "O"
                },
                {
                  "type": "text",
                  "text": "” and “"
                },
                {
                  "type": "codeVoice",
                  "code": "¨"
                },
                {
                  "type": "text",
                  "text": "”, and if "
                },
                {
                  "type": "codeVoice",
                  "code": "glyphRange"
                },
                {
                  "type": "text",
                  "text": " encloses only the first or second glyph, then "
                },
                {
                  "type": "codeVoice",
                  "code": "actualGlyphRange"
                },
                {
                  "type": "text",
                  "text": " is set to enclose both glyphs."
                }
              ]
            }
          ]
        }
      ]
    },
