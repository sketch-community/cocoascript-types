      "parameters": [
        {
          "name": "leftCapWidth",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value to use for the left cap width. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " if you want the entire image to be horizontally stretchable. For a discussion of how a non-zero value affects the image, see the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uiimage/1624148-leftcapwidth"
                },
                {
                  "type": "text",
                  "text": " property."
                }
              ]
            }
          ]
        },
        {
          "name": "topCapHeight",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value to use for the top cap height. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " if you want the entire image to be vertically stretchable. For a discussion of how a non-zero value affects the image, see the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uiimage/1624116-topcapheight"
                },
                {
                  "type": "text",
                  "text": " property."
                }
              ]
            }
          ]
        }
