        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Symbol images are vector-based images that you use for your app's iconography. The value of this property is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/swift/true"
            },
            {
              "type": "text",
              "text": " if the image is either a system-provided symbol image or a custom symbol image that you supplied in your asset catalog. The value is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/swift/false"
            },
            {
              "type": "text",
              "text": " for all other image types. "
            }
          ]
        }
      ]
    }
