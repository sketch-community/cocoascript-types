      "url": "/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributeunspecified",
      "abstract": [
        {
          "type": "text",
          "text": "The default value for views. The view is flipped when switching between left-to-right and right-to-left layouts."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributeplayback": {
      "title": "UISemanticContentAttributePlayback",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributeplayback",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributeplayback",
      "abstract": [
        {
          "type": "text",
          "text": "A view representing the playback controls, such as Play, Rewind, or Fast Forward buttons or playhead scrubbers. These views are not flipped when switching between left-to-right and right-to-left layouts."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributespatial": {
      "title": "UISemanticContentAttributeSpatial",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributespatial",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributespatial",
      "abstract": [
        {
          "type": "text",
          "text": "A view representing a directional control, such as a segment control for text alignment, or a D-pad control for a game.  These views are not flipped when switching between left-to-right and right-to-left layouts."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributeforcelefttoright": {
      "title": "UISemanticContentAttributeForceLeftToRight",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uisemanticcontentattribute/uisemanticcontentattributeforcelefttoright",
