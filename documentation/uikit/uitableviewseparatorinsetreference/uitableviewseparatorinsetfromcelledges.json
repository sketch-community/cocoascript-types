        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When this style is active, setting the left and right insets to "
            },
            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " would result in a separator to extend from the entire distance between the left and right edges of the cell. Setting the insets to other values would inset the separator by the specified amount. "
            }
          ]
        }
      ]
