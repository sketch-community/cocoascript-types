          "name": "title",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The title of the menu."
                }
              ]
            }
          ]
        },
        {
          "name": "image",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The image to display next to the menu's title. "
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The unique identifier for the menu. When creating standard menus for your app, specify an appropriate constant defined in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uimenu/identifier"
                },
                {
                  "type": "text",
                  "text": ". For custom menus, specify a custom reverse domain name value or specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to let this method create a unique identifier for you. "
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Additional configuration options for the menu. For a list of possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uimenu/options"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "children",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The menu elements contained in the menu. Specify leaf menu elements using "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uiaction"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uicommand"
                },
                {
                  "type": "text",
                  "text": ", or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uikeycommand"
                },
                {
                  "type": "text",
                  "text": " objects, and specify submenus using "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uimenu"
                },
                {
                  "type": "text",
                  "text": " objects. You may specify an empty array if the menu has no child menu elements. "
                }
              ]
            }
          ]
        }
