          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The previous locations of the items. This parameter contains the last set of index paths reported by the collection view during the movement sequence. "
                }
              ]
            }
          ]
        },
