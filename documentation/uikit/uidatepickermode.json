      "title": "UIDatePickerModeTime",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidatepickermode/uidatepickermodetime",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidatepickermode/uidatepickermodetime",
      "abstract": [
        {
          "type": "text",
          "text": "A mode that displays the date in hours, minutes, and (optionally) an AM/PM designation. The exact items shown and their order depend upon the locale set. An example of this mode is [ 6 | 53 | PM ]."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidatepickermode/uidatepickermodedate": {
      "title": "UIDatePickerModeDate",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidatepickermode/uidatepickermodedate",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidatepickermode/uidatepickermodedate",
      "abstract": [
        {
          "type": "text",
          "text": "A mode that displays the date in months, days of the month, and years. The exact order of these items depends on the locale setting. An example of this mode is [ November | 15 | 2007 ]."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidatepickermode/uidatepickermodedateandtime": {
      "title": "UIDatePickerModeDateAndTime",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uidatepickermode/uidatepickermodedateandtime",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uidatepickermode/uidatepickermodedateandtime",
      "abstract": [
        {
          "type": "text",
          "text": "A mode that displays the date as unified day of the week, month, and day of the month values, plus hours, minutes, and (optionally) an AM/PM designation. The exact order and format of these items depends on the locale set. An example of this mode is [ Wed Nov 15 | 6 | 53 | PM ]."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uidatepickermode/uidatepickermodecountdowntimer": {
      "title": "UIDatePickerModeCountDownTimer",
