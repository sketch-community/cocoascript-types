    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "title",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string to display in the title area of the action sheet. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to display any text in the title area."
                }
              ]
            }
          ]
        },
        {
          "name": "delegate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The receiverâ€™s delegate object. Although this parameter may be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the delegate is used to respond to taps in the action sheet and should usually be provided."
                }
              ]
            }
          ]
        },
        {
          "name": "cancelButtonTitle",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The title of the cancel button. This button is added to the action sheet automatically and assigned an appropriate index, which is available from the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uiactionsheet/1622866-cancelbuttonindex"
                },
                {
                  "type": "text",
                  "text": " property. This button is displayed in black to indicate that it represents the cancel action. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want a cancel button or are presenting the action sheet on an iPad. "
                }
              ]
            }
          ]
        },
        {
          "name": "destructiveButtonTitle",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The title of the destructive button. This button is added to the action sheet automatically and assigned an appropriate index, which is available from the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uiactionsheet/1622863-destructivebuttonindex"
                },
                {
                  "type": "text",
                  "text": " property. This button is displayed in red to indicate that it represents a destructive behavior. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want a destructive button. "
                }
              ]
            }
          ]
        },
        {
          "name": "otherButtonTitles, ...",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The titles of any additional buttons you want to add. This parameter consists of a "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "-terminated, comma-separated list of strings. For example, to specify two additional buttons, you could specify the value "
                },
                {
                  "type": "codeVoice",
                  "code": "@\"Button 1\", @\"Button 2\", nil"
                },
                {
                  "type": "text",
                  "text": ".  "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A newly initialized action sheet."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The action sheet automatically sets the appearance of the destructive and cancel buttons. If the action sheet contains only one button, it does not apply the custom colors associated with the destructive and cancel buttons. "
            }
          ]
        }
      ]
