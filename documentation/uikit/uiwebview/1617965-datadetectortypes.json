        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this property to specify the types of data (phone numbers, HTTP links, email address, and so on) that should be automatically converted to clickable URLs in the web view. When clicked, the web view opens the app responsible for handling the URL type and passes it the URL."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "See the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uidatadetectortypes"
            },
            {
              "type": "text",
              "text": " enumeration for the types of data available for automatic detection."
            }
          ]
        }
      ]
