        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value on both iPad and iPhone is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ". To make media play automatically when loaded, set this property to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " and ensure the "
            },
            {
              "type": "codeVoice",
              "code": "<audio>"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "<video>"
            },
            {
              "type": "text",
              "text": " element you want to play has the "
            },
            {
              "type": "codeVoice",
              "code": "autoplay"
            },
            {
              "type": "text",
              "text": " attribute set."
            }
          ]
        }
