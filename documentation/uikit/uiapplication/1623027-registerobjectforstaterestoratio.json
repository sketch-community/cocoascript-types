              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object to be registered with the restoration archive. The object must adopt the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uistaterestoring"
                },
                {
                  "type": "text",
