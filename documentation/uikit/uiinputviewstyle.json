    "doc://com.apple.documentation/documentation/uikit/uiinputviewstyle/uiinputviewstyledefault": {
      "title": "UIInputViewStyleDefault",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiinputviewstyle/uiinputviewstyledefault",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiinputviewstyle/uiinputviewstyledefault",
      "abstract": [
        {
          "type": "text",
          "text": "Apply blur behaviors to the view so that it looks like it belongs with the keyboard. Do not apply tinting effects. This style is intended for input views that attach to the top of the keyboard and have a similar theme but that do not look like the keyboard itself. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiinputviewstyle/uiinputviewstylekeyboard": {
      "title": "UIInputViewStyleKeyboard",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiinputviewstyle/uiinputviewstylekeyboard",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiinputviewstyle/uiinputviewstylekeyboard",
      "abstract": [
        {
          "type": "text",
          "text": "Apply both blur and tinting effects to the view to mimic the keyboard background. This style makes the input view match the keyboard appearance and can be used to replace or extend the keyboard."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiinputview/1619471-inputviewstyle": {
