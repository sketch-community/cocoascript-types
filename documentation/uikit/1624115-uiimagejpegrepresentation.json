          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The quality of the resulting JPEG image, expressed as a value from "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "codeVoice",
                  "code": "1.0"
                },
                {
                  "type": "text",
                  "text": ". The value "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": " represents the maximum compression (or lowest quality) while the value "
                },
                {
                  "type": "codeVoice",
                  "code": "1.0"
                },
                {
                  "type": "text",
                  "text": " represents the least compression (or best quality). "
                }
              ]
            }
          ]
        }
      ]
    },
