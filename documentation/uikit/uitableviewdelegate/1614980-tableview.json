              "inlineContent": [
                {
                  "type": "text",
                  "text": "The table view that is making this request."
                }
              ]
            }
          ]
        },
        {
          "name": "action",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A selector type identifying the "
                },
                {
                  "type": "codeVoice",
                  "code": "copy:"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "paste:"
                },
                {
                  "type": "text",
                  "text": " method of the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uiresponderstandardeditactions"
                },
                {
                  "type": "text",
                  "text": " informal protocol."
                }
              ]
            }
          ]
        },
        {
          "name": "indexPath",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index path of the row."
                }
              ]
            }
          ]
        },
        {
          "name": "sender",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object that initially sent the "
                },
                {
                  "type": "codeVoice",
                  "code": "copy:"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "paste:"
                },
                {
                  "type": "text",
                  "text": " message. "
                }
              ]
            }
