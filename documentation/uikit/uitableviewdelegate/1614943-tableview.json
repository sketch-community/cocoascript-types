        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An index path that confirms or alters the selected row. Return an "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsindexpath"
            },
            {
              "type": "text",
              "text": " other than "
            },
            {
              "type": "codeVoice",
              "code": "indexPath"
            },
            {
              "type": "text",
              "text": " if you want another cell to be selected. Return "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if you don't want the row selected."
            }
          ]
