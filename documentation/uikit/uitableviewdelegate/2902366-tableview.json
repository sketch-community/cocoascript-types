          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The swipe actions to display next to the leading edge of the row. Return "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if you want the table to display the default set of actions."
            }
          ]
        }
      ]
