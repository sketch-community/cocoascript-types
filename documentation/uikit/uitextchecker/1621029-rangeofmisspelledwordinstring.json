      "parameters": [
        {
          "name": "stringToCheck",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The string to check for misspelled words."
                }
              ]
            }
          ]
        },
        {
          "name": "range",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range of "
                },
                {
                  "type": "codeVoice",
                  "code": "stringToCheck"
                },
                {
                  "type": "text",
                  "text": " to check for a misspelled word. "
                }
              ]
            }
          ]
        },
        {
          "name": "startingOffset",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The offset within "
                },
                {
                  "type": "codeVoice",
                  "code": "range"
                },
                {
                  "type": "text",
                  "text": " of "
                },
                {
                  "type": "codeVoice",
                  "code": "stringToCheck"
                },
                {
                  "type": "text",
                  "text": " to begin checking for misspelled words."
                }
              ]
            }
          ]
        },
        {
          "name": "wrapFlag",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " to continue checking from the beginning of "
                },
                {
                  "type": "codeVoice",
                  "code": "range"
                },
                {
                  "type": "text",
                  "text": " if no misspelled word is found between "
                },
                {
                  "type": "codeVoice",
                  "code": "startingOffset"
                },
                {
                  "type": "text",
                  "text": " and the end of "
                },
                {
                  "type": "codeVoice",
                  "code": "range"
                },
                {
                  "type": "text",
                  "text": ". Specify "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": " to have spell-checking end at the end of "
                },
                {
                  "type": "codeVoice",
                  "code": "range"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "language",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The language of the of words to be checked for correct spelling. This string is a ISO 639-1 language code or a combined ISO 639-1 language code and ISO 3166-1 regional code (for example, "
                },
                {
                  "type": "codeVoice",
                  "code": "fr_FR"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The range of the first misspelled word encountered or "
            },
            {
              "type": "codeVoice",
              "code": "{NSNotFound, 0}"
            },
            {
              "type": "text",
              "text": " if none is found."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To search an entire string or a range within that string, call this method in a loop, resetting "
            },
            {
              "type": "codeVoice",
              "code": "startingOffset"
            },
            {
              "type": "text",
              "text": " from each returned range, until you reach the end of the string or specified range within the string. "
            }
          ]
        }
      ]
    }
