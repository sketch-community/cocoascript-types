      ],
      "parameters": [
        {
          "name": "mass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The effective mass of the animated property. This value must be greater than "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "stiffness",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The spring stiffness coefficient. Higher values correspond to a stiffer spring that yields a greater amount of force for moving objects. "
                }
              ]
            }
          ]
        },
        {
          "name": "damping",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The damping force to apply to the spring’s motion. This value is used to compute the damping ratio."
                }
              ]
            }
          ]
        },
        {
          "name": "velocity",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The target property’s initial rate of change at the start of the spring animation. If the target property doesn’t change, specify a vector with "
                },
                {
                  "type": "codeVoice",
                  "code": "dx"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "dy"
                },
                {
                  "type": "text",
                  "text": " components of "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "For details about how to calculate this velocity, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uispringtimingparameters/1649909-initialvelocity"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized spring timing parameters object or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the object could not be created. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The damping ratio for the spring is computed from the formula "
            },
            {
              "type": "codeVoice",
              "code": "damping"
            },
            {
              "type": "text",
              "text": " / (2 * sqrt ("
            },
            {
              "type": "codeVoice",
              "code": "stiffness"
            },
            {
              "type": "text",
              "text": " * "
            },
            {
              "type": "codeVoice",
              "code": "mass"
            },
            {
              "type": "text",
              "text": ")). "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
