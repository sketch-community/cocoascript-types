            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogress"
            },
            {
              "type": "text",
              "text": " object. (Progress updates are animated.) Set the property to "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " when you want to update the progress manually. The default value of this property is "
            },
