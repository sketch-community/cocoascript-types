{
  "abstract": [
    {
      "type": "text",
      "text": "The animator object is active and animations are either running or paused. An animator moves to this state after the first call to "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimating/1649786-startanimation"
    },
    {
      "type": "text",
      "text": " or "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimating/1649843-pauseanimation"
    },
    {
      "type": "text",
      "text": ". It stays in the active state until the animations finish naturally or until you call the "
    },
    {
      "type": "reference",
      "isActive": true,
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimating/1649750-stopanimation"
    },
    {
      "type": "text",
      "text": " method. "
    }
  ],
