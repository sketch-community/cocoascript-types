    "doc://com.apple.documentation/documentation/uikit/uiviewanimatingstate/uiviewanimatingstateinactive": {
      "title": "UIViewAnimatingStateInactive",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimatingstate/uiviewanimatingstateinactive",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimatingstate/uiviewanimatingstateinactive",
      "abstract": [
        {
          "type": "text",
          "text": "The animations have not yet started executing. This is the initial state of the animator object. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimatingstate/uiviewanimatingstateactive": {
      "title": "UIViewAnimatingStateActive",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimatingstate/uiviewanimatingstateactive",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimatingstate/uiviewanimatingstateactive",
      "abstract": [
        {
          "type": "text",
          "text": "The animator object is active and animations are either running or paused. An animator moves to this state after the first call to "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimating/1649786-startanimation"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimating/1649843-pauseanimation"
        },
        {
          "type": "text",
          "text": ". It stays in the active state until the animations finish naturally or until you call the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimating/1649750-stopanimation"
        },
        {
          "type": "text",
          "text": " method. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimatingstate/uiviewanimatingstatestopped": {
      "title": "UIViewAnimatingStateStopped",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimatingstate/uiviewanimatingstatestopped",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimatingstate/uiviewanimatingstatestopped",
      "abstract": [
        {
          "type": "text",
          "text": "The animation is stopped. Putting an animation into this state ends the animation and leaves any animatable properties at their current values, instead of updating them to their intended final values. An animation cannot be started while in this state."
        }
      ]
    },
