      "parameters": [
        {
          "name": "pickerView",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An object representing the picker view requesting the data."
                }
              ]
            }
          ]
        },
        {
          "name": "row",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A zero-indexed number identifying a row of "
                },
                {
                  "type": "codeVoice",
                  "code": "component"
                },
                {
                  "type": "text",
                  "text": ". Rows are numbered top-to-bottom."
                }
              ]
            }
          ]
        },
        {
          "name": "component",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A zero-indexed number identifying a component of "
                },
                {
                  "type": "codeVoice",
                  "code": "pickerView"
                },
                {
                  "type": "text",
                  "text": ". Components are numbered left-to-right."
                }
              ]
            }
          ]
        },
        {
          "name": "view",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A view object that was previously used for this row, but is now hidden and cached by the picker view."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A view object to use as the content of "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": ". The object can be any subclass of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiview"
            },
            {
              "type": "text",
              "text": ", such as "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uilabel"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimageview"
            },
            {
              "type": "text",
              "text": ", or even a custom view. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the previously used view (the "
            },
            {
              "type": "codeVoice",
              "code": "view"
            },
            {
              "type": "text",
              "text": " parameter) is adequate, return that. If you return a different view, the previously used view is released. The picker view centers the returned view in the rectangle for "
            },
            {
              "type": "codeVoice",
              "code": "row"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
