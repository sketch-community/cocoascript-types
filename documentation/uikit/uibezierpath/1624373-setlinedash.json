      ],
      "parameters": [
        {
          "name": "pattern",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A C-style array of floating point values that contains the lengths (measured in points) of the line segments and gaps in the pattern. The values in the array alternate, starting with the first line segment length, followed by the first gap length, followed by the second line segment length, and so on."
                }
              ]
            }
          ]
        },
        {
          "name": "count",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of values in "
                },
                {
                  "type": "codeVoice",
                  "code": "pattern"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "phase",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The offset at which to start drawing the pattern, measured in points along the dashed-line pattern. For example, a phase value of "
                },
                {
                  "type": "codeVoice",
                  "code": "6"
                },
                {
                  "type": "text",
                  "text": " for the pattern 5-2-3-2 would cause drawing to begin in the middle of the first gap."
                }
              ]
            }
          ]
        }
      ]
    }
