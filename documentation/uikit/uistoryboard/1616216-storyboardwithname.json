      "parameters": [
        {
          "name": "name",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the storyboard resource file without the filename extension. This method raises an exception if this parameter is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ".  "
                }
              ]
            }
          ]
        },
        {
          "name": "storyboardBundleOrNil",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The bundle containing the storyboard file and its related resources. If you specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", this method looks in the main bundle of the current application."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A storyboard object for the specified file. If no storyboard resource file matching "
            },
            {
              "type": "codeVoice",
              "code": "name"
            },
            {
              "type": "text",
              "text": " exists, an exception is thrown with description: "
            },
            {
              "type": "codeVoice",
              "code": "Could not find a storyboard named 'XXXXXX' in bundle..."
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method to retrieve the storyboard object containing the view controller graph you want to access. All of the resources associated with the storyboard must be in the bundle indicated by the "
            },
            {
              "type": "codeVoice",
              "code": "storyboardBundleOrNil"
            },
            {
              "type": "text",
              "text": " parameter. "
            }
          ]
        }
      ]
    }
