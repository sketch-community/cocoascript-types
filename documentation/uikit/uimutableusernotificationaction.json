      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiacceleration",
      "abstract": [
        {
          "type": "text",
          "text": "An instance of the "
        },
        {
          "type": "codeVoice",
          "code": "UIAcceleration"
        },
        {
          "type": "text",
          "text": " class, called an "
        },
        {
          "type": "emphasis",
          "inlineContent": [
            {
              "type": "text",
              "text": "acceleration event"
            }
          ]
        },
        {
          "type": "text",
          "text": ", represents immediate, three-dimensional acceleration data. To receive accelerometer events, register an application object as a delegate of the shared "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uiaccelerometer"
        },
        {
          "type": "text",
          "text": " object, as described in "
        },
