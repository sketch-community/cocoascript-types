              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A block that determines the appropriate color values based on the specified traits. This block returns a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/uikit/uicolor"
                },
                {
                  "type": "text",
                  "text": " object and takes a single parameter:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "traits"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The trait collection to use when generating the color information. Always use the traits in this collection, and not the traits of the current environment, when determining the color information."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": []
            }
          ]
