      "abstract": [
        {
          "type": "text",
          "text": "The end point of the animation. Use this constant when you want the final values for any animatable properties—that is, you want to refer to the values you specified in your animation blocks.  "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimatingposition/uiviewanimatingpositionstart": {
      "title": "UIViewAnimatingPositionStart",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimatingposition/uiviewanimatingpositionstart",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimatingposition/uiviewanimatingpositionstart",
      "abstract": [
        {
          "type": "text",
          "text": "The beginning of the animation. Use this constant when you want the starting values for any animatable properties—that is, the values of the properties before you applied any animations.  "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/uikit/uiviewanimatingposition/uiviewanimatingpositioncurrent": {
      "title": "UIViewAnimatingPositionCurrent",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/uikit/uiviewanimatingposition/uiviewanimatingpositioncurrent",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/uikit/uiviewanimatingposition/uiviewanimatingpositioncurrent",
      "abstract": [
        {
          "type": "text",
          "text": "The current position. Use this constant when you want the most recent value set by an animator object."
        }
      ]
    },
