        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
            },
            {
              "type": "text",
              "text": " for "
            },
            {
              "type": "codeVoice",
              "code": "title"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "image"
            },
            {
              "type": "text",
              "text": " to not display that element."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, the item displays the same image regardless of its selected state. To display a different image for the selected state, set its "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uitabbaritem/1617072-selectedimage"
            },
            {
              "type": "text",
              "text": " property. The item creates the images it displays from the alpha values in the source images. To prevent system tinting, use images with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uiimagerenderingmode/uiimagerenderingmodealwaysoriginal"
            },
            {
              "type": "text",
              "text": " rendering mode. The item clips any image that’s larger than its bounds."
            }
          ]
        }
      ]
    }
