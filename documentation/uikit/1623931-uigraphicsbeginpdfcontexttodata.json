            },
            {
              "kind": "identifier",
              "text": "UIGraphicsBeginPDFContextToData"
            },
            {
              "kind": "text",
              "text": "("
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:objc(cs)NSMutableData",
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmutabledata",
