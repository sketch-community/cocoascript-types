      "parameters": [
        {
          "name": "previewInteraction",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The preview interaction associated with the current user input"
                }
              ]
            }
          ]
        },
        {
          "name": "transitionProgress",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The progress through the preview phase of the transition. A "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgfloat"
                },
                {
                  "type": "text",
                  "text": " with a value from "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "codeVoice",
                  "code": "1"
                }
              ]
            }
          ]
        },
        {
          "name": "ended",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A "
                },
                {
                  "type": "codeVoice",
                  "code": "Boolean"
                },
                {
                  "type": "text",
                  "text": " whose value indicates whether the preview phase of the transition is complete"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method is called repeatedly during the preview phase of the preview interaction. Use the supplied "
            },
            {
              "type": "codeVoice",
              "code": "transitionProgress"
            },
            {
              "type": "text",
              "text": " parameter to update the UI to reflect the progress of the interaction. For example, the "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "peek"
                }
              ]
            },
            {
              "type": "text",
              "text": " effect in view controller preview transitions progressively blurs everything except the appropriate view."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "ended"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " throughout the preview phase and becomes "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " as the phase is completed. The preview interaction then transitions to the commit phase, so you should use this point to update the UI as required."
            }
          ]
        }
      ]
    }
