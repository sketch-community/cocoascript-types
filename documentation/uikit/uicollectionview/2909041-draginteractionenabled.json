        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value of this property is true on iPad and false on iPhone. Changing the value to true on iPhone makes it possible to drag content from the collection view to another app on iPhone and to receive content from other apps."
            }
          ]
        }
      ]
