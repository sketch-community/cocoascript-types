          "inlineContent": [
            {
              "type": "text",
              "text": "The default value in this property is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uicollectionviewreorderingcadence/uicollectionviewreorderingcadenceimmediate"
