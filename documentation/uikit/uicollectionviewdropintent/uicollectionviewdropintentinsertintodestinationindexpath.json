        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this option when the drop target has nested content. Dropping items with this proposal causes them to be added to the drop target's children. For example, if the drop target is a folder, use this option to add the items to the contents of that folder."
            }
          ]
        }
