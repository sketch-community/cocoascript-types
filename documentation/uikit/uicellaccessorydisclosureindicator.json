        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A disclosure indicator is a chevron that points in the trailing direction. This accessory appears on the trailing edge of the cell."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this cell accessory to indicate that users can tap on the cell to disclose additional content."
            }
          ]
        }
