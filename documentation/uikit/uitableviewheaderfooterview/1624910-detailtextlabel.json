  "deprecationSummary": [
    {
      "type": "paragraph",
      "inlineContent": [
        {
          "type": "text",
          "text": "Use a content configuration to manage the view’s text instead. Use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewheaderfooterview/3600597-defaultcontentconfiguration"
        },
        {
          "type": "text",
          "text": " to get a default list content configuration, set your secondary text to the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uilistcontentconfiguration/3600540-secondarytext"
        },
        {
          "type": "text",
          "text": " property of the configuration, and apply the configuration by setting it to the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/uikit/uitableviewheaderfooterview/3600596-contentconfiguration"
        },
        {
          "type": "text",
          "text": " property of the view."
        }
      ]
    }
