          "name": "view",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The view that contains the stylus’s touch. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to get the azimuth angle that is relative to the touch’s window."
                }
              ]
            }
          ]
        }
      ]
