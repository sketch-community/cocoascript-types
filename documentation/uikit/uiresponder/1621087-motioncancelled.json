              "inlineContent": [
                {
                  "type": "text",
                  "text": "An event-subtype constant indicating the kind of motion associated with "
                },
                {
                  "type": "codeVoice",
                  "code": "event"
                },
                {
                  "type": "text",
                  "text": ". A common motion is shaking, which is indicated by "
