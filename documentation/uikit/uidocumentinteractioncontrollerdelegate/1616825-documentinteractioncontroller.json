          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The bundle identifier of the application that is about to open the document. This value corresponds to the value in the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFBundleIdentifier"
                },
                {
                  "type": "text",
                  "text": " key of the application’s "
                },
                {
                  "type": "codeVoice",
                  "code": "Info.plist"
                },
                {
                  "type": "text",
                  "text": " file."
                }
