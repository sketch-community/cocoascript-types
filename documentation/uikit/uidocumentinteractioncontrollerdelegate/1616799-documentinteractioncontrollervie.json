        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The view controller to use when presenting the document preview. The return value must not be "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
