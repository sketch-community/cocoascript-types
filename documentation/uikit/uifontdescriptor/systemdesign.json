        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use these constants to specify a system-provided typeface design, such as SF Pro ("
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uifontdescriptor/systemdesign/3151799-default"
            },
            {
              "type": "text",
              "text": "), SF Mono ("
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uifontdescriptor/systemdesign/3152921-monospaced"
            },
            {
              "type": "text",
              "text": "), New York ("
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uifontdescriptor/systemdesign/3152922-serif"
            },
            {
              "type": "text",
              "text": "), or SF Compact Rounded ("
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/uikit/uifontdescriptor/systemdesign/3151800-rounded"
            },
            {
              "type": "text",
              "text": ")."
            }
          ]
        }
      ]
    }
