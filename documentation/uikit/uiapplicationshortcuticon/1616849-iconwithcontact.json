          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/contacts/cncontact"
                },
                {
                  "type": "text",
                  "text": " contact object to derive the icon from."
                }
              ]
            }
          ]
        }
      ]
