      "parameters": [
        {
          "name": "recordName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name that identifies the record. The string must contain only ASCII characters, must not exceed 255 characters, and must not start with an underscore. If you specify "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
                },
                {
                  "type": "text",
                  "text": " or an empty string for this parameter, the method throws an exception."
                }
              ]
            }
          ]
        },
        {
          "name": "zoneID",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The ID of the zone where you want to store the record. This parameter must not be "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized record ID object, or "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
            },
            {
              "type": "text",
              "text": " if CloudKit can’t create the object. "
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method when you create or search for records in a zone other than the default zone. The value in the "
            },
            {
              "type": "codeVoice",
              "code": "zoneID"
            },
            {
              "type": "text",
              "text": " parameter must represent a zone that already exists in the database. If the record zone doesn’t exist, save the corresponding "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkit/ckrecordzone"
            },
            {
              "type": "text",
              "text": " object to the database before attempting to save any "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/cloudkit/ckrecord"
            },
            {
              "type": "text",
              "text": " objects in that zone. "
            }
          ]
        }
      ]
    }
