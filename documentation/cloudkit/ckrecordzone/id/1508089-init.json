      "parameters": [
        {
          "name": "zoneName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name that identifies the record zone. Zone names consist of up to 255 ASCII characters, and don’t start with an underscore. To specify the default zone of the current database, use  "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/cloudkit/ckrecordzone/id/3003397-defaultzonename"
                },
                {
                  "type": "text",
                  "text": ". This parameter must not be "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
                },
                {
                  "type": "text",
                  "text": " or an empty string."
                }
              ]
            }
          ]
        },
        {
          "name": "ownerName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The user who creates the record zone. To specify the current user, use "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/cloudkit/ckcurrentuserdefaultname"
                },
                {
                  "type": "text",
                  "text": ". If you provide "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
                },
                {
                  "type": "text",
                  "text": " or an empty string for this parameter, the method throws an exception. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new record zone ID, or "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nil-2gl"
            },
            {
              "type": "text",
              "text": " if CloudKit can’t create the record zone ID. "
            }
          ]
        }
      ]
