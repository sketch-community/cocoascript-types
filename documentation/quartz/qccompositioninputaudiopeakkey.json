{
  "abstract": [
    {
      "type": "text",
      "text": "A number input port whose key is "
    },
    {
      "type": "codeVoice",
      "code": "inputAudioPeak"
    },
    {
      "type": "text",
      "text": ". The value must be in the "
    },
    {
      "type": "codeVoice",
      "code": "[0,1]"
    },
    {
      "type": "text",
      "text": " range as a mono signal with no decay applied."
    }
  ],
