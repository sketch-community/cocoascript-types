            {
              "type": "text",
              "text": "Once a "
            },
            {
              "type": "codeVoice",
              "code": "QLPreviewView"
            },
            {
              "type": "text",
              "text": " is closed, it won’t accept any more preview items. You only need to call this method if "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/qlpreviewview/1503457-shouldclosewithwindow"
            },
            {
              "type": "text",
              "text": " is set to "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": ". If you don’t close a "
            },
            {
              "type": "codeVoice",
              "code": "QLPreviewView"
            },
            {
              "type": "text",
              "text": " when you are done using it, your app will leak memory."
            }
