    "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolgraphicanimation": {
      "title": "QCCompositionProtocolGraphicAnimation",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolgraphicanimation",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qccompositionprotocolgraphicanimation",
      "abstract": [
        {
          "type": "text",
          "text": "A composition that renders a generic graphical animation. It has the option to use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputprimarycolorkey"
        },
        {
          "type": "text",
          "text": " for the primary color of the animation, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputsecondarycolorkey"
        },
        {
          "type": "text",
          "text": " for the secondary color of the animation, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputpacekey"
        },
        {
          "type": "text",
          "text": " for the global pace of the animation, and "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputpreviewmodekey"
        },
        {
          "type": "text",
          "text": " to indicate if the animation should run in lower-quality for preview purposes."
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolgraphictransition": {
      "title": "QCCompositionProtocolGraphicTransition",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolgraphictransition",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qccompositionprotocolgraphictransition",
      "abstract": [
        {
          "type": "text",
          "text": "A  composition that performs a transition between two images, using a transition time in range of "
        },
        {
          "type": "codeVoice",
          "code": "0"
        },
        {
          "type": "text",
          "text": " to "
        },
        {
          "type": "codeVoice",
          "code": "1"
        },
        {
          "type": "text",
          "text": ". A conforming composition must use the input keys "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputsourceimagekey"
        },
        {
          "type": "text",
          "text": " for the starting image and "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputdestinationimagekey"
        },
        {
          "type": "text",
          "text": " for the image to transition to. The composition can optionally use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputpreviewmodekey"
        },
        {
          "type": "text",
          "text": " to indicate if the animation should run in lower-quality for preview purposes."
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolimagefilter": {
      "title": "QCCompositionProtocolImageFilter",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolimagefilter",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qccompositionprotocolimagefilter",
      "abstract": [
        {
          "type": "text",
          "text": "A  composition that  applies an effect to a source image.  A conforming composition must use the input key "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputimagekey"
        },
        {
          "type": "text",
          "text": " for the source image and "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionoutputimagekey"
        },
        {
          "type": "text",
          "text": " for the output image. The composition can optionally use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputxkey"
        },
        {
          "type": "text",
          "text": " to specify the X position of the center point of the effect, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputykey"
        },
        {
          "type": "text",
          "text": " to specify the Y position of the center point of the effect, and"
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputpreviewmodekey"
        },
        {
          "type": "text",
          "text": " to indicate if the animation should run in lower-quality for preview purposes."
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolscreensaver": {
      "title": "QCCompositionProtocolScreenSaver",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolscreensaver",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qccompositionprotocolscreensaver",
      "abstract": [
        {
          "type": "text",
          "text": "A composition that can be used as a screen saver. The composition has the option to use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputscreenimagekey"
        },
        {
          "type": "text",
          "text": " for a screenshot image of the screen that the screen saver runs on, "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputpreviewmodekey"
        },
        {
          "type": "text",
          "text": " to indicate if the animation should run in lower-quality for preview purposes, and "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionoutputwebpageurlkey"
        },
        {
          "type": "text",
          "text": " for a  URL to open in the default web browser when screen saver exits (only allowed if screen saver password is disabled)."
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qccomposition/standard_protocols/qccompositionprotocolimagetransition": {
      "title": "QCCompositionProtocolImageTransition",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccomposition/standard_protocols/qccompositionprotocolimagetransition",
      "kind": "symbol",
      "role": "pseudoSymbol",
      "url": "/documentation/quartz/qccomposition/standard_protocols/qccompositionprotocolimagetransition",
      "abstract": [
        {
          "type": "text",
          "text": "A  composition that performs a transition between two images, using a parametric time value to drives the transition from start (at time "
        },
        {
          "type": "codeVoice",
          "code": "0"
        },
        {
          "type": "text",
          "text": ") to end (at time "
        },
        {
          "type": "codeVoice",
          "code": "1"
        },
        {
          "type": "text",
          "text": "). A conforming composition must use the input keys "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputimagekey"
        },
        {
          "type": "text",
          "text": " for the starting image and "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputdestinationimagekey"
        },
        {
          "type": "text",
          "text": " for the ending image. The composition can optionally use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputpreviewmodekey"
        },
        {
          "type": "text",
          "text": " to indicate if the animation should run in lower-quality for preview purposes."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolrssvisualizer": {
      "title": "QCCompositionProtocolRSSVisualizer",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolrssvisualizer",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qccompositionprotocolrssvisualizer",
      "abstract": [
        {
          "type": "text",
          "text": "A  composition that acts as a visualizer for an RSS feed.  A conforming composition must use the input key "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputrssfeedurlkey"
        },
        {
          "type": "text",
          "text": " for the URL to use for the RSS feed. It can optionally use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputrssarticledurationkey"
        },
        {
          "type": "text",
          "text": " to specify the duration of each feed article."
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolmusicvisualizer": {
      "title": "QCCompositionProtocolMusicVisualizer",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionprotocolmusicvisualizer",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qccompositionprotocolmusicvisualizer",
      "abstract": [
        {
          "type": "text",
          "text": "A  composition that acts as a visualizer for music.  A conforming composition must use the input key "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputaudiopeakkey"
        },
        {
          "type": "text",
          "text": " for the instantaneous audio peak and the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputaudiospectrumkey"
        },
        {
          "type": "text",
          "text": "  for the instantaneous audio spectrum. It can optionally use the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputtrackinfokey"
        },
        {
          "type": "text",
          "text": " to indicate it receives information about the current  track and the "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositioninputtracksignalkey"
        },
        {
          "type": "text",
          "text": " to indicate the start of a new track."
        }
      ],
      "deprecated": true
    },
