    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You access the internal settings of a custom patch through key-value coding (KVC). All the KVC keys that represent the internal settings of the custom patch must be listed in its "
            },
            {
              "type": "codeVoice",
              "code": "plugInKeys"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The view controller for a custom patch expects"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "the nib file "
                    },
                    {
                      "type": "codeVoice",
                      "code": "File's Owner"
                    },
                    {
                      "type": "text",
                      "text": "  class set to the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "QCPlugInViewController"
                    },
                    {
                      "type": "text",
                      "text": " class"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "the view outlet connected to the view that contains the editing controls"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The controls are bound to the "
            },
            {
              "type": "codeVoice",
              "code": "File's Owner"
            },
            {
              "type": "text",
              "text": "  as the target and "
            },
            {
              "type": "codeVoice",
              "code": "plugIn.XXX"
            },
            {
              "type": "text",
              "text": " as the model key path, where "
            },
            {
              "type": "codeVoice",
              "code": "XXX"
            },
            {
              "type": "text",
              "text": " is the KVC key for a given internal setting of the custom patch instance."
            }
          ]
        }
      ]
    }
