          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value to set for the input port. The input port must be at the root patch of the composition. The data type of the "
                },
                {
                  "type": "codeVoice",
                  "code": "value"
                },
                {
                  "type": "text",
                  "text": " argument must match the input port. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/quartz/qcportattributetypekey"
                },
                {
                  "type": "text",
                  "text": " for the data types accepted by a particular port type."
                }
              ]
            }
          ]
        },
        {
          "name": "key",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key associated with the input port of the composition. This method throws an exception if "
                },
                {
                  "type": "codeVoice",
                  "code": "key"
                },
                {
                  "type": "text",
                  "text": " is invalid."
                }
              ]
            }
          ]
        }
