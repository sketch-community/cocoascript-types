      "parameters": [
        {
          "name": "cameraDeviceView",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The camera device view that sent the message."
                }
              ]
            }
          ]
        },
        {
          "name": "file",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The camera file."
                }
              ]
            }
          ]
        },
        {
          "name": "url",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL to save the data."
                }
              ]
            }
          ]
        },
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The data."
                }
              ]
            }
          ]
        },
        {
          "name": "error",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Any errors encountered during downloading."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/ikcameradeviceview"
            },
            {
              "type": "text",
              "text": " "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/ikcameradeviceview/1503771-transfermode"
            },
            {
              "type": "text",
              "text": " property is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/ikcameradeviceviewtransfermode/ikcameradeviceviewtransfermodefilebased"
            },
            {
              "type": "text",
              "text": " then camera file will have been saved to  "
            },
            {
              "type": "codeVoice",
              "code": "url"
            },
            {
              "type": "text",
              "text": ". The URL will be a complete path to save the file, including a ‘sequence number’ if the file already exists."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/ikcameradeviceview/1503771-transfermode"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/ikcameradeviceviewtransfermode/ikcameradeviceviewtransfermodememorybased"
            },
            {
              "type": "text",
              "text": " then the "
            },
            {
              "type": "codeVoice",
              "code": "data"
            },
            {
              "type": "text",
              "text": " parameter contains the image data and can be handled as appropriate for your application."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In case of an error, the passed data (both "
            },
            {
              "type": "codeVoice",
              "code": "url"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "data"
            },
            {
              "type": "text",
              "text": ") will be "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "error"
            },
            {
              "type": "text",
              "text": " (which may come directly from the camera module / or the ImageCaptureCore framework) will describe why the download or save failed."
            }
          ]
        }
      ]
    }
