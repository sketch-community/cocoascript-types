    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "codeVoice",
              "code": "IKFilterBrowserPanel"
            },
            {
              "type": "text",
              "text": " object can be displayed as:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "a separate panel, that is, a utility window that floats on top of document windows"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "a modal dialog"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "a sheet, that is, a dialog that is attached to its parent window and must be dismissed by the user"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "a view that an application can insert into a custom user  interface"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "codeVoice",
              "code": "IKFilterBrowserPanel"
            },
            {
              "type": "text",
              "text": " object can be configured through a style mask to use either the default or brushed metal look for windows. The size and number of visible controls are specified through an options dictionary. An "
            },
            {
              "type": "codeVoice",
              "code": "IKFilterBrowserPanel"
            },
            {
              "type": "text",
              "text": " object communicates selection changes through notifications."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "IKFilterBrowserPanel"
            },
            {
              "type": "text",
              "text": " class allows the user to create filter collections that are stored with the "
            },
            {
              "type": "codeVoice",
              "code": "filterCollections"
            },
            {
              "type": "text",
              "text": " key in the "
            },
            {
              "type": "codeVoice",
              "code": "com.apple.CoreImageKit.plist"
            },
            {
              "type": "text",
              "text": " property list located in "
            },
            {
              "type": "codeVoice",
              "code": "~/Library/Preferences/"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
