    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "comes from the system-wide composition repository  ("
                    },
                    {
                      "type": "codeVoice",
                      "code": "/Library/Compositions"
                    },
                    {
                      "type": "text",
                      "text": " and"
                    },
                    {
                      "type": "codeVoice",
                      "code": " ~/Library/Compositions"
                    },
                    {
                      "type": "text",
                      "text": ") where it can be accessed by any application through the methods of the "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionrepository"
                    },
                    {
                      "type": "text",
                      "text": " class"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "is created from an arbitrary source (typically a file on disk) using one of its methods"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This class cannot be subclassed."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A "
            },
            {
              "type": "codeVoice",
              "code": "QCComposition"
            },
            {
              "type": "text",
              "text": " object has the following information associated with it and that you can obtain by using the appropriate method of the "
            },
            {
              "type": "codeVoice",
              "code": "QCComposition"
            },
            {
              "type": "text",
              "text": " class:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Attributes include the name and description of the composition, copyright information, and whether or not its provided by macOS (built-in)."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The protocols that the composition conforms to. A "
                    },
                    {
                      "type": "newTerm",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "composition protocol"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "  defines a set of required and optional input parameters and output results. "
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Many methods of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/qcrenderer"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/qccompositionlayer"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/qcview"
            },
            {
              "type": "text",
              "text": " classes take a "
            },
            {
              "type": "codeVoice",
              "code": "QCComposition"
            },
            {
              "type": "text",
              "text": " object as a parameter."
            }
          ]
        }
      ]
    }
