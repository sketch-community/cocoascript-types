      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The Quartz Composer engine calls this method when results are no longer being pulled from the  custom patch. You can optionally override this execution method to perform custom tasks at that time."
            }
          ]
        }
