    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An array of keys used for key-value coding (KVC) of the internal settings."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You must override this method if your patch provides a Settings pane.  This keys are used for automatic serialization of the internal settings and are also used by the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/qcpluginviewcontroller"
            },
            {
              "type": "text",
              "text": " instance for the Settings pane. The implementation is straightforward; the keys are strings that represent the instance variables used for the Settings pane. For example, the "
            },
            {
              "type": "codeVoice",
              "code": "plugInKeys"
            },
            {
              "type": "text",
              "text": " method for these instance variables:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "@property(ivar, byref) NSColor * systemColor;",
            "@property(ivar, byref) NSConfiguration * systemConfiguration;"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557118",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "are:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "+ (NSArray*) plugInKeys",
            "{",
            "    return [NSArray arrayWithObjects: @\"systemColor\",",
            "                                      @\"systemConfiguration\",",
            "                                      nil];",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2557122",
            "title": "Listing 2"
          }
        }
      ]
    }
