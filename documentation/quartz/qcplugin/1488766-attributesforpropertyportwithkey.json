          "inlineContent": [
            {
              "type": "text",
              "text": "A dictionary that contains key-value pairs for the port’s attributes. The keys must be one or more of the constants defined in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/qcplugin/input_and_output_port_attributes"
