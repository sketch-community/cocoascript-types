          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An opaque object , conforming to the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/quartz/qcplugincontext"
                },
                {
                  "type": "text",
                  "text": " protocol, that represents the execution context of the "
                },
                {
                  "type": "codeVoice",
                  "code": "QCPlugIn"
                },
                {
                  "type": "text",
                  "text": " object. Do not retain this object or use it outside of the scope of this method."
                }
              ]
            }
          ]
