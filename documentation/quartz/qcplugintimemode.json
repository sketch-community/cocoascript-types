    "doc://com.apple.documentation/documentation/quartz/qcplugintimemode/kqcplugintimemodenone": {
      "title": "kQCPlugInTimeModeNone",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qcplugintimemode/kqcplugintimemodenone",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qcplugintimemode/kqcplugintimemodenone",
      "abstract": [
        {
          "type": "text",
          "text": "No time dependency. The custom patch does not depend on time at all. (It does not use the "
        },
        {
          "type": "codeVoice",
          "code": "time"
        },
        {
          "type": "text",
          "text": "  parameter of the "
        },
        {
          "type": "codeVoice",
          "code": "execute:atTime:withArguments:"
        },
        {
          "type": "text",
          "text": " method.)"
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qcplugintimemode/kqcplugintimemodeidle": {
      "title": "kQCPlugInTimeModeIdle",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qcplugintimemode/kqcplugintimemodeidle",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qcplugintimemode/kqcplugintimemodeidle",
      "abstract": [
        {
          "type": "text",
          "text": "An idle time dependency. The custom patch does not depend on time but needs the system to execute it periodically. For example if the custom patch connects to a piece of hardware, to ensure that it pulls data from the hardware, you would set the custom patch time dependency to idle time mode. This time mode is  typically used with providers.]] "
        }
      ],
      "deprecated": true
    },
    "doc://com.apple.documentation/documentation/quartz/qcplugintimemode/kqcplugintimemodetimebase": {
      "title": "kQCPlugInTimeModeTimeBase",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/quartz/qcplugintimemode/kqcplugintimemodetimebase",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/quartz/qcplugintimemode/kqcplugintimemodetimebase",
      "abstract": [
        {
          "type": "text",
          "text": "A time base dependency. The custom patch does depend on time explicitly and has a time base defined by the system. (It uses the "
        },
        {
          "type": "codeVoice",
          "code": "time"
        },
        {
          "type": "text",
          "text": "  parameter of the "
        },
        {
          "type": "codeVoice",
          "code": "execute:atTime:withArguments:"
        },
        {
          "type": "text",
          "text": " method.)"
        }
      ],
      "deprecated": true
    },
