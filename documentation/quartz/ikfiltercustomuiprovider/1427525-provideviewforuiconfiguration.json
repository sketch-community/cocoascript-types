      "parameters": [
        {
          "name": "inUIConfiguration",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary that specifies the size of the controls.  Provide the key "
                },
                {
                  "type": "codeVoice",
                  "code": "IKUISizeFlavor"
                },
                {
                  "type": "text",
                  "text": " and one of the following values: "
                },
                {
                  "type": "codeVoice",
                  "code": "IKUISizeMini"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "IKUISizeSmall"
                },
                {
                  "type": "text",
                  "text": ", or  "
                },
                {
                  "type": "codeVoice",
                  "code": "IKUISizeRegular"
                },
                {
                  "type": "text",
                  "text": ". For more information on these constants, see "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "User Interface Options"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " in CIFilter Image Kit Additions."
                }
              ]
            }
          ]
        },
        {
          "name": "inKeys",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of the input keys for which you do "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "not"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " want to provide a user interface. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you want all input keys to be represented in the user interface."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/quartz/ikfilteruiview"
            },
            {
              "type": "text",
              "text": " object or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the filter is unable to provide a view. If "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", the Image Kit framework will attempt to provide a user interface."
            }
          ]
        }
      ]
    },
