      "role": "symbol",
      "url": "/documentation/coregraphics/cglinejoin/kcglinejoinbevel",
      "abstract": [
        {
          "type": "text",
          "text": "A join with a squared-off end. Core Graphics draws the line to extend beyond the endpoint of the path, for a distance of 1/2 the line’s width."
        }
      ]
    },
