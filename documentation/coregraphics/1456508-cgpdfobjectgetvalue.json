        {
          "name": "object",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A PDF object."
                }
              ]
            }
          ]
        },
        {
          "name": "type",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A PDF object type."
                }
              ]
            }
          ]
        },
        {
          "name": "value",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If the "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": " parameter is a PDF object of the specified type, then on return contains that object, otherwise the value is unspecified."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Returns "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " if the specified object is a PDF object of the specified type, otherwise "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The function gets the value of the "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " parameter. If the type of "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " is equal to the type specified, then:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "value"
                    },
                    {
                      "type": "text",
                      "text": " parameter is not a null pointer, then the value of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "object"
                    },
                    {
                      "type": "text",
                      "text": " is copied to "
                    },
                    {
                      "type": "codeVoice",
                      "code": "value"
                    },
                    {
                      "type": "text",
                      "text": ", and the function returns "
                    },
                    {
                      "type": "reference",
                      "isActive": false,
                      "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "value"
                    },
                    {
                      "type": "text",
                      "text": " parameter is a null pointer, then the function simply returns "
                    },
                    {
                      "type": "reference",
                      "isActive": false,
                      "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                    },
                    {
                      "type": "text",
                      "text": ". This allows you to test whether "
                    },
                    {
                      "type": "codeVoice",
                      "code": "object"
                    },
                    {
                      "type": "text",
                      "text": " is of the type specified."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the type of "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfobjecttype/kcgpdfobjecttypeinteger"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "type"
            },
            {
              "type": "text",
              "text": " is equal to "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfobjecttype/kcgpdfobjecttypereal"
            },
            {
              "type": "text",
              "text": ", then the value of "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " is converted to floating point, the result copied to "
            },
            {
              "type": "codeVoice",
              "code": "value"
            },
            {
              "type": "text",
              "text": ", and the function returns "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": ". If none of the preceding conditions is met, returns "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
