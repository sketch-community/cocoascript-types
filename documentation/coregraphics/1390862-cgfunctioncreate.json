      "parameters": [
        {
          "name": "info",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to user-defined storage for data that you want to pass to your callbacks. You need to make sure that the data persists for as long as itâ€™s needed, which can be beyond the scope in which the Core Graphics function is used. "
                }
              ]
            }
          ]
        },
        {
          "name": "domainDimension",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of inputs."
                }
              ]
            }
          ]
        },
        {
          "name": "domain",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of ("
                },
                {
                  "type": "codeVoice",
                  "code": "2*domainDimension"
                },
                {
                  "type": "text",
                  "text": ") floats used to specify the valid intervals of input values. For each k from "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "codeVoice",
                  "code": "(domainDimension - 1)"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "domain[2*k]"
                },
                {
                  "type": "text",
                  "text": " must be less than or equal to "
                },
                {
                  "type": "codeVoice",
                  "code": "domain[2*k+1]"
                },
                {
                  "type": "text",
                  "text": ", and the "
                },
                {
                  "type": "codeVoice",
                  "code": "k"
                },
                {
                  "type": "text",
                  "text": "th input value will be clipped to lie in the interval "
                },
                {
                  "type": "codeVoice",
                  "code": "domain[2*k] â‰¤ input[k] â‰¤ domain[2*k+1]"
                },
                {
                  "type": "text",
                  "text": ". If this parameter is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", then the input values are not clipped."
                }
              ]
            }
          ]
        },
        {
          "name": "rangeDimension",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of outputs."
                }
              ]
            }
          ]
        },
        {
          "name": "range",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "codeVoice",
                  "code": "(2*rangeDimension)"
                },
                {
                  "type": "text",
                  "text": " floats that specifies the valid intervals of output values. For each "
                },
                {
                  "type": "codeVoice",
                  "code": "k"
                },
                {
                  "type": "text",
                  "text": " from "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "codeVoice",
                  "code": "(rangeDimension - 1)"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "range[2*k]"
                },
                {
                  "type": "text",
                  "text": " must be less than or equal to "
                },
                {
                  "type": "codeVoice",
                  "code": "range[2*k+1]"
                },
                {
                  "type": "text",
                  "text": ", and the "
                },
                {
                  "type": "codeVoice",
                  "code": "k"
                },
                {
                  "type": "text",
                  "text": "th output value will be clipped to lie in the interval "
                },
                {
                  "type": "codeVoice",
                  "code": "range[2*k] â‰¤ output[k] â‰¤ range[2*k+1]"
                },
                {
                  "type": "text",
                  "text": ". \tIf this parameter is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", then the output values are not clipped."
                }
              ]
            }
          ]
        },
        {
          "name": "callbacks",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a callback function table. This table should contain pointers to the callbacks you provide to implement the semantics of this Core Graphics function.\t Core Graphics makes a copy of your table, so, for example, you could safely pass in a pointer to a structure on the stack."
                }
              ]
            }
          ]
        }
      ]
    },
