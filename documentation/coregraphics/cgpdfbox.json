    "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfmediabox": {
      "title": "kCGPDFMediaBox",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfmediabox",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgpdfbox/kcgpdfmediabox",
      "abstract": [
        {
          "type": "text",
          "text": "The page media box—a rectangle, expressed in default user space units, that defines the boundaries of the physical medium on which the page is intended to be displayed or printed"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfcropbox": {
      "title": "kCGPDFCropBox",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfcropbox",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgpdfbox/kcgpdfcropbox",
      "abstract": [
        {
          "type": "text",
          "text": "The page crop box—a rectangle, expressed in default user space units, that defines the visible region of default user space. When the page is displayed or printed, its contents are to be clipped to this rectangle."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfbleedbox": {
      "title": "kCGPDFBleedBox",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfbleedbox",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgpdfbox/kcgpdfbleedbox",
      "abstract": [
        {
          "type": "text",
          "text": "The page bleed box—a rectangle, expressed in default user space units, that defines the region to which the contents of the page should be clipped when output in a production environment."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdftrimbox": {
      "title": "kCGPDFTrimBox",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdftrimbox",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgpdfbox/kcgpdftrimbox",
      "abstract": [
        {
          "type": "text",
          "text": "The page trim box—a rectangle, expressed in default user space units, that defines the intended dimensions of the finished page after trimming."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfartbox": {
      "title": "kCGPDFArtBox",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox/kcgpdfartbox",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgpdfbox/kcgpdfartbox",
      "abstract": [
        {
          "type": "text",
          "text": "The page art box—a rectangle, expressed in default user space units, defining the extent of the page’s meaningful content (including potential white space) as intended by the page’s creator."
        }
      ]
    },
