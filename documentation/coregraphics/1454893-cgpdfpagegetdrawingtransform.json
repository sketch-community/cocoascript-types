        {
          "name": "page",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A PDF page."
                }
              ]
            }
          ]
        },
        {
          "name": "box",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A constant that specifies the type of box. For possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgpdfbox"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "rect",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Quartz rectangle."
                }
              ]
            }
          ]
        },
        {
          "name": "rotate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer, that must be a multiple of "
                },
                {
                  "type": "codeVoice",
                  "code": "90"
                },
                {
                  "type": "text",
                  "text": ", that specifies the angle by which the specified rectangle is rotated clockwise."
                }
              ]
            }
          ]
        },
        {
          "name": "preserveAspectRatio",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value that specifies whether or not the aspect ratio should be preserved. A value of "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " specifies that the aspect ratio should be preserved."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An affine transform that maps the box specified by the "
            },
            {
              "type": "codeVoice",
              "code": "box"
            },
            {
              "type": "text",
              "text": " parameter to the rectangle specified by the "
            },
            {
              "type": "codeVoice",
              "code": "rect"
            },
            {
              "type": "text",
              "text": " parameter."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Quartz constructs the affine transform as follows:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Computes the effective rectangle by intersecting the rectangle associated with "
                    },
                    {
                      "type": "codeVoice",
                      "code": "box"
                    },
                    {
                      "type": "text",
                      "text": " and the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "/MediaBox"
                    },
                    {
                      "type": "text",
                      "text": " entry of the specified page."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Rotates the effective rectangle according to the page’s "
                    },
                    {
                      "type": "codeVoice",
                      "code": "/Rotate"
                    },
                    {
                      "type": "text",
                      "text": " entry."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Centers the resulting rectangle on "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rect"
                    },
                    {
                      "type": "text",
                      "text": ".If the value of the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rotate"
                    },
                    {
                      "type": "text",
                      "text": " parameter is non-zero, then the rectangle is rotated clockwise by rotate degrees. The value of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rotate"
                    },
                    {
                      "type": "text",
                      "text": " must be a multiple of 90."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Scales the rectangle, if necessary, so that it coincides with the edges of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rect"
                    },
                    {
                      "type": "text",
                      "text": ". If the value of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "preserveAspectRatio"
                    },
                    {
                      "type": "text",
                      "text": " parameter is "
                    },
                    {
                      "type": "reference",
                      "isActive": false,
                      "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                    },
                    {
                      "type": "text",
                      "text": ", then the final rectangle coincides with the edges of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rect"
                    },
                    {
                      "type": "text",
                      "text": " only in the more restrictive dimension."
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
