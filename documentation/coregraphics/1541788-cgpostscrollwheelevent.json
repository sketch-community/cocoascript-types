      "parameters": [
        {
          "name": "wheelCount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of scrolling devices, up to a maximum of 3."
                }
              ]
            }
          ]
        },
        {
          "name": "wheel1",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A value that reflects the movement of the primary scrolling device on the mouse."
                }
              ]
            }
          ]
        },
        {
          "name": "...",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Up to two values that reflect the movements of the other scrolling devices on the mouse (if any)."
                }
              ]
            }
          ]
        }
      ]
    },
