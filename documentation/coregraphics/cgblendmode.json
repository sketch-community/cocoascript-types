    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolordodge": {
      "title": "kCGBlendModeColorDodge",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolordodge",
