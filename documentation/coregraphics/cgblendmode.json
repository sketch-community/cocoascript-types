      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodenormal",
      "abstract": [
        {
          "type": "text",
          "text": "Paints the source image samples over the background image samples."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodemultiply": {
      "title": "kCGBlendModeMultiply",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodemultiply",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodemultiply",
      "abstract": [
        {
          "type": "text",
          "text": "Multiplies the source image samples with the background image samples. This results in colors that are at least as dark as either of the two contributing sample colors."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodescreen": {
      "title": "kCGBlendModeScreen",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodescreen",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodescreen",
      "abstract": [
        {
          "type": "text",
          "text": "Multiplies the inverse of the source image samples with the inverse of the background image samples, resulting in colors that are at least as light as either of the two contributing sample colors."
        }
      ]
    },
