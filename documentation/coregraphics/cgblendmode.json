    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolor": {
      "title": "kCGBlendModeColor",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolor",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodecolor",
      "abstract": [
        {
          "type": "text",
          "text": "Uses the luminance values of the background with the hue and saturation values of the source image. This mode preserves the gray levels in the image. You can use this mode to color monochrome images or to tint color images."
        }
      ]
    },
    "devLink-1719815": {
