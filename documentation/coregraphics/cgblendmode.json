    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeexclusion": {
      "title": "kCGBlendModeExclusion",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeexclusion",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodeexclusion",
      "abstract": [
        {
          "type": "text",
          "text": "Produces an effect similar to that produced by "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedifference"
        },
        {
          "type": "text",
          "text": ", but with lower contrast. Source image sample values that are black don’t produce a change; white inverts the background color values."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodehue": {
      "title": "kCGBlendModeHue",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodehue",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodehue",
      "abstract": [
        {
          "type": "text",
          "text": "Uses the luminance and saturation values of the background with the hue of the source image."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesaturation": {
      "title": "kCGBlendModeSaturation",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesaturation",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodesaturation",
      "abstract": [
        {
          "type": "text",
          "text": "Uses the luminance and hue values of the background with the saturation of the source image. Areas of the background that have no saturation (that is, pure gray areas) don’t produce a change."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeluminosity": {
      "title": "kCGBlendModeLuminosity",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeluminosity",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodeluminosity",
      "abstract": [
        {
          "type": "text",
          "text": "Uses the hue and saturation of the background with the luminance of the source image. This mode creates an effect that is inverse to the effect created by "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolor"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeclear": {
      "title": "kCGBlendModeClear",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeclear",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodeclear",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = 0"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecopy": {
      "title": "kCGBlendModeCopy",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecopy",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodecopy",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesourcein": {
      "title": "kCGBlendModeSourceIn",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesourcein",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodesourcein",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S*Da"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesourceout": {
      "title": "kCGBlendModeSourceOut",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesourceout",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodesourceout",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S*(1 - Da)"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesourceatop": {
      "title": "kCGBlendModeSourceAtop",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodesourceatop",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodesourceatop",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S*Da + D*(1 - Sa)"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationover": {
      "title": "kCGBlendModeDestinationOver",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationover",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodedestinationover",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S*(1 - Da) + D"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationin": {
      "title": "kCGBlendModeDestinationIn",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationin",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodedestinationin",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = D*Sa"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationout": {
      "title": "kCGBlendModeDestinationOut",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationout",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodedestinationout",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = D*(1 - Sa)"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationatop": {
      "title": "kCGBlendModeDestinationAtop",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodedestinationatop",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodedestinationatop",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S*(1 - Da) + D*Sa"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodexor": {
      "title": "kCGBlendModeXOR",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodexor",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodexor",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = S*(1 - Da) + D*(1 - Sa)"
        },
        {
          "type": "text",
          "text": ". This XOR mode is only nominally related to the classical bitmap XOR operation, which is not supported by Core Graphics"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeplusdarker": {
      "title": "kCGBlendModePlusDarker",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodeplusdarker",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodeplusdarker",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = MAX(0, 1 - ((1 - D) + (1 - S)))"
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodepluslighter": {
      "title": "kCGBlendModePlusLighter",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodepluslighter",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodepluslighter",
      "abstract": [
        {
          "type": "codeVoice",
          "code": "R = MIN(1, S + D)"
        }
      ]
    },
