      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodecolordodge",
      "abstract": [
        {
          "type": "text",
          "text": "Brightens the background image samples to reflect the source image samples. Source image sample values that specify black do not produce a change."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolorburn": {
      "title": "kCGBlendModeColorBurn",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgblendmode/kcgblendmodecolorburn",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coregraphics/cgblendmode/kcgblendmodecolorburn",
      "abstract": [
        {
          "type": "text",
          "text": "Darkens the background image samples to reflect the source image samples. Source image sample values that specify white do not produce a change."
        }
      ]
    },
