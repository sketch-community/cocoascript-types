      "parameters": [
        {
          "name": "info",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to data of any type or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ". When Core Graphics calls the functions specified in the "
                },
                {
                  "type": "codeVoice",
                  "code": "callbacks"
                },
                {
                  "type": "text",
                  "text": " parameter, it sends each of the functions this pointer."
                }
              ]
            }
          ]
        },
        {
          "name": "size",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A value that specifies the number of bytes that the data provider contains."
                }
              ]
            }
          ]
        },
        {
          "name": "callbacks",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgdataprovider/cgdataproviderdirectaccesscallbacks"
                },
                {
                  "type": "text",
                  "text": " structure that specifies the callback functions you implement to handle the data provider’s basic memory management. For a complete description, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgdataprovider/cgdataproviderdirectaccesscallbacks"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
