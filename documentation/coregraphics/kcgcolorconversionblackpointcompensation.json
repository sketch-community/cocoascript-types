        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "ICC profiles specify how to convert the lightest level of white between color spaces, but they do not specify how black should be converted. To account for this, set a value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
            },
            {
              "type": "text",
              "text": " for this key when creating a color conversion with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/2118077-cgcolorconversioninfocreatefroml"
            },
            {
              "type": "text",
              "text": " function."
            }
          ]
        }
      ]
    }
