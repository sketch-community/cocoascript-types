        {
          "name": "pixelFormat",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The desired Core Media pixel format of the output frame data. The value must be one of the following:"
                }
              ]
            },
            {
              "type": "unorderedList",
              "items": [
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "'BGRA'"
                        },
                        {
                          "type": "text",
                          "text": ": Packed Little Endian ARGB8888"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "'l10r'"
                        },
                        {
                          "type": "text",
                          "text": ": Packed Little Endian ARGB2101010"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "'420v'"
                        },
                        {
                          "type": "text",
                          "text": ": 2-plane \"video\" range YCbCr 4:2:0"
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "codeVoice",
                          "code": "'420f'"
                        },
                        {
                          "type": "text",
                          "text": ": 2-plane \"full\" range YCbCr 4:2:0"
                        }
                      ]
                    }
                  ]
                }
              ]
