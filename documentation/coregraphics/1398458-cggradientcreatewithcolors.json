    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "space",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The color space to use for the gradient. You cannot use a pattern or indexed color space."
                }
              ]
            }
          ]
        },
        {
          "name": "colors",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A non-empty array of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgcolor"
                },
                {
                  "type": "text",
                  "text": " objects that should be in the color space specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "space"
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "codeVoice",
                  "code": "space"
                },
                {
                  "type": "text",
                  "text": " is not "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", each color will be converted (if necessary) to that color space and the gradient will drawn in that color space. Otherwise, each color will be converted to and drawn in the GenericRGB color space. "
                }
              ]
            }
          ]
        },
        {
          "name": "locations",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The location for each color provided in "
                },
                {
                  "type": "codeVoice",
                  "code": "colors"
                },
                {
                  "type": "text",
                  "text": "; each location must be a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgfloat"
                },
                {
                  "type": "text",
                  "text": " value in the range of "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " to "
                },
                {
                  "type": "codeVoice",
                  "code": "1"
                },
                {
                  "type": "text",
                  "text": ", inclusive. If "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "1"
                },
                {
                  "type": "text",
                  "text": " are not in the "
                },
                {
                  "type": "codeVoice",
                  "code": "locations"
                },
                {
                  "type": "text",
                  "text": " array, Quartz uses the colors provided that are closest to "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "1"
                },
                {
                  "type": "text",
                  "text": " for those locations. "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "codeVoice",
                  "code": "locations"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", the first color in "
                },
                {
                  "type": "codeVoice",
                  "code": "colors"
                },
                {
                  "type": "text",
                  "text": " is assigned to location "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ", the last color in "
                },
                {
                  "type": "codeVoice",
                  "code": "colors"
                },
                {
                  "type": "text",
                  "text": " is assigned to location "
                },
                {
                  "type": "codeVoice",
                  "code": "1"
                },
                {
                  "type": "text",
                  "text": ", and intervening colors are assigned locations that are at equal intervals in between."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "locations"
                },
                {
                  "type": "text",
                  "text": " array should contain the same number of items as the "
                },
                {
                  "type": "codeVoice",
                  "code": "colors"
                },
                {
                  "type": "text",
                  "text": " array."
                }
              ]
            }
          ]
        }
      ]
    },
