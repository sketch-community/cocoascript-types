    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "path",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The path to copy."
                }
              ]
            }
          ]
        },
        {
          "name": "transform",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an affine transformation matrix, or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if no transformation is needed. If specified, Core Graphics applies the transformation to elements of the converted path before adding them to the new path."
                }
              ]
            }
          ]
        },
        {
          "name": "phase",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A value that specifies how far into the dash pattern the line starts, in units of the user space. For example, passing a value of "
                },
                {
                  "type": "codeVoice",
                  "code": "3"
                },
                {
                  "type": "text",
                  "text": " means the line is drawn with the dash pattern starting at three units from its beginning. Passing a value of "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": " draws a line starting with the beginning of a dash pattern."
                }
              ]
            }
          ]
        },
        {
          "name": "lengths",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of values that specify the lengths of the painted segments and unpainted segments, respectively, of the dash pattern—or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " for no dash pattern."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "For example, passing an array with the values "
                },
                {
                  "type": "codeVoice",
                  "code": "[2,3]"
                },
                {
                  "type": "text",
                  "text": " sets a dash pattern that alternates between a 2-user-space-unit-long painted segment and a 3-user-space-unit-long unpainted segment. Passing the values "
                },
                {
                  "type": "codeVoice",
                  "code": "[1,3,4,2]"
                },
                {
                  "type": "text",
                  "text": " sets the pattern to a 1-unit painted segment, a 3-unit unpainted segment, a 4-unit painted segment, and a 2-unit unpainted segment."
                }
              ]
            }
          ]
        },
        {
          "name": "count",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If the "
                },
                {
                  "type": "codeVoice",
                  "code": "lengths"
                },
                {
                  "type": "text",
                  "text": " parameter specifies an array, pass the number of elements in the array. Otherwise, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new, immutable path. You are responsible for releasing this object."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The new path is created so that filling the new path draws the same pixels as stroking the original path with the specified dash parameters."
            }
          ]
        }
      ]
    }
