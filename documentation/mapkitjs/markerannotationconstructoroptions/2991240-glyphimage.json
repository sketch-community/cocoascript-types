      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The glyph image values should be object literals containing absolute or relative URLs to standard, 2x, and 3x Retina display assets. Only "
            },
            {
              "type": "codeVoice",
              "code": "\"1\""
            },
            {
              "type": "text",
              "text": " is required. The image should be 20 x 20 pixels. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default glyph image of a pin is used if "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/markerannotationconstructoroptions/2991240-glyphimage"
            },
            {
              "type": "text",
              "text": " is set to "
            },
            {
              "type": "codeVoice",
              "code": "null"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "undefined"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "codeVoice",
              "code": "\"\" "
            },
            {
              "type": "text",
              "text": "(empty). If you specify both a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/markerannotationconstructoroptions/2991240-glyphimage"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/markerannotationconstructoroptions/2991241-glyphtext"
            },
            {
              "type": "text",
              "text": ", the glyph image is ignored, and the glyph text is displayed."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
