        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/map/2973933-selectedannotation"
            },
            {
              "type": "text",
              "text": " is either:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/annotation"
                    },
                    {
                      "type": "text",
                      "text": " object, if one is selected, or"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "null"
                    },
                    {
                      "type": "text",
                      "text": ", if no annotations are selected."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An annotation is selected if its "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/annotation/2973832-selected"
            },
            {
              "type": "text",
              "text": " property is "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": ". To deselect all annotations, set this attribute to "
            },
            {
              "type": "codeVoice",
              "code": "null"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To select an annotation that is already part of the map, set this attribute to the desired annotation."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the selected annotation is removed from the map (as an effect of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/map/2973925-removeannotation"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/map/2973926-removeannotations"
            },
            {
              "type": "text",
              "text": ", or setting a new set of annotations with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/map/2973912-annotations"
            },
            {
              "type": "text",
              "text": " property), it is deselected before being removed."
            }
          ]
        }
