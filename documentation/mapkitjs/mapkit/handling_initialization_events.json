  "primaryContentSections": [
    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "mapkit"
            },
            {
              "type": "text",
              "text": " object emits two events to indicate the success or failure of a configuration operation. MapKit is configured when it initializes."
            }
          ]
        },
        {
          "type": "table",
          "header": "row",
          "rows": [
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Event"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Summary"
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "configuration-change"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "MapKit configuration changed due to either a successful initialization or a refresh. The event has the property "
                    },
                    {
                      "type": "codeVoice",
                      "code": "status"
                    },
                    {
                      "type": "text",
                      "text": " (String), which indicates the configuration change status."
                    }
                  ]
                },
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "Initialized"
                    },
                    {
                      "type": "text",
                      "text": ": MapKit successfully initialized and configured."
                    }
                  ]
                },
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "Refreshed"
                    },
                    {
                      "type": "text",
                      "text": ": MapKit configuration updated."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "error"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "MapKit failed to initialize. The event has the  property "
                    },
                    {
                      "type": "codeVoice",
                      "code": "status"
                    },
                    {
                      "type": "text",
                      "text": " (String), which indicates the status response."
                    }
                  ]
                },
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "Unauthorized"
                    },
                    {
                      "type": "text",
                      "text": ": The authorization token provided is invalid."
                    }
                  ]
                },
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "Too Many Requests"
                    },
                    {
                      "type": "text",
                      "text": ": The authorization token provided is based on a Maps ID, which has exceeded its allowed daily usage."
                    }
                  ]
                }
              ]
            ]
          ],
          "metadata": {
            "anchor": "3011695",
            "title": "Table 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "MapKit JS will envoke these events asyncrounously upon success or failure of the initialization request. A common use-case is shown in the following example:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "mapkit.init({ authorizationCallback: function(done) { done(\"your-token\"); }, ... });",
            "mapkit.addEventListener(\"configuration-change\", function(event) {",
            "    switch (event.status) {",
            "    case \"Initialized\":",
            "        // MapKit JS initialized and configured.",
            "        break;",
            "    case \"Refreshed\":",
            "        // MapKit JS configuration updated.",
            "        break;",
            "    }",
            "});",
            ""
          ],
          "syntax": "javascript",
          "metadata": {
            "anchor": "3011697",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
