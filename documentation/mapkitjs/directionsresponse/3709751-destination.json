        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/coordinate"
            },
            {
              "type": "text",
              "text": " to request directions with "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/directions/2973874-route"
            },
            {
              "type": "text",
              "text": ", MapKit JS returns a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/mapkit/coordinate"
            },
            {
              "type": "text",
              "text": " in the direction’s response. If you use a string or a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/place"
            },
            {
              "type": "text",
              "text": " to request directions, the property may be "
            },
            {
              "type": "codeVoice",
              "code": "null"
            },
            {
              "type": "text",
              "text": " or a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/place"
            },
            {
              "type": "text",
              "text": " instance."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When MapKit JS returns a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/mapkitjs/place"
            },
            {
              "type": "text",
              "text": " instance, it may contain additional details that weren’t included in the original item used to request directions."
            }
          ]
        }
      ]
    }
