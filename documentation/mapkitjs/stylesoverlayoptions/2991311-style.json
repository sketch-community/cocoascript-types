      "parameters": [
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An optional object literal of style properties (defined below)."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following table lists the available style properties."
            }
          ]
        },
        {
          "type": "table",
          "header": "none",
          "rows": [
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "strong",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Property"
                        }
                      ]
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "strong",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "Summary"
                        }
                      ]
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "strokeColor"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The stroke color of a line. This can be any valid CSS color value. The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rgb(0, 122, 255)"
                    },
                    {
                      "type": "text",
                      "text": ". Set to "
                    },
                    {
                      "type": "codeVoice",
                      "code": "null"
                    },
                    {
                      "type": "text",
                      "text": " to have no line stroke."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "strokeOpacity"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The opacity to apply to strokes as a number between "
                    },
                    {
                      "type": "codeVoice",
                      "code": "0"
                    },
                    {
                      "type": "text",
                      "text": " and "
                    },
                    {
                      "type": "codeVoice",
                      "code": "1"
                    },
                    {
                      "type": "text",
                      "text": ". The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "1"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "lineWidth"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The line width of the stroke line for overlays in CSS pixels. The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "1"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "lineCap"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The shape of the ends of the line. The three options for line endings are "
                    },
                    {
                      "type": "codeVoice",
                      "code": "butt"
                    },
                    {
                      "type": "text",
                      "text": " (squared-off ends), "
                    },
                    {
                      "type": "codeVoice",
                      "code": "round"
                    },
                    {
                      "type": "text",
                      "text": " (rounded ends), or "
                    },
                    {
                      "type": "codeVoice",
                      "code": "square"
                    },
                    {
                      "type": "text",
                      "text": " (ends that have a half-square extension). The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "round"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "lineJoin"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The style to use when drawing joins between line segments. The three options are "
                    },
                    {
                      "type": "codeVoice",
                      "code": "miter"
                    },
                    {
                      "type": "text",
                      "text": " (join is a sharp corner), "
                    },
                    {
                      "type": "codeVoice",
                      "code": "round"
                    },
                    {
                      "type": "text",
                      "text": " (join is a rounded corner), or "
                    },
                    {
                      "type": "codeVoice",
                      "code": "bevel"
                    },
                    {
                      "type": "text",
                      "text": " (join is a beveled corner). The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "round"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "lineDash"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "An array to use as the line's dash pattern where the numbers represent line and gap lengths in CSS pixels. For example, "
                    },
                    {
                      "type": "codeVoice",
                      "code": "[10, 5]"
                    },
                    {
                      "type": "text",
                      "text": " means draw for 10 pixels, leave a 5-pixel gap, and repeat. MapKit JS copies and duplicates the array if there are an odd number of elements in it. Set to an empty array ("
                    },
                    {
                      "type": "codeVoice",
                      "code": "[]"
                    },
                    {
                      "type": "text",
                      "text": ") to draw solid lines. The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "[]"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "lineDashOffset"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The number of CSS pixels to offset drawing of the dash pattern. This has no effect if "
                    },
                    {
                      "type": "codeVoice",
                      "code": "lineDash"
                    },
                    {
                      "type": "text",
                      "text": " is set to draw solid lines. The default value is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "0"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "fillColor"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The color to use for filling shapes. This can be any valid CSS color value. The default fill color is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "rgb(0, 122, 255)"
                    },
                    {
                      "type": "text",
                      "text": ". Set to "
                    },
                    {
                      "type": "codeVoice",
                      "code": "null"
                    },
                    {
                      "type": "text",
                      "text": " to have no fill."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "fillOpacity"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The opacity to apply to fills as a number between "
                    },
                    {
                      "type": "codeVoice",
                      "code": "0"
                    },
                    {
                      "type": "text",
                      "text": " and "
                    },
                    {
                      "type": "codeVoice",
                      "code": "1"
                    },
                    {
                      "type": "text",
                      "text": ". The default opacity is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "0.1"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            ],
            [
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "fillRule"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The rule to use for determining whether a point is inside or outside a polygon. This can be either the nonzero winding rule ("
                    },
                    {
                      "type": "codeVoice",
                      "code": "nonzero"
                    },
                    {
                      "type": "text",
                      "text": "), the default, or the even-odd rule ("
                    },
                    {
                      "type": "codeVoice",
                      "code": "evenodd"
                    },
                    {
                      "type": "text",
                      "text": ")."
                    }
                  ]
                }
              ]
            ]
          ],
          "metadata": {
            "anchor": "2994144",
            "title": "Table 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
