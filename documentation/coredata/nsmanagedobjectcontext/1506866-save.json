          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSError"
                },
                {
                  "type": "text",
                  "text": " object. You do not need to create an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSError"
                },
                {
                  "type": "text",
                  "text": " object. The save operation aborts after the first failure if you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
