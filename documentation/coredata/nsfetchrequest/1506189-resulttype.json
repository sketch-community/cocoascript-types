        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsfetchrequestresulttype/nsmanagedobjectresulttype"
            },
