      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The attribute must be of type "
            },
            {
              "type": "codeVoice",
              "code": "NSTransformedAttributeType"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The transformer must output an "
            },
            {
              "type": "codeVoice",
              "code": "NSData"
            },
            {
              "type": "text",
              "text": " object from "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsvaluetransformer/1402004-transformedvalue"
            },
            {
              "type": "text",
              "text": " and must allow reverse transformations."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If this value is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", Core Data uses a default a transformer that uses "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoding"
            },
            {
              "type": "text",
              "text": " to archive and unarchive the attribute value."
            }
          ]
        }
      ]
    }
