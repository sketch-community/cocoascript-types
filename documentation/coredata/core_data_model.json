  "primaryContentSections": [
    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In most cases, you describe your app’s data model using Xcode’s data model editor. "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobjectmodel"
            },
            {
              "type": "text",
              "text": " represents the "
            },
            {
              "type": "codeVoice",
              "code": ".xcdatamodeld"
            },
            {
              "type": "text",
              "text": " file in your project’s source list. This is where you define entities that you use to generate "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobject"
            },
            {
              "type": "text",
              "text": " subclasses for Core Data to manage. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The entities you create are "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsentitydescription"
            },
            {
              "type": "text",
              "text": " instances. Entities’ properties are subclasses of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nspropertydescription"
            },
            {
              "type": "text",
              "text": ", namely "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsattributedescription"
            },
            {
              "type": "text",
              "text": " for attributes, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsrelationshipdescription"
            },
            {
              "type": "text",
              "text": " for relationships, and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsfetchedpropertydescription"
            },
            {
              "type": "text",
              "text": " for fetched properties."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The various attribute types are enumerated in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsattributetype"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
  ],
