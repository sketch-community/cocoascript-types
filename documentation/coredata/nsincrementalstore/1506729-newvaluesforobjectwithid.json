            {
              "type": "codeVoice",
              "code": "objectID"
            },
            {
              "type": "text",
              "text": " cannot be found, the method should return "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " and—if "
