        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A new, autoreleased, fully configured instance of the class for the entity named "
            },
            {
              "type": "codeVoice",
              "code": "entityName"
            },
            {
              "type": "text",
              "text": ". The instance has its entity description set and is inserted it into "
            },
            {
              "type": "codeVoice",
              "code": "context"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
