    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "entity",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The entity of which to create an instance."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The model associated with "
                },
                {
                  "type": "codeVoice",
                  "code": "context"
                },
                {
                  "type": "text",
                  "text": "'s persistent store coordinator must contain "
                },
                {
                  "type": "codeVoice",
                  "code": "entity"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The context into which the new instance is inserted."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized instance of the appropriate class for "
            },
            {
              "type": "codeVoice",
              "code": "entity"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "NSManagedObject"
            },
            {
              "type": "text",
              "text": " uses dynamic class generation to support the Objective-C 2 properties feature (see Declared Properties) by automatically creating a subclass of the class appropriate for "
            },
            {
              "type": "codeVoice",
              "code": "entity"
            },
            {
              "type": "text",
              "text": ". "
            },
            {
              "type": "codeVoice",
              "code": "initWithEntity:insertIntoManagedObjectContext:"
            },
            {
              "type": "text",
              "text": " therefore returns an instance of the appropriate class for "
            },
            {
              "type": "codeVoice",
              "code": "entity"
            },
            {
              "type": "text",
              "text": ". The dynamically-generated subclass will be based on the class specified by the entity, so specifying a custom class in your model will supersede the class passed to "
            },
            {
              "type": "codeVoice",
              "code": "alloc"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "context"
            },
            {
              "type": "text",
              "text": " is not "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", this method invokes "
            },
            {
              "type": "codeVoice",
              "code": "[context insertObject:self]"
            },
            {
              "type": "text",
              "text": " (which causes "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobject/1506548-awakefrominsert"
            },
            {
              "type": "text",
              "text": " to be invoked)."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You are discouraged from overriding this method—you should instead override "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobject/1506548-awakefrominsert"
            },
            {
              "type": "text",
              "text": " and/or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobject/1506424-awakefromfetch"
            },
            {
              "type": "text",
              "text": " (if there is logic common to these methods, it should be factored into a third method which is invoked from both). If you do perform custom initialization in this method, you may cause problems with undo and redo operations. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In many applications, there is no need to subsequently assign a newly-created managed object to a particular store—see "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobjectcontext/1506436-assignobject"
            },
            {
              "type": "text",
              "text": ". If your application has multiple stores and you do need to assign an object to a specific store, you should not do so in a managed object's initializer method. Such an assignment is controller- not model-level logic."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "This method is the designated initializer for "
                },
                {
                  "type": "codeVoice",
                  "code": "NSManagedObject"
                },
                {
                  "type": "text",
                  "text": ". You must not initialize a managed object simply by sending it "
                },
                {
                  "type": "codeVoice",
                  "code": "init"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ],
          "style": "important"
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Special Considerations",
          "anchor": "1681199"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you override "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/coredata/nsmanagedobject/1506357-initwithentity"
            },
            {
              "type": "text",
              "text": ", you "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "must"
                }
              ]
            },
            {
              "type": "text",
              "text": " ensure that you set "
            },
            {
              "type": "codeVoice",
              "code": "self"
            },
            {
              "type": "text",
              "text": " to the return value from invocation of "
            },
            {
              "type": "codeVoice",
              "code": "super"
            },
            {
              "type": "text",
              "text": "’s implementation, as shown in the following example:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (id)initWithEntity:(NSEntityDescription*)entity insertIntoManagedObjectContext:(NSManagedObjectContext*)context",
            "{",
            "    self = [super initWithEntity:entity insertIntoManagedObjectContext:context];",
            "    if (self != nil) {",
            "        // Perform additional initialization.",
            "    }",
            "    return self;",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555688",
            "title": "Listing 1"
          }
        }
      ]
    }
