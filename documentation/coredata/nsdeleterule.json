    "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nsnoactiondeleterule": {
      "title": "NSNoActionDeleteRule",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nsnoactiondeleterule",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coredata/nsdeleterule/nsnoactiondeleterule",
      "abstract": [
        {
          "type": "text",
          "text": "If the object is deleted, no modifications are made to objects at the destination of the relationship. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nsnullifydeleterule": {
      "title": "NSNullifyDeleteRule",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nsnullifydeleterule",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coredata/nsdeleterule/nsnullifydeleterule",
      "abstract": [
        {
          "type": "text",
          "text": "If the object is deleted, back pointers from the objects to which it is related are nullified."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nscascadedeleterule": {
      "title": "NSCascadeDeleteRule",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nscascadedeleterule",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coredata/nsdeleterule/nscascadedeleterule",
      "abstract": [
        {
          "type": "text",
          "text": "If the object is deleted, the destination object or objects of this relationship are also deleted."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nsdenydeleterule": {
      "title": "NSDenyDeleteRule",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coredata/nsdeleterule/nsdenydeleterule",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coredata/nsdeleterule/nsdenydeleterule",
      "abstract": [
        {
          "type": "text",
          "text": "If the destination of this relationship is not "
        },
        {
          "type": "codeVoice",
          "code": "nil"
        },
        {
          "type": "text",
          "text": ", the delete creates a validation error."
        }
      ]
    },
