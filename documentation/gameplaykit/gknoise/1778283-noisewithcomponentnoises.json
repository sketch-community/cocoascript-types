    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "noises",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The array of noise objects to combine."
                }
              ]
            }
          ]
        },
        {
          "name": "selectionNoise",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A noise object whose values determine which object from the "
                },
                {
                  "type": "codeVoice",
                  "code": "noises"
                },
                {
                  "type": "text",
                  "text": " array contributes to which region of the generated output."
                }
              ]
            }
          ]
        },
        {
          "name": "componentBoundaries",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of numbers specifying which values in the "
                },
                {
                  "type": "codeVoice",
                  "code": "selectionNoise"
                },
                {
                  "type": "text",
                  "text": " field correspond to which noise object in the "
                },
                {
                  "type": "codeVoice",
                  "code": "noises"
                },
                {
                  "type": "text",
                  "text": " array."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The count of this array must be one less than the count of the "
                },
                {
                  "type": "codeVoice",
                  "code": "noises"
                },
                {
                  "type": "text",
                  "text": " array."
                }
              ]
            }
          ]
        },
        {
          "name": "blendDistances",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of numbers specifying the breadth of a smooth transition for each boundary specified in the "
                },
                {
                  "type": "codeVoice",
                  "code": "componentBoundaries"
                },
                {
                  "type": "text",
                  "text": " array."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The count of this array must be one less than the count of the "
                },
                {
                  "type": "codeVoice",
                  "code": "noises"
                },
                {
                  "type": "text",
                  "text": " array (matching the count of the "
                },
                {
                  "type": "codeVoice",
                  "code": "componentBoundaries"
                },
                {
                  "type": "text",
                  "text": " array)."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
