      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An audio engine instance is automatically created for you when the scene is created. You can use methods and properties on a scene’s audio engine for overall control of all of its child audio nodes. The following code shows how a scene’s overall volume can be reduced from its default of 1.0 down to 0.2 and then paused:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let scene = SKScene()",
            "scene.audioEngine.mainMixerNode.outputVolume = 0.2",
            "scene.audioEngine.pause()"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "2506185",
            "title": "Listing 1"
          }
        }
      ]
    }
