    "doc://com.apple.documentation/documentation/spritekit/skrepeatmode/skrepeatmodeclamp": {
      "title": "SKRepeatModeClamp",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/spritekit/skrepeatmode/skrepeatmodeclamp",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/spritekit/skrepeatmode/skrepeatmodeclamp",
      "abstract": [
        {
          "type": "text",
          "text": "When a sample is calculated, the time value is clamped to the range of time values found in the sequence. For example, if the last keyframe’s time value is "
        },
        {
          "type": "codeVoice",
          "code": "0.5"
        },
        {
          "type": "text",
          "text": ", a sample at any time value from "
        },
        {
          "type": "codeVoice",
          "code": "0.5"
        },
        {
          "type": "text",
          "text": " to "
        },
        {
          "type": "codeVoice",
          "code": "1.0"
        },
        {
          "type": "text",
          "text": " returns the last keyframe’s value."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/spritekit/skrepeatmode/skrepeatmodeloop": {
      "title": "SKRepeatModeLoop",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/spritekit/skrepeatmode/skrepeatmodeloop",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/spritekit/skrepeatmode/skrepeatmodeloop",
      "abstract": [
        {
          "type": "text",
          "text": "When a sample is calculated, the sequence loops back to the beginning of the sequence. For example, if the last keyframe’s time value is "
        },
        {
          "type": "codeVoice",
          "code": "0.5"
        },
        {
          "type": "text",
          "text": ", then a sample at any time value from "
        },
        {
          "type": "codeVoice",
          "code": "0.5"
        },
        {
          "type": "text",
          "text": " to "
        },
        {
          "type": "codeVoice",
          "code": "1.0"
        },
        {
          "type": "text",
          "text": " returns the same value as the sequence did from "
        },
        {
          "type": "codeVoice",
          "code": "0.0"
        },
        {
          "type": "text",
          "text": " to "
        },
        {
          "type": "codeVoice",
          "code": "0.5"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
