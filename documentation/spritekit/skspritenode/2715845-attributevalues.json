      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "All nodes have their own copy of an attribute value and therefore the attribute values can be different per-node across the same "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/spritekit/skshader"
            },
            {
              "type": "text",
              "text": ". If instead you need all nodes to share the same value, use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/spritekit/skuniform"
            },
            {
              "type": "text",
              "text": ". Uniforms can change values every frame, but uniforms cannot vary per-node like attributes can. "
            }
          ]
        }
      ]
    }
