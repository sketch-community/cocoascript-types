      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The frame is the smallest rectangle that contains the node’s content, taking into account the node’s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/spritekit/sknode/1483087-xscale"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/spritekit/sknode/1483046-yscale"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/spritekit/sknode/1483089-zrotation"
            },
            {
              "type": "text",
              "text": " properties. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Since "
            },
            {
              "type": "codeVoice",
              "code": "SKNode"
            },
            {
              "type": "text",
              "text": " does not draw content of its own, its frame size is arbitrary; it's the visual subclasses of "
            },
            {
              "type": "codeVoice",
              "code": "SKNode"
            },
            {
              "type": "text",
              "text": " that do draw that define the frame's size with a meaningful value that encloses its visual content. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To get a rect that encloses all the child nodes of an "
            },
            {
              "type": "codeVoice",
              "code": "SKNode"
            },
            {
              "type": "text",
              "text": " parent object, use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/spritekit/sknode/1483066-calculateaccumulatedframe"
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        }
      ]
    }
