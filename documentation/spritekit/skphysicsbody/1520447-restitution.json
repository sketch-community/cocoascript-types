            {
              "type": "codeVoice",
              "code": "0.0"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": ". The default value is "
            },
            {
              "type": "codeVoice",
              "code": "0.2"
            },
