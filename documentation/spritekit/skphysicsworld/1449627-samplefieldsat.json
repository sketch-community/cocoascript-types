    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The sample is calculated as if a physics body is placed at that position in the scene. The body is assumed to have a mass of "
            },
            {
              "type": "codeVoice",
              "code": "1.0"
            },
            {
              "type": "text",
              "text": ", with no charge or velocity. The body is affected by all field nodes."
            }
          ]
        }
      ]
    }
