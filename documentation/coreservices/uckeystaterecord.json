    "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390646-stateentrycount": {
      "title": "stateEntryCount",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390646-stateentrycount",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/uckeystaterecord/1390646-stateentrycount",
      "abstract": [
        {
          "type": "text",
          "text": "An unsigned 16-bit integer specifying the number of elements in the "
        },
        {
          "type": "codeVoice",
          "code": "stateEntryData"
        },
        {
          "type": "text",
          "text": " field’s array for a given dead-key state record."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390413-stateentrydata": {
      "title": "stateEntryData",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390413-stateentrydata",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/uckeystaterecord/1390413-stateentrydata",
      "abstract": [
        {
          "type": "text",
          "text": "An array of dead-key state entries, whose size depends on their format, but which will always be a multiple of 4 bytes. Each entry maps from the current dead-key state to the Unicode character(s) that result when a given character key is pressed or to the next dead-key state, if any. The format of the entry is specified by the "
        },
        {
          "type": "codeVoice",
          "code": "stateEntryFormat"
        },
        {
          "type": "text",
          "text": " field to be either that of type "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystateentryterminal"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystateentryrange"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390450-stateentryformat": {
      "title": "stateEntryFormat",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390450-stateentryformat",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/uckeystaterecord/1390450-stateentryformat",
      "abstract": [
        {
          "type": "text",
          "text": "An unsigned 16-bit integer specifying the format of the data in the "
        },
        {
          "type": "codeVoice",
          "code": "stateEntryData"
        },
        {
          "type": "text",
          "text": " field’s array. This should be 0 if the "
        },
        {
          "type": "codeVoice",
          "code": "stateEntryCount"
        },
        {
          "type": "text",
          "text": " field is set to 0. Currently available values are "
        },
        {
          "type": "codeVoice",
          "code": "kUCKeyStateEntryTerminalFormat"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "kUCKeyStateEntryRangeFormat"
        },
        {
          "type": "text",
          "text": "; see "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/coreservices/1390376-key_state_entry_formats"
        },
        {
          "type": "text",
          "text": " for descriptions of these values."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390580-statezerochardata": {
      "title": "stateZeroCharData",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390580-statezerochardata",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/uckeystaterecord/1390580-statezerochardata",
      "abstract": [
        {
          "type": "text",
          "text": "A value of type "
        },
        {
          "type": "codeVoice",
          "code": "UCKeyCharSeq"
        },
        {
          "type": "text",
          "text": " specifying the Unicode character(s) produced from a given key code while no dead-key state is in effect. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390554-statezeronextstate": {
      "title": "stateZeroNextState",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/uckeystaterecord/1390554-statezeronextstate",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/uckeystaterecord/1390554-statezeronextstate",
      "abstract": [
        {
          "type": "text",
          "text": "An unsigned 16-bit integer specifying the dead-key state produced from a given key code when no previous dead-key state is in effect. If the "
        },
        {
          "type": "codeVoice",
          "code": "UCKeyStateRecord"
        },
        {
          "type": "text",
          "text": " structure does not initiate a dead-key state (but only provides terminators for other dead-key states), this will be 0. A non-zero value specifies the resulting new dead-key state and refers to the current state entry within the "
        },
        {
          "type": "codeVoice",
          "code": "stateEntryData[]"
        },
        {
          "type": "text",
          "text": " field for the following dead-key state record that is applied."
        }
      ]
    },
