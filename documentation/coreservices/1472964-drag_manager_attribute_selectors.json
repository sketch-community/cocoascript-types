      "url": "/documentation/coreservices/1472964-drag_manager_attribute_selectors/gestaltdragmgrpresent",
      "abstract": [
        {
          "type": "text",
          "text": "If the bit specified by this mask is set, the Drag Manager functions are available."
        }
      ]
    },
