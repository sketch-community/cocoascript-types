      "abstract": [
        {
          "type": "text",
          "text": "The Gestalt selector passed to determine what features of the Drag Manager are present. Passing the "
        },
        {
          "type": "codeVoice",
          "code": "gestaltDragMgrAttr"
        },
        {
          "type": "text",
          "text": " constant produces a 32-bit value whose bits you should test to determine what Drag Manager functionality is available."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1472964-drag_manager_attribute_selectors/gestaltdragmgrpresent": {
