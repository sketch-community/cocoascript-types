      "url": "/documentation/coreservices/1471733-display_manager_attribute_select/gestaltdisplaymgrcanconfirm",
      "abstract": [
        {
          "type": "text",
          "text": "Not yet supported. Most commonly comes up for display modes that are not marked "
        },
        {
          "type": "codeVoice",
          "code": "kModeSafe"
        },
        {
          "type": "text",
          "text": ". There is currently no system support for trying an unsafe mode and then restoring if the user does not confirm. When this is supported, this bit will be set."
        }
      ]
