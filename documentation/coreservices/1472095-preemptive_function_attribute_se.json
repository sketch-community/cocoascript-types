      "abstract": [
        {
          "type": "text",
          "text": "The Gestalt selector passed to determine the availability of preemptive system software functions. The "
        },
        {
          "type": "codeVoice",
          "code": "Gestalt"
        },
        {
          "type": "text",
          "text": " function produces a 32-bit value that you should test to determine which what type of preemptive calls are allowed. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1472095-preemptive_function_attribute_se/gestaltmpfilemanager": {
      "title": "gestaltMPFileManager",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1472095-preemptive_function_attribute_se/gestaltmpfilemanager",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1472095-preemptive_function_attribute_se/gestaltmpfilemanager",
      "abstract": [
        {
          "type": "text",
          "text": "If this bit is set, you can call preemptively safe File Manager functions."
        }
      ]
    },
