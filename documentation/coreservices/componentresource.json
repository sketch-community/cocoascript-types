    "doc://com.apple.documentation/documentation/coreservices/componentresource/1516623-component": {
      "title": "component",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/componentresource/1516623-component",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/componentresource/1516623-component",
      "abstract": [
        {
          "type": "text",
          "text": "A resource specification structure that specifies the type and ID of the component code resource. The "
        },
        {
          "type": "codeVoice",
          "code": "resType"
        },
        {
          "type": "text",
          "text": " field of the resource specification structure may contain any value. The component’s main entry point must be at offset 0 in the resource. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/componentresource/1516543-componenticon": {
      "title": "componentIcon",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/componentresource/1516543-componenticon",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/componentresource/1516543-componenticon",
      "abstract": [
        {
          "type": "text",
          "text": "A resource specification structure that specifies the resource type and ID for the icon for a component. Component icons are stored as 32-by-32 bit maps. Typically, the icon is stored in a resource of type "
        },
        {
          "type": "codeVoice",
          "code": "'ICON'"
        },
        {
          "type": "text",
          "text": ". Note that this icon is not used by the Finder you supply an icon only so that other components or applications can display your component’s icon in a dialog box if needed."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/componentresource/1516441-componentinfo": {
      "title": "componentInfo",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/componentresource/1516441-componentinfo",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/componentresource/1516441-componentinfo",
      "abstract": [
        {
          "type": "text",
          "text": "A resource specification structure that specifies the resource type and ID for the information string that describes the component. This is a Pascal string. Typically, the information string is stored in a resource of type "
        },
        {
          "type": "codeVoice",
          "code": "'STR'"
        },
        {
          "type": "text",
          "text": ". You might use the information stored in this resource in a Get Info dialog box."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/componentresource/1516658-componentname": {
      "title": "componentName",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/componentresource/1516658-componentname",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/componentresource/1516658-componentname",
      "abstract": [
        {
          "type": "text",
          "text": "A resource specification structure that specifies the resource type and ID for the name of the component. This is a Pascal string. Typically, the name is stored in a resource of type "
        },
        {
          "type": "codeVoice",
          "code": "'STR '"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
