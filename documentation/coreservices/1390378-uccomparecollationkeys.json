                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " for the "
                },
                {
                  "type": "codeVoice",
                  "code": "order"
                },
                {
                  "type": "text",
                  "text": " parameter and only use the "
                },
                {
                  "type": "codeVoice",
                  "code": "equivalent"
                },
                {
                  "type": "text",
                  "text": " parameter’s result. (Note that either the "
                },
                {
                  "type": "codeVoice",
                  "code": "equivalent"
                },
                {
                  "type": "text",
                  "text": " or the "
                },
                {
                  "type": "codeVoice",
                  "code": "order"
                },
