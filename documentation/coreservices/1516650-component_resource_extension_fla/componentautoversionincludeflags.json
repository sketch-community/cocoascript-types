    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Specify this flag if you want the Component Manager to include the "
            },
            {
              "type": "codeVoice",
              "code": "componentFlags"
            },
            {
              "type": "text",
              "text": " field of the component description structure when it searches for identical components in the process of performing automatic version control for your component. If you do not specify this flag, the Component Manager searches only the "
            },
            {
              "type": "codeVoice",
              "code": "componentType"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "componentSubType"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "componentManufacturer"
            },
            {
              "type": "text",
              "text": " fields."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Note that the setting of the "
            },
            {
              "type": "codeVoice",
              "code": "componentAutoVersionIncludeFlags"
            },
            {
              "type": "text",
              "text": " flag affects automatic version control only and does not affect the search operations performed by "
            },
            {
              "type": "codeVoice",
              "code": "FindNextComponent"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "CountComponents"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
