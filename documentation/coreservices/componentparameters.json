      "role": "symbol",
      "url": "/documentation/coreservices/componentparameters/1516330-paramsize",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies the number of bytes of parameter data for this request. The actual parameters are stored in the "
        },
        {
          "type": "codeVoice",
          "code": "params"
        },
        {
          "type": "text",
          "text": " field."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/componentparameters/1516563-params": {
      "title": "params",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/componentparameters/1516563-params",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/componentparameters/1516563-params",
      "abstract": [
        {
          "type": "text",
          "text": "An array that contains the parameters specified by the application that called your component. You can use the "
        },
        {
          "type": "codeVoice",
          "code": "CallComponentRoutine"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "codeVoice",
          "code": "CallComponentRoutineWithStorage"
        },
        {
          "type": "text",
          "text": " function to convert this array into a Pascal-style invocation of a subroutine in your component."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/componentparameters/1516601-what": {
      "title": "what",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/componentparameters/1516601-what",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/componentparameters/1516601-what",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies the type of request. Component designers define the meaning of positive values and assign them to requests that are supported by components of a given type. Negative values are reserved for definition by Apple. See “Result Codes” for Apple-defined request code values."
        }
      ]
    },
