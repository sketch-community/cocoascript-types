{
  "abstract": [
    {
      "type": "text",
      "text": "This is set if the new control flag bits "
    },
    {
      "type": "codeVoice",
      "code": "kUnicodeForceASCIIRangeBit"
    },
    {
      "type": "text",
      "text": " and "
    },
    {
      "type": "codeVoice",
      "code": "kUnicodeNoHalfwidthCharsBit"
    },
    {
      "type": "text",
      "text": " are supported for use with the functions "
    },
    {
      "type": "codeVoice",
      "code": "ConvertFromTextToUnicode"
    },
    {
      "type": "text",
      "text": ", "
    },
    {
      "type": "codeVoice",
      "code": "ConvertFromUnicodeToText"
    },
    {
      "type": "text",
      "text": ", and so forth."
    }
  ],
