    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The bundle identifier of the default handler for the specified content type in the specified roles, or "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " if no handler is available."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This function returns the user’s currently preferred default handler for the specified content type. Say, for example, that "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreservices/1444955-lssetdefaultrolehandlerforconten"
            },
            {
              "type": "text",
              "text": " has been used to set “com.Apple.TextEdit” for the “public.xml” content type. When a file whose content type is “public.xml” is double-clicked, TextEdit will be launched to open the file. If you call "
            },
            {
              "type": "codeVoice",
              "code": "LSCopyDefaultRoleHandlerForContentType(CFSTR(“public.xml”), kLSRolesAll)"
            },
            {
              "type": "text",
              "text": ", the string "
            },
            {
              "type": "codeVoice",
              "code": "com.apple.TextEdit"
            },
            {
              "type": "text",
              "text": " is returned."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/bundleresources/information_property_list/cfbundledocumenttypes"
            },
            {
              "type": "text",
              "text": " key in an app’s "
            },
            {
              "type": "codeVoice",
              "code": "Info.plist"
            },
            {
              "type": "text",
              "text": " can be used to set an app’s content handling capabilities. The "
            },
            {
              "type": "codeVoice",
              "code": "LSItemContentTypes"
            },
            {
              "type": "text",
              "text": " key is particularly useful because it supports the use of UTIs in document claims."
            }
          ]
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Version-Notes",
          "anchor": "1818366"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Thread-safe since OS X v10.4."
            }
          ]
        }
      ]
    }
