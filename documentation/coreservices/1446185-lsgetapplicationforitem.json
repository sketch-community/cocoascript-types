        {
          "name": "inItemRef",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a file-system reference designating the item whose preferred application is requested; see the "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "File Manager Reference"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " in the Carbon File Management Documentation for a description of the "
                },
                {
                  "type": "codeVoice",
                  "code": "FSRef"
                },
                {
                  "type": "text",
                  "text": " data type."
                }
              ]
            }
          ]
        },
        {
          "name": "inRolesMask",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bit mask specifying the applicationâ€™s desired role or roles with respect to the designated item; see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsrolesmask"
                },
                {
                  "type": "text",
                  "text": " for a description of this mask. If the role is unimportant, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "kLSRolesAll"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
