        {
          "name": "inputEncoding",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The text encoding specification describing the source text. "
                }
              ]
            }
          ]
        },
        {
          "name": "destinationEncodings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, an array of specifications for the destination encodings to which the converter can directly convert the source encoding. You should use the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1571805-teccountdestinationtextencodings"
                },
                {
                  "type": "text",
                  "text": " function to determine how large an array to allocate."
                }
              ]
            }
          ]
        },
        {
          "name": "maxDestinationEncodings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum number of destination text encodings that the array can contain. "
                }
              ]
            }
          ]
        },
        {
          "name": "actualDestinationEncodings",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, a pointer to the number of text encoding specifications in the destination encodings array."
                }
              ]
            }
          ]
        }
      ]
    },
