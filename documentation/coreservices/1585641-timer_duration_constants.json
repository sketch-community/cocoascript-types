    "doc://com.apple.documentation/documentation/coreservices/1585641-timer_duration_constants/kdurationimmediate": {
      "title": "kDurationImmediate",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1585641-timer_duration_constants/kdurationimmediate",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1585641-timer_duration_constants/kdurationimmediate",
      "abstract": [
        {
          "type": "text",
          "text": "The task times out immediately, whether or not the event has occurred. If the event occurred, the return status is "
        },
        {
          "type": "codeVoice",
          "code": "noErr"
        },
        {
          "type": "text",
          "text": ". If the event did not occur, the return status is "
        },
        {
          "type": "codeVoice",
          "code": "kMPTimeoutErr"
        },
        {
          "type": "text",
          "text": " (assuming no other errors occurred). "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1585641-timer_duration_constants/kdurationforever": {
      "title": "kDurationForever",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1585641-timer_duration_constants/kdurationforever",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1585641-timer_duration_constants/kdurationforever",
      "abstract": [
        {
          "type": "text",
          "text": "The task waits forever. The blocking call waits until either the event occurs, or until the object being waited upon (such as a message queue) is deleted."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1585641-timer_duration_constants/kdurationmillisecond": {
      "title": "kDurationMillisecond",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1585641-timer_duration_constants/kdurationmillisecond",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1585641-timer_duration_constants/kdurationmillisecond",
      "abstract": [
        {
          "type": "text",
          "text": "The task waits one millisecond before timing out. "
        }
      ]
    },
