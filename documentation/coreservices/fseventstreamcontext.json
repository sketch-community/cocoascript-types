      "abstract": [
        {
          "type": "text",
          "text": "The callback used to create a descriptive string representation of the info pointer (or the data pointed to by the info pointer) for debugging purposes. This can be NULL."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/fseventstreamcontext/1446169-info": {
      "title": "info",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/fseventstreamcontext/1446169-info",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/fseventstreamcontext/1446169-info",
      "abstract": [
        {
          "type": "text",
          "text": "An arbitrary client-defined value (for instance, a pointer) to be associated with the stream and passed to the callback when it is invoked. If a non-NULL value is supplied for the retain callback the framework will use it to retain this value. If a non-NULL value is supplied for the release callback then when the stream is deallocated it will be used to release this value. This can be NULL."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/fseventstreamcontext/1450554-release": {
      "title": "release",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/fseventstreamcontext/1450554-release",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/fseventstreamcontext/1450554-release",
      "abstract": [
        {
          "type": "text",
          "text": "The callback used release a retain on the info pointer. This can be NULL."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/fseventstreamcontext/1450539-retain": {
