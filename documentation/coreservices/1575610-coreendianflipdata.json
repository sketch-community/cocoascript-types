      ],
      "parameters": [
        {
          "name": "dataDomain",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "OSType"
                },
                {
                  "type": "text",
                  "text": " value that specifies the domain of the flipper callback you want to invoke. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1575588-domain_types/kcoreendianresourcemanagerdomain"
                },
                {
                  "type": "text",
                  "text": "\tif your callback applies to resource data. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1575588-domain_types/kcoreendianappleeventmanagerdomain"
                },
                {
                  "type": "text",
                  "text": " if your callback applies to Apple event data. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1575588-domain_types"
                },
                {
                  "type": "text",
                  "text": " for more information."
                }
              ]
            }
          ]
        },
        {
          "name": "dataType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "OSType"
                },
                {
                  "type": "text",
                  "text": " value that specifies the type of data that needs to be byte-swapped. This is the four character code of the resource type or Apple event. This never needs to be byte-swapped even though GDB and Xcode display the resource in byte-swapped order."
                }
              ]
            }
          ]
        },
        {
          "name": "id",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The resource ID of the data type. The Resource Manager byte-swaps this for you so you can compare the resource ID against constants in your code. If the data is not a resource, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the first byte of the data to be byte swapped."
                }
              ]
            }
          ]
        },
        {
          "name": "dataLen",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The length of the data (in bytes) to be byte swapped."
                }
              ]
            }
          ]
        },
        {
          "name": "currentlyNative",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value that indicates the direction to byte swap. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " when the data specified by the "
                },
                {
                  "type": "codeVoice",
                  "code": "data"
                },
                {
                  "type": "text",
                  "text": " parameter uses the byte ordering of the currently executing code. On a PowerPC system, "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " specifies that the data is in big-endian format. On an x86 system, "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " specifies that the data is in little-endian format. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code. Returns "
            },
            {
              "type": "codeVoice",
              "code": "noErr"
            },
            {
              "type": "text",
              "text": " ifthe data is byte swapped and "
            },
            {
              "type": "codeVoice",
              "code": "handlerNotFound"
            },
            {
              "type": "text",
              "text": " ifthe data is not byte swapped. Note that data is only byte swappedif it needs to be byte swapped."
            }
          ]
        }
      ]
    }
