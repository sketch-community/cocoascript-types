          "name": "fromType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The descriptor type of the data coerced by the handler. For a list of AppleScriptâ€™s predefined descriptor types, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1542788-descriptor_type_constants"
                },
                {
                  "type": "text",
                  "text": ". See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/desctype"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "toType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The descriptor type of the resulting data. For a list of AppleScriptâ€™s predefined descriptor types, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1542788-descriptor_type_constants"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "handler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A universal procedure pointer. On return, a pointer to the specified handler, if a coercion table entry exists that exactly matches the values supplied in the parameters "
                },
                {
                  "type": "codeVoice",
                  "code": "fromType"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "toType"
                },
                {
                  "type": "text",
                  "text": ". See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aecoercionhandlerupp"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "handlerRefcon",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a reference constant. On return, the reference constant from the coercion table entry for the specified coercion handler. The Apple Event Manager passes this reference constant to the handler each time it calls the handler. The reference constant may have a value of 0."
                }
              ]
            }
          ]
        },
        {
          "name": "fromTypeIsDesc",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a Boolean value. The "
                },
                {
                  "type": "codeVoice",
                  "code": "AEGetCoercionHandler"
                },
                {
                  "type": "text",
                  "text": " function returns a value of "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": " in this parameter if the coercion handler expects the data as a descriptor or "
                },
                {
                  "type": "codeVoice",
                  "code": "FALSE"
                },
                {
                  "type": "text",
                  "text": ", if the coercion handler expects a pointer to the data."
                }
              ]
            }
          ]
        },
        {
          "name": "isSysHandler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies the coercion table to get the handler from. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": " to get the handler from the system coercion table or "
                },
                {
                  "type": "codeVoice",
                  "code": "FALSE"
                },
                {
                  "type": "text",
                  "text": " to get the handler from your applicationâ€™s coercion table. Use of the system coercion table is not recommended."
                }
              ]
            }
          ]
        }
      ]
