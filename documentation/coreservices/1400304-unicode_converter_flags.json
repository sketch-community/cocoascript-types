    "doc://com.apple.documentation/documentation/coreservices/1400304-unicode_converter_flags/ktecaddforceasciichangesbit": {
      "title": "kTECAddForceASCIIChangesBit",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1400304-unicode_converter_flags/ktecaddforceasciichangesbit",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1400304-unicode_converter_flags/ktecaddforceasciichangesbit",
      "abstract": [
        {
          "type": "text",
          "text": "This is set if the new control flag bits "
        },
        {
          "type": "codeVoice",
          "code": "kUnicodeForceASCIIRangeBit"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "kUnicodeNoHalfwidthCharsBit"
        },
        {
          "type": "text",
          "text": " are supported for use with the functions "
        },
        {
          "type": "codeVoice",
          "code": "ConvertFromTextToUnicode"
        },
        {
          "type": "text",
          "text": ", "
        },
        {
          "type": "codeVoice",
          "code": "ConvertFromUnicodeToText"
        },
        {
          "type": "text",
          "text": ", and so forth."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1400304-unicode_converter_flags/ktecpreferredencodingfixbit": {
      "title": "kTECPreferredEncodingFixBit",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1400304-unicode_converter_flags/ktecpreferredencodingfixbit",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1400304-unicode_converter_flags/ktecpreferredencodingfixbit",
      "abstract": [
        {
          "type": "text",
          "text": "This is set to indicate that if a preferred encoding is specified for "
        },
        {
          "type": "codeVoice",
          "code": "CreateUnicodeToTextRunInfo"
        },
        {
          "type": "text",
          "text": " and related functions, they handle it correctly even if it does not match the system script."
        }
      ]
