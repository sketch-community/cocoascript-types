      "url": "/documentation/coreservices/1400304-unicode_converter_flags/ktectextrunbitclearfixbit",
      "abstract": [
        {
          "type": "text",
          "text": "This is set if "
        },
        {
          "type": "codeVoice",
          "code": "ConvertFromUnicodeToTextRun"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "ConvertFromUnicodeToScriptCodeRun"
        },
        {
          "type": "text",
          "text": " function correctly if the "
        },
        {
          "type": "codeVoice",
          "code": "kUnicodeTextRunBit"
        },
        {
          "type": "text",
          "text": " is clear."
        }
      ]
    },
