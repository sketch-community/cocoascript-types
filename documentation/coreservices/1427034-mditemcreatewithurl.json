          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A file URL to the file from which to create the "
                },
                {
                  "type": "codeVoice",
                  "code": "MDItem"
                },
                {
                  "type": "text",
                  "text": ". The file must exist."
                }
              ]
