      "parameters": [
        {
          "name": "selector",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The selector code that triggers the function. This should be a four-character sequence similar to those defined in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/carbon_core/gestalt_manager#1667886"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "response",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On output, the information associated with the selector code."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code. See "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreservices/carbon_core/gestalt_manager"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Your selector function places the requested information in the "
            },
            {
              "type": "codeVoice",
              "code": "response"
            },
            {
              "type": "text",
              "text": " parameter and returns a result code. If the information is not available, the selector function returns the appropriate error code, which the "
            },
            {
              "type": "codeVoice",
              "code": "Gestalt"
            },
            {
              "type": "text",
              "text": " function returns as its function result."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A selector function can call "
            },
            {
              "type": "codeVoice",
              "code": "Gestalt"
            },
            {
              "type": "text",
              "text": " or even other selector functions. "
            }
          ]
        }
      ]
    }
