        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the "
            },
            {
              "type": "codeVoice",
              "code": "gestaltStandardFile58"
            },
            {
              "type": "text",
              "text": " flag bit is set, you can call the four new procedures—"
            },
            {
              "type": "codeVoice",
              "code": "StandardPutFile"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "StandardGetFile"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "CustomPutFile"
            },
            {
              "type": "text",
              "text": ", and "
            },
            {
              "type": "codeVoice",
              "code": "CustomGetFile"
            },
            {
              "type": "text",
              "text": "—introduced with System 7. (The name of the constant reflects the enabling of selectors 5 through 8 on the trap macro that handles the Standard File Package.)"
            }
          ]
        }
      ]
    }
