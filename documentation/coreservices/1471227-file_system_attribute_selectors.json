    "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestaltfsattr": {
      "title": "gestaltFSAttr",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestaltfsattr",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471227-file_system_attribute_selectors/gestaltfsattr",
      "abstract": [
        {
          "type": "text",
          "text": "A selector you pass to the "
        },
        {
          "type": "codeVoice",
          "code": "Gestalt"
        },
        {
          "type": "text",
          "text": " function. If the Gestalt function returns "
        },
        {
          "type": "codeVoice",
          "code": "noErr"
        },
        {
          "type": "text",
          "text": ", the "
        },
        {
          "type": "codeVoice",
          "code": "response"
        },
        {
          "type": "text",
          "text": " parameter contains a 32-bit value specifying the features of the file system."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestaltfullextfsdispatching": {
      "title": "gestaltFullExtFSDispatching",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestaltfullextfsdispatching",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471227-file_system_attribute_selectors/gestaltfullextfsdispatching"
    },
    "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestalthasfsspeccalls": {
      "title": "gestaltHasFSSpecCalls",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestalthasfsspeccalls",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471227-file_system_attribute_selectors/gestalthasfsspeccalls",
      "abstract": [
        {
          "type": "text",
          "text": "If this bit is set in the "
        },
        {
          "type": "codeVoice",
          "code": "response"
        },
        {
          "type": "text",
          "text": " parameter, the operating environment provides the file system specification ("
        },
        {
          "type": "codeVoice",
          "code": "FSSpec"
        },
        {
          "type": "text",
          "text": ") versions of the basic file-manipulation functions, as well as the "
        },
        {
          "type": "codeVoice",
          "code": "FSMakeFSSpec"
        },
        {
          "type": "text",
          "text": " function."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestalthasfilesystemmanager": {
      "title": "gestaltHasFileSystemManager",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471227-file_system_attribute_selectors/gestalthasfilesystemmanager",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471227-file_system_attribute_selectors/gestalthasfilesystemmanager",
      "abstract": [
        {
          "type": "text",
          "text": "If this bit is set in the "
        },
        {
          "type": "codeVoice",
          "code": "response"
        },
        {
          "type": "text",
          "text": " parameter, the File System Manager is present. See the "
        },
        {
          "type": "emphasis",
          "inlineContent": [
            {
              "type": "text",
              "text": "Guide to the File System Manager"
            }
          ]
        },
        {
          "type": "text",
          "text": " for more information about the File System Manager. "
        }
      ]
    },
