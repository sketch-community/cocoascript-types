      ],
      "parameters": [
        {
          "name": "invocation",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The method invocation reference."
                }
              ]
            }
          ]
        },
        {
          "name": "typeNamespace",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The fully resolved namespace for a specific type. For example, this field could be: "
                },
                {
                  "type": "codeVoice",
                  "code": "CFSTR(\"http://www.Myserver.com/myNameSpace\")"
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", the default namespace will be used."
                }
              ]
            }
          ]
        },
        {
          "name": "typeName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The non-qualified type name. Note that this is the XML /name/ to be deserialized, not the name of the type. For example, if the XML is  "
                },
                {
                  "type": "codeVoice",
                  "code": "<Sharename xsi:type=\"xsd:string\">Album-9</Sharename>"
                },
                {
                  "type": "text",
                  "text": ", the "
                },
                {
                  "type": "codeVoice",
                  "code": "typeName"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "Sharename"
                },
                {
                  "type": "text",
                  "text": ", not "
                },
                {
                  "type": "codeVoice",
                  "code": "string"
                },
                {
                  "type": "text",
                  "text": ". This parameter must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "deserializationProc",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A "
                },
                {
                  "type": "codeVoice",
                  "code": "ProcPtr"
                },
                {
                  "type": "text",
                  "text": " to the callback to be called to perform the deserialization."
                }
              ]
            }
          ]
        },
        {
          "name": "context",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a "
                },
                {
                  "type": "codeVoice",
                  "code": "WSClientContext"
                },
                {
                  "type": "text",
                  "text": ". The structure will be copied."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Specifies a callback to be made when parsing an XML method response.  Used to deserialize types the default deserializer does not handle. The callback should return a "
            },
            {
              "type": "codeVoice",
              "code": "CFTypeRef"
            },
            {
              "type": "text",
              "text": " containing the deserialized object value.  If the callback returns "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": ", the default deserializer is used."
            }
          ]
        }
      ]
    }
  ],
