{
  "abstract": [
    {
      "type": "text",
      "text": "Extracts data from an Apple event array created with the "
    },
    {
      "type": "codeVoice",
      "code": "AEPutArray"
    },
    {
      "type": "text",
      "text": " function and stores it as a standard array of fixed size items in the specified buffer."
    }
  ],
