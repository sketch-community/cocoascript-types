{
  "abstract": [
    {
      "type": "text",
      "text": "The maximum number of milliseconds that can passbefore an update notification is sent. This value is advisory, inthat the notification will be triggered at some point after "
    },
    {
      "type": "codeVoice",
      "code": "update_max_ms"
    },
    {
      "type": "text",
      "text": " millisecondshave passed since the query began accumulating results. This valueis used only during the live-update phase of a query."
    }
  ],
