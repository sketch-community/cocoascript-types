        {
          "name": "fromFile",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the starting point for a relative search. You may pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you do not need relative search information in the alias record."
                }
              ]
            }
          ]
        },
