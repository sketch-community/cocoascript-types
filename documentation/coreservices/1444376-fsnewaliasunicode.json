          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the parent directory of the target."
                }
              ]
            }
          ]
        },
        {
          "name": "targetNameLength",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of Unicode characters in the target's name."
                }
              ]
            }
          ]
        },
        {
          "name": "targetName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to the Unicode name of the target."
                }
              ]
            }
          ]
        },
        {
          "name": "inAlias",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an alias handle. On return, this handler refers to the newly created alias record."
                }
              ]
            }
          ]
        },
        {
          "name": "isDirectory",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a Boolean value. On input, if the target does not exist, set the value to "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " if the target is a directory or "
                },
                {
                  "type": "codeVoice",
                  "code": "false"
                },
                {
                  "type": "text",
                  "text": " if it is not. (Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you are not sure whether the target is a directory.) On output, if the target exists, the value is "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " if the target is a directory, "
                },
                {
                  "type": "codeVoice",
                  "code": "false"
                },
                {
                  "type": "text",
                  "text": " if it is not."
                }
              ]
            }
          ]
        }
      ]
