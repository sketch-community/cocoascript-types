    "doc://com.apple.documentation/documentation/coreservices/lslaunchurlspec/1443566-appurl": {
      "title": "appURL",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/lslaunchurlspec/1443566-appurl",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/lslaunchurlspec/1443566-appurl",
      "abstract": [
        {
          "type": "text",
          "text": "A Core Foundation URL reference designating the application to launch; see the "
        },
        {
          "type": "emphasis",
          "inlineContent": [
            {
              "type": "text",
              "text": "CFURL Reference"
            }
          ]
        },
        {
          "type": "text",
          "text": " in the Core Foundation Reference Documentation for a description of the "
        },
        {
          "type": "codeVoice",
          "code": "CFURLRef"
        },
        {
          "type": "text",
          "text": " data type.The URL must have scheme "
        },
        {
          "type": "codeVoice",
          "code": "file"
        },
        {
          "type": "text",
          "text": " and contain a valid path to an application file or application bundle. Set this field to "
        },
        {
          "type": "codeVoice",
          "code": "NULL"
        },
        {
          "type": "text",
          "text": " to request that each item in the "
        },
        {
          "type": "codeVoice",
          "code": "itemURLs"
        },
        {
          "type": "text",
          "text": " array be opened in its own preferred application."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/lslaunchurlspec/1448168-asyncrefcon": {
      "title": "asyncRefCon",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/lslaunchurlspec/1448168-asyncrefcon",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/lslaunchurlspec/1448168-asyncrefcon",
      "abstract": [
        {
          "type": "text",
          "text": "A pointer to an arbitrary application-defined value, passed in the Carbon event notifying you of an application’s launch or termination (if you have registered for such notification). The value of this field can be "
        },
        {
          "type": "codeVoice",
          "code": "NULL"
        },
        {
          "type": "text",
          "text": "."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/lslaunchurlspec/1443759-itemurls": {
      "title": "itemURLs",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/lslaunchurlspec/1443759-itemurls",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/lslaunchurlspec/1443759-itemurls",
      "abstract": [
        {
          "type": "text",
          "text": "A reference to an array of Core Foundation URL references designating the item or items to open; see the "
        },
        {
          "type": "emphasis",
          "inlineContent": [
            {
              "type": "text",
              "text": "CFArray Reference"
            }
          ]
        },
        {
          "type": "text",
          "text": " in the Core Foundation Reference Documentation for a description of the "
        },
        {
          "type": "codeVoice",
          "code": "CFArrayRef"
        },
        {
          "type": "text",
          "text": " data type. The value of this field can be "
        },
        {
          "type": "codeVoice",
          "code": "NULL"
        },
        {
          "type": "text",
          "text": ", in which case the application designated by "
        },
        {
          "type": "codeVoice",
          "code": "appURL"
        },
        {
          "type": "text",
          "text": " will be launched without opening any items."
        }
      ]
    },
