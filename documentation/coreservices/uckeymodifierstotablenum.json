      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The Unicode keyboard-layout ("
            },
            {
              "type": "codeVoice",
              "code": "'uchr'"
            },
            {
              "type": "text",
              "text": ") resource contains the data necessary to map virtual key codes to Unicode character codes for a given keyboard layout. The "
            },
            {
              "type": "codeVoice",
              "code": "'uchr'"
            },
            {
              "type": "text",
              "text": " format consists of a header information section and five key mapping data sections. The "
            },
            {
              "type": "codeVoice",
              "code": "UCKeyModifiersToTableNum"
            },
            {
              "type": "text",
              "text": " type is used in the first key mapping section of the "
            },
            {
              "type": "codeVoice",
              "code": "'uchr'"
            },
            {
              "type": "text",
              "text": " resource. It maps a modifier key combination to a particular key-code-to-character table number. "
            }
          ]
        }
      ]
    }
