      "parameters": [
        {
          "name": "inItems",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of values of type "
                },
                {
                  "type": "codeVoice",
                  "code": "FSRef"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "inItemCount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of items specified in "
                },
                {
                  "type": "codeVoice",
                  "code": "inItems"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "inRole",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A value of type "
                },
                {
                  "type": "codeVoice",
                  "code": "LSRolesMask"
                },
                {
                  "type": "text",
                  "text": " specifying one or more roles. If the role doesnâ€™t matter, use "
                },
                {
                  "type": "codeVoice",
                  "code": "kLSRolesAll"
                },
                {
                  "type": "text",
                  "text": ". For possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsrolesmask"
                },
                {
                  "type": "text",
                  "text": ". If the "
                },
                {
                  "type": "codeVoice",
                  "code": "inAppParams"
                },
                {
                  "type": "text",
                  "text": " parameter is not "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", this parameter is ignored."
                }
              ]
            }
          ]
        },
        {
          "name": "inAEParam",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "AEKeyDesc"
                },
                {
                  "type": "text",
                  "text": " that is to be attached to the Apple Event(s) generated by Launch Services with the specified "
                },
                {
                  "type": "codeVoice",
                  "code": "AEKeyword"
                },
                {
                  "type": "text",
                  "text": ". This parameter can be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "inAppParams",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsapplicationparameters"
                },
                {
                  "type": "text",
                  "text": " structure specifying the application to launch and its launch parameters, in which case the "
                },
                {
                  "type": "codeVoice",
                  "code": "inRole"
                },
                {
                  "type": "text",
                  "text": " parameter is ignored. This parameter can be "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", in which case an application is selected that can handle each input item in at least one of the roles specified by the "
                },
                {
                  "type": "codeVoice",
                  "code": "inRole"
                },
                {
                  "type": "text",
                  "text": " parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "outPSNs",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On input, a pointer to a caller-allocated buffer or "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you donâ€™t want to receive process serial number (PSN) information. If not "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " on input, on return, the buffer contains at each index the PSN that was used to open the item at the same index of the input item array ("
                },
                {
                  "type": "codeVoice",
                  "code": "inItems"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            }
          ]
        },
        {
          "name": "inMaxPSNCount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum number of PSNs that the buffer pointed to by "
                },
                {
                  "type": "codeVoice",
                  "code": "outPSNs"
                },
                {
                  "type": "text",
                  "text": " can hold."
                }
              ]
            }
          ]
        }
      ]
    },
