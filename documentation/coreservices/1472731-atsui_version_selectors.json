      "abstract": [
        {
          "type": "text",
          "text": "Specifies the version of ATSUI installed on the user’s system. You pass this selector to the "
        },
        {
          "type": "codeVoice",
          "code": "Gestalt"
        },
        {
          "type": "text",
          "text": " function. On return, the "
        },
        {
          "type": "codeVoice",
          "code": "Gestalt"
        },
        {
          "type": "text",
          "text": " function passes back a value that represents the version of ATSUI installed on the user’s system. "
        }
      ]
    },
