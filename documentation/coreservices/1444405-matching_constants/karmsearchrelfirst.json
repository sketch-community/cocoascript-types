      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "kARMSearch"
            },
            {
              "type": "text",
              "text": " is also set, perform a relative search before the absolute search. (If "
            },
            {
              "type": "codeVoice",
              "code": "kARMSearch"
            },
            {
              "type": "text",
              "text": " is also set and the target is found through the absolute search, "
            },
            {
              "type": "codeVoice",
              "code": "MatchAlias"
            },
            {
              "type": "text",
              "text": " sets the "
            },
            {
              "type": "codeVoice",
              "code": "needsUpdate"
            },
            {
              "type": "text",
              "text": " flag to "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": ".) If neither "
            },
            {
              "type": "codeVoice",
              "code": "kARMSearch"
            },
            {
              "type": "text",
              "text": " nor "
            },
            {
              "type": "codeVoice",
              "code": "kARMSearchMore"
            },
            {
              "type": "text",
              "text": " is set, perform only a relative search. If "
            },
            {
              "type": "codeVoice",
              "code": "kARMSearch"
            },
            {
              "type": "text",
              "text": " is not set but "
            },
            {
              "type": "codeVoice",
              "code": "kARMSearchMore"
            },
            {
              "type": "text",
              "text": " is set, perform a relative search followed by an exhaustive search."
            }
          ]
        }
      ]
    }
