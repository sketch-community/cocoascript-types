        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the corresponding bit is set, then the starting offset for the "
            },
            {
              "type": "codeVoice",
              "code": "UCFindTextBreak"
            },
            {
              "type": "text",
              "text": " function is assumed to be in the word containing the character following the offset; this is the normal case when searching forward. If the corresponding bit is clear, then the starting offset for "
            },
            {
              "type": "codeVoice",
              "code": "UCFindTextBreak"
            },
            {
              "type": "text",
              "text": " is assumed to be in the word containing the character preceding the offset; this is the normal case when searching backward."
            }
          ]
        }
      ]
