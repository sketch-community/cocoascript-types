      "parameters": [
        {
          "name": "inItemRef",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a file-system reference designating the item about which information is requested; see the "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "File Manager Reference"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " in the Carbon File Management Documentation for a description of the "
                },
                {
                  "type": "codeVoice",
                  "code": "FSRef"
                },
                {
                  "type": "text",
                  "text": " data type."
                }
              ]
            }
          ]
        },
        {
          "name": "inWhichInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Flags specifying what information to obtain; see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsrequestedinfo"
                },
                {
                  "type": "text",
                  "text": " for a description of these flags."
                }
              ]
            }
          ]
        },
        {
          "name": "outItemInfo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an item-information record that, on return, will contain the requested information; see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsiteminforecord"
                },
                {
                  "type": "text",
                  "text": " for a description of this structure."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If you request the item’s filename extension (field "
                },
                {
                  "type": "codeVoice",
                  "code": "extension"
                },
                {
                  "type": "text",
                  "text": " of the item-information record, requested by flag "
                },
                {
                  "type": "codeVoice",
                  "code": "kLSRequestExtension"
                },
                {
                  "type": "text",
                  "text": "), you are responsible for releasing the Core Foundation string object in which the extension is returned."
                }
              ]
            }
          ]
        }
      ]
    },
