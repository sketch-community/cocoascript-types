      ],
      "parameters": [
        {
          "name": "dataDomain",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "OSType"
                },
                {
                  "type": "text",
                  "text": " value that specifies the domain to which the flipper callback applies. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1575588-domain_types/kcoreendianresourcemanagerdomain"
                },
                {
                  "type": "text",
                  "text": "if your callback applies to resource data. Pass "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1575588-domain_types/kcoreendianappleeventmanagerdomain"
                },
                {
                  "type": "text",
                  "text": " if your callback applies to Apple event data. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1575588-domain_types"
                },
                {
                  "type": "text",
                  "text": " for more information."
                }
              ]
            }
          ]
        },
        {
          "name": "dataType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "OSType"
                },
                {
                  "type": "text",
                  "text": " value that specifies the type of data for which you want your flipper callback installed. This is the four character code of the resource type or Apple event."
                }
              ]
            }
          ]
        },
        {
          "name": "proc",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to your flipper callback. The flipper callback is installed into a per-process table that is searched before the system table."
                }
              ]
            }
          ]
        },
        {
          "name": "refCon",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A 32-bit value containing or referring to data needed by the callback."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code. Returns "
            },
            {
              "type": "codeVoice",
              "code": "noErr"
            },
            {
              "type": "text",
              "text": " ifyour flipper callback is installed."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You should install the callback by calling the function "
            },
            {
              "type": "codeVoice",
              "code": "CoreEndianInstallFlipper"
            },
            {
              "type": "text",
              "text": " when yourapplication calls its initialization routine or when you open yourresource file. "
            }
          ]
        }
      ]
    }
