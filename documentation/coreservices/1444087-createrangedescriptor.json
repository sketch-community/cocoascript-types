              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to an object specifier that identifies the last Apple event object in the range. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aedesc"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
