      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Specifies a container of one or more objects and a test to perform on the objects."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The key data for "
            },
            {
              "type": "codeVoice",
              "code": "formWhose"
            },
            {
              "type": "text",
              "text": " is specified by a whose descriptor, which is a coerced Apple event record of descriptor type "
            },
            {
              "type": "codeVoice",
              "code": "typeWhoseDescriptor"
            },
            {
              "type": "text",
              "text": ". The data for a whose descriptor consists of two keyword-specified descriptors with the keywords "
            },
            {
              "type": "codeVoice",
              "code": "keyAEIndex"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "keyAETest"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "See also the description for "
            },
            {
              "type": "codeVoice",
              "code": "formTest"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
