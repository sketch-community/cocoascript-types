{
  "abstract": [
    {
      "type": "text",
      "text": "If this bit is set in the "
    },
    {
      "type": "codeVoice",
      "code": "response"
    },
    {
      "type": "text",
      "text": " parameter, the File Manager no longer supports the low memory globals "
    },
    {
      "type": "codeVoice",
      "code": "FCBSPtr"
    },
    {
      "type": "text",
      "text": " and "
    },
    {
      "type": "codeVoice",
      "code": "FSFCBLen"
    },
    {
      "type": "text",
      "text": ". All access to file or fork control blocks must use the File System Manager utility functions instead. "
    }
  ],
