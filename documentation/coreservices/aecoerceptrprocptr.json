                {
                  "type": "text",
                  "text": "The desired descriptor type for the resulting descriptor. For a list of AppleScript’s predefined descriptor types, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1542788-descriptor_type_constants"
                },
