      ],
      "parameters": [
        {
          "name": "inItemURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Core Foundation URL reference designating the source item (the item to test for acceptance by the target application); see the "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "CFURL Reference"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " in the Core Foundation Reference Documentation for a description of the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFURLRef"
                },
                {
                  "type": "text",
                  "text": " data type."
                }
              ]
            }
          ]
        },
        {
          "name": "inTargetURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Core Foundation URL reference designating the target application; see the "
                },
                {
                  "type": "emphasis",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "CFURL Reference"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " in the Core Foundation Reference Documentation for a description of the "
                },
                {
                  "type": "codeVoice",
                  "code": "CFURLRef"
                },
                {
                  "type": "text",
                  "text": " data type. The URL must have scheme "
                },
                {
                  "type": "codeVoice",
                  "code": "file"
                },
                {
                  "type": "text",
                  "text": " and contain a valid path to an application file or application bundle."
                }
              ]
            }
          ]
        },
        {
          "name": "inRolesMask",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bit mask specifying the target app’s desired role or roles with respect to the source item; see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsrolesmask"
                },
                {
                  "type": "text",
                  "text": " for a description of this mask. This parameter applies only to URLs with a scheme component of "
                },
                {
                  "type": "codeVoice",
                  "code": "file"
                },
                {
                  "type": "text",
                  "text": ", and is ignored for all other schemes. If the role is unimportant, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "kLSRolesAll"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "inFlags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Flags specifying behavior to observe during the acceptance test; see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsacceptanceflags"
                },
                {
                  "type": "text",
                  "text": " for a description of these flags."
                }
              ]
            }
          ]
        },
        {
          "name": "outAcceptsItem",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a Boolean value that, on return, will indicate whether the target application can accept the source item with at least one of the specified roles."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A result code; see "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/coreservices/launch_services#1661359"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the item URL’s scheme is "
            },
            {
              "type": "codeVoice",
              "code": "file"
            },
            {
              "type": "text",
              "text": " (designating either a file or a directory), the acceptance test is based on the designated item’s filename extension, file type, and creator signature, along with the role specified by the "
            },
            {
              "type": "codeVoice",
              "code": "inRolesMask"
            },
            {
              "type": "text",
              "text": " parameter; otherwise, it is based on the URL scheme (such as "
            },
            {
              "type": "codeVoice",
              "code": "http"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "codeVoice",
              "code": "ftp"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "codeVoice",
              "code": "mailto"
            },
            {
              "type": "text",
              "text": ")."
            }
          ]
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Version-Notes",
          "anchor": "1675609"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Thread-safe since Mac OS version 10.2."
            }
          ]
        }
      ]
    }
