      ],
      "parameters": [
        {
          "name": "functionClass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The keyword for the special handler to get. You can specify any of the constants described in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1572726-special_handler_callback_constan"
                },
                {
                  "type": "text",
                  "text": ". See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aekeyword"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "handler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A universal procedure pointer. On return, a pointer to the specified special handler, if one exists that matches the value supplied in the "
                },
                {
                  "type": "codeVoice",
                  "code": "functionClass"
                },
                {
                  "type": "text",
                  "text": " parameter. See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aeeventhandlerupp"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "isSysHandler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Specifies the special handler dispatch table to get the handler from. Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "TRUE"
                },
                {
                  "type": "text",
                  "text": " to get the handler from the system special handler dispatch table or "
                },
                {
                  "type": "codeVoice",
                  "code": "FALSE"
                },
                {
                  "type": "text",
                  "text": " to get the handler from your application’s special handler dispatch table. Use of the system special handler dispatch table is not recommended."
                }
              ]
            }
          ]
        }
      ]
    },
