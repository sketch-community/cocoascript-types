      "abstract": [
        {
          "type": "text",
          "text": "The "
        },
        {
          "type": "codeVoice",
          "code": "Gestalt"
        },
        {
          "type": "text",
          "text": " selector you pass to determine which Translation Manager attributes are present."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationmgrexists": {
      "title": "gestaltTranslationMgrExists",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationmgrexists",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationmgrexists",
      "abstract": [
        {
          "type": "text",
          "text": "If "
        },
        {
          "type": "codeVoice",
          "code": "true"
        },
        {
          "type": "text",
          "text": ", the Translation Manager is present."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationmgrhintorder": {
      "title": "gestaltTranslationMgrHintOrder",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationmgrhintorder",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationmgrhintorder",
      "abstract": [
        {
          "type": "text",
          "text": "In earlier versions of the Translation Manager, the scrap hints in the "
        },
        {
          "type": "codeVoice",
          "code": "DoTranslateScrapProcPtr"
        },
        {
          "type": "text",
          "text": " function were reversed. In later versions, this was fixed. If this bit is true, this bug fix is in effect."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationppcavail": {
      "title": "gestaltTranslationPPCAvail",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationppcavail",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationppcavail",
      "abstract": [
        {
          "type": "text",
          "text": "If "
        },
        {
          "type": "codeVoice",
          "code": "true"
        },
        {
          "type": "text",
          "text": ", the PowerPC Translation Library is available, and you can call the Translation Manager from native PowerPC code."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationgetpathapiavail": {
      "title": "gestaltTranslationGetPathAPIAvail",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationgetpathapiavail",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/1471996-translation_manager_attribute_se/gestalttranslationgetpathapiavail",
      "abstract": [
        {
          "type": "text",
          "text": "If true, the functions "
        },
        {
          "type": "codeVoice",
          "code": "GetFileTranslationPath"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "GetPathTranslationDialog"
        },
        {
          "type": "text",
          "text": " are available."
        }
      ]
    },
