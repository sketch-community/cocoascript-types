      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/coreservices/uckeyboardtypeheader/1390499-keyboardtypefirst",
      "abstract": [
        {
          "type": "text",
          "text": "An unsigned 32-bit integer specifying the first keyboard type in this entry. For the initial entry (that is, the default entry) in an array of "
        },
        {
          "type": "codeVoice",
          "code": "UCKeyboardTypeHeader"
        },
        {
          "type": "text",
          "text": " structures, you should set this value to 0. The initial "
        },
        {
          "type": "codeVoice",
          "code": "UCKeyboardTypeHeader"
        },
        {
          "type": "text",
          "text": " entry is used if the keyboard type passed to the function "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/coreservices/1390584-uckeytranslate"
        },
        {
          "type": "text",
          "text": " does not match any other entry, that is, if it is not within the range of values specified by "
        },
        {
          "type": "codeVoice",
          "code": "keyboardTypeFirst"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "keyboardTypeLast"
        },
        {
          "type": "text",
          "text": " for any entry."
        }
      ]
    },
