      "url": "/documentation/coreservices/1473015-colorsync_manager_attribute_sele/gestalthighlevelmatching",
      "abstract": [
        {
          "type": "text",
          "text": "This constant is provided for backward compatibility only. Bit 0 of the "
        },
        {
          "type": "codeVoice",
          "code": "Gestalt"
        },
        {
          "type": "text",
          "text": " response value is always set if ColorSync is present."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/coreservices/1473015-colorsync_manager_attribute_sele/gestaltcolormatchinglibloaded": {
      "title": "gestaltColorMatchingLibLoaded",
