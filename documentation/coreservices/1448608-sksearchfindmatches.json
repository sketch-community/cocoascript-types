            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@CFIndex",
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfindex",
              "text": "CFIndex"
            },
            {
              "kind": "text",
              "text": " inMaximumCount, "
            },
            {
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@SKDocumentID",
              "identifier": "doc://com.apple.documentation/documentation/coreservices/skdocumentid",
              "text": "SKDocumentID"
            },
