    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "New-person view controllers are modal view controllers that manage a set of view controllers used to create a contact ("
            },
            {
              "type": "codeVoice",
              "code": "ABPersonRef"
            },
            {
              "type": "text",
              "text": ") and edit its properties."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "New-person view controllers must be used with a navigation controller in order to function properly.  It is recommended that you present a new-person view controller modally."
                }
              ]
            }
          ],
          "style": "important"
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Subclassing",
          "anchor": "1679777"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The "
            },
            {
              "type": "codeVoice",
              "code": "ABNewPersonViewController"
            },
            {
              "type": "text",
              "text": " class does not support subclassing."
            }
          ]
        }
      ]
    }
