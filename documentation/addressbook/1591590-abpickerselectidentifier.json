        {
          "name": "inPicker",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The people-picker window to manipulate."
                }
              ]
            }
          ]
        },
        {
          "name": "inPerson",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The person with the multi-value property with the value to select."
                }
              ]
            }
          ]
        },
        {
          "name": "inIdentifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The identifier of the value to select in the multi-value property."
                }
              ]
            }
          ]
        },
        {
          "name": "inExtendSelection",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": " if you want to add the value to the list of selected values in the multi-value property of the desired person; "
                },
                {
                  "type": "codeVoice",
                  "code": "false"
                },
                {
                  "type": "text",
                  "text": " if you want the value to be the only value selected in the list."
                }
              ]
            }
          ]
        }
      ]
    }
