      "parameters": [
        {
          "name": "property",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the property to search on, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "kABAddressProperty"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "kABLastNameProperty"
                },
                {
                  "type": "text",
                  "text": ". This name cannot be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ". For a full list of the properties, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/addressbook/address_book_constants/default_record_properties"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/addressbook/address_book_constants/default_person_properties"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "label",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The label name for a multivalue list, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "kABAddressHomeLabel"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "kABPhoneWorkLabel"
                },
                {
                  "type": "text",
                  "text": ", or a user-specified label, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "Summer Home"
                },
                {
                  "type": "text",
                  "text": ". If the specified property does not have multiple values, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ". If the specified property does have multiple values, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to search all the values. For a full list of label names, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/addressbook/address_book_constants/default_multivalue_list_labels"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/addressbook/address_book_constants/generic_multivalue_list_labels"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "key",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key name for a dictionary, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "kABAddressCityKey"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "kABAddressStreetKey"
                },
                {
                  "type": "text",
                  "text": ". If the specified property is not a dictionary, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ". If the specified property is a dictionary, pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to search all keys. For a full list of key names, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/addressbook/address_book_constants/address_keys"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "value",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "What you’re searching for. If "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", then the only supported value for "
                },
                {
                  "type": "codeVoice",
                  "code": "comparison"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "kABEqual"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "kABNotEqual"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "comparison",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of comparison to perform, such as "
                },
                {
                  "type": "codeVoice",
                  "code": "kABEqual"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "kABPrefixMatchCaseInsensitive"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
