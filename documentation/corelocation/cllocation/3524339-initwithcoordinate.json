      "parameters": [
        {
          "name": "coordinate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A coordinate structure containing the latitude and longitude values."
                }
              ]
            }
          ]
        },
        {
          "name": "altitude",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The altitude value for the location."
                }
              ]
            }
          ]
        },
        {
          "name": "hAccuracy",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The radius of uncertainty for the geographical coordinate, measured in meters. Specify a negative number to indicate that the geographical coordinate is invalid."
                }
              ]
            }
          ]
        },
        {
          "name": "vAccuracy",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The accuracy of the altitude value, measured in meters. Specify a negative number to indicate that the altitude is invalid."
                }
              ]
            }
          ]
        },
        {
          "name": "course",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The direction of travel for the location, measured in degrees relative to due north and continuing clockwise around the compass."
                }
              ]
            }
          ]
        },
        {
          "name": "courseAccuracy",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The accuracy of the course value, measured in degrees. Specify a negative number to indicate that the course is invalid."
                }
              ]
            }
          ]
        },
        {
          "name": "speed",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The current speed associated with this location, measured in meters per second."
                }
              ]
            }
          ]
        },
        {
          "name": "speedAccuracy",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The accuracy of the speed value, measured in meters per second. Specify a negative number to indicate that the speed is invalid."
                }
              ]
            }
          ]
        },
        {
          "name": "timestamp",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time to associate with the location object. Typically, you specify the current time."
                }
              ]
            }
          ]
        }
      ]
