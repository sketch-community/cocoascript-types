      "parameters": [
        {
          "name": "center",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The center point of the geographic region to monitor."
                }
              ]
            }
          ]
        },
        {
          "name": "radius",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The distance (measured in meters) from the center point of the geographic region to the edge of the circular boundary."
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A unique identifier to associate with the region object. You use this identifier to differentiate regions within your application. This value must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
