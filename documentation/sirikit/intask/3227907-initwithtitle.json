      "parameters": [
        {
          "name": "title",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The title text that describes the task. This string is what the user enters for the task details."
                }
              ]
            }
          ]
        },
        {
          "name": "status",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The current state of the task. Tasks can be completed or not completed."
                }
              ]
            }
          ]
        },
        {
          "name": "taskType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Information about how the task behaves in your app. Use this parameter to specify whether the task can be marked as completed."
                }
              ]
            }
          ]
        },
        {
          "name": "spatialEventTrigger",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A location-based trigger that reminds the user to complete the task."
                }
              ]
            }
          ]
        },
        {
          "name": "temporalEventTrigger",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A time-based trigger that reminds the user to complete the task. "
                }
              ]
            }
          ]
        },
        {
          "name": "createdDateComponents",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date on which the task was initially created."
                }
              ]
            }
          ]
        },
        {
          "name": "modifiedDateComponents",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The last date on which the task was updated. "
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The unique identifier that your app uses to identify the task. "
                }
              ]
            }
          ]
        },
        {
          "name": "priority",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The priority for the task."
                }
              ]
            }
          ]
        }
      ]
