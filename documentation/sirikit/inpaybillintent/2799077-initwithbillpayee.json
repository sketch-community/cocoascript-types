    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "billPayee",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The entity that receives the payment. The user must configure payees in advance using your app. The payee in this parameter should correspond to one of the user’s known entities. "
                }
              ]
            }
          ]
        },
        {
          "name": "fromAccount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The user account providing the funds for the transaction. The user must configure an account with your financial service before attempting to pay bills with that account. Use the information in the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inpaymentaccount"
                },
                {
                  "type": "text",
                  "text": " object to identify which account the user wants to use. "
                }
              ]
            }
          ]
        },
        {
          "name": "transactionAmount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The amount of money to transfer from the user’s account to the bill payee. The object in this parameter may specify a numeric value or a relative value, such as the current balance due. For more information, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inpaymentamount"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "transactionScheduledDate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date on which to make the payment. "
                }
              ]
            }
          ]
        },
        {
          "name": "transactionNote",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A note to include with the transaction. Users can include notes for logging additional information about the payment. You should also save notes in your own system so that users can view them later."
                }
              ]
            }
          ]
        },
        {
          "name": "billType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of bill to pay. Use this parameter to categorize the type of bill. You should also save this information in your own system so that users can search for bill payments by type."
                }
              ]
            }
          ]
        },
        {
          "name": "dueDate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date by which the user must pay the bill. When scheduling the bill payment, use this parameter to validate that the payment isn’t too late."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized intent object or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the object could not be created."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Normally, you don’t create instances of this class yourself. Instead, SiriKit creates instances when the user asks to pay a bill. However, you can use this method during testing to create intent objects in order to validate your intent handling code paths."
            }
          ]
        }
      ]
    }
