    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "radioType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of tuner that applies to the radio station. "
                }
              ]
            }
          ]
        },
        {
          "name": "frequency",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The broadcast frequency of the radio station. Specify the frequency as a floating-point number. "
                }
              ]
            }
          ]
        },
        {
          "name": "stationName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the radio station."
                }
              ]
            }
          ]
        },
        {
          "name": "channel",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The channel number assigned to the station. The value in this property is typically used to specify satellite channel numbers but may represent any designation for a radio channel. If you specify a value for the "
                },
                {
                  "type": "codeVoice",
                  "code": "presetNumber"
                },
                {
                  "type": "text",
                  "text": " parameter, do not specify a value for this parameter. "
                }
              ]
            }
          ]
        },
        {
          "name": "presetNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The radio preset to use. Specify an integer that corresponds to a programmable preset slot. If you specify a value for the "
                },
                {
                  "type": "codeVoice",
                  "code": "channelNumber"
                },
                {
                  "type": "text",
                  "text": " parameter, do not specify a value for this parameter."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized intent object or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the object could not be created."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Normally, you do not create instances of this class yourself. Instead, SiriKit creates instances when the user asks to change the radio station. However, you can use this method during testing to create intent objects in order to validate your intent handling code paths."
            }
          ]
        }
      ]
    }
