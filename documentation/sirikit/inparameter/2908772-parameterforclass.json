      "parameters": [
        {
          "name": "aClass",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The intent or intent response class that contains the specified key path. For example, when specifying properties of a ride request intent, you would specify the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inrequestrideintent"
                },
                {
                  "type": "text",
                  "text": " class."
                }
              ]
            }
          ]
        },
        {
          "name": "keyPath",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The path to a property of the specified class. For example, to specify the key path for the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inrequestrideintent/1649623-pickuplocation"
                },
                {
                  "type": "text",
                  "text": " property of a ride request, specify the string "
                },
                {
                  "type": "codeVoice",
                  "code": "pickupLocation"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized parameter object."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If any properties in the key path contain an array or ordered set, use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/sirikit/inparameter/2908766-setindex"
            },
            {
              "type": "text",
              "text": " method to specify which object in those properties to use. "
            }
          ]
        }
      ]
