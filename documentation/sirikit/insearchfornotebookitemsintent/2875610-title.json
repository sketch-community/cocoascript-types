        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When this property contains a valid string, search for that string in the title text of notes, tasks, or task lists. For notes, use this string to match only the portion of the note that you use to summarize the note to the user. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "When the value of this property is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", do not use the title text as part of your search criteria."
            }
          ]
        }
      ]
