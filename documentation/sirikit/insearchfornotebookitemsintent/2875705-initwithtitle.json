      "parameters": [
        {
          "name": "title",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The text to search for in the title of the note, task, or task list. "
                }
              ]
            }
          ]
        },
        {
          "name": "content",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The text to search for in the content of a note. "
                }
              ]
            }
          ]
        },
        {
          "name": "itemType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of items to search. Use this parameter to restrict your search to items of the specified type. "
                }
              ]
            }
          ]
        },
        {
          "name": "status",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The completion status of the task. Use this parameter to restrict your search to tasks whose status matches the specified value."
                }
              ]
            }
          ]
        },
        {
          "name": "location",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The location value to use during your search. When the "
                },
                {
                  "type": "codeVoice",
                  "code": "locationSearchType"
                },
                {
                  "type": "text",
                  "text": " parameter is "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inlocationsearchtype/inlocationsearchtypebylocationtrigger"
                },
                {
                  "type": "text",
                  "text": ", use this parameter to restrict your search to reminders that are triggered near the specified location.   "
                }
              ]
            }
          ]
        },
        {
          "name": "locationSearchType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of location search to perform. Use this parameter to specify when a search for location-based reminders should occur. "
                }
              ]
            }
          ]
        },
        {
          "name": "dateTime",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time value to use during your search. Use the value of the "
                },
                {
                  "type": "codeVoice",
                  "code": "dateSearchType"
                },
                {
                  "type": "text",
                  "text": " parameter to determine how to apply the value of this parameter. "
                }
              ]
            }
          ]
        },
        {
          "name": "dateSearchType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of date-based search to perform. Use this parameter to specify when to search for notes with the specified creation or modification date or when to search for reminders based on the date on which they are delivered. "
                }
              ]
            }
          ]
        }
      ]
    },
