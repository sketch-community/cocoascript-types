    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "recipients",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The recipients of the messages. You can specify multiple people to search for messages received by multiple users."
                }
              ]
            }
          ]
        },
        {
          "name": "senders",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The senders of the messages. You can specify multiple people to search for messages sent by multiple users. "
                }
              ]
            }
          ]
        },
        {
          "name": "searchTerms",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of terms to look for in the message contents. Each term constitutes a search string to be matched in the message contents."
                }
              ]
            }
          ]
        },
        {
          "name": "attributes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Additional message attributes to consider during the search. For a list of possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inmessageattributeoptions"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "dateTimeRange",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The dates associated with the messages. Use this parameter to specify the range of dates to search."
                }
              ]
            }
          ]
        },
        {
          "name": "identifiers",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The identifier strings of the messages. Use this parameter when you want to specify messages based on their unique ID. "
                }
              ]
            }
          ]
        },
        {
          "name": "notificationIdentifiers",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of identifiers from "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/usernotifications/unnotification"
                },
                {
                  "type": "text",
                  "text": " objects. Use this parameter to search the notifications associated with your app. "
                }
              ]
            }
          ]
        },
        {
          "name": "speakableGroupNames",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of strings representing any group addresses receiving the message. "
                }
              ]
            }
          ]
        },
        {
          "name": "conversationIdentifiers",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of conversation identifiers. Use this parameter to search one or more conversations."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized intent or "
