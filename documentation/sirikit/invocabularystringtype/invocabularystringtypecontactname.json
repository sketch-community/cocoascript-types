      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, you might include the strings “Jon Smith” or “Apple Inc”. Don’t include names already in the Contacts app on the user’s device. Instead, use this type to define contacts that are exclusive to your app."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Siri puts your contact names in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/sirikit/inperson/1638822-namecomponents"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/sirikit/inperson/1638921-displayname"
            },
            {
              "type": "text",
              "text": " properties of any "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/sirikit/inperson"
            },
            {
              "type": "text",
              "text": " object associated with intents related to calls, messages, payments, and photos."
            }
          ]
        }
      ]
    }
