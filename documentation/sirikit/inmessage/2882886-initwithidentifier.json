      "parameters": [
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The app-specific unique identifier for the message. Specify a value that you can use to identify the same message later. "
                }
              ]
            }
          ]
        },
        {
          "name": "conversationIdentifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The identifier for the conversation containing the message. Use this parameter to associate individual messages with a larger discussion among a specific group of users. "
                }
              ]
            }
          ]
        },
        {
          "name": "content",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Text contained in the message."
                }
              ]
            }
          ]
        },
        {
          "name": "dateSent",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date that the message was sent."
                }
              ]
            }
          ]
        },
        {
          "name": "sender",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The user that sent the message. Specify an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inperson"
                },
                {
                  "type": "text",
                  "text": " object with enough information for you to uniquely identify the user later."
                }
              ]
            }
          ]
        },
        {
          "name": "recipients",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The users that received the message. Provide an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inperson"
                },
                {
                  "type": "text",
                  "text": " object for each recipient. Each object must contain enough information for you to uniquely identify the user later."
                }
              ]
            }
          ]
        },
        {
          "name": "messageType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of content contained in the message."
                }
              ]
            }
          ]
        }
      ]
    },
