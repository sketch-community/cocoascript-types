      ],
      "parameters": [
        {
          "name": "handle",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string containing information that your app uses to identify the driver. You might use an email address, phone number, account name, or any information that lets you identify the user uniquely.  "
                }
              ]
            }
          ]
        },
        {
          "name": "nameComponents",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the driver, specified as a set of name components."
                }
              ]
            }
          ]
        },
        {
          "name": "image",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A picture of the driver."
                }
              ]
            }
          ]
        },
        {
          "name": "rating",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The driver’s rating. Strings should be short and informative. For example, you might specify the string “Good” or a string containing Unicode star (★, ☆) characters."
                }
              ]
            }
          ]
        },
        {
          "name": "phoneNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The driver’s phone number. The user should be able to contact the driver at this number to coordinate ride details. "
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
