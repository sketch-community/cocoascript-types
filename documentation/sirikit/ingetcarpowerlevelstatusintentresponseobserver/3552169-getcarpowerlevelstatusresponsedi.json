      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You call this method to deliver updated power level information to Maps, which only requests updates during an active navigation session. During that session, Maps regularly asks for the vehicle’s power level status using "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/sirikit/ingetcarpowerlevelstatusintent"
            },
            {
              "type": "text",
              "text": ". Only call this method when there’s an abrupt change in the vehicle’s battery charge—a 3 percent or greater drop—since handling the last intent."
            }
          ]
        }
      ]
    }
