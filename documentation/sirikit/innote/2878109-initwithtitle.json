      "parameters": [
        {
          "name": "title",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The title text for the note. This string represents the initial text that users see for the note in your interface. "
                }
              ]
            }
          ]
        },
        {
          "name": "contents",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The main content of the note. Use this parameter to specify the text and images that the note contains. "
                }
              ]
            }
          ]
        },
        {
          "name": "groupName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The container in which the note resides. For example, if your app organizes notes into named folders, use this property to specify the name of the folder. "
                }
              ]
            }
          ]
        },
        {
          "name": "createdDateComponents",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date on which the note was created."
                }
              ]
            }
          ]
        },
        {
          "name": "modifiedDateComponents",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date on which the note was last modified. "
                }
              ]
            }
          ]
        },
        {
          "name": "identifier",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A unique identifier that you associate with the note internally. "
                }
              ]
            }
          ]
        }
      ]
    },
