          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block you call with your response. You must call this block within your implementation of this method and pass an instance of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/inlistcarsintentresponse"
                },
                {
                  "type": "text",
                  "text": " that contains a status code, and a list of the user’s electric vehicles, if available."
                }
              ]
            }
          ]
