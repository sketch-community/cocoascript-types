      "parameters": [
        {
          "name": "profileNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An integer number to use when saving the profile. This value represents the index of the profile in your system. The index can be a soft index inside your vehicle’s electronics systems or it can correspond to physical buttons that the user presses. "
                }
              ]
            }
          ]
        },
        {
          "name": "profileName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the profile."
                }
              ]
            }
          ]
        }
      ]
    },
    {
