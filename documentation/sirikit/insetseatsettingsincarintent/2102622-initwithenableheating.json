      "parameters": [
        {
          "name": "enableHeating",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value indicating whether to enable the seat heater system. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to modify this setting."
                }
              ]
            }
          ]
        },
        {
          "name": "enableCooling",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value indicating whether to enable the seat cooling system. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to modify this setting."
                }
              ]
            }
          ]
        },
        {
          "name": "enableMassage",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A Boolean value indicating whether to enable the seat massage system. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to modify this setting."
                }
              ]
            }
          ]
        },
        {
          "name": "seat",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The seat position to modify."
                }
              ]
            }
          ]
        },
        {
          "name": "level",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The heating, cooling, or massage level, specified as an integer indicating the position of the corresponding controls. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to use this parameter to make the change. "
                }
              ]
            }
          ]
        },
        {
          "name": "relativeLevelSetting",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The heating, cooling, or massage level, specified as a change relative to the current level. Specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if you do not want to use this parameter to make the change. "
                }
              ]
            }
          ]
        }
      ]
    },
