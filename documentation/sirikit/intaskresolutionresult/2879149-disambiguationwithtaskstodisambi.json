          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/sirikit/intask"
                },
                {
                  "type": "text",
                  "text": " objects for the user to choose from. "
                }
              ]
            }
          ]
        }
