        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this property to specify the minimum party size that the restaurant allows. The system provides the user with this information and prevents the user from booking reservations when the party size is below this value."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Specify "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " for this property if there is no minimum party size for the restaurant."
            }
          ]
        }
      ]
    }
