      "parameters": [
        {
          "name": "dateCreated",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range of date stamps of the photos. "
                }
              ]
            }
          ]
        },
        {
          "name": "locationCreated",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The geographic location of the photos."
                }
              ]
            }
          ]
        },
        {
          "name": "albumName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the album that contains the photos."
                }
              ]
            }
          ]
        },
        {
          "name": "searchTerms",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of terms to look for in the photo. These terms can refer to tags, keywords, descriptions, or names of people associated with the photo. "
                }
              ]
            }
          ]
        },
        {
          "name": "includedAttributes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The attributes that are present in the photos. This parameter may contain multiple attributes. "
                }
              ]
            }
          ]
        },
        {
          "name": "excludedAttributes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The attributes that aren’t present in the photos. This parameter may contain multiple attributes. "
                }
              ]
            }
          ]
        },
        {
          "name": "peopleInPhoto",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "One or more users present in the photos."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
