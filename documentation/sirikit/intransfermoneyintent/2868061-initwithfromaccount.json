      "parameters": [
        {
          "name": "fromAccount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The account containing the funds to be transferred.  "
                }
              ]
            }
          ]
        },
        {
          "name": "toAccount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The account to receive the funds. "
                }
              ]
            }
          ]
        },
        {
          "name": "transactionAmount",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The amount of money to transfer between the accounts. When transferring money between accounts, you typically specify a fixed amount of money to transfer. "
                }
              ]
            }
          ]
        },
        {
          "name": "transactionScheduledDate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The date on which to transfer the money. "
                }
              ]
            }
          ]
        },
        {
          "name": "transactionNote",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An optional note to include with the transaction."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized intent object or "
