        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The cookie will be sent with requests for this path in the cookie's domain, and all paths that have this prefix. A path of "
            },
            {
              "type": "codeVoice",
              "code": "\"/\""
            },
            {
              "type": "text",
              "text": " means the cookie will be sent for all URLs in the domain."
            }
          ]
        }
      ]
