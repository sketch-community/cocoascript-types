    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusatend": {
      "title": "NSStreamStatusAtEnd",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusatend",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatusatend",
      "abstract": [
        {
          "type": "text",
          "text": "There is no more data to read, or no more data can be written to the stream. When this status is returned, the stream is in a “non-blocking” mode and no data are available."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusclosed": {
      "title": "NSStreamStatusClosed",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusclosed",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatusclosed",
      "abstract": [
        {
          "type": "text",
          "text": "The stream is closed ("
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsstream/1410399-close"
        },
        {
          "type": "text",
          "text": " has been called on it)."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatuserror": {
      "title": "NSStreamStatusError",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatuserror",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatuserror",
      "abstract": [
        {
          "type": "text",
          "text": "The remote end of the connection can’t be contacted, or the connection has been severed for some other reason. "
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusnotopen": {
      "title": "NSStreamStatusNotOpen",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusnotopen",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatusnotopen",
      "abstract": [
        {
          "type": "text",
          "text": "The stream is not open for reading or writing. This status is returned before the underlying call to open a stream but after it’s been created."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusopen": {
      "title": "NSStreamStatusOpen",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusopen",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatusopen",
      "abstract": [
        {
          "type": "text",
          "text": "The stream is open, but no reading or writing is occurring."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusopening": {
      "title": "NSStreamStatusOpening",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusopening",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatusopening",
      "abstract": [
        {
          "type": "text",
          "text": "The stream is in the process of being opened for reading or for writing. For network streams, this status might include the time after the stream was opened, but while network DNS resolution is happening."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusreading": {
      "title": "NSStreamStatusReading",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatusreading",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatusreading",
      "abstract": [
        {
          "type": "text",
          "text": "Data is being read from the stream. This status would be returned if code on another thread were to call "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsstream/1413038-streamstatus"
        },
        {
          "type": "text",
          "text": " on the stream while a "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsinputstream/1411544-read"
        },
        {
          "type": "text",
          "text": " call ("
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsinputstream"
        },
        {
          "type": "text",
          "text": ") was in progress."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatuswriting": {
      "title": "NSStreamStatusWriting",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsstreamstatus/nsstreamstatuswriting",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsstreamstatus/nsstreamstatuswriting",
      "abstract": [
        {
          "type": "text",
          "text": "Data is being written to the stream. This status would be returned if code on another thread were to call "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsstream/1413038-streamstatus"
        },
        {
          "type": "text",
          "text": " on the stream while a "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsoutputstream/1410720-write"
        },
        {
          "type": "text",
          "text": " call ("
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsoutputstream"
        },
        {
          "type": "text",
          "text": ") was in progress."
        }
      ]
    },
