      "parameters": [
        {
          "name": "interval",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The time interval on which to publish events. For example, a value of "
                },
                {
                  "type": "codeVoice",
                  "code": "0.5"
                },
                {
                  "type": "text",
                  "text": " publishes an event approximately every half-second."
                }
              ]
            }
          ]
        },
        {
          "name": "tolerance",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The allowed timing variance when emitting events. Defaults to "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", which allows any variance."
                }
              ]
            }
          ]
        },
        {
          "name": "runLoop",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The run loop on which the timer runs."
                }
              ]
            }
          ]
        },
        {
          "name": "mode",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The run loop mode in which to run the timer."
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Scheduler options passed to the timer. Defaults to "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
