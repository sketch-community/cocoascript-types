    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The getter for this property retains any percent encoding this component may have. Setting this properties assumes the component string is already correctly percent encoded. Attempting to set an incorrectly percent encoded string will cause a "
            },
            {
              "type": "codeVoice",
              "code": "fatalError"
            },
            {
              "type": "text",
              "text": ". Although ‘;’ is a legal path character, it is recommended that it be percent-encoded for best compatibility with "
            },
            {
              "type": "codeVoice",
              "code": "URL"
            },
            {
              "type": "text",
              "text": " ("
            },
            {
              "type": "codeVoice",
              "code": "String.addingPercentEncoding(withAllowedCharacters:)"
            },
            {
              "type": "text",
              "text": " will percent-encode any ‘;’ characters if you pass "
            },
            {
              "type": "codeVoice",
              "code": "CharacterSet.urlUserAllowed"
            },
            {
              "type": "text",
              "text": ")."
            }
          ]
        }
      ]
    }
