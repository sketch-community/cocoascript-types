      ],
      "parameters": [
        {
          "name": "startIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Head of the group of indexes to shift."
                }
              ]
            }
          ]
        },
        {
          "name": "delta",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Amount and direction of the shift. Positive integers shift the indexes to the right. Negative integers shift the indexes to the left."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The group of indexes shifted is made up by "
            },
            {
              "type": "codeVoice",
              "code": "startIndex"
            },
            {
              "type": "text",
              "text": " and the indexes that follow it in the set."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A left shift deletes the indexes in a range the length of "
            },
            {
              "type": "codeVoice",
              "code": "delta"
            },
            {
              "type": "text",
              "text": " preceding "
            },
            {
              "type": "codeVoice",
              "code": "startIndex"
            },
            {
              "type": "text",
              "text": " from the set."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A right shift inserts empty space in the range "
            },
            {
              "type": "codeVoice",
              "code": "("
            },
            {
              "type": "codeVoice",
              "code": "startIndex"
            },
            {
              "type": "codeVoice",
              "code": ","
            },
            {
              "type": "codeVoice",
              "code": "delta"
            },
            {
              "type": "codeVoice",
              "code": ")"
            },
            {
              "type": "text",
              "text": " in the receiver."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The resulting indexes must all be in the range "
            },
            {
              "type": "codeVoice",
              "code": "0 .. NSNotFound - 1"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
