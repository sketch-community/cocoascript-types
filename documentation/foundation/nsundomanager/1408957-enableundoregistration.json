            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsundomanager/1412239-disableundoregistration"
            },
            {
              "type": "text",
              "text": " message. Undo registration isnâ€™t actually re-enabled until an enable message balances the last disable message in effect. Raises an "
            },
            {
              "type": "codeVoice",
              "code": "NSInternalInconsistencyException"
            },
            {
              "type": "text",
              "text": " if invoked while no "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsundomanager/1412239-disableundoregistration"
            },
            {
              "type": "text",
              "text": " message is in effect."
            }
          ]
        }
