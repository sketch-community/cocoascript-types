          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of "
                },
                {
                  "type": "codeVoice",
                  "code": "NSString"
                },
                {
                  "type": "text",
                  "text": " objects that supplies the arguments to the task. If "
                },
                {
                  "type": "codeVoice",
                  "code": "arguments"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", an "
                },
                {
                  "type": "codeVoice",
