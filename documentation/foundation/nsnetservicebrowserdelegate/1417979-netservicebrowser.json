          ]
        },
        {
          "name": "netService",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Network service found by "
                },
                {
                  "type": "codeVoice",
                  "code": "netServiceBrowser"
                },
                {
                  "type": "text",
                  "text": ". The delegate can use this object to connect to and use the service."
                }
              ]
            }
          ]
        },
        {
          "name": "moreServicesComing",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " when "
                },
                {
                  "type": "codeVoice",
                  "code": "netServiceBrowser"
                },
                {
                  "type": "text",
                  "text": " is waiting for additional services. "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": " when there are no additional services."
                }
              ]
            }
          ]
        }
      ]
