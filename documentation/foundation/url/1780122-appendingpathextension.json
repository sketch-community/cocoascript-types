      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the URL has an empty path (e.g., "
            },
            {
              "type": "codeVoice",
              "code": "http://www.example.com"
            },
            {
              "type": "text",
              "text": "), then this function will return the URL unchanged."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Certain special characters (for example, Unicode Right-To-Left marks) cannot be used as path extensions. If any of those are contained in "
            },
            {
              "type": "codeVoice",
              "code": "pathExtension"
            },
            {
              "type": "text",
              "text": ", the function will return the URL unchanged."
            }
          ]
        }
      ]
    }
