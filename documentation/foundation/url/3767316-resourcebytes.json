      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this property with Swift’s "
            },
            {
              "type": "codeVoice",
              "code": "for"
            },
            {
              "type": "text",
              "text": "-"
            },
            {
              "type": "codeVoice",
              "code": "await"
            },
            {
              "type": "text",
              "text": "-"
            },
            {
              "type": "codeVoice",
              "code": "in"
            },
            {
              "type": "text",
              "text": " syntax, to read the contents of a URL byte-by-byte, like this:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "guard let url = URL(string: \"https://www.example.com\") else {",
            "    return",
            "}",
            "do {",
            "    // Read each byte of the data as it becomes available.",
            "    for try await byte in url.resourceBytes",
            "    {",
            "        // Do something with byte.",
            "    }",
            "} catch {",
            "    print (\"Error: \\(error)\")",
            "}"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3849751",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To wait for the entire resource to load, use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession"
            },
            {
              "type": "text",
              "text": " method "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession/3767353-data"
            },
            {
              "type": "text",
              "text": " with the "
            },
            {
              "type": "codeVoice",
              "code": "await"
            },
            {
              "type": "text",
              "text": " keyword. "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession"
            },
            {
              "type": "text",
              "text": " also offers methods to upload data to a URL endpoint and download a URL’s contents to a file."
            }
          ]
        }
      ]
