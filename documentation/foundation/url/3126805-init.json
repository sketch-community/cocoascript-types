      ],
      "parameters": [
        {
          "name": "data",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The bookmark data used to construct a URL."
                }
              ]
            }
          ]
        },
        {
          "name": "options",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Options taken into account when resolving the bookmark data."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "To resolve a security-scoped bookmark to support App Sandbox, include the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurl/bookmarkresolutionoptions/1412803-withsecurityscope"
                },
                {
                  "type": "text",
                  "text": " option."
                }
              ]
            }
          ]
        },
        {
          "name": "relativeTo",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The base URL that the bookmark data is relative to."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If you’re resolving a security-scoped bookmark to obtain a security-scoped URL, use this parameter as follows:"
                }
              ]
            },
            {
              "type": "unorderedList",
              "items": [
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "To resolve an app-scoped bookmark, use a value of "
                        },
                        {
                          "type": "codeVoice",
                          "code": "nil"
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "To resolve a document-scoped bookmark, use the "
                        },
                        {
                          "type": "emphasis",
                          "inlineContent": [
                            {
                              "type": "text",
                              "text": "absolute"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " path (despite this parameter’s name) to the document from which you retrieved the bookmark."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "App Sandbox doesn’t restrict which URL values you can pass to this parameter."
                }
              ]
            }
          ]
        },
        {
          "name": "bookmarkDataIsStale",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On return, if "
                },
                {
                  "type": "codeVoice",
                  "code": "true"
                },
                {
                  "type": "text",
                  "text": ", the bookmark data is stale. Your app should create a new bookmark using the returned URL and use it in place of any stored copies of the existing bookmark."
                }
              ]
            }
          ]
        }
      ]
    }
