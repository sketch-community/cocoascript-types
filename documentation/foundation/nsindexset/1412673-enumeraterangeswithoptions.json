      "parameters": [
        {
          "name": "opts",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bitmask that specifies the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsenumerationoptions"
                },
                {
                  "type": "text",
                  "text": " for the enumeration (whether it should be performed concurrently and whether it should be performed in reverse order)."
                }
              ]
            }
          ]
        },
        {
          "name": "block",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block to apply to elements in the index set."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block takes two arguments:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "range"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The range of objects of the elements in the index set."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "stop"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "A reference to a Boolean value. The block can set the value to "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                          },
                          {
                            "type": "text",
                            "text": " to stop further processing of the array. The stop argument is an out-only argument. You should only ever set this Boolean to "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                          },
                          {
                            "type": "text",
                            "text": " within the Block."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, the enumeration starts with the first object and continues serially through the indexed set range to the last object in the range. You can specify "
            },
            {
              "type": "codeVoice",
              "code": "NSEnumerationConcurrent"
            },
            {
              "type": "text",
              "text": " and/or "
            },
            {
              "type": "codeVoice",
              "code": "NSEnumerationReverse"
            },
            {
              "type": "text",
              "text": " as enumeration options to modify this behavior."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method executes synchronously."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If the Block parameter is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " this method will raise an exception."
                }
              ]
            }
          ],
          "style": "important"
        }
      ]
    }
