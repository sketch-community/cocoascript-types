    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "name",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the notification to register for delivery to the observer block. Specify a notification name to deliver only entries with this notification name."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the sender doesn’t use notification names as criteria for delivery."
                }
              ]
            }
          ]
        },
        {
          "name": "obj",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object that sends notifications to the observer block. Specify a sender to deliver only notifications from this sender."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the notification center doesn’t use the sender as criteria for the delivery."
                }
              ]
            }
          ]
        },
        {
          "name": "queue",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The operation queue where the "
                },
                {
                  "type": "codeVoice",
                  "code": "block"
                },
                {
                  "type": "text",
                  "text": " runs."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the block runs synchronously on the posting thread."
                }
              ]
            }
          ]
        },
        {
          "name": "block",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block that executes when receiving a notification."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The notification center copies the block. The notification center strongly holds the copied block until you remove the observer registration."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block takes one argument: the notification."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An opaque object to act as the observer. Notification center strongly holds this return value until you remove the observer registration."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If a notification triggers more than one observer block, the blocks can all execute concurrently (but on their queue or on the current thread)."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following example shows how you can register to receive locale change notifications."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSNotificationCenter *center = [NSNotificationCenter defaultCenter];",
            "NSOperationQueue *mainQueue = [NSOperationQueue mainQueue];",
            "self.localeChangeObserver = [center addObserverForName:NSCurrentLocaleDidChangeNotification object:nil",
            "    queue:mainQueue usingBlock:^(NSNotification *note) {",
            " ",
            "        NSLog(@\"The user's locale changed to: %@\", [[NSLocale currentLocale] localeIdentifier]);",
            "    }];"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555724",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": []
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "To avoid a retain cycle, use a weak reference to "
                },
                {
                  "type": "codeVoice",
                  "code": "self"
                },
                {
                  "type": "text",
                  "text": " inside the block when "
                },
                {
                  "type": "codeVoice",
                  "code": "self"
                },
                {
                  "type": "text",
                  "text": " contains the observer as a strong reference."
                }
              ]
            }
          ],
          "style": "tip"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Unregister an observer to stop receiving notifications. To unregister an observer, use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationcenter/1413994-removeobserver"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationcenter/1407263-removeobserver"
            },
            {
              "type": "text",
              "text": " with the most specific detail possible. For example, if you used a name and object to register the observer, use the name and object to remove it."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You must invoke "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationcenter/1413994-removeobserver"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationcenter/1407263-removeobserver"
            },
            {
              "type": "text",
              "text": " before the system deallocates any object that "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationcenter/1411723-addobserverforname"
            },
            {
              "type": "text",
              "text": " specifies."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        },
        {
          "type": "codeListing",
          "code": [
            "NSNotificationCenter *center = [NSNotificationCenter defaultCenter];",
            "[center removeObserver:self.localeChangeObserver];"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555726",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Another common practice is to create a one-time notification by removing the observer from within the observation block, as in the following example."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSNotificationCenter * __weak center = [NSNotificationCenter defaultCenter];",
            "id __block token = [center addObserverForName:@\"OneTimeNotification\"",
            "                                       object:nil",
            "                                        queue:[NSOperationQueue mainQueue]",
            "                                   usingBlock:^(NSNotification *note) {",
            "                                       NSLog(@\"Received the notification!\");",
            "                                       [center removeObserver:token];",
            "                                   }];"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555729",
            "title": "Listing 3"
          }
        }
      ]
    }
