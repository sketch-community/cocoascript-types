            {
              "type": "codeVoice",
              "code": "launchd.plist"
            },
            {
              "type": "text",
              "text": " contained in "
            },
            {
              "type": "codeVoice",
              "code": "~/Library/LaunchAgents"
            },
            {
              "type": "text",
              "text": ", or a daemon launched by launchd with a "
            },
            {
              "type": "codeVoice",
              "code": "launchd.plist"
            },
            {
              "type": "text",
              "text": " contained in "
            },
            {
              "type": "codeVoice",
