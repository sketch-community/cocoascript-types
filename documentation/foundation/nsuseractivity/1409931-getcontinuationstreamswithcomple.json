          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The completion handler block that returns streams."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block takes three arguments:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "codeVoice",
                        "code": "inputStream"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The stream from which the continuing app can read data written by the originating app."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "codeVoice",
                        "code": "outputStream"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The stream to which the continuing app writes data to be read by the originating app."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "codeVoice",
                        "code": "error"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "If successful, "
                          },
                          {
                            "type": "codeVoice",
                            "code": "nil"
                          },
                          {
                            "type": "text",
                            "text": "; if not successful, an "
                          },
                          {
                            "type": "reference",
                            "isActive": true,
                            "identifier": "doc://com.apple.documentation/documentation/foundation/nserror"
                          },
                          {
                            "type": "text",
                            "text": " object that encapsulates the reason why the streams could not be created."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
