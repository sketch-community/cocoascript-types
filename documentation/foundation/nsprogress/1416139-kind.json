        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property identifies the kind of progress being made, such as "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogresskindfile"
            },
            {
              "type": "text",
              "text": ". It can be "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the value of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogress/1417251-localizeddescription"
            },
            {
              "type": "text",
              "text": " property has not previously been set to a non-nil value, the default "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogress/1417251-localizeddescription"
            },
            {
              "type": "text",
              "text": " getter uses the progress kind to determine how to use the values of other properties, as well as values in the user info dictionary, to create a string that is presentable to the user."
            }
          ]
        }
      ]
