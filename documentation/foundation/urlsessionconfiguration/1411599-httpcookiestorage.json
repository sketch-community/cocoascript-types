      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property determines the cookie storage object used by all tasks within sessions based on this configuration."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To disable cookie storage, set this property to "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For default and background sessions, the default value is the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/httpcookiestorage/1416095-shared"
            },
            {
              "type": "text",
              "text": " cookie storage object."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessionconfiguration/1410529-ephemeral"
            },
            {
              "type": "text",
              "text": " sessions, the default value is a private cookie storage object that stores data in memory only, and is destroyed when you invalidate the session."
            }
          ]
        }
      ]
    }
