    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property determines the maximum number of simultaneous connections made to each host by tasks within sessions based on this configuration."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This limit is per session, so if you use multiple sessions, your app as a whole may exceed this limit. Additionally, depending on your connection to the Internet, a session may use a lower limit than the one you specify."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The default value is "
            },
            {
              "type": "codeVoice",
              "code": "6"
            },
            {
              "type": "text",
              "text": " in macOS, or "
            },
            {
              "type": "codeVoice",
              "code": "4"
            },
            {
              "type": "text",
              "text": " in iOS."
            }
          ]
        }
      ]
    }
