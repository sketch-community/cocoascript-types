        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Option specifying whether to only consider suggested string encodings. Use this only if you specify a value for "
            },
            {
              "type": "codeVoice",
              "code": "NSStringEncodingDetectionSuggestedEncodingsKey"
            },
            {
              "type": "text",
              "text": ". The corresponding value for this key is an "
            },
            {
              "type": "codeVoice",
              "code": "NSNumber"
            },
            {
              "type": "text",
              "text": " object containing a Boolean value. By default, this value is "
            },
            {
              "type": "codeVoice",
              "code": "@(NO)"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
