    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The receiver first completes gathering any unprocessed results. If a query is stopped before the gathering phase finishes, it does not post an "
            },
            {
              "type": "codeVoice",
              "code": "NSMetadataQueryDidStartGatheringNotification"
            },
            {
              "type": "text",
              "text": " notification."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You call this function to stop a query that is generating too many results to be useful but you still want to access the available results. If the receiver is sent a "
            },
            {
              "type": "codeVoice",
              "code": "startQuery"
            },
            {
              "type": "text",
              "text": " message after performing this method, the existing results are discarded."
            }
          ]
        }
      ]
    }
