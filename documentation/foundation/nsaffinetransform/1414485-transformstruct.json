            {
              "type": "text",
              "text": ", and the six-element structure defined by the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsaffinetransformstruct"
            },
            {
              "type": "text",
              "text": " structure is of the form:"
            }
          ]
