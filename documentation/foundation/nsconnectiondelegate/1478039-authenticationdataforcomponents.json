          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array containing the elements of a network message, in the form of "
                },
                {
                  "type": "codeVoice",
                  "code": "NSPort"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "NSData"
                },
                {
                  "type": "text",
                  "text": " objects. "
                }
              ]
            }
          ]
        }
      ]
