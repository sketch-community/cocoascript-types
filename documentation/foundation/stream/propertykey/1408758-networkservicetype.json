  "abstract": [
    {
      "type": "text",
      "text": "The type of service for the stream. Providing the service type allows the system to properly handle certain attributes of the stream, including routing and suspension behavior. Most streams do not need to set this property. See "
    },
    {
      "type": "codeVoice",
      "code": "Stream Service Types"
    },
    {
      "type": "text",
      "text": " for a list of possible values."
    }
