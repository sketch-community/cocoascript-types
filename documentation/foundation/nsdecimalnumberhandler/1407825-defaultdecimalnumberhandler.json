    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This default decimal number handler rounds to the closest possible return value. It assumes your need for precision does not exceed 38 significant digits, and it raises an exception when its "
            },
            {
              "type": "codeVoice",
              "code": "NSDecimalNumber"
            },
            {
              "type": "text",
              "text": " object tries to divide by "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": " or when its "
            },
            {
              "type": "codeVoice",
              "code": "NSDecimalNumber"
            },
            {
              "type": "text",
              "text": " object produces a number too big or too small to be represented."
            }
          ]
        }
      ]
    }
