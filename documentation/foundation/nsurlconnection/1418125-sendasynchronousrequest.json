    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "request",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL request to load. The "
                },
                {
                  "type": "codeVoice",
                  "code": "request"
                },
                {
                  "type": "text",
                  "text": " object is deep-copied as part of the initialization process. Changes made to "
                },
                {
                  "type": "codeVoice",
                  "code": "request"
                },
                {
                  "type": "text",
                  "text": " after this method returns do not affect the request that is used for the loading process."
                }
              ]
            }
          ]
        },
        {
          "name": "queue",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The operation queue to which the handler block is dispatched when the request completes or failed."
                }
              ]
            }
          ]
        },
        {
          "name": "handler",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The handler block to execute."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the request completes successfully, the "
            },
            {
              "type": "codeVoice",
              "code": "data"
            },
            {
              "type": "text",
              "text": " parameter of the handler block contains the resource data, and the "
            },
            {
              "type": "codeVoice",
              "code": "error"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ".  If the request fails, the "
            },
            {
              "type": "codeVoice",
              "code": "data"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " and the error parameter contain information about the failure."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If authentication is required in order to download the request, the required credentials must be specified as part of the URL. If authentication fails, or credentials are missing, the connection will attempt to continue without credentials. If the request finishes with a "
            },
            {
              "type": "codeVoice",
              "code": "401 Unauthorized"
            },
            {
              "type": "text",
              "text": " status code, the "
            },
            {
              "type": "codeVoice",
              "code": "response"
            },
            {
              "type": "text",
              "text": " parameter is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", the "
            },
            {
              "type": "codeVoice",
              "code": "data"
            },
            {
              "type": "text",
              "text": " parameter contains the resource data, and the "
            },
            {
              "type": "codeVoice",
              "code": "error"
            },
            {
              "type": "text",
              "text": " parameter is an "
            },
            {
              "type": "codeVoice",
              "code": "NSError"
            },
            {
              "type": "text",
              "text": " with the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/1508628-url_loading_system_error_codes/nsurlerrorusercancelledauthentication"
            },
            {
              "type": "text",
              "text": " code in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlerrordomain"
            },
            {
              "type": "text",
              "text": " error domain."
            }
          ]
        }
      ]
    }
