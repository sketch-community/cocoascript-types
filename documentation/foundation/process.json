    "doc://com.apple.documentation/documentation/foundation/process/1413681-didterminatenotification": {
      "title": "didTerminateNotification",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/process/1413681-didterminatenotification",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/process/1413681-didterminatenotification",
      "abstract": [
        {
          "type": "text",
          "text": "Posted when the task has stopped execution. This notification can be posted either when the task has exited normally or as a result of "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/process/1409620-terminate"
        },
        {
          "type": "text",
          "text": " being sent to the "
        },
        {
          "type": "codeVoice",
          "code": "NSTask"
        },
        {
          "type": "text",
          "text": " object. If the "
        },
        {
          "type": "codeVoice",
          "code": "NSTask"
        },
        {
          "type": "text",
          "text": " object gets released, however, this notification will not get sent, as the port the message would have been sent on was released as part of the task release. The observer method can use "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/process/1415801-terminationstatus"
        },
        {
          "type": "text",
          "text": " to determine why the task died. See Ending an NSTask for an example."
        }
      ],
      "fragments": [
        {
          "kind": "text",
          "text": "class let "
        },
        {
          "kind": "identifier",
          "text": "didTerminateNotification"
        },
        {
          "kind": "text",
          "text": ": NSNotification.Name"
        }
