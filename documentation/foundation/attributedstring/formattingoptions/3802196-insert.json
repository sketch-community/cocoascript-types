          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In the following example, the "
            },
            {
              "type": "codeVoice",
              "code": ".secondDay"
            },
            {
              "type": "text",
              "text": " shipping option is added to the "
            },
            {
              "type": "codeVoice",
              "code": "freeOptions"
            },
            {
              "type": "text",
              "text": " option set if "
            },
            {
              "type": "codeVoice",
              "code": "purchasePrice"
            },
            {
              "type": "text",
              "text": " is greater than 50.0. For the "
            },
            {
              "type": "codeVoice",
              "code": "ShippingOptions"
            },
            {
              "type": "text",
              "text": " declaration, see the "
            },
            {
              "type": "codeVoice",
              "code": "OptionSet"
            },
            {
              "type": "text",
              "text": " protocol discussion."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let purchasePrice = 87.55",
            "",
            "var freeOptions: ShippingOptions = [.standard, .priority]",
            "if purchasePrice > 50 {",
            "    freeOptions.insert(.secondDay)",
            "}",
            "print(freeOptions.contains(.secondDay))",
            "// Prints \"true\""
          ],
