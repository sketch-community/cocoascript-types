    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "(true, newMember)"
            },
            {
              "type": "text",
              "text": " if "
            },
            {
              "type": "codeVoice",
              "code": "newMember"
            },
            {
              "type": "text",
              "text": " was not contained in "
            },
            {
              "type": "codeVoice",
              "code": "self"
            },
            {
              "type": "text",
              "text": ". Otherwise, returns "
            },
            {
              "type": "codeVoice",
              "code": "(false, oldMember)"
            },
            {
              "type": "text",
              "text": ", where "
            },
            {
              "type": "codeVoice",
              "code": "oldMember"
            },
            {
              "type": "text",
              "text": " is the member of the set equal to "
            },
            {
              "type": "codeVoice",
              "code": "newMember"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In the following example, the "
            },
            {
              "type": "codeVoice",
              "code": ".secondDay"
            },
            {
              "type": "text",
              "text": " shipping option is added to the "
            },
            {
              "type": "codeVoice",
              "code": "freeOptions"
            },
            {
              "type": "text",
              "text": " option set if "
            },
            {
              "type": "codeVoice",
              "code": "purchasePrice"
            },
            {
              "type": "text",
              "text": " is greater than 50.0. For the "
            },
            {
              "type": "codeVoice",
              "code": "ShippingOptions"
            },
            {
              "type": "text",
              "text": " declaration, see the "
            },
            {
              "type": "codeVoice",
              "code": "OptionSet"
            },
            {
              "type": "text",
              "text": " protocol discussion."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let purchasePrice = 87.55",
            "",
            "var freeOptions: ShippingOptions = [.standard, .priority]",
            "if purchasePrice > 50 {",
            "    freeOptions.insert(.secondDay)",
            "}",
            "print(freeOptions.contains(.secondDay))",
            "// Prints \"true\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3802241",
            "title": "Listing 1"
          }
        }
      ]
