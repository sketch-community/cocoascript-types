        },
        {
          "type": "codeListing",
          "code": [
            "var numbers = [1, 2, 3, 4, 5]",
            "numbers.append(contentsOf: 10...15)",
            "print(numbers)",
            "// Prints \"[1, 2, 3, 4, 5, 10, 11, 12, 13, 14, 15]\""
          ],
