      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "To check whether a collection is empty, use its "
            },
            {
              "type": "codeVoice",
              "code": "isEmpty"
            },
            {
              "type": "text",
              "text": " property instead of comparing "
            },
            {
              "type": "codeVoice",
              "code": "count"
            },
            {
              "type": "text",
              "text": " to zero. Unless the collection guarantees random-access performance, calculating "
            },
            {
              "type": "codeVoice",
              "code": "count"
            },
            {
              "type": "text",
              "text": " can be an O("
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "n"
                }
              ]
            },
            {
              "type": "text",
              "text": ") operation."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Complexity: O(1) if the collection conforms to "
            },
            {
              "type": "codeVoice",
              "code": "RandomAccessCollection"
            },
            {
              "type": "text",
              "text": "; otherwise, O("
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "n"
                }
              ]
            },
            {
              "type": "text",
              "text": "), where "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "n"
                }
              ]
            },
            {
              "type": "text",
              "text": " is the length of the collection."
            }
          ]
        }
      ]
    }
