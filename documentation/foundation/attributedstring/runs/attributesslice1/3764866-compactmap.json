          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An array of the non-"
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " results of calling "
            },
            {
              "type": "codeVoice",
              "code": "transform"
            },
            {
              "type": "text",
              "text": " with each element of the sequence."
            }
          ]
        }
