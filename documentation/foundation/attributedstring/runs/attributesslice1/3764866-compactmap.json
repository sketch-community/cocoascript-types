    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this method to receive an array of non-optional values when your transformation produces an optional value."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, note the difference in the result of using "
            },
            {
              "type": "codeVoice",
              "code": "map"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "compactMap"
            },
            {
              "type": "text",
              "text": " with a transformation that returns an optional "
            },
            {
              "type": "codeVoice",
              "code": "Int"
            },
            {
              "type": "text",
              "text": " value."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let possibleNumbers = [\"1\", \"2\", \"three\", \"///4///\", \"5\"]",
            "",
            "let mapped: [Int?] = possibleNumbers.map { str in Int(str) }",
            "// [1, 2, nil, nil, 5]",
            "",
            "let compactMapped: [Int] = possibleNumbers.compactMap { str in Int(str) }",
            "// [1, 2, 5]"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3764867",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Complexity: O("
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "m"
                }
              ]
            },
            {
              "type": "text",
              "text": " + "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "n"
                }
              ]
            },
            {
              "type": "text",
              "text": "), where "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "n"
                }
              ]
            },
            {
              "type": "text",
              "text": " is the length of this sequence and "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "m"
                }
              ]
            },
            {
              "type": "text",
              "text": " is the length of the result."
