      ],
      "parameters": [
        {
          "name": "start",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The index at which to start the resulting subsequence. "
                },
                {
                  "type": "codeVoice",
                  "code": "start"
                },
                {
                  "type": "text",
                  "text": " must be a valid index of the collection."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A subsequence starting at the "
            },
            {
              "type": "codeVoice",
              "code": "start"
            },
            {
              "type": "text",
              "text": " position."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following example searches for the index of the number "
            },
            {
              "type": "codeVoice",
              "code": "40"
            },
            {
              "type": "text",
              "text": " in an array of integers, and then prints the suffix of the array starting at that index:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let numbers = [10, 20, 30, 40, 50, 60]",
            "if let i = numbers.firstIndex(of: 40) {",
            "    print(numbers.suffix(from: i))",
            "}",
            "// Prints \"[40, 50, 60]\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3765220",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Passing the collection’s "
            },
            {
              "type": "codeVoice",
              "code": "endIndex"
            },
            {
              "type": "text",
              "text": " as the "
            },
            {
              "type": "codeVoice",
              "code": "start"
            },
            {
              "type": "text",
              "text": " parameter results in an empty subsequence."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "print(numbers.suffix(from: numbers.endIndex))",
            "// Prints \"[]\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3765221",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Using the "
            },
            {
              "type": "codeVoice",
              "code": "suffix(from:)"
            },
            {
              "type": "text",
              "text": " method is equivalent to using a partial range from the index as the collection’s subscript. The subscript notation is preferred over "
            },
            {
              "type": "codeVoice",
              "code": "suffix(from:)"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "if let i = numbers.firstIndex(of: 40) {",
            "    print(numbers[i...])",
            "}",
            "// Prints \"[40, 50, 60]\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3765222",
            "title": "Listing 3"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Complexity: O(1)"
            }
          ]
        }
      ]
    }
