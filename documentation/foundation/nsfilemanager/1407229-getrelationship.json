          "name": "outRelationship",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A pointer to a variable in which to put the relationship between "
                },
                {
                  "type": "codeVoice",
                  "code": "directoryURL"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "otherURL"
                },
                {
                  "type": "text",
                  "text": ". For a list of possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlrelationship"
                },
                {
                  "type": "text",
                  "text": ".  "
                }
              ]
            }
          ]
        },
        {
          "name": "directoryURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL of the directory that potentially contains the item in "
                },
                {
                  "type": "codeVoice",
                  "code": "otherURL"
                },
                {
                  "type": "text",
                  "text": ". The URL in this parameter must specify a directory. This parameter must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "otherURL",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL of the file or directory whose relationship to "
                },
                {
                  "type": "codeVoice",
                  "code": "directoryURL"
                },
                {
                  "type": "text",
                  "text": " is being tested. This parameter must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "error",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "On input, a pointer to an error object. If an error occurs, this pointer is set to an actual error object containing the error information. You may specify "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " for this parameter if you do not want the error information."
                }
              ]
            }
          ]
        }
      ]
