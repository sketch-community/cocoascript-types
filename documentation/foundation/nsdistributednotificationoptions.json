    "doc://com.apple.documentation/documentation/foundation/nsnotificationdeliverimmediately": {
      "title": "NSNotificationDeliverImmediately",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationdeliverimmediately",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsnotificationdeliverimmediately",
      "abstract": [
        {
          "type": "text",
          "text": "When set, the notification is delivered immediately to all observers, regardless of their suspension behavior or suspension state. When not set, allows the normal suspension behavior of notification observers to take place."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsnotificationposttoallsessions": {
      "title": "NSNotificationPostToAllSessions",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationposttoallsessions",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsnotificationposttoallsessions",
      "abstract": [
        {
          "type": "text",
          "text": "When set, the notification is posted to all sessions. When not set, the notification is sent only to applications within the same login session as the posting task."
        }
      ]
    },
