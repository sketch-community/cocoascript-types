      "parameters": [
        {
          "name": "portName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the desired port."
                }
              ]
            }
          ]
        },
        {
          "name": "hostName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the host. "
                },
                {
                  "type": "codeVoice",
                  "code": "hostName"
                },
                {
                  "type": "text",
                  "text": " is an Internet domain name (for example, “"
                },
                {
                  "type": "codeVoice",
                  "code": "sales.anycorp.com"
                },
                {
                  "type": "text",
                  "text": "”) or IP address (IPv4 or IPv6). If "
                },
                {
                  "type": "codeVoice",
                  "code": "hostName"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " or empty, the local host is checked. If "
                },
                {
                  "type": "codeVoice",
                  "code": "hostName"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "@”*”"
                },
                {
                  "type": "text",
                  "text": ", all hosts on the local network are checked."
                }
              ]
            }
          ]
        },
        {
          "name": "portNumber",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "portNumber"
                },
                {
                  "type": "text",
                  "text": " parameter is ignored."
                }
              ]
            }
          ]
        }
      ]
    },
