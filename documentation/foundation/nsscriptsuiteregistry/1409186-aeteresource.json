      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": " This method is typically invoked to implement the "
            },
            {
              "type": "codeVoice",
              "code": "get aete"
            },
            {
              "type": "text",
              "text": " Apple event for an application that provides scriptability information in the script suite format. The "
            },
            {
              "type": "codeVoice",
              "code": "languageName"
            },
            {
              "type": "text",
              "text": " argument is the name of a language for which a localized resource directory (such as "
            },
            {
              "type": "codeVoice",
              "code": "English.lproj"
            },
            {
              "type": "text",
              "text": ") exists. This language indication specifies the set of "
            },
            {
              "type": "codeVoice",
              "code": ".scriptTerminology"
            },
            {
              "type": "text",
              "text": " files to be used to generate the data. "
            },
            {
              "type": "codeVoice",
              "code": "NSScriptSuiteRegistry"
            },
            {
              "type": "text",
              "text": " does not create an "
            },
            {
              "type": "codeVoice",
              "code": "'aete'"
            },
            {
              "type": "text",
              "text": " resource unless this method is called."
            }
          ]
        }
      ]
    }
