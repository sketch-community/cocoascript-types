      ],
      "parameters": [
        {
          "name": "object",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The value for "
                },
                {
                  "type": "codeVoice",
                  "code": "aKey"
                },
                {
                  "type": "text",
                  "text": ". A strong reference to the object is maintained by the dictionary."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Passing "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " will cause any object corresponding to "
                },
                {
                  "type": "codeVoice",
                  "code": "aKey"
                },
                {
                  "type": "text",
                  "text": " to be removed from the dictionary."
                }
              ]
            }
          ]
        },
        {
          "name": "aKey",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key for "
                },
                {
                  "type": "codeVoice",
                  "code": "value"
                },
                {
                  "type": "text",
                  "text": ". The key is copied (using "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nscopying/1410311-copywithzone"
                },
                {
                  "type": "text",
                  "text": "; keys must conform to the "
                },
                {
                  "type": "codeVoice",
                  "code": "NSCopying"
                },
                {
                  "type": "text",
                  "text": " protocol). If "
                },
                {
                  "type": "codeVoice",
                  "code": "aKey"
                },
                {
                  "type": "text",
                  "text": " already exists in the dictionary, "
                },
                {
                  "type": "codeVoice",
                  "code": "anObject"
                },
                {
                  "type": "text",
                  "text": " takes its place."
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Raises an "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsinvalidargumentexception"
                    },
                    {
                      "type": "text",
                      "text": " if "
                    },
                    {
                      "type": "codeVoice",
                      "code": "aKey"
                    },
                    {
                      "type": "text",
                      "text": " is "
                    },
                    {
                      "type": "codeVoice",
                      "code": "nil"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ],
              "style": "important"
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method has the same behavior as the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmutabledictionary/1411616-setobject"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You shouldn’t need to call this method directly. Instead, this method is called when setting an object for a key using subscripting."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "id value = @\"someValue\";",
            "mutableDictionary[@\"someKey\"] = value;"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555711",
            "title": "Listing 1"
          }
        }
      ]
