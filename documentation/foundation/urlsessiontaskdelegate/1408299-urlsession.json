        {
          "name": "session",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The session containing the data task."
                }
              ]
            }
          ]
        },
        {
          "name": "task",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The data task."
                }
              ]
            }
          ]
        },
        {
          "name": "bytesSent",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of bytes sent since the last time this delegate method was called."
                }
              ]
            }
          ]
        },
        {
          "name": "totalBytesSent",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The total number of bytes sent so far."
                }
              ]
            }
          ]
        },
        {
          "name": "totalBytesExpectedToSend",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The expected length of the body data. The URL loading system can determine the length of the upload data in three ways:"
                }
              ]
            },
            {
              "type": "unorderedList",
              "items": [
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "From the length of the "
                        },
                        {
                          "type": "codeVoice",
                          "code": "NSData"
                        },
                        {
                          "type": "text",
                          "text": " object provided as the upload body."
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "From the length of the file on disk provided as the upload body of an upload task ("
                        },
                        {
                          "type": "emphasis",
                          "inlineContent": [
                            {
                              "type": "text",
                              "text": "not"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " a download task)."
                        }
                      ]
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "type": "paragraph",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "From the "
                        },
                        {
                          "type": "codeVoice",
                          "code": "Content-Length"
                        },
                        {
                          "type": "text",
                          "text": " in the request object, if you explicitly set it."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Otherwise, the value is "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlsessiontransfersizeunknown"
                },
                {
                  "type": "text",
                  "text": " ("
                },
                {
                  "type": "codeVoice",
                  "code": "-1"
                },
                {
                  "type": "text",
                  "text": ") if you provided a stream or body data object, or zero ("
                },
                {
                  "type": "codeVoice",
                  "code": "0"
                },
                {
                  "type": "text",
                  "text": ") if you did not."
                }
              ]
            }
          ]
        }
      ]
    },
