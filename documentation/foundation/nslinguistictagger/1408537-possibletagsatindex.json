        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Returns an array of possible tags for the tag scheme at the specified location, starting with the most likely tag scheme.  For some tag schemes only a single tag will be returned, but for others a list of possibilities will be provided."
            }
          ]
        }
      ]
