    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
            },
            {
              "type": "text",
              "text": " is a subclass of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/port"
            },
            {
              "type": "text",
              "text": " that allows for local (on the same machine) communication only. A companion class, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/socketport"
            },
            {
              "type": "text",
              "text": ", allows for both local and remote communication, but may be more expensive than "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
            },
            {
              "type": "text",
              "text": " for the local case."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
            },
            {
              "type": "text",
              "text": " defines no additional methods over those already defined by "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/port"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
                },
                {
                  "type": "text",
                  "text": " conforms to the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nscoding"
                },
                {
                  "type": "text",
                  "text": " protocol, but only supports coding by an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsportcoder"
                },
                {
                  "type": "text",
                  "text": " object. "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/port"
                },
                {
                  "type": "text",
                  "text": " and its subclasses do not support archiving."
                }
              ]
            }
          ],
          "style": "note",
          "name": "Note"
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Avoid "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
                },
                {
                  "type": "text",
                  "text": ". There's little reason to use "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
                },
                {
                  "type": "text",
                  "text": " rather than "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsmachport"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/socketport"
                },
                {
                  "type": "text",
                  "text": ". There's no particular performance or functionality advantage. It is recommended avoiding its use."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/messageport"
                },
                {
                  "type": "text",
                  "text": " may be deprecated in the macOS 10.6 or later."
                }
              ]
            }
          ],
          "style": "important"
        }
      ]
    }
