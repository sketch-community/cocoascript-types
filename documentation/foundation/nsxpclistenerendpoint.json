          "inlineContent": [
            {
              "type": "text",
              "text": "An instance of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxpclistenerendpoint"
            },
            {
              "type": "text",
              "text": " may be retrieved from an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxpclistener"
            },
            {
              "type": "text",
              "text": " instance and sent over existing "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxpcconnection"
            },
            {
              "type": "text",
              "text": "s. A process may then use the endpoint to create a new "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxpcconnection"
            },
            {
              "type": "text",
              "text": " to the original "
