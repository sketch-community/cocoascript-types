      "parameters": [
        {
          "name": "transform",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The transformation to apply. For a list of possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsmutablestring/string_transformations"
                },
                {
                  "type": "text",
                  "text": ". If the specified transform does not exist, the receiver is not modified, and this method returns "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "reverse",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Whether an inverse transform should be used. If the specified transform does not have an inverse, the receiver is not modified, and this method returns "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "range",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range of the string to transform. "
                },
                {
                  "type": "codeVoice",
                  "code": "range"
                },
                {
                  "type": "text",
                  "text": " must not exceed the bounds of the receiver."
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Raises an "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSRangeException"
                    },
                    {
                      "type": "text",
                      "text": " if any part of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "aRange"
                    },
                    {
                      "type": "text",
                      "text": " lies beyond the end of the string. "
                    }
                  ]
                }
              ],
              "style": "important"
            }
          ]
        },
        {
          "name": "resultingRange",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If the transform was successfully applied, upon return contains the range of the transformed string."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
