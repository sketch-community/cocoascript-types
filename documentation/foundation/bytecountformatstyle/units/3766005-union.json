        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This example uses the "
            },
            {
              "type": "codeVoice",
              "code": "union(_:)"
            },
            {
              "type": "text",
              "text": " method to add two more shipping options to the default set."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let defaultShipping = ShippingOptions.standard",
            "let memberShipping = defaultShipping.union([.secondDay, .priority])",
            "print(memberShipping.contains(.priority))",
            "// Prints \"true\""
