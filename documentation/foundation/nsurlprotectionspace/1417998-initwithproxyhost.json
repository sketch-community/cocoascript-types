      "parameters": [
        {
          "name": "host",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The host of the proxy server for the protection space object."
                }
              ]
            }
          ]
        },
        {
          "name": "port",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The port for the protection space object. If "
                },
                {
                  "type": "codeVoice",
                  "code": "port"
                },
                {
                  "type": "text",
                  "text": " is 0 the default port for the specified proxy type is used, for example, port 80 for HTTP. Note that servers can, and do, treat these values differently."
                }
              ]
            }
          ]
        },
        {
          "name": "proxyType",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of proxy server. The value of "
                },
                {
                  "type": "codeVoice",
                  "code": "proxyType"
                },
                {
                  "type": "text",
                  "text": " should be set to one of the values specified in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlprotectionspace/nsurlprotectionspace_proxy_types"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "realm",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string indicating a protocol specific subdivision of the host. "
                },
                {
                  "type": "codeVoice",
                  "code": "realm"
                },
                {
                  "type": "text",
                  "text": " may be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " if there is no specified realm or if the protocol doesn’t support realms."
                }
              ]
            }
          ]
        },
        {
          "name": "authenticationMethod",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The type of authentication to use. "
                },
                {
                  "type": "codeVoice",
                  "code": "authenticationMethod"
                },
                {
                  "type": "text",
                  "text": " should be set to one of the values in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlprotectionspace/nsurlprotectionspace_authentication_method_constants"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " to use the default, "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlauthenticationmethoddefault"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
