        {
          "name": "presentation",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The style to use when describing a relative date, such as “1 day ago” or “yesterday”."
                }
              ]
            }
          ]
        },
        {
          "name": "unitsStyle",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The style to use when formatting the quantity or the name of the unit, such as “1 day ago” or “one day ago”."
                }
              ]
            }
          ]
        },
        {
          "name": "locale",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The locale to use when formatting the relative date."
                }
              ]
            }
          ]
        },
