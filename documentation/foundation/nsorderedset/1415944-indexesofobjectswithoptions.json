    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "opts",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A bitmask that specifies the options for the enumeration (whether it should be performed concurrently and whether it should be performed in reverse order)."
                }
              ]
            }
          ]
        },
        {
          "name": "predicate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block to apply to elements in the ordered set."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block takes three arguments:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "obj"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The element in the ordered set."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "Term"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The index of the element in the ordered set."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "stop"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "A reference to a Boolean value. The block can set the value to "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                          },
                          {
                            "type": "text",
                            "text": " to stop further processing of the set. The "
                          },
                          {
                            "type": "codeVoice",
                            "code": "stop"
                          },
                          {
                            "type": "text",
                            "text": " argument is an out-only argument. You should only ever set this value to "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                          },
                          {
                            "type": "text",
                            "text": " within the block."
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The index whose corresponding value in the ordered set passes the test specified by "
            },
            {
              "type": "codeVoice",
              "code": "predicate"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "opts"
            },
            {
              "type": "text",
              "text": ". If the "
            },
            {
              "type": "codeVoice",
              "code": "opts"
            },
            {
              "type": "text",
              "text": " bitmask specifies reverse order, then the last item that matches is returned. Otherwise, the index of the first matching object is returned. If no objects in the ordered set pass the test, returns "
            },
            {
              "type": "codeVoice",
              "code": "NSNotFound"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "By default, the enumeration starts with the first object and continues serially through the ordered set to the last object. You can specify "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsenumerationoptions/nsenumerationconcurrent"
            },
            {
              "type": "text",
              "text": " and/or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsenumerationoptions/nsenumerationreverse"
            },
            {
              "type": "text",
              "text": " as enumeration options to modify this behavior."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": " If the block parameter or "
                },
                {
                  "type": "codeVoice",
                  "code": "s"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", this method raises an exception."
                }
              ]
            }
          ],
          "style": "important"
        }
      ]
    }
