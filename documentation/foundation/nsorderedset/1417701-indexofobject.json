      "parameters": [
        {
          "name": "object",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An object for which to search in the ordered set."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If this value is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", throws an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsinvalidargumentexception"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "range",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range within the array to search for "
                },
                {
                  "type": "codeVoice",
                  "code": "object"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If r exceeds the bounds of the ordered set (if the location plus length of the range is greater than the count of the ordered set), throws an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsrangeexception"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "opts",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Options for the search. For possible values, see "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "name": "cmp",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A comparator block used to compare the object obj with elements in the ordered set."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If this value is "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": ", throws an "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsinvalidargumentexception"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinginsertionindex"
            },
            {
              "type": "text",
              "text": " option is not specified:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "object"
                    },
                    {
                      "type": "text",
                      "text": " is found and neither "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal"
                    },
                    {
                      "type": "text",
                      "text": " nor "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal"
                    },
                    {
                      "type": "text",
                      "text": " is specified, returns the matching object's index."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal"
                    },
                    {
                      "type": "text",
                      "text": " or "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal"
                    },
                    {
                      "type": "text",
                      "text": " option is also specified, returns the index of equal objects. "
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the object is not found, returns "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSNotFound"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinginsertionindex"
            },
            {
              "type": "text",
              "text": " option is specified, returns the index at which you should insert "
            },
            {
              "type": "codeVoice",
              "code": "obj"
            },
            {
              "type": "text",
              "text": " in order to maintain a sorted array:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "object"
                    },
                    {
                      "type": "text",
                      "text": " is found and neither "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal"
                    },
                    {
                      "type": "text",
                      "text": " nor "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal"
                    },
                    {
                      "type": "text",
                      "text": " is specified, returns the matching object’s index."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal"
                    },
                    {
                      "type": "text",
                      "text": " or  "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal"
                    },
                    {
                      "type": "text",
                      "text": " option is also specified, returns the index of the equal objects."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If the object is not found, returns the index of the least greater object, or the index at the end of the array if the object is larger than all other elements. "
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The elements in the ordered set  must have already been sorted using the comparator "
            },
            {
              "type": "codeVoice",
              "code": "cmp"
            },
            {
              "type": "text",
              "text": ". If the ordered set is not sorted, the result is undefined."
            }
          ]
        }
      ]
    }
