        {
          "name": "request",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The URL request to download. The "
                },
                {
                  "type": "codeVoice",
                  "code": "request"
                },
                {
                  "type": "text",
                  "text": " object is deep-copied as part of the initialization process. Changes made to "
                },
                {
                  "type": "codeVoice",
                  "code": "request"
                },
                {
                  "type": "text",
                  "text": " after this method returns do not affect the request that is used for the loading process."
                }
              ]
            }
          ]
        },
        {
          "name": "delegate",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The delegate for the download. This object will receive delegate messages as the download progresses. Delegate messages will be sent on the thread which calls this method. For the download to work correctly the calling thread’s run loop must be operating in the default run loop mode."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "NSURLDownload"
                },
                {
                  "type": "text",
                  "text": " class maintains a strong reference to this delegate object."
                }
              ]
            }
          ]
        }
      ]
