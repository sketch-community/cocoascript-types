    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Set "
            },
            {
              "type": "codeVoice",
              "code": "hidesScaleName"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " to exclude the unit name from a formatted temperature string. For example, "
            },
            {
              "type": "codeVoice",
              "code": "90°"
            },
            {
              "type": "text",
              "text": " rather than "
            },
            {
              "type": "codeVoice",
              "code": "90°F"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "90°C"
            },
            {
              "type": "text",
              "text": " with the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/measurement/formatstyle/unitwidth/3816376-narrow"
            },
            {
              "type": "text",
              "text": " unit width, or "
            },
            {
              "type": "codeVoice",
              "code": "90 degrees"
            },
            {
              "type": "text",
              "text": " rather than "
            },
            {
              "type": "codeVoice",
              "code": "90 degrees Celcius"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "90 degrees Fahrenheit"
            },
            {
              "type": "text",
              "text": " with the "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/measurement/formatstyle/unitwidth/3816377-wide"
            },
            {
              "type": "text",
              "text": " width."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Hiding the unit name only affects the presentation of the measurement. Unless you specify "
            },
            {
              "type": "codeVoice",
              "code": "asProvided"
            },
            {
              "type": "text",
              "text": " as the "
            },
            {
              "type": "codeVoice",
              "code": "usage"
            },
            {
              "type": "text",
              "text": ", the system converts the temperature to the unit that the locale uses."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
