    "doc://com.apple.documentation/documentation/foundation/nsgrammarrange": {
      "title": "NSGrammarRange",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsgrammarrange",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsgrammarrange",
      "abstract": [
        {
          "type": "text",
          "text": "The value for the "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarRange"
        },
        {
          "type": "text",
          "text": " dictionary key should be an "
        },
        {
          "type": "codeVoice",
          "code": "NSValue"
        },
        {
          "type": "text",
          "text": " containing an "
        },
        {
          "type": "codeVoice",
          "code": "NSRange"
        },
        {
          "type": "text",
          "text": ", a subrange of the sentence range used as the return value, whose location should be an offset from the beginning of the sentence--so, for example, an "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarRange"
        },
        {
          "type": "text",
          "text": " for the first four characters of the overall sentence range should be "
        },
        {
          "type": "codeVoice",
          "code": "{0, 4}"
        },
        {
          "type": "text",
          "text": ". If the "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarRange"
        },
        {
          "type": "text",
          "text": " key is not present in the dictionary it is assumed to be equal to the overall sentence range."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsgrammaruserdescription": {
      "title": "NSGrammarUserDescription",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsgrammaruserdescription",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsgrammaruserdescription",
      "abstract": [
        {
          "type": "text",
          "text": "The value for the "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarUserDescription"
        },
        {
          "type": "text",
          "text": " dictionary key should be an "
        },
        {
          "type": "codeVoice",
          "code": "NSString"
        },
        {
          "type": "text",
          "text": " containing descriptive text about that range, to be presented directly to the user; it is intended that the user description should provide enough information to allow the user to correct the problem. It is recommended that "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarUserDescription"
        },
        {
          "type": "text",
          "text": " be supplied in all cases, however, "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarUserDescription"
        },
        {
          "type": "text",
          "text": " or "
        },
        {
          "type": "codeVoice",
          "code": "NSGrammarCorrections"
        },
        {
          "type": "text",
          "text": " must be supplied in order for correction guidance to be presented to the user."
        }
      ]
    },
