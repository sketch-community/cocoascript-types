          "inlineContent": [
            {
              "type": "text",
              "text": "The operation queue that started the operation or "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the queue could not be determined."
            }
          ]
