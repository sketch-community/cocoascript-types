          "inlineContent": [
            {
              "type": "text",
              "text": "Zero is the identity element for addition. For any value, "
            },
            {
              "type": "codeVoice",
              "code": "x + .zero == x"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": ".zero + x == x"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
