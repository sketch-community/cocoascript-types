        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Because "
            },
            {
              "type": "codeVoice",
              "code": "NSXMLDocument"
            },
            {
              "type": "text",
              "text": " is a subclass of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmlnode"
            },
            {
              "type": "text",
              "text": ", you can also use the relevant input and output options described in "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmlnode#1664244"
            },
            {
              "type": "text",
              "text": " in the "
            },
            {
              "type": "codeVoice",
              "code": "NSXMLNode"
            },
            {
              "type": "text",
              "text": " class reference. You can specify input options in the "
            },
            {
              "type": "codeVoice",
              "code": "NSXMLDocument"
            },
            {
              "type": "text",
              "text": " methods "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmldocument/1418467-initwithcontentsofurl"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmldocument/1413086-initwithdata"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmldocument/1416228-initwithxmlstring"
            },
            {
              "type": "text",
              "text": ". The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmldocument/1412424-xmldatawithoptions"
            },
            {
              "type": "text",
              "text": " method takes output options."
            }
