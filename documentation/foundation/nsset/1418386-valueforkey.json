          "inlineContent": [
            {
              "type": "text",
              "text": "A set containing the results of invoking "
            },
            {
              "type": "codeVoice",
              "code": "valueForKey:"
            },
            {
              "type": "text",
              "text": " (with the argument "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": ") on each of the receiving set's members."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The returned set might not have the same number of members as the receiving set. The returned set will not contain any elements corresponding to instances of "
            },
            {
              "type": "codeVoice",
              "code": "valueForKey:"
            },
            {
              "type": "text",
              "text": " returning "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " (note that this is in contrast with "
            },
            {
              "type": "codeVoice",
              "code": "NSArray"
            },
            {
              "type": "text",
              "text": "’s implementation, which may put "
            },
            {
              "type": "codeVoice",
              "code": "NSNull"
            },
            {
              "type": "text",
              "text": " values in the arrays it returns)."
            }
          ]
        }
