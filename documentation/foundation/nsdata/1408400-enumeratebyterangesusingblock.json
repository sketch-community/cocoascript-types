          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block to apply to byte ranges in the array."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The block takes three arguments:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "bytes"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The bytes for the current range. This pointer is valid until the data object is deallocated."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "byteRange"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The range of the current data bytes."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "stop"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "A reference to a Boolean value. The block can set the value to "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                          },
                          {
                            "type": "text",
                            "text": " to stop further processing of the data. The stop argument is an out-only argument. You should only ever set this Boolean to "
                          },
                          {
                            "type": "reference",
                            "isActive": false,
                            "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                          },
                          {
                            "type": "text",
                            "text": " within the Block."
                          }
                        ]
                      }
                    ]
                  }
                }
