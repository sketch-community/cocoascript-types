      "parameters": [
        {
          "name": "numericString",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A numeric string."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Besides digits, "
                },
                {
                  "type": "codeVoice",
                  "code": "numericString"
                },
                {
                  "type": "text",
                  "text": " can include an initial "
                },
                {
                  "type": "codeVoice",
                  "code": "+"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "–"
                },
                {
                  "type": "text",
                  "text": "; a single "
                },
                {
                  "type": "codeVoice",
                  "code": "E"
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "codeVoice",
                  "code": "e"
                },
                {
                  "type": "text",
                  "text": ", to indicate the exponent of a number in scientific notation; and a single decimal separator character to divide the fractional from the integral part of the number."
                }
              ]
            }
          ]
        },
        {
          "name": "locale",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A dictionary that defines the locale (specifically the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nslocaledecimalseparator"
                },
                {
                  "type": "text",
                  "text": ") to use to interpret the number in "
                },
                {
                  "type": "codeVoice",
                  "code": "numericString"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        }
      ]
    },
    {
