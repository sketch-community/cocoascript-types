          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Posted when the BSSID of any WLAN interface changes. The "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "object"
                }
              ]
            },
            {
              "type": "text",
              "text": " for this notification is the corresponding BSD interface name. This notification does not contain a "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "userInfo"
                }
              ]
            },
            {
              "type": "text",
              "text": " dictionary."
            }
          ]
