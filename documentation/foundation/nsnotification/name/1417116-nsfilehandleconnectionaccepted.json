              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "codeVoice",
                      "code": "NSFileHandleNotificationFileHandleItem"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSFileHandle"
                    },
                    {
                      "type": "text",
                      "text": " object representing the “near” end of a socket connection"
                    }
                  ]
                }
              ]
            ],
