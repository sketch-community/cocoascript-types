          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A string that specifies a date and time value in the international string representation format—"
                },
                {
                  "type": "codeVoice",
                  "code": "YYYY-MM-DD HH:MM:SS ±HHMM"
                },
                {
                  "type": "text",
                  "text": ", where "
                },
                {
                  "type": "codeVoice",
                  "code": "±HHMM"
                },
                {
                  "type": "text",
                  "text": " is a time zone offset in hours and minutes from UTC (for example, “"
                },
                {
                  "type": "codeVoice",
                  "code": "2001-03-24 10:45:32 +0600"
                },
                {
                  "type": "text",
                  "text": "”)."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "You must specify all fields of the format string, including the time zone offset, which must have a plus or minus sign prefix. "
                }
              ]
            }
          ]
        }
      ]
    },
