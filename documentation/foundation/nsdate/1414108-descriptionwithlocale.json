      "parameters": [
        {
          "name": "locale",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An "
                },
                {
                  "type": "codeVoice",
                  "code": "NSLocale"
                },
                {
                  "type": "text",
                  "text": " object."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If you pass "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "NSDate"
                },
                {
                  "type": "text",
                  "text": " formats the date in the same way as the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsdate/1409767-description"
                },
                {
                  "type": "text",
                  "text": " property. "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "In OS X v10.4 and earlier, this parameter was an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSDictionary"
                },
                {
                  "type": "text",
                  "text": " object. If you pass in an "
                },
                {
                  "type": "codeVoice",
                  "code": "NSDictionary"
                },
                {
                  "type": "text",
                  "text": " object in OS X v10.5, "
                },
                {
                  "type": "codeVoice",
                  "code": "NSDate"
                },
                {
                  "type": "text",
                  "text": " uses the default user locale—the same as if you passed in "
                },
                {
                  "type": "codeVoice",
                  "code": "[NSLocale currentLocale]."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A string representation of the receiver, using the given locale, or if the locale argument is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", in the international format "
            },
            {
              "type": "codeVoice",
              "code": "YYYY-MM-DD HH:MM:SS ±HHMM"
            },
            {
              "type": "text",
              "text": ", where "
            },
            {
              "type": "codeVoice",
              "code": "±HHMM"
            },
            {
              "type": "text",
              "text": " represents the time zone offset in hours and minutes from UTC (for example, “"
            },
            {
              "type": "codeVoice",
              "code": "2001-03-24 10:45:32 +0600"
            },
            {
              "type": "text",
              "text": "”)"
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In OS X v10.4 and earlier, "
            },
            {
              "type": "codeVoice",
              "code": "localeDictionary"
            },
            {
              "type": "text",
              "text": " is an "
            },
            {
              "type": "codeVoice",
              "code": "NSDictionary"
            },
            {
              "type": "text",
              "text": " object containing locale data. To use the user's preferences, you can use "
            },
            {
              "type": "codeVoice",
              "code": "[[NSUserDefaults standardUserDefaults] dictionaryRepresentation]."
            }
          ]
        }
      ]
    }
