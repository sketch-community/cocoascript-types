        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The new characters inherit the attributes of the first replaced character from "
            },
            {
              "type": "codeVoice",
              "code": "aRange"
            },
            {
              "type": "text",
              "text": ". Where the length of "
            },
            {
              "type": "codeVoice",
              "code": "aRange"
            },
            {
              "type": "text",
              "text": " is 0, the new characters inherit the attributes of the character preceding "
            },
            {
              "type": "codeVoice",
              "code": "aRange"
            },
            {
              "type": "text",
              "text": " if it has any, otherwise of the character following "
            },
            {
              "type": "codeVoice",
              "code": "aRange"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Raises an "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsrangeexception"
            },
            {
              "type": "text",
              "text": " if any part of "
            },
            {
              "type": "codeVoice",
              "code": "aRange"
            },
            {
              "type": "text",
              "text": " lies beyond the end of the receiver’s characters."
            }
