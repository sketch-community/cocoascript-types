          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "NSProgress"
            },
            {
              "type": "text",
              "text": " of this kind is assumed to use bytes as the unit of work being done, and the default implementation of "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogress/1417251-localizeddescription"
            },
            {
              "type": "text",
              "text": " takes advantage of that to return more specific text than it could otherwise. The "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogressfiletotalcountkey"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogressfilecompletedcountkey"
            },
            {
              "type": "text",
              "text": " keys in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsprogress/1413314-userinfo"
            },
            {
              "type": "text",
              "text": " dictionary are used for the overall count of files."
            }
          ]
        }
      ]
    }
