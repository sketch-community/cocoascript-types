          "inlineContent": [
            {
              "type": "text",
              "text": "Extensions can use this notification to adjust their activity when they become inactive. For example, you might use this notification to save any unsaved data to prevent it from being lost. The "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " parameter contains the "
            },
            {
              "type": "codeVoice",
              "code": "NSExtensionContext"
            },
            {
              "type": "text",
              "text": " object. This notification does not contain a "
            },
            {
              "type": "codeVoice",
              "code": "userInfo"
            },
            {
              "type": "text",
              "text": " dictionary. "
            }
