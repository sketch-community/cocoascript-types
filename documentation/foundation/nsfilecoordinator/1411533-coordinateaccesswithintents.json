                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsfilecoordinatorwritingoptions/nsfilecoordinatorwritingfordeleting"
                    },
                    {
                      "type": "text",
                      "text": ", "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsfilecoordinatorwritingoptions/nsfilecoordinatorwritingformoving"
                    },
                    {
                      "type": "text",
                      "text": ", or "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsfilecoordinatorwritingoptions/nsfilecoordinatorwritingforreplacing"
                    },
                    {
                      "type": "text",
                      "text": " options are used, "
