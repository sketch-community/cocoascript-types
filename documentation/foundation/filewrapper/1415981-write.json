          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Option flags for writing to the node located at "
                },
                {
                  "type": "codeVoice",
                  "code": "url"
                },
                {
                  "type": "text",
                  "text": ". See "
