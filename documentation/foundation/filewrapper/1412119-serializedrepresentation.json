        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property contains a data object in the format used by the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/appkit/nspasteboard/pasteboardtype/1533590-filecontents"
            },
            {
              "type": "text",
              "text": " pasteboard type. This data object is also suitable for passing to "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/1407515-init"
            },
            {
              "type": "text",
              "text": ".  "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property may be "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the user modifies the contents of the file system node after you call "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/1411645-read"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/1415658-init"
            },
            {
              "type": "text",
              "text": ", but before "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper"
            },
            {
              "type": "text",
              "text": " has read the contents of the file.  You can use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/readingoptions/1416989-immediate"
            },
            {
              "type": "text",
              "text": " reading option to reduce the likelihood of this problem."
            }
          ]
        }
      ]
    }
