        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property may contain "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " if the user modifies the file after you call "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/1411645-read"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/1415658-init"
            },
            {
              "type": "text",
              "text": " but before "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper"
            },
            {
              "type": "text",
              "text": " has read the contents of the file. Use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filewrapper/readingoptions/1416989-immediate"
            },
            {
              "type": "text",
              "text": " reading option to reduce the likelihood of that problem."
            }
          ]
        },
        {
          "level": 3,
          "type": "heading",
          "text": "Special Considerations",
          "anchor": "1679821"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property raises "
            },
            {
              "type": "codeVoice",
              "code": "NSInternalInconsistencyException"
            },
            {
              "type": "text",
              "text": " if the file wrapper object is not a regular-file file wrapper."
            }
          ]
        }
      ]
    }
