          "name": "diskCapacity",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The disk capacity of the cache, in bytes."
                }
              ]
            }
          ]
        },
        {
          "name": "path",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "In macOS, "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": " is the location at which to store the on-disk cache."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "In iOS, "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": " is the name of a subdirectory of the application’s default cache directory in which to store the on-disk cache (the subdirectory is created if it does not exist)."
                }
              ]
            }
          ]
        }
      ]
    },
