            },
            {
              "type": "text",
              "text": " if the registration is successful, "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": " otherwise. The only failure condition is if "
            },
            {
              "type": "codeVoice",
              "code": "protocolClass"
            },
            {
              "type": "text",
              "text": " is not a subclass of "
            },
            {
              "type": "reference",
              "isActive": false,
