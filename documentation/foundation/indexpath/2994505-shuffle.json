        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use the "
            },
            {
              "type": "codeVoice",
              "code": "shuffle()"
            },
            {
              "type": "text",
              "text": " method to randomly reorder the elements of an array."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "var names = [\"Alejandro\", \"Camila\", \"Diego\", \"Luciana\", \"Luis\", \"Sofía\"]",
            "names.shuffle(using: myGenerator)",
            "// names == [\"Luis\", \"Camila\", \"Luciana\", \"Sofía\", \"Alejandro\", \"Diego\"]"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "2994506",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method is equivalent to calling "
            },
            {
              "type": "codeVoice",
              "code": "shuffle(using:)"
            },
            {
              "type": "text",
              "text": ", passing in the system’s default random generator."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Complexity: O("
            },
