        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This code excerpt creates a character set object containing the lowercase English alphabetic characters:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSRange lcEnglishRange;",
            "NSCharacterSet *lcEnglishLetters;",
            " ",
            "lcEnglishRange.location = (unsigned int)'a';",
            "lcEnglishRange.length = 26;",
            "lcEnglishLetters = [NSCharacterSet characterSetWithRange:lcEnglishRange];"
          ],
