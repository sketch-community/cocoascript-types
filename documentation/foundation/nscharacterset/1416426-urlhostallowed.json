    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The host component of a URL is usually the component immediately after the first two leading slashes. If the URL contains a username and password, the host component is the component after the "
            },
            {
              "type": "codeVoice",
              "code": "@"
            },
            {
              "type": "text",
              "text": " sign. For example, in the URL "
            },
            {
              "type": "codeVoice",
              "code": "http://username:password@www.example.com/index.html"
            },
            {
              "type": "text",
              "text": ", the host component is "
            },
            {
              "type": "codeVoice",
              "code": "www.example.com"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
