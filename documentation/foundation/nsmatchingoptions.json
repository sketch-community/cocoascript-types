      "url": "/documentation/foundation/nsmatchingoptions/nsmatchingwithoutanchoringbounds",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that "
        },
        {
          "type": "codeVoice",
          "code": "^"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "$"
        },
        {
          "type": "text",
          "text": " will not automatically match the beginning and end of the search range, but will still match the beginning and end of the entire string. This constant has no effect if the search range contains the entire string. See "
