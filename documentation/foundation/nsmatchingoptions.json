      "url": "/documentation/foundation/nsmatchingoptions/nsmatchingwithtransparentbounds",
      "abstract": [
        {
          "type": "text",
          "text": "Specifies that matching may examine parts of the string beyond the bounds of the search range, for purposes such as word boundary detection, lookahead, etc. This constant has no effect if the search range contains the entire string. See "
        },
        {
          "type": "reference",
          "isActive": true,
