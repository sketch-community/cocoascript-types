          "name": "flag",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": ", the array is written to an auxiliary location, and then the auxiliary location is renamed to "
                },
                {
                  "type": "codeVoice",
                  "code": "aURL"
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": ", the array is written directly to "
                },
                {
                  "type": "codeVoice",
                  "code": "aURL"
                },
                {
                  "type": "text",
                  "text": ". The "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " option guarantees that "
                },
                {
                  "type": "codeVoice",
                  "code": "aURL"
                },
                {
                  "type": "text",
                  "text": ", if it exists at all, won’t be corrupted even if the system should crash during writing."
                }
              ]
            }
          ]
        }
