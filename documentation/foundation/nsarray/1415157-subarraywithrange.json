    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "range"
            },
            {
              "type": "text",
              "text": " isn’t within the receiving array’s range of elements, an "
            },
            {
              "type": "codeVoice",
              "code": "NSRangeException"
            },
            {
              "type": "text",
              "text": " is raised."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, the following code example creates an array containing the elements found in the first half of "
            },
            {
              "type": "codeVoice",
              "code": "wholeArray"
            },
            {
              "type": "text",
              "text": " (assuming "
            },
            {
              "type": "codeVoice",
              "code": "wholeArray"
            },
            {
              "type": "text",
              "text": " exists)."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSArray *halfArray;",
            "NSRange theRange;",
            " ",
            "theRange.location = 0;",
            "theRange.length = [wholeArray count] / 2;",
            " ",
            "halfArray = [wholeArray subarrayWithRange:theRange];"
          ],
