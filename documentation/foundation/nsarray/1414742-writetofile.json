      "parameters": [
        {
          "name": "path",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The path at which to write the contents of the array."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": " contains a tilde (~) character, you must expand it with "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1407716-stringbyexpandingtildeinpath"
                },
                {
                  "type": "text",
                  "text": " before invoking this method."
                }
              ]
            }
          ]
        },
        {
          "name": "flag",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": ", the array is written to an auxiliary file, and then the auxiliary file is renamed to "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": ". If "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
                },
                {
                  "type": "text",
                  "text": ", the array is written directly to "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": ". The "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes"
                },
                {
                  "type": "text",
                  "text": " option guarantees that "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": ", if it exists at all, won’t be corrupted even if the system should crash during writing."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
