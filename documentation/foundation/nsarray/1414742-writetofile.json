            },
            {
              "type": "text",
              "text": " if the file is written successfully, otherwise "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
