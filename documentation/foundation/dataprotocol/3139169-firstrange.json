          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An example of searching a data buffer converted from a string:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let data = \"0123456789\".data(using: .utf8)!",
            "let pattern = \"456\".data(using: .utf8)!",
            "let foundRange = data.firstRange(of: pattern)",
            "",
            "// foundRange == Range(4..<7)"
