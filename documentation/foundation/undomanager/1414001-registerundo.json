      "parameters": [
        {
          "name": "target",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The target of the undo operation. "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The undo manager maintains an unowned reference to "
                },
                {
                  "type": "codeVoice",
                  "code": "target "
                },
                {
                  "type": "text",
                  "text": "to prevent retain cycles."
                }
              ]
            }
          ]
        },
        {
          "name": "aSelector",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The selector for the undo operation."
                }
              ]
            }
          ]
        },
        {
          "name": "anObject",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The argument sent with the selector. "
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The undo manager maintains a strong reference to "
                },
                {
                  "type": "codeVoice",
                  "code": "anO"
                },
                {
                  "type": "codeVoice",
                  "code": "bject"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
