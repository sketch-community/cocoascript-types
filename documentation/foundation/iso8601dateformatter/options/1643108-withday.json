        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The date representation includes the day. The format for day is inferred based on provided options:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/iso8601dateformatter/options/1643290-withmonth"
                    },
                    {
                      "type": "text",
                      "text": " is specified, "
                    },
                    {
                      "type": "codeVoice",
                      "code": "dd"
                    },
                    {
                      "type": "text",
                      "text": " is used."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "If "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/iso8601dateformatter/options/1643128-withweekofyear"
                    },
                    {
                      "type": "text",
                      "text": " is specified, "
                    },
                    {
                      "type": "codeVoice",
                      "code": "ee"
                    },
                    {
                      "type": "text",
                      "text": " is used."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Otherwise, "
                    },
                    {
                      "type": "codeVoice",
                      "code": "DDD"
                    },
                    {
                      "type": "text",
                      "text": " is used."
                    }
                  ]
                }
              ]
            }
          ]
        }
