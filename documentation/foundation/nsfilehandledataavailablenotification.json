      "abstract": [
        {
          "type": "text",
          "text": "This notification is posted when an "
        },
        {
          "type": "codeVoice",
          "code": "NSFileHandle"
        },
        {
          "type": "text",
          "text": " object establishes a socket connection between two processes, creates an "
        },
        {
          "type": "codeVoice",
          "code": "NSFileHandle"
        },
        {
          "type": "text",
          "text": " object for one end of the connection, and makes this object available to observers by putting it in the "
        },
        {
          "type": "codeVoice",
          "code": "userInfo"
        },
        {
          "type": "text",
          "text": " dictionary."
        }
      ]
    },
