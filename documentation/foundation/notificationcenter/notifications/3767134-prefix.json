      ],
      "parameters": [
        {
          "name": "count",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The maximum number of elements to return. The value of "
                },
                {
                  "type": "codeVoice",
                  "code": "count"
                },
                {
                  "type": "text",
                  "text": " must be greater than or equal to zero."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An asynchronous sequence starting at the beginning of the base sequence with at most "
            },
            {
              "type": "codeVoice",
              "code": "count"
            },
            {
              "type": "text",
              "text": " elements."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use "
            },
            {
              "type": "codeVoice",
              "code": "prefix(_:)"
            },
            {
              "type": "text",
              "text": " to reduce the number of elements produced by the asynchronous sequence."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, an asynchronous sequence called "
            },
            {
              "type": "codeVoice",
              "code": "Counter"
            },
            {
              "type": "text",
              "text": " produces "
            },
            {
              "type": "codeVoice",
              "code": "Int"
            },
            {
              "type": "text",
              "text": " values from "
            },
            {
              "type": "codeVoice",
              "code": "1"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "10"
            },
            {
              "type": "text",
              "text": ". The "
            },
            {
              "type": "codeVoice",
              "code": "prefix(_:)"
            },
            {
              "type": "text",
              "text": " method causes the modified sequence to pass through the first six values, then end."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "for await number in Counter(howHigh: 10).prefix(6) {",
            "    print(\"\\(number) \")",
            "}",
            "// prints \"1 2 3 4 5 6\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3839769",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the count passed to "
            },
            {
              "type": "codeVoice",
              "code": "prefix(_:)"
            },
            {
              "type": "text",
              "text": " exceeds the number of elements in the base sequence, the result contains all of the elements in the sequence."
            }
          ]
        }
      ]
    }
