      "parameters": [
        {
          "name": "transform",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An error-throwing mapping closure. "
                },
                {
                  "type": "codeVoice",
                  "code": "transform"
                },
                {
                  "type": "text",
                  "text": " accepts an element of this sequence as its parameter and returns a transformed value of the same or of a different type. If "
                },
                {
                  "type": "codeVoice",
                  "code": "transform"
                },
                {
                  "type": "text",
                  "text": " throws an error, the sequence ends."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An asynchronous sequence that contains, in order, the non-"
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " elements produced by the "
            },
            {
              "type": "codeVoice",
              "code": "transform"
            },
            {
              "type": "text",
              "text": " closure. The sequence ends either when the base sequence ends or when "
            },
            {
              "type": "codeVoice",
              "code": "transform"
            },
            {
              "type": "text",
              "text": " throws an error."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use the "
            },
            {
              "type": "codeVoice",
              "code": "compactMap(_:)"
            },
            {
              "type": "text",
              "text": " method to transform every element received from a base asynchronous sequence, while also discarding any "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " results from the closure. Typically, you use this to transform from one type of element to another."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, an asynchronous sequence called "
            },
            {
              "type": "codeVoice",
              "code": "Counter"
            },
            {
              "type": "text",
              "text": " produces "
            },
            {
              "type": "codeVoice",
              "code": "Int"
            },
            {
              "type": "text",
              "text": " values from "
            },
            {
              "type": "codeVoice",
              "code": "1"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "5"
            },
            {
              "type": "text",
              "text": ". The closure provided to the "
            },
            {
              "type": "codeVoice",
              "code": "compactMap(_:)"
            },
            {
              "type": "text",
              "text": " method takes each "
            },
            {
              "type": "codeVoice",
              "code": "Int"
            },
            {
              "type": "text",
              "text": " and looks up a corresponding "
            },
            {
              "type": "codeVoice",
              "code": "String"
            },
            {
              "type": "text",
              "text": " from a "
            },
            {
              "type": "codeVoice",
              "code": "romanNumeralDict"
            },
            {
              "type": "text",
              "text": " dictionary. Since there is no key for "
            },
            {
              "type": "codeVoice",
              "code": "4"
            },
            {
              "type": "text",
              "text": ", the closure returns "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " in this case, which "
            },
            {
              "type": "codeVoice",
              "code": "compactMap(_:)"
            },
            {
              "type": "text",
              "text": " omits from the transformed asynchronous sequence. When the value is "
            },
            {
              "type": "codeVoice",
              "code": "5"
            },
            {
              "type": "text",
              "text": ", the closure throws "
            },
            {
              "type": "codeVoice",
              "code": "MyError"
            },
            {
              "type": "text",
              "text": ", terminating the sequence."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let romanNumeralDict: [Int : String] =",
            "    [1: \"I\", 2: \"II\", 3: \"III\", 5: \"V\"]",
            "",
            "do {",
            "    let stream = Counter(howHigh: 5)",
            "        .compactMap { (value) throws -> String? in",
            "            if value == 5 {",
            "                throw MyError()",
            "            }",
            "            return romanNumeralDict[value]",
            "        }",
            "    for try await numeral in stream {",
            "        print(\"\\(numeral) \", terminator: \" \")",
            "    }",
            "} catch {",
            "    print(\"Error: \\(error)\")",
            "}",
            "// Prints: I  II  III  Error: MyError()"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3839755",
            "title": "Listing 1"
          }
        }
      ]
    }
