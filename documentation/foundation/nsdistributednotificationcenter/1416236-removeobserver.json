      "parameters": [
        {
          "name": "notificationObserver",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Observer to remove from the dispatch table. Specify an observer to remove only entries for this observer. When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the receiver does not use notification observers as criteria for removal."
                }
              ]
            }
          ]
        },
        {
          "name": "notificationName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Name of the notification to remove from dispatch table. Specify a notification name to remove only entries that specify this notification name. When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the receiver does not use notification names as criteria for removal."
                }
              ]
            }
          ]
        },
        {
          "name": "notificationSender",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Sender to remove from the dispatch table. Specify a notification sender to remove only entries that specify this sender. When "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ", the receiver does not use notification senders as criteria for removal."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Be sure to invoke this method with "
            },
            {
              "type": "codeVoice",
              "code": "notificationName:nil notificationSender:nil"
            },
            {
              "type": "text",
              "text": " (or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationcenter/1413994-removeobserver"
            },
            {
              "type": "text",
              "text": ") before deallocating the observer object."
            }
          ]
        }
