          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " if the sequence contains an element that satisfies "
            },
            {
              "type": "codeVoice",
              "code": "predicate"
            },
            {
              "type": "text",
              "text": "; otherwise, "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
