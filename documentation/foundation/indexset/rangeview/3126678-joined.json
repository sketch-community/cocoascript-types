          "inlineContent": [
            {
              "type": "text",
              "text": "A flattened view of the elements of this sequence of sequences."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, an array of three ranges is flattened so that the elements of each range can be iterated in turn."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let ranges = [0..<3, 8..<10, 15..<17]",
            "",
            "// A for-in loop over 'ranges' accesses each range:",
            "for range in ranges {",
            "  print(range)",
            "}",
            "// Prints \"0..<3\"",
            "// Prints \"8..<10\"",
            "// Prints \"15..<17\"",
            "",
            "// Use 'joined()' to access each element of each range:",
            "for index in ranges.joined() {",
            "    print(index, terminator: \" \")",
            "}",
            "// Prints: \"0 1 2 8 9 15 16\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3126679",
            "title": "Listing 1"
          }
        }
      ]
    }
