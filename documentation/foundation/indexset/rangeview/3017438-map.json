        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, "
            },
            {
              "type": "codeVoice",
              "code": "map"
            },
            {
              "type": "text",
              "text": " is used first to convert the names in the array to lowercase strings and then to count their characters."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let cast = [\"Vivien\", \"Marlon\", \"Kim\", \"Karl\"]",
            "let lowercaseNames = cast.map { $0.lowercased() }",
            "// 'lowercaseNames' == [\"vivien\", \"marlon\", \"kim\", \"karl\"]",
            "let letterCounts = cast.map { $0.count }",
            "// 'letterCounts' == [6, 6, 3, 4]"
          ],
