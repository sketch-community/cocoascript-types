          "name": "anAttribute",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An XML node object representing an attribute. If the receiver already has an attribute with the same name, "
                },
                {
                  "type": "codeVoice",
                  "code": "anAttribute"
                },
                {
                  "type": "text",
                  "text": " replaces the old attribute."
                }
              ]
            }
          ]
        }
      ]
    },
