      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The order of multiple attributes is preserved if the "
            },
            {
              "type": "codeVoice",
              "code": "NSXMLPreserveAttributeOrder"
            },
            {
              "type": "text",
              "text": " option is specified when the element is created."
            }
          ]
        }
