          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The array of keys. If the parameter is nil, an exception is thrown. If the array of keys is empty, an empty key set is returned."
                }
              ]
            }
          ]
        }
      ]
