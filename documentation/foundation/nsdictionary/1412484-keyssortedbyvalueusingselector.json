            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "codeVoice",
                  "code": "comparator"
                },
                {
                  "type": "text",
                  "text": " method should return "
                },
                {
                  "type": "codeVoice",
                  "code": "NSOrderedAscending"
                },
                {
                  "type": "text",
                  "text": " if the dictionary value is smaller than the argument, "
                },
                {
                  "type": "codeVoice",
                  "code": "NSOrderedDescending"
                },
                {
                  "type": "text",
                  "text": " if the dictionary value is larger than the argument, and "
                },
                {
                  "type": "codeVoice",
                  "code": "NSOrderedSame"
                },
                {
                  "type": "text",
                  "text": " if they are equal."
                }
              ]
            }
