    {
      "kind": "content",
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Set a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmorphologycustompronoun"
            },
            {
              "type": "text",
              "text": " instance on a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmorphology"
            },
            {
              "type": "text",
              "text": " instance when you want to provide a langauge-specific customization of pronoun use in that language. Different languages have different requirements for the grammatical information needed to apply a custom pronoun, so you set custom pronoun behavior on a per-language basis."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The example below shows how to create English “ze” and “hir” custom pronouns:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let ze = [[NSMorphologyCustomPronoun alloc] init];",
            "ze.subjectForm = \"ze\";",
            "ze.objectForm = \"hir\";",
            "ze.possessiveForm = \"hir\";",
            "ze.possessiveAdjectiveForm = \"hir\";",
            "ze.reflexiveForm = \"hirself\";"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "3814304",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmorphologycustompronoun"
            },
            {
              "type": "text",
              "text": " only supports third-person pronouns. Use this feature when your app needs to refer to third parties with a specific pronoun."
            }
          ]
        }
      ]
    }
  ],
