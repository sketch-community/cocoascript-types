          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The formatter interprets a negative component value as a date in the past. "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let components = DateComponents(day: -2)",
            "let formatter = RelativeDateTimeFormatter()",
            "print(formatter.localizedString(from: components))",
            "// Outputs:  2 days ago"
          ],
