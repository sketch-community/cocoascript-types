    "doc://com.apple.documentation/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviordrop": {
      "title": "NSNotificationSuspensionBehaviorDrop",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviordrop",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviordrop",
      "abstract": [
        {
          "type": "text",
          "text": "The server does not queue any notifications with this name and object until "
        },
        {
          "type": "reference",
          "isActive": true,
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsdistributednotificationcenter/1407301-suspended"
        },
        {
          "type": "text",
          "text": " with an argument of "
        },
        {
          "type": "reference",
          "isActive": false,
          "identifier": "doc://com.apple.documentation/documentation/objectivec/no"
        },
        {
          "type": "text",
          "text": " is called."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviorcoalesce": {
      "title": "NSNotificationSuspensionBehaviorCoalesce",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviorcoalesce",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviorcoalesce",
      "abstract": [
        {
          "type": "text",
          "text": "The server only queues the last notification of the specified name and object; earlier notifications are dropped. In cover methods for which suspension behavior is not an explicit argument, "
        },
        {
          "type": "codeVoice",
          "code": "NSNotificationSuspensionBehaviorCoalesce"
        },
        {
          "type": "text",
          "text": " is the default."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviorhold": {
      "title": "NSNotificationSuspensionBehaviorHold",
      "type": "topic",
      "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviorhold",
      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsnotificationsuspensionbehavior/nsnotificationsuspensionbehaviorhold",
      "abstract": [
        {
          "type": "text",
          "text": "The server holds all matching notifications until the queue has been filled (queue size determined by the server), at which point the server may flush queued notifications."
        }
      ]
    },
