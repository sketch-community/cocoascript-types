          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The selector for the message to send to the target. This selector must take only one argument and must not have a return value."
                }
              ]
            }
          ]
        },
        {
          "name": "aTarget",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The object that will receive the message "
                },
                {
                  "type": "codeVoice",
                  "code": "aSelector"
                },
                {
                  "type": "text",
                  "text": " on the new thread."
                }
              ]
            }
          ]
        },
        {
          "name": "anArgument",
          "content": [
            {
              "type": "paragraph",
