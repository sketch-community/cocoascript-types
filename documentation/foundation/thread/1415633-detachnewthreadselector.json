    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The objects "
            },
            {
              "type": "codeVoice",
              "code": "aTarget"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "anArgument"
            },
            {
              "type": "text",
              "text": " are retained during the execution of the detached thread, then released. The detached thread is exited (using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/thread/1409404-exit"
            },
            {
              "type": "text",
              "text": " class method) as soon as "
            },
            {
              "type": "codeVoice",
              "code": "aTarget"
            },
            {
              "type": "text",
              "text": " has completed executing the "
            },
            {
              "type": "codeVoice",
              "code": "aSelector"
            },
            {
              "type": "text",
              "text": " method."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If this thread is the first thread detached in the application, this method posts the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnotification/name/1417567-nswillbecomemultithreaded"
            },
            {
              "type": "text",
              "text": " with object "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " to the default notification center."
            }
          ]
        }
      ]
    }
