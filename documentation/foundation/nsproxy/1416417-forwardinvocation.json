    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "codeVoice",
              "code": "NSProxy"
            },
            {
              "type": "text",
              "text": "’s implementation merely raises "
            },
            {
              "type": "codeVoice",
              "code": "NSInvalidArgumentException"
            },
            {
              "type": "text",
              "text": ". Override this method in your subclass to handle "
            },
            {
              "type": "codeVoice",
              "code": "anInvocation"
            },
            {
              "type": "text",
              "text": " appropriately, at the very least by setting its return value."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For example, if your proxy merely forwards messages to an instance variable named "
            },
            {
              "type": "codeVoice",
              "code": "realObject"
            },
            {
              "type": "text",
              "text": ", it can implement "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsproxy/1416417-forwardinvocation"
            },
            {
              "type": "text",
              "text": " like this:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- (void)forwardInvocation:(NSInvocation *)anInvocation",
            "{",
            "    [anInvocation setTarget:realObject];",
            "    [anInvocation invoke];",
            "    return;",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555789",
            "title": "Listing 1"
          }
        }
      ]
    }
