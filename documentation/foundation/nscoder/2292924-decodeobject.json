    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the coder responds "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/swift/true"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoder/1409845-requiressecurecoding"
            },
            {
              "type": "text",
              "text": ", then the coder calls "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoder/1411455-failwitherror"
            },
            {
              "type": "text",
              "text": " in either of the following cases:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The class indicated by "
                    },
                    {
                      "type": "codeVoice",
                      "code": "cls"
                    },
                    {
                      "type": "text",
                      "text": " doesn’t implement "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nssecurecoding"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The unarchived class doesn’t match "
                    },
                    {
                      "type": "codeVoice",
                      "code": "cls"
                    },
                    {
                      "type": "text",
                      "text": ", nor do any of its superclasses."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the coder doesn’t require secure coding, it ignores the "
            },
            {
              "type": "codeVoice",
              "code": "cls"
            },
            {
              "type": "text",
              "text": " parameter and does not check the decoded object."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
