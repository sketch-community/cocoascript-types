      "role": "symbol",
      "url": "/documentation/foundation/nsregularexpressionoptions/nsregularexpressiondotmatcheslineseparators",
      "abstract": [
        {
          "type": "text",
          "text": "Allow "
        },
        {
          "type": "codeVoice",
          "code": "."
        },
        {
          "type": "text",
          "text": " to match any character, including line separators."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsregularexpressionoptions/nsregularexpressionanchorsmatchlines": {
      "title": "NSRegularExpressionAnchorsMatchLines",
