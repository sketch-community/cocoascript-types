      "role": "symbol",
      "url": "/documentation/foundation/nsregularexpressionoptions/nsregularexpressionuseunixlineseparators",
      "abstract": [
        {
          "type": "text",
          "text": "Treat only "
        },
        {
          "type": "codeVoice",
          "code": "\\n"
        },
        {
          "type": "text",
          "text": " as a line separator (otherwise, all standard line separators are used)."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsregularexpressionoptions/nsregularexpressionuseunicodewordboundaries": {
