      "kind": "symbol",
      "role": "symbol",
      "url": "/documentation/foundation/nsregularexpressionoptions/nsregularexpressionanchorsmatchlines",
      "abstract": [
        {
          "type": "text",
          "text": "Allow "
        },
        {
          "type": "codeVoice",
          "code": "^"
        },
        {
          "type": "text",
          "text": " and "
        },
        {
          "type": "codeVoice",
          "code": "$"
        },
        {
          "type": "text",
          "text": " to match the start and end of lines."
        }
      ]
    },
    "doc://com.apple.documentation/documentation/foundation/nsregularexpressionoptions/nsregularexpressionuseunixlineseparators": {
      "title": "NSRegularExpressionUseUnixLineSeparators",
