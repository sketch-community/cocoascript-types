        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This option is available for "
            },
            {
              "type": "codeVoice",
              "code": "NSArray"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "NSIndexSet"
            },
            {
              "type": "text",
              "text": " classes; its behavior is undefined for "
            },
            {
              "type": "codeVoice",
              "code": "NSDictionary"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "NSSet"
            },
            {
              "type": "text",
              "text": " classes, or when combined with the "
            },
            {
              "type": "codeVoice",
              "code": "NSEnumerationConcurrent"
            },
            {
              "type": "text",
              "text": " flag."
            }
          ]
        }
