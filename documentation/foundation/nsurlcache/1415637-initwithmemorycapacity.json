          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "In macOS, "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": " is the location at which to store the on-disk cache."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "In iOS, "
                },
                {
                  "type": "codeVoice",
                  "code": "path"
                },
                {
                  "type": "text",
                  "text": " is the name of a subdirectory of the application’s default cache directory in which to store the on-disk cache (the subdirectory is created if it does not exist)."
                }
              ]
            }
          ]
