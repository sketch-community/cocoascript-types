          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A localized version of the string designated by "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " in table "
            },
            {
              "type": "codeVoice",
              "code": "tableName"
            },
            {
              "type": "text",
              "text": ". This method returns the following when key is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " or not found in table:"
            }
          ]
        },
