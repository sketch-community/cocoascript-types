      "parameters": [
        {
          "name": "priority",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A number specifying the relative priority of preserving the resources in the group specified by "
                },
                {
                  "type": "codeVoice",
                  "code": "tag"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Possible values are between "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "codeVoice",
                  "code": "1.0"
                },
                {
                  "type": "text",
                  "text": ". The default is "
                },
                {
                  "type": "codeVoice",
                  "code": "0.0"
                },
                {
                  "type": "text",
                  "text": ". The system will attempt to purge resources with lower priorities first."
                }
              ]
            }
          ]
        },
        {
          "name": "tags",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A set of tag names specifying resources stored in the bundle. Must not be "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": ". An exception is thrown if any of the tags in the set do not exist in your app."
                }
              ]
            }
          ]
        }
      ]
    }
