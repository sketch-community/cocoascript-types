      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyvaluechange/nskeyvaluechangesetting"
            },
            {
              "type": "text",
              "text": " indicates that the observed object has received a "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1415969-setvalue"
            },
            {
              "type": "text",
              "text": " message, or that the key-value-coding-compliant set method for the key has been invoked, or that one of the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1416222-willchangevalueforkey"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobject/1411809-didchangevalueforkey"
            },
            {
              "type": "text",
              "text": " methods has otherwise been invoked."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A value of "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyvaluechange/nskeyvaluechangeinsertion"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyvaluechange/nskeyvaluechangeremoval"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyvaluechange/nskeyvaluechangereplacement"
            },
            {
              "type": "text",
              "text": " indicates that mutating messages have been sent a key-value observing compliant collection proxy, or that one of the key-value-coding-compliant collection mutation methods for the key has been invoked, or a collection will change or did change method has been otherwise been invoked."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You can use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber/1413324-unsignedintegervalue"
            },
            {
              "type": "text",
              "text": " method on the "
            },
            {
              "type": "codeVoice",
              "code": "NSNumber"
            },
            {
              "type": "text",
              "text": " object to retrieve the value of the change kind."
            }
          ]
        }
      ]
    }
