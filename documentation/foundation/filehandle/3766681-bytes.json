    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use the "
            },
            {
              "type": "codeVoice",
              "code": "for"
            },
            {
              "type": "text",
              "text": "-"
            },
            {
              "type": "codeVoice",
              "code": "await"
            },
            {
              "type": "text",
              "text": "-"
            },
            {
              "type": "codeVoice",
              "code": "in"
            },
            {
              "type": "text",
              "text": " syntax to iterate over the bytes in this sequence. For text files, you can also use its "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filehandle/asyncbytes/3766657-characters"
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filehandle/asyncbytes/3766680-unicodescalars"
            },
            {
              "type": "text",
              "text": ", or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filehandle/asyncbytes/3766668-lines"
            },
            {
              "type": "text",
              "text": " properties to retrieve the contents in a more convenient format. Since all of these properties conform to "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/swift/asyncsequence"
            },
            {
              "type": "text",
              "text": ", as does "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/filehandle/3766681-bytes"
            },
            {
              "type": "text",
              "text": " itself, you can use methods defined by "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/swift/asyncsequence"
            },
            {
              "type": "text",
              "text": " to perform powerful inline processing. For example, you can skip the first "
            },
            {
              "type": "codeVoice",
              "code": "n"
            },
            {
              "type": "text",
              "text": " bytes of the file with "
            },
            {
              "type": "codeVoice",
              "code": "myFileHandle.bytes.prefix(n)"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Rather than creating a "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filehandle"
                },
                {
                  "type": "text",
                  "text": " to read a file asynchronously, you can instead use a "
                },
                {
                  "type": "codeVoice",
                  "code": "file://"
                },
                {
                  "type": "text",
                  "text": " URL in combination with the "
                },
                {
                  "type": "codeVoice",
                  "code": "async"
                },
                {
                  "type": "text",
                  "text": "-"
                },
                {
                  "type": "codeVoice",
                  "code": "await"
                },
                {
                  "type": "text",
                  "text": " methods in "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession"
                },
                {
                  "type": "text",
                  "text": ". These include the "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession/3767350-bytes"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession/3767351-bytes"
                },
                {
                  "type": "text",
                  "text": " methods that deliver an asynchronous sequence of bytes, and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession/3767352-data"
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession/3767353-data"
                },
                {
                  "type": "text",
                  "text": " to return the file’s entire contents at once."
                }
              ]
            }
          ],
          "style": "tip"
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
