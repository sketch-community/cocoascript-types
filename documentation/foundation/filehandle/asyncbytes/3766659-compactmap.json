      "parameters": [
        {
          "name": "transform",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A mapping closure. "
                },
                {
                  "type": "codeVoice",
                  "code": "transform"
                },
                {
                  "type": "text",
                  "text": " accepts an element of this sequence as its parameter and returns a transformed value of the same or of a different type."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An asynchronous sequence that contains, in order, the non-"
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " elements produced by the "
            },
            {
              "type": "codeVoice",
              "code": "transform"
            },
            {
              "type": "text",
              "text": " closure."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use the "
            },
            {
              "type": "codeVoice",
              "code": "compactMap(_:)"
            },
            {
              "type": "text",
              "text": " method to transform every element received from a base asynchronous sequence, while also discarding any "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " results from the closure. Typically, you use this to transform from one type of element to another."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, an asynchronous sequence called "
            },
            {
              "type": "codeVoice",
              "code": "Counter"
            },
            {
              "type": "text",
              "text": " produces "
            },
            {
              "type": "codeVoice",
              "code": "Int"
            },
            {
              "type": "text",
              "text": " values from "
            },
            {
              "type": "codeVoice",
              "code": "1"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "5"
            },
            {
              "type": "text",
              "text": ". The closure provided to the "
            },
            {
              "type": "codeVoice",
              "code": "compactMap(_:)"
            },
            {
              "type": "text",
              "text": " method takes each "
            },
            {
              "type": "codeVoice",
              "code": "Int"
            },
            {
              "type": "text",
              "text": " and looks up a corresponding "
            },
            {
              "type": "codeVoice",
              "code": "String"
            },
            {
              "type": "text",
              "text": " from a "
            },
            {
              "type": "codeVoice",
              "code": "romanNumeralDict"
            },
            {
              "type": "text",
              "text": " dictionary. Because there is no key for "
            },
            {
              "type": "codeVoice",
              "code": "4"
            },
            {
              "type": "text",
              "text": ", the closure returns "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": " in this case, which "
            },
            {
              "type": "codeVoice",
              "code": "compactMap(_:)"
            },
            {
              "type": "text",
              "text": " omits from the transformed asynchronous sequence."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let romanNumeralDict: [Int : String] =",
            "    [1: \"I\", 2: \"II\", 3: \"III\", 5: \"V\"]",
            "    ",
            "let stream = Counter(howHigh: 5)",
            "    .compactMap { romanNumeralDict[$0] }",
            "for await numeral in stream {",
            "    print(\"\\(numeral) \", terminator: \" \")",
            "}",
            "// Prints: I  II  III  V"
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "3839736",
            "title": "Listing 1"
          }
        }
      ]
    }
