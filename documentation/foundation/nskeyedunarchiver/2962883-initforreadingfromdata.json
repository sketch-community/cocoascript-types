        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This initializer enables "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyedunarchiver/1410824-requiressecurecoding"
