    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The unarchiver may fail to decode an entry for the following reasons:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "The keyed archive data is corrupt or missing."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A type mismatch occurs, such as expecting a class by calling "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nscoder/2292924-decodeobject"
                    },
                    {
                      "type": "text",
                      "text": ", but the unarchiver encounters a numeric value for that key instead. This also occurs when "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyedunarchiver/1418107-decodeintforkey"
                    },
                    {
                      "type": "text",
                      "text": " encounters a value encoded as floating-point, or vice versa."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "A secure coding violation occurs. This happens when attempting to decode an object that doesn’t conform to "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nssecurecoding"
                    },
                    {
                      "type": "text",
                      "text": ". This also happens when the encoded type doesn’t match any of the classes passed to "
                    },
                    {
                      "type": "reference",
                      "isActive": true,
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nskeyedunarchiver/2962885-unarchivedobjectofclasses"
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
