    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "block",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The Block is applied to the object to be evaluated."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The Block takes three arguments and returns a value:"
                }
              ]
            },
            {
              "type": "termList",
              "items": [
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "evaluatedObject"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "The object to be evaluated."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "expressions"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "An array of predicate expressions that evaluates to a collection."
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "term": {
                    "inlineContent": [
                      {
                        "type": "text",
                        "text": "context"
                      }
                    ]
                  },
                  "definition": {
                    "content": [
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "A dictionary that the expression can use to store temporary state for one predicate evaluation."
                          }
                        ]
                      },
                      {
                        "type": "paragraph",
                        "inlineContent": [
                          {
                            "type": "text",
                            "text": "Note that "
                          },
                          {
                            "type": "codeVoice",
                            "code": "context"
                          },
                          {
                            "type": "text",
                            "text": " is mutable, and that it can only be accessed during the evaluation of the expression. You must not attempt to retain it for use elsewhere. ]"
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The Block returns the "
                },
                {
                  "type": "codeVoice",
                  "code": "evaluatedObject"
                },
                {
                  "type": "text",
                  "text": ". "
                }
              ]
            }
          ]
        },
        {
          "name": "arguments",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array containing "
                },
                {
                  "type": "codeVoice",
                  "code": "NSExpression"
                },
                {
                  "type": "text",
                  "text": " objects that will be used as parameters during the invocation of selector."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "For a selector taking no parameters, the array should be empty. For a selector taking one or more parameters, the array should contain one "
                },
                {
                  "type": "codeVoice",
                  "code": "NSExpression"
                },
                {
                  "type": "text",
                  "text": " object which will evaluate to an instance of the appropriate type for each parameter."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "If there is a mismatch between the number of parameters expected and the number you provide during evaluation, an exception may be raised or missing parameters may simply be replaced by "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " (which occurs depends on how many parameters are provided, and whether you have over- or underflow)."
                }
              ]
            },
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "See "
                },
                {
                  "type": "reference",
                  "isActive": true,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsexpression/1413747-expressionforfunction"
                },
                {
                  "type": "text",
                  "text": " for a complete list of arguments."
                }
              ]
            }
          ]
        }
      ]
    },
