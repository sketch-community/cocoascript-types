          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The canonical form of "
            },
            {
              "type": "codeVoice",
              "code": "request"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
