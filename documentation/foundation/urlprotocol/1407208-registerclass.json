            },
            {
              "type": "text",
              "text": " if the registration is successful, "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/swift/false"
            },
            {
              "type": "text",
              "text": " otherwise. The only failure condition is if "
            },
