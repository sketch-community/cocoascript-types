        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Constants such as "
            },
            {
              "type": "codeVoice",
              "code": "kAutoGenerateReturnID"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "kAnyTransactionID"
            },
            {
              "type": "text",
              "text": " are defined in "
            },
            {
              "type": "codeVoice",
              "code": "AE.framework"
            },
            {
              "type": "text",
              "text": ", a subframework of "
            },
            {
              "type": "codeVoice",
              "code": "ApplicationServices.framework"
            },
            {
