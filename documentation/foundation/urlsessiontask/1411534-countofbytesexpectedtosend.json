    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The URL loading system can determine the length of the upload data in three ways:"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "From the length of the data object provided as the upload body."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "From the length of the file on disk provided as the upload body of an upload task ("
                    },
                    {
                      "type": "emphasis",
                      "inlineContent": [
                        {
                          "type": "text",
                          "text": "not"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " a download task)."
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "From the "
                    },
                    {
                      "type": "codeVoice",
                      "code": "Content-Length"
                    },
                    {
                      "type": "text",
                      "text": " in the request object, if you explicitly set it."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Otherwise, the value is "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlsessiontransfersizeunknown"
            },
            {
              "type": "text",
              "text": " ("
            },
            {
              "type": "codeVoice",
              "code": "-1"
            },
            {
              "type": "text",
              "text": ") if you provided a stream or body data object, or zero ("
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": ") if you did not."
            }
          ]
        }
      ]
    }
