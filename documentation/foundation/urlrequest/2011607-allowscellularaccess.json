        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Setting this property to "
            },
            {
              "type": "codeVoice",
              "code": "true"
            },
            {
              "type": "text",
              "text": " makes the request eligible to run over cellular, subject to other considerations (including, but not limited to, the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessionconfiguration"
            },
            {
              "type": "text",
              "text": "'s "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessionconfiguration/1409406-allowscellularaccess"
            },
            {
              "type": "text",
              "text": " property). Setting this value to "
            },
            {
              "type": "codeVoice",
              "code": "false"
            },
            {
              "type": "text",
              "text": " ensures that the request will never run over cellular."
            }
          ]
        }
      ]
    }
