      "parameters": [
        {
          "name": "startIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Upon return, contains the index of the first character of the paragraph containing the beginning of "
                },
                {
                  "type": "codeVoice",
                  "code": "aRange"
                },
                {
                  "type": "text",
                  "text": ". Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you do not need this value (in which case the work to compute the value isn’t performed)."
                }
              ]
            }
          ]
        },
        {
          "name": "endIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Upon return, contains the index of the first character past the terminator of the paragraph containing the end of "
                },
                {
                  "type": "codeVoice",
                  "code": "aRange"
                },
                {
                  "type": "text",
                  "text": ". Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you do not need this value (in which case the work to compute the value isn’t performed)."
                }
              ]
            }
          ]
        },
        {
          "name": "contentsEndIndex",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Upon return, contains the index of the first character of the terminator of the paragraph containing the end of "
                },
                {
                  "type": "codeVoice",
                  "code": "aRange"
                },
                {
                  "type": "text",
                  "text": ". Pass "
                },
                {
                  "type": "codeVoice",
                  "code": "NULL"
                },
                {
                  "type": "text",
                  "text": " if you do not need this value (in which case the work to compute the value isn’t performed)."
                }
              ]
            }
          ]
        },
        {
          "name": "aRange",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A range within the receiver. The value must not exceed the bounds of the receiver."
                }
              ]
            }
          ]
        }
      ]
