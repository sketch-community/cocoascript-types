          "inlineContent": [
            {
              "type": "text",
              "text": "An "
            },
            {
              "type": "codeVoice",
              "code": "NSArray"
            },
            {
              "type": "text",
              "text": " object containing substrings from the receiver that have been divided by "
            },
            {
              "type": "codeVoice",
              "code": "separator"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The substrings in the array appear in the order they did in the receiver. Adjacent occurrences of the separator string produce empty strings in the result. Similarly, if the string begins or ends with the separator, the first or last substring, respectively, is empty. For example, this code fragment:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSString *list = @\"Karin, Carrie, David\";",
            "NSArray *listItems = [list componentsSeparatedByString:@\", \"];"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555853",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "produces the array "
            },
            {
              "type": "codeVoice",
              "code": "@[@\"Karin\", @\"Carrie\", @\"David\"]"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "list"
            },
            {
              "type": "text",
              "text": " begins with a comma and space—for example, "
            },
            {
              "type": "codeVoice",
              "code": "@\", Norman, Stanley, Fletcher\""
            },
            {
              "type": "text",
              "text": "—the array has these contents: "
            },
            {
              "type": "codeVoice",
              "code": "@[@\"\", @\"Norman\", @\"Stanley\", @\"Fletcher\"]"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "list"
            },
            {
              "type": "text",
              "text": " has no separators—for example, "
            },
            {
              "type": "codeVoice",
              "code": "@\"Karin\""
            },
            {
              "type": "text",
              "text": "—the array contains the string itself, in this case "
            },
            {
              "type": "codeVoice",
              "code": "@[@\"Karin\"]"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    }
