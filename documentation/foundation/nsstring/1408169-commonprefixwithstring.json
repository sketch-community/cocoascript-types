          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Options for the comparison. The following search options may be specified by combining them with the C bitwise "
                },
                {
                  "type": "codeVoice",
                  "code": "OR"
                },
                {
                  "type": "text",
                  "text": " operator: "
                },
                {
                  "type": "codeVoice",
                  "code": "NSCaseInsensitiveSearch"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "NSLiteralSearch"
                },
                {
                  "type": "text",
                  "text": ". See "
                },
                {
                  "type": "link",
                  "title": "String Programming Guide",
                  "destination": "https://developer.apple.com/library/archive/documentation/Cocoa/Conceptual/Strings/introStrings.html#//apple_ref/doc/uid/10000035i"
                },
                {
                  "type": "text",
                  "text": " for details on these options."
                }
              ]
