      "parameters": [
        {
          "name": "buffer",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Upon return, contains the characters from the receiver. "
                },
                {
                  "type": "codeVoice",
                  "code": "buffer"
                },
                {
                  "type": "text",
                  "text": " must be large enough to contain the characters in the range "
                },
                {
                  "type": "codeVoice",
                  "code": "aRange"
                },
                {
                  "type": "text",
                  "text": " ("
                },
                {
                  "type": "codeVoice",
                  "code": "aRange.length*sizeof(unichar)"
                },
                {
                  "type": "text",
                  "text": ")."
                }
              ]
            }
          ]
        },
        {
          "name": "aRange",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The range of characters to retrieve. The range must not exceed the bounds of the receiver."
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Raises an "
                    },
                    {
                      "type": "codeVoice",
                      "code": "NSRangeException"
                    },
                    {
                      "type": "text",
                      "text": " if any part of "
                    },
                    {
                      "type": "codeVoice",
                      "code": "aRange"
                    },
                    {
                      "type": "text",
                      "text": " lies beyond the bounds of the receiver."
                    }
                  ]
                }
              ],
              "style": "important"
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method does not add a "
            },
            {
              "type": "codeVoice",
              "code": "NULL"
            },
            {
              "type": "text",
              "text": " character. "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The abstract implementation of this method uses "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1414645-characteratindex"
            },
            {
              "type": "text",
              "text": " repeatedly, correctly extracting the characters, though very inefficiently. Subclasses should override it to provide a fast implementation."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "You should always use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1416036-rangeofcomposedcharactersequence"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1410993-rangeofcomposedcharactersequence"
            },
            {
              "type": "text",
              "text": " method to determine character boundaries, so that any surrogate pairs or character clusters are handled correctly."
            }
          ]
        }
      ]
    }
