      "parameters": [
        {
          "name": "capacity",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The number of bytes the data object can initially contain."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "An initialized "
            },
            {
              "type": "codeVoice",
              "code": "NSMutableData"
            },
            {
              "type": "text",
              "text": " object capable of holding "
            },
            {
              "type": "codeVoice",
              "code": "capacity"
            },
            {
              "type": "text",
              "text": " bytes."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The returned object has the same memory alignment guarantees as "
            },
            {
              "type": "codeVoice",
              "code": "malloc(_:)"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This method doesn’t necessarily allocate the requested memory right away. Mutable data objects allocate additional memory as needed, so "
            },
            {
              "type": "codeVoice",
              "code": "capacity"
            },
            {
              "type": "text",
              "text": " simply establishes the object’s initial capacity. When it does allocate the initial memory, though, it allocates the specified amount. This method sets the length of the data object to "
            },
            {
              "type": "codeVoice",
              "code": "0"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the capacity specified in "
            },
            {
              "type": "codeVoice",
              "code": "capacity"
            },
            {
              "type": "text",
              "text": " is greater than four memory pages in size, this method may round the amount of requested memory up to the nearest full page."
            }
          ]
        }
      ]
    }
