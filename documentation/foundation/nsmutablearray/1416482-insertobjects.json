      ],
      "parameters": [
        {
          "name": "objects",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "An array of objects to insert into the receiving array."
                }
              ]
            }
          ]
        },
        {
          "name": "indexes",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The indexes at which the objects in "
                },
                {
                  "type": "codeVoice",
                  "code": "objects"
                },
                {
                  "type": "text",
                  "text": " should be inserted. The count of locations in "
                },
                {
                  "type": "codeVoice",
                  "code": "indexes"
                },
                {
                  "type": "text",
                  "text": " must equal the count of "
                },
                {
                  "type": "codeVoice",
                  "code": "objects"
                },
                {
                  "type": "text",
                  "text": ". For more details, see the Discussion."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Each object in "
            },
            {
              "type": "codeVoice",
              "code": "objects"
            },
            {
              "type": "text",
              "text": " is inserted into the receiving array in turn at the corresponding location specified in "
            },
            {
              "type": "codeVoice",
              "code": "indexes"
            },
            {
              "type": "text",
              "text": " after earlier insertions have been made. The implementation is conceptually similar to that illustrated in the following example:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "- void insertObjects:(NSArray *)additions atIndexes:(NSIndexSet *)indexes",
            "{",
            "    NSUInteger currentIndex = [indexes firstIndex];",
            "    NSUInteger i, count = [indexes count];",
            " ",
            "    for (i = 0; i < count; i++)",
            "    {",
            "        [self insertObject:[additions objectAtIndex:i] atIndex:currentIndex];",
            "        currentIndex = [indexes indexGreaterThanIndex:currentIndex];",
            "    }",
            "}"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555712",
            "title": "Listing 1"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The resulting behavior is illustrated by the following example: "
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSMutableArray *array = [NSMutableArray arrayWithObjects: @\"one\", @\"two\", @\"three\", @\"four\", nil];",
            "NSArray *newAdditions = [NSArray arrayWithObjects: @\"a\", @\"b\", nil];",
            "NSMutableIndexSet *indexes = [NSMutableIndexSet indexSetWithIndex:1];",
            "[indexes addIndex:3];",
            "[array insertObjects:newAdditions atIndexes:indexes];",
            "NSLog(@\"array: %@\", array);",
            " ",
            "// Output: array: (one, a, two, b, three, four)"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555716",
            "title": "Listing 2"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The locations specified by "
            },
            {
              "type": "codeVoice",
              "code": "indexes"
            },
            {
              "type": "text",
              "text": " may therefore only exceed the bounds of the receiving array if one location specifies the count of the array or the count of the array after preceding insertions, and other locations exceeding the bounds do so in a contiguous fashion from that location, as illustrated in the following examples."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, both new objects are appended to the end of the array."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSMutableArray *array = [NSMutableArray arrayWithObjects: @\"one\", @\"two\", @\"three\", @\"four\", nil];",
            "NSArray *newAdditions = [NSArray arrayWithObjects: @\"a\", @\"b\", nil];",
            "NSMutableIndexSet *indexes = [NSMutableIndexSet indexSetWithIndex:5];",
            "[indexes addIndex:4];",
            "[array insertObjects:newAdditions atIndexes:indexes];",
            "NSLog(@\"array: %@\", array);",
            " ",
            "// Output: array: (one, two, three, four, a, b)"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555718",
            "title": "Listing 3"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you replace "
            },
            {
              "type": "codeVoice",
              "code": "[indexes addIndex:4]"
            },
            {
              "type": "text",
              "text": " with "
            },
            {
              "type": "codeVoice",
              "code": "[indexes addIndex:6]"
            },
            {
              "type": "text",
              "text": " (so that the indexes are 5 and 6), then the application will fail with an out of bounds exception."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "In this example, two objects are added into the middle of the array, and another at the current end of the array (index 4) which means that it is third from the end of the modified array."
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "NSMutableArray *array = [NSMutableArray arrayWithObjects: @\"one\", @\"two\", @\"three\", @\"four\", nil];",
            "NSArray *newAdditions = [NSArray arrayWithObjects: @\"a\", @\"b\", @\"c\", nil];",
            "NSMutableIndexSet *indexes = [NSMutableIndexSet indexSetWithIndex:1];",
            "[indexes addIndex:2];",
            "[indexes addIndex:4];",
            "[array insertObjects:newAdditions atIndexes:indexes];",
            "NSLog(@\"array: %@\", array);",
            " ",
            "// Output: array: (one, a, b, two, c, three, four)"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "2555721",
            "title": "Listing 4"
          }
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you replace "
            },
            {
              "type": "codeVoice",
              "code": "[indexes addIndex:4]"
            },
            {
              "type": "text",
              "text": " with "
            },
            {
              "type": "codeVoice",
              "code": "[indexes addIndex:6]"
            },
            {
              "type": "text",
              "text": " (so that the indexes are 1, 2, and 6), then the output is "
            },
            {
              "type": "codeVoice",
              "code": "(one, a, b, two, three, four, c)"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If "
            },
            {
              "type": "codeVoice",
              "code": "objects"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "indexes"
            },
            {
              "type": "text",
              "text": " is "
            },
            {
              "type": "codeVoice",
              "code": "nil"
            },
            {
              "type": "text",
              "text": ", this method will raises an exception."
            }
          ]
        }
      ]
    }
