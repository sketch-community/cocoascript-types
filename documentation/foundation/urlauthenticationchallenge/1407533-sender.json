    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If you are using the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession"
            },
            {
              "type": "text",
              "text": " API, this value is purely informational, because you "
            },
            {
              "type": "emphasis",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "must"
                }
              ]
            },
            {
              "type": "text",
              "text": " respond to authentication challenges in your "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessiondelegate"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessiontaskdelegate"
            },
            {
              "type": "text",
              "text": " implementations, by passing "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession/authchallengedisposition"
            },
            {
              "type": "text",
              "text": " constants to the provided completion handler blocks."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "However, if you are using the legacy "
            },
            {
              "type": "codeVoice",
              "code": "NSURLConnection"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "codeVoice",
              "code": "NSURLDownload"
            },
            {
              "type": "text",
              "text": " API, you use this object directly in your authentication handler delegate method. With these APIs, after you finish processing the authentication challenge, you respond by calling methods defined in the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlauthenticationchallengesender"
            },
            {
              "type": "text",
              "text": " protocol on this sender."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Do not call methods directly on this object if you are using the "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession"
                },
                {
                  "type": "text",
                  "text": " API."
                }
              ]
            }
          ],
          "style": "warning"
        },
        {
          "type": "paragraph",
          "inlineContent": []
        }
      ]
    }
