    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "name": "key",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The key for a string in the specified table."
                }
              ]
            },
            {
              "type": "aside",
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Xcode can’t export localizations for strings whose "
                    },
                    {
                      "type": "codeVoice",
                      "code": "key"
                    },
                    {
                      "type": "text",
                      "text": " is a string variable, "
                    },
                    {
                      "type": "codeVoice",
                      "code": "nil"
                    },
                    {
                      "type": "text",
                      "text": ", or an empty string."
                    }
                  ]
                }
              ],
              "style": "note",
              "name": "Note"
            }
          ]
        },
        {
          "name": "tableName",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The name of the table containing the key-value pairs. Also, the suffix for the strings file (a file with the "
                },
                {
                  "type": "codeVoice",
                  "code": ".strings"
                },
                {
                  "type": "text",
                  "text": " extension) to store the localized string. The default table in "
                },
                {
                  "type": "codeVoice",
                  "code": "Localizable.strings "
                },
                {
                  "type": "text",
                  "text": "is used when "
                },
                {
                  "type": "codeVoice",
                  "code": "tableName"
                },
                {
                  "type": "text",
                  "text": " is "
                },
                {
                  "type": "codeVoice",
                  "code": "nil"
                },
                {
                  "type": "text",
                  "text": " or an empty string."
                }
              ]
            }
          ]
        },
        {
          "name": "comment",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The comment to place above the key-value pair in the strings file. This parameter provides the translator with some context about the localized string’s presentation to the user."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The result of invoking "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbundle/1417694-localizedstringforkey"
            },
            {
              "type": "text",
              "text": " on the main bundle, passing the specified "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " and "
            },
            {
              "type": "codeVoice",
              "code": "tableName"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this macro to automatically generate a strings file named "
            },
            {
              "type": "codeVoice",
              "code": "[tableName].strings"
            },
            {
              "type": "text",
              "text": " from your code when exporting localizations from Xcode or the "
            },
            {
              "type": "codeVoice",
              "code": "genstrings"
            },
            {
              "type": "text",
              "text": " utility. You can specify Unicode characters in "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " using "
            },
            {
              "type": "codeVoice",
              "code": "\\\\Uxxxx"
            },
            {
              "type": "text",
              "text": "—see the "
            },
            {
              "type": "codeVoice",
              "code": "-u"
            },
            {
              "type": "text",
              "text": " option for the "
            },
            {
              "type": "codeVoice",
              "code": "genstrings"
            },
            {
              "type": "text",
              "text": " utility."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The initial value for "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " in the strings file is "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": ". To avoid collisions between words or phrases with multiple meanings, use a unique "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": " for each use of the same phrase. Use the "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nslocalizedstringwithdefaultvalue"
            },
            {
              "type": "text",
              "text": " macro to specify another value for "
            },
            {
              "type": "codeVoice",
              "code": "key"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "For information about inserting plural nouns and units into localized strings, see "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/xcode/localizing-strings-that-contain-plurals"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "As of OS X 10.11 and iOS 9, "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbundle"
            },
            {
              "type": "text",
              "text": " is thread-safe. As such, you can safely call "
            },
            {
              "type": "reference",
              "isActive": false,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nslocalizedstringfromtable"
            },
            {
              "type": "text",
              "text": " from any execution context."
            }
          ]
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The values for "
                },
                {
                  "type": "codeVoice",
                  "code": "key"
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "codeVoice",
                  "code": "tableName"
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "codeVoice",
                  "code": "comment"
                },
                {
                  "type": "text",
                  "text": " must be string literal values. Xcode can read these values from source code to automatically create localization tables when exporting localizations, but it doesn’t resolve string variables. If you want to use string variables, manually create a strings file and use "
                },
                {
                  "type": "reference",
                  "isActive": false,
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsbundle/1417694-localizedstringforkey"
                },
                {
                  "type": "text",
                  "text": " instead. For more information on strings files, see "
                },
                {
                  "type": "link",
                  "title": "String Resources",
                  "destination": "https://developer.apple.com/library/archive/documentation/Cocoa/Conceptual/LoadingResources/Strings/Strings.html#//apple_ref/doc/uid/10000051i-CH6"
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ],
          "style": "important"
        },
        {
          "type": "aside",
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "Break up long string values into consecutive string literals."
                }
              ]
            }
          ],
          "style": "tip"
        },
        {
          "type": "codeListing",
          "code": [
            "NSLocalizedStringFromTable(",
            "    @\"Did you know that venus flytraps have flowers\"",
            "        @\" atop very long stems?\\nThe long stem keeps\"",
            "        @\" insects a safe distance away from their\"",
            "        @\" digestive leaves below.\",",
            "    @\"Localized\",",
            "    @\"An interesting fact about venus flytraps shown\"",
            "        @\" on the loading screen.\");"
          ],
          "syntax": "occ",
          "metadata": {
            "anchor": "3783135",
            "title": "Listing 1"
          }
        }
      ]
    }
