    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "If the bundle’s executable code is not yet loaded, this method dynamically loads it into memory. Classes (and categories) are loaded from just one file within the bundle directory; this code file has the same name as the directory, but without the extension (“"
            },
            {
              "type": "codeVoice",
              "code": ".bundle"
            },
            {
              "type": "text",
              "text": "”, “"
            },
            {
              "type": "codeVoice",
              "code": ".app"
            },
            {
              "type": "text",
              "text": "”, “"
            },
            {
              "type": "codeVoice",
              "code": ".framework"
            },
            {
              "type": "text",
              "text": "”). As a side effect of code loading, the receiver posts "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/bundle/1416137-didloadnotification"
            },
            {
              "type": "text",
              "text": " after all classes and categories have been loaded; see "
            },
            {
              "type": "codeVoice",
              "code": "Notifications"
            },
            {
              "type": "text",
              "text": " for details. "
            }
          ]
        }
      ]
    }
