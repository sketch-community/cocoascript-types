        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Raises an "
            },
            {
              "type": "codeVoice",
              "code": "NSRangeException"
            },
            {
              "type": "text",
              "text": " if "
            },
            {
              "type": "codeVoice",
              "code": "index"
            },
            {
              "type": "text",
              "text": " is beyond the end of the string being scanned."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "This property is useful for backing up to rescan after an error."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Rather than setting the scan location directly to skip known sequences of characters, use "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsscanner/1407591-scanstring"
            },
            {
              "type": "text",
              "text": " or "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsscanner/1417575-scancharactersfromset"
            },
            {
              "type": "text",
              "text": ", which allow you to verify that the expected substring (or set of characters) is in fact present."
            }
          ]
        }
      ]
    }
