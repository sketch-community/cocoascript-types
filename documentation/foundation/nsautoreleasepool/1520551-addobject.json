      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The same object may be added several times to the active pool and, when the pool is deallocated, it will receive a "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/1418956-nsobject/1571957-release"
            },
            {
              "type": "text",
              "text": " message for each time it was added."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Normally you don’t invoke this method directly—you send "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/objectivec/1418956-nsobject/1571951-autorelease"
            },
            {
              "type": "text",
              "text": " to "
            },
            {
              "type": "codeVoice",
              "code": "object"
            },
            {
              "type": "text",
              "text": " instead."
            }
          ]
        }
      ]
    }
