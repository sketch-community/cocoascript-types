      "kind": "content",
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "A localized date format string representing the date format components given in "
            },
            {
              "type": "codeVoice",
              "code": "template"
            },
            {
              "type": "text",
              "text": ", arranged appropriately for the locale specified by "
            },
            {
              "type": "codeVoice",
              "code": "locale"
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The returned string may not contain exactly those components given in "
            },
            {
              "type": "codeVoice",
              "code": "template"
            },
            {
              "type": "text",
              "text": ", but may—for example—have locale-specific adjustments applied."
            }
          ]
        }
      ]
    },
    {
      "kind": "content",
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Different locales have different conventions for the ordering of date components. You use this method to get an appropriate format string for a given set of components for a specified locale (typically you use the current locale—see "
            },
            {
              "type": "reference",
              "isActive": true,
              "identifier": "doc://com.apple.documentation/documentation/foundation/nslocale/1409990-current"
            },
            {
              "type": "text",
              "text": "). "
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "The following example shows the difference between the date formats for British and American English:"
            }
          ]
        },
        {
          "type": "codeListing",
          "code": [
            "let usLocale = Locale(identifier: \"en_US\")",
            "let gbLocale = Locale(identifier: \"en_GB\")",
            "let template = \"yMMMMd\"",
            " ",
            "let usDateFormat = DateFormatter.dateFormat(fromTemplate: template, options: 0, locale: usLocale)!",
            "// Date format for English (United States): \"MMMM d, y\"",
            "let gbDateFormat = DateFormatter.dateFormat(fromTemplate: template, options: 0, locale: gbLocale)!",
            "// Date format for English (United Kingdom): \"d MMMM y\""
          ],
          "syntax": "swift",
          "metadata": {
            "anchor": "2555590",
            "title": "Listing 1"
          }
        }
      ]
